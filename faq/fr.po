#: faq-en.html:585
msgid "/usr/src/glibc-build/sysd-dirs-tmp"
msgstr ""

#. type: Content of: <dl><dd><div><p>
#: faq-en.html:586
msgid "/bin/sh: line 1: BEGIN { subdirs = \"\"; inhibit = \"\" };"
msgstr ""

#. type: Content of: <dl><dd><div><p>
#: faq-en.html:587
