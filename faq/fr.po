msgid "gcc main.c"
msgstr "gcc main.c"

#. type: Content of: <dl><dl><dd><div><p>
#: complete.html:886 complete.html:894
msgid "sk ~/tmp $ cat &gt; main.c"
msgstr "sk ~/tmp $ cat &gt; main.c"

#. type: Content of: <dl><dl><dd><div><p>
#: complete.html:887
msgid "void main() { exit(0); }"
msgstr "void main() { exit(0); }"

#. type: Content of: <dl><dl><dd><div><p>
