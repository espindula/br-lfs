msgid "cd /usr"
msgstr "cd /usr"

#. type: Content of: <dl><dl><dd><div><p>
#: complete.html:586
msgid "ln -s X11R6 X11"
msgstr "ln -s X11R6 X11"

#. type: Content of: <dl><dl><dd><div><p>
#: complete.html:587
msgid "cd include"
