      </listitem>
    </itemizedlist>

    <bridgehead renderas="sect3">Dépendances de GNOME Keyring Manager</bridgehead>

    <bridgehead renderas="sect4">Required</bridgehead>
    <para role="required"><xref linkend="libgnomeui"/> and
    <xref linkend="gnome-doc-utils"/></para>

