<sect1 id="telepathy-farstream" xreflabel="telepathy-farstream-&telepathy-farstream-version;">
  <?dbhtml filename="telepathy-farstream.html"?>
  <sect1info>
    <othername>$LastChangedBy: krejzi $</othername>
