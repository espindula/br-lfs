"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/lib/gtk-vnc.xml:109
#, no-wrap
#| msgid ""
#| "<userinput>mkdir build &amp;&amp;\n"
#| "cd    build &amp;&amp;\n"
#| "\n"
#| "meson --prefix=/usr .. &amp;&amp;\n"
#| "ninja</userinput>"
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson --prefix=/usr --buildtype=release .. &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson --prefix=/usr --buildtype=release .. &amp;&amp;\n"
"ninja</userinput>"

