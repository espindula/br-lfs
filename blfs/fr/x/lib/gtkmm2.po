#. type: Content of: <sect1><sect2><title>
#: blfs-en/x/lib/gtkmm2.xml:90
msgid "Installation of Gtkmm"
msgstr "Installation de Gtkmm"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/gtkmm2.xml:93
msgid "First, fix the documents directory name:"
msgstr "Pour commencer, corrigez le nom du répertoire de documentation:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/lib/gtkmm2.xml:96
#, no-wrap
msgid ""
"<userinput>sed -e '/^libdocdir =/ s/$(book_name)/gtkmm-&gtkmm2-version;/' \\\n"
"    -i docs/Makefile.in</userinput>"
msgstr ""
"<userinput>sed -e '/^libdocdir =/ s/$(book_name)/gtkmm-&gtkmm2-version;/' \\\n"
"    -i docs/Makefile.in</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/gtkmm2.xml:100
msgid ""
"Install <application>Gtkmm</application> by running the following commands:"
msgstr ""
"Installez <application>Gtkmm</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/lib/gtkmm2.xml:104
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/gtkmm2.xml:108
msgid "To test the results, issue: <command>make check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/gtkmm2.xml:112
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/lib/gtkmm2.xml:115
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

