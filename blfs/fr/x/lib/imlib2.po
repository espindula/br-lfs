#: blfs-en/x/lib/imlib2.xml:100
msgid "Installation of imlib2"
msgstr "Installation de imlib2"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/imlib2.xml:103
msgid ""
"Install <application>imlib2</application> by running the following commands:"
msgstr ""
"Installez <application>imlib2</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/lib/imlib2.xml:107
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/imlib2.xml:111
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'a pas de suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/imlib2.xml:115
msgid "Now, as the &root; user:"
msgstr "Maintenant, en tant qu'utilisateur &root;&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/lib/imlib2.xml:118
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/imlib2.xml:121
msgid "If you generated the API documentation, install it as the &root; user:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/lib/imlib2.xml:124
#, no-wrap
msgid ""
"<userinput>install -v -m755 -d            /usr/share/doc/imlib2-&imlib2-version;/html &amp;&amp;\n"
"install -v -m644    doc/html/* /usr/share/doc/imlib2-&imlib2-version;/html</userinput>"
msgstr ""
"<userinput>install -v -m755 -d            /usr/share/doc/imlib2-&imlib2-version;/html &amp;&amp;\n"
