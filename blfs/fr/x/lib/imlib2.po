#. type: Content of: <sect1><sect2><title>
#: blfs-en/x/lib/imlib2.xml:98
msgid "Installation of imlib2"
msgstr "Installation de imlib2"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/imlib2.xml:101
msgid ""
"Install <application>imlib2</application> by running the following commands:"
msgstr ""
"Installez <application>imlib2</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/lib/imlib2.xml:105
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/imlib2.xml:109
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/imlib2.xml:113
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/lib/imlib2.xml:116
#, no-wrap
msgid ""
"<userinput>make install &amp;&amp;\n"
"install -v -m755 -d                        /usr/share/doc/imlib2-&imlib2-version; &amp;&amp;\n"
"install -v -m644    doc/{*.gif,index.html} /usr/share/doc/imlib2-&imlib2-version;</userinput>"
msgstr ""
"<userinput>make install &amp;&amp;\n"
"install -v -m755 -d                        /usr/share/doc/imlib2-&imlib2-version; &amp;&amp;\n"
"install -v -m644    doc/{*.gif,index.html} /usr/share/doc/imlib2-&imlib2-version;</userinput>"

