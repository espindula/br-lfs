#| "            -system-harfbuzz           \\\n"
#| "            -system-sqlite             \\\n"
#| "            -nomake examples           \\\n"
#| "            -no-rpath                  \\\n"
#| "            -optimized-qmake           \\\n"
#| "            -skip qtwebengine          &amp;&amp;\n"
#| "make</userinput>"
