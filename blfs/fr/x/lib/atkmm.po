#. type: Content of: <sect1><sect2><title>
#: blfs-en/x/lib/atkmm.xml:86
msgid "Installation of Atkmm"
msgstr "Installation de Atkmm"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/atkmm.xml:89
msgid ""
"Install <application>Atkmm</application> by running the following commands:"
msgstr ""
"Installez <application>Atkmm</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/lib/atkmm.xml:93
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson --prefix=/usr --buildtype=release .. &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson --prefix=/usr --buildtype=release .. &amp;&amp;\n"
"ninja</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/atkmm.xml:100
msgid "This package does not come with a testsuite."
msgstr "Ce paquet n'a pas de suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/atkmm.xml:104
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/lib/atkmm.xml:107
#, no-wrap
msgid "<userinput>ninja install</userinput>"
msgstr "<userinput>ninja install</userinput>"

