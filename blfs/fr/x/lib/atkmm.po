#. type: Content of: <sect1><sect2><title>
#: blfs-en/x/lib/atkmm.xml:87
msgid "Installation of Atkmm"
msgstr "Installation de Atkmm"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/atkmm.xml:90
msgid "First, fix the documents directory name:"
msgstr "Pour commencer, corrigez le nom du répertoire de documentation&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/lib/atkmm.xml:93
#, no-wrap
msgid ""
"<userinput>sed -e '/^libdocdir =/ s/$(book_name)/atkmm-&atkmm-version;/' \\\n"
"    -i doc/Makefile.in</userinput>"
msgstr ""
"<userinput>sed -e '/^libdocdir =/ s/$(book_name)/atkmm-&atkmm-version;/' \\\n"
"    -i doc/Makefile.in</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/atkmm.xml:97
msgid ""
"Install <application>Atkmm</application> by running the following commands:"
msgstr ""
"Installez <application>Atkmm</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/lib/atkmm.xml:101
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/atkmm.xml:105
msgid "This package does not come with a testsuite."
msgstr "Ce paquet n'a pas de suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/atkmm.xml:109
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem class=\"username\">root</"
"systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/lib/atkmm.xml:112
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

