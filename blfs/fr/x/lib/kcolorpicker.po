msgid "Installation of kColorPicker"
msgstr "Installation de kColorPicker"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/kcolorpicker.xml:83
msgid ""
"Install <application>kColorPicker</application> by running the following "
"commands:"
msgstr ""
"Installez <application>kColorPicker</application> en exécutant les commandes"
" suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/lib/kcolorpicker.xml:87
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      .. &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      .. &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
