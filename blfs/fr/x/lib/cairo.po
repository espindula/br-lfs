"<userinput>./configure --prefix=/usr    \\\n"
"            --disable-static \\\n"
"            --enable-tee &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr    \\\n"
"            --disable-static \\\n"
"            --enable-tee &amp;&amp;\n"
"make</userinput>"

