#. type: Content of: <sect1><sect2><title>
#: blfs-en/x/lib/libdazzle.xml:91
msgid "Installation of libdazzle"
msgstr "Installation de libdazzle"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/libdazzle.xml:94
msgid ""
"Install <application>libdazzle</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libdazzle</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/lib/libdazzle.xml:98
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson --prefix=/usr .. &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson --prefix=/usr .. &amp;&amp;\n"
"ninja</userinput>"

#. Two tests may fail due to a theme parsing error.
#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/libdazzle.xml:105
msgid "To test the results, issue: <command>ninja test</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>ninja test</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/libdazzle.xml:110
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/lib/libdazzle.xml:113
#, no-wrap
msgid "<userinput>ninja install</userinput>"
msgstr "<userinput>ninja install</userinput>"

