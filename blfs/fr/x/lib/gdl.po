#: blfs-en/x/lib/gdl.xml:86
msgid "Installation of GDL"
msgstr "Installation de GDL"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/gdl.xml:89
msgid ""
"Install <application>GDL</application> by running the following commands:"
msgstr ""
"Installez <application>GDL</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/lib/gdl.xml:93
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/gdl.xml:97
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/gdl.xml:101
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/lib/gdl.xml:104
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

