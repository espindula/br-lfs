#: blfs-en/x/lib/freeglut.xml:102
msgid "Installation of Freeglut"
msgstr "Installation de Freeglut"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/freeglut.xml:111
msgid ""
"Install <application>Freeglut</application> by running the following "
"commands:"
msgstr ""
"Installez <application>Freeglut</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/lib/freeglut.xml:115
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr       \\\n"
"      -DCMAKE_BUILD_TYPE=Release        \\\n"
"      -DFREEGLUT_BUILD_DEMOS=OFF        \\\n"
"      -DFREEGLUT_BUILD_STATIC_LIBS=OFF  \\\n"
"      -Wno-dev .. &amp;&amp;\n"
"\n"
"make</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr       \\\n"
"      -DCMAKE_BUILD_TYPE=Release        \\\n"
"      -DFREEGLUT_BUILD_DEMOS=OFF        \\\n"
"      -DFREEGLUT_BUILD_STATIC_LIBS=OFF  \\\n"
"      -Wno-dev .. &amp;&amp;\n"
"\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/lib/freeglut.xml:127
