#. type: Content of: <sect1><sect2><title>
#: blfs-en/x/icons/oxygen-icons5.xml:87
msgid "Installation of oxygen-icons5"
msgstr "Installation de oxygen-icons5"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/icons/oxygen-icons5.xml:90
msgid "First, enable scalable icons:"
msgstr "D'abord, activez les icônes redimensionnables&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/icons/oxygen-icons5.xml:93
#, no-wrap
msgid ""
"<userinput>sed -i '/( oxygen/ s/)/scalable )/' CMakeLists.txt</userinput>"
msgstr ""
"<userinput>sed -i '/( oxygen/ s/)/scalable )/' CMakeLists.txt</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/icons/oxygen-icons5.xml:96
msgid ""
"Install <application>oxygen-icons5</application> by running the following "
"commands:"
msgstr ""
"Installez <application>oxygen-icons5</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/icons/oxygen-icons5.xml:100
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr -Wno-dev ..</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr -Wno-dev ..</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/icons/oxygen-icons5.xml:106
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/icons/oxygen-icons5.xml:110
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/icons/oxygen-icons5.xml:113
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

