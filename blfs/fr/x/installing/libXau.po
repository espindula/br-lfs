#: blfs-en/x/installing/libXau.xml:80
msgid "Installation of libXau"
msgstr "Installation de libXau"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/installing/libXau.xml:83
msgid ""
"Install <application>libXau</application> by running the following commands:"
msgstr ""
"Installez <application>libXau</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/installing/libXau.xml:87
#, no-wrap
msgid ""
"<userinput>./configure $XORG_CONFIG &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure $XORG_CONFIG &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/installing/libXau.xml:91
msgid "To test the results, issue: <command>make check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/installing/libXau.xml:95
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/installing/libXau.xml:98
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

