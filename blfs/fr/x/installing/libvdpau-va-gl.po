"<userinput>sed -e '/#include &lt;stdlib.h&gt;/a #include &lt;string&gt;' -i src/api-device.cc &amp;&amp;\n"
"\n"
"mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_BUILD_TYPE=Release -DCMAKE_INSTALL_PREFIX=$XORG_PREFIX .. &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>sed -e '/#include &lt;stdlib.h&gt;/a #include &lt;string&gt;' -i src/api-device.cc &amp;&amp;\n"
"\n"
"mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_BUILD_TYPE=Release -DCMAKE_INSTALL_PREFIX=$XORG_PREFIX .. &amp;&amp;\n"
"make</userinput>"

