msgid "Installation of xcb-util-wm"
msgstr "Installation de xcb-util-wm"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/installing/xcb-util-wm.xml:95
msgid ""
"Install <application>xcb-util-wm</application> by running the following "
"commands:"
msgstr ""
"Installez <application>xcb-util-wm</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/installing/xcb-util-wm.xml:99
#, no-wrap
msgid ""
"<userinput>./configure $XORG_CONFIG &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure $XORG_CONFIG &amp;&amp;\n"
"make</userinput>"

#. To test the results, issue:
#.       <command>
#. LD_LIBRARY_PATH=$XORG_PREFIX/lib make check</command>.
#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/installing/xcb-util-wm.xml:103
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/installing/xcb-util-wm.xml:109
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/installing/xcb-util-wm.xml:112
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

