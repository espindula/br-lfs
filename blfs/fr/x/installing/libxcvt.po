#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/installing/libxcvt.xml:85
msgid ""
"Install <application>libxcvt</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libxcvt</application> en lanant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/x/installing/libxcvt.xml:89
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson setup --prefix=$XORG_PREFIX --buildtype=release .. &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson setup --prefix=$XORG_PREFIX --buildtype=release .. &amp;&amp;\n"
"ninja</userinput>"

