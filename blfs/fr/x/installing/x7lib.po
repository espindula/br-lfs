#: blfs-en/x/installing/x7lib.xml:165
msgid "Required"
msgstr "Requises"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/x/installing/x7lib.xml:167
msgid "<xref linkend=\"fontconfig\"/> and <xref linkend=\"libxcb\"/>"
