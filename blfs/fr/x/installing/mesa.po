#| "            --sysconfdir=/etc               \\\n"
#| "            --enable-texture-float          \\\n"
#| "            --enable-gles1                  \\\n"
#| "            --enable-gles2                  \\\n"
#| "            --enable-osmesa                 \\\n"
#| "            --enable-xa                     \\\n"
#| "            --enable-gbm                    \\\n"
#| "            --enable-glx-tls                \\\n"
#| "            --with-egl-platforms=\"drm,x11\"  \\\n"
