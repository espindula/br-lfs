#. type: Content of: <sect1><sect2><title>
#: blfs-en/kde/add/libkcddb.xml:87
msgid "Installation of libkcddb"
msgstr "Installation de libkcddb"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/kde/add/libkcddb.xml:90
msgid ""
"Install <application>libkcddb</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libkcddb</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/kde/add/libkcddb.xml:94
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=&kde-dir;   \\\n"
"      -DCMAKE_BUILD_TYPE=Release           \\\n"
"      -DCMAKE_MINIMUM_REQUIRED_VERSION=3.0 \\\n"
"      -Wno-dev .. &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=&kde-dir;   \\\n"
"      -DCMAKE_BUILD_TYPE=Release           \\\n"
"      -DCMAKE_MINIMUM_REQUIRED_VERSION=3.0 \\\n"
"      -Wno-dev .. &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/kde/add/libkcddb.xml:104
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem class=\"username\">root</"
"systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/kde/add/libkcddb.xml:107
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

