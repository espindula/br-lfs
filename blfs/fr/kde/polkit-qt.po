#. type: Content of: <sect1><sect2><title>
#: blfs-en/kde/polkit-qt.xml:85
msgid "Installation of Polkit-Qt"
msgstr "Installation de Polkit-Qt"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/kde/polkit-qt.xml:87
msgid ""
"Install the Qt4 based version of <application>Polkit-Qt</application> by "
"running the following commands:"
msgstr ""
"Installez the Qt4 based version of <application>Polkit-Qt</application> en "
"lançant les commandes suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/kde/polkit-qt.xml:90
#, no-wrap
msgid ""
"<userinput>source /usr/bin/setqt4 &amp;&amp;\n"
"\n"
"mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      -DCMAKE_INSTALL_LIBDIR=lib  \\\n"
"      -DUSE_QT4=TRUE              \\\n"
"      -Wno-dev .. &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>source /usr/bin/setqt4 &amp;&amp;\n"
"\n"
"mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      -DCMAKE_INSTALL_LIBDIR=lib  \\\n"
"      -DUSE_QT4=TRUE              \\\n"
"      -Wno-dev .. &amp;&amp;\n"
"make</userinput>"

