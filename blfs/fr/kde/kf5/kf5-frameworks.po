"ln -sfv /etc/dbus-1         $KF5_PREFIX/etc         &amp;&amp;\n"
"ln -sfv /usr/share/dbus-1   $KF5_PREFIX/share       &amp;&amp;\n"
"ln -sfv /usr/share/polkit-1 $KF5_PREFIX/share</userinput>"
