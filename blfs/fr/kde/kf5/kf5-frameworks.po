"          src/klineedit.cpp ;;\n"
"        kwayland*) sed -i '/&lt;wayland-xdg-output-server-proto/a #include &lt;QHash&gt;' \\\n"
"          src/server/xdgoutput_interface.cpp ;;\n"
"      esac  \n"
