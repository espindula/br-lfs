#, no-wrap
msgid ""
"<userinput>cat &gt; ~/.xinitrc &lt;&lt; \"EOF\"\n"
"<literal>dbus-launch --exit-with-session $KF5_PREFIX/bin/startplasma-x11</literal>\n"
"EOF\n"
"\n"
"startx</userinput>"
