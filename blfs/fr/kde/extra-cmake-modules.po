msgstr "Installation de Extra Cmake Modules"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/kde/extra-cmake-modules.xml:98
msgid "First, fix detection of Python-3.10 modules:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/kde/extra-cmake-modules.xml:101
#, no-wrap
msgid ""
"<userinput>sed 's/10)/11)/' -i find-"
"modules/FindPythonModuleGeneration.cmake</userinput>"
msgstr ""
"<userinput>sed 's/10)/11)/' -i find-"
"modules/FindPythonModuleGeneration.cmake</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/kde/extra-cmake-modules.xml:105
msgid ""
"Install <application>Extra Cmake Modules</application> by running the "
"following commands:"
msgstr ""
"Installez <application>Extra Cmake Modules</application> en lançant les "
"commandes suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/kde/extra-cmake-modules.xml:109
#, no-wrap
#| msgid ""
#| "<userinput>sed -i '/\"lib64\"/s/64//' kde-modules/KDEInstallDirs.cmake &amp;&amp;\n"
#| "\n"
#| "sed -e '/PACKAGE_INIT/i set(SAVE_PACKAGE_PREFIX_DIR \"${PACKAGE_PREFIX_DIR}\")' \\\n"
#| "    -e '/^include/a set(PACKAGE_PREFIX_DIR \"${SAVE_PACKAGE_PREFIX_DIR}\")' \\\n"
#| "    -i ECMConfig.cmake.in &amp;&amp;\n"
#| "\n"
#| "mkdir build &amp;&amp;\n"
#| "cd    build &amp;&amp;\n"
#| "\n"
#| "cmake -DCMAKE_INSTALL_PREFIX=/usr .. &amp;&amp;\n"
#| "make</userinput>"
msgid ""
"<userinput>sed -i '/\"lib64\"/s/64//' kde-modules/KDEInstallDirsCommon.cmake &amp;&amp;\n"
"\n"
"sed -e '/PACKAGE_INIT/i set(SAVE_PACKAGE_PREFIX_DIR \"${PACKAGE_PREFIX_DIR}\")' \\\n"
"    -e '/^include/a set(PACKAGE_PREFIX_DIR \"${SAVE_PACKAGE_PREFIX_DIR}\")' \\\n"
"    -i ECMConfig.cmake.in &amp;&amp;\n"
"\n"
"mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr .. &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>sed -i '/\"lib64\"/s/64//' kde-modules/KDEInstallDirsCommon.cmake &amp;&amp;\n"
"\n"
"sed -e '/PACKAGE_INIT/i set(SAVE_PACKAGE_PREFIX_DIR \"${PACKAGE_PREFIX_DIR}\")' \\\n"
"    -e '/^include/a set(PACKAGE_PREFIX_DIR \"${SAVE_PACKAGE_PREFIX_DIR}\")' \\\n"
"    -i ECMConfig.cmake.in &amp;&amp;\n"
"\n"
"mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr .. &amp;&amp;\n"
"make</userinput>"

