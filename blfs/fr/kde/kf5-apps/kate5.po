#. type: Content of: <sect1><sect2><title>
#: blfs-en/kde/kf5-apps/kate5.xml:90
msgid "Installation of Kate"
msgstr "Installation de Kate"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/kde/kf5-apps/kate5.xml:93
msgid ""
"Install <application>Kate</application> by running the following commands:"
msgstr ""
"Installez <application>Kate</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/kde/kf5-apps/kate5.xml:97
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=$KF5_PREFIX  \\\n"
"      -DCMAKE_BUILD_TYPE=Release          \\\n"
"      -DLIB_INSTALL_DIR=lib               \\\n"
"      -DBUILD_TESTING=OFF                 \\\n"
"      -Wno-dev .. &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=$KF5_PREFIX  \\\n"
"      -DCMAKE_BUILD_TYPE=Release          \\\n"
"      -DLIB_INSTALL_DIR=lib               \\\n"
"      -DBUILD_TESTING=OFF                 \\\n"
"      -Wno-dev .. &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/kde/kf5-apps/kate5.xml:108
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/kde/kf5-apps/kate5.xml:112
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/kde/kf5-apps/kate5.xml:115
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

