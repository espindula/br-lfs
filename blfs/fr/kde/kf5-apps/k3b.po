"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=$KF5_PREFIX \\\n"
"      -DCMAKE_BUILD_TYPE=Release         \\\n"
"      -DLIB_INSTALL_DIR=lib              \\\n"
"      -DBUILD_TESTING=OFF                \\\n"
"      -Wno-dev ..                        &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=$KF5_PREFIX \\\n"
"      -DCMAKE_BUILD_TYPE=Release         \\\n"
"      -DLIB_INSTALL_DIR=lib              \\\n"
"      -DBUILD_TESTING=OFF                \\\n"
"      -Wno-dev ..                        &amp;&amp;\n"
"make</userinput>"

