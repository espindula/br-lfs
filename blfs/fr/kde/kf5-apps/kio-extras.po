#. type: Content of: <sect1><sect2><title>
#: blfs-en/kde/kf5-apps/kio-extras.xml:97
msgid "Installation of kio-extras"
msgstr "Installation de kio-extras"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/kde/kf5-apps/kio-extras.xml:100
msgid ""
"Install <application>kio-extras</application> by running the following "
"commands:"
msgstr ""
"Installez <application>kio-extras</application> en lanant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/kde/kf5-apps/kio-extras.xml:104
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=$KF5_PREFIX \\\n"
"      -DCMAKE_BUILD_TYPE=Release         \\\n"
"      -DBUILD_TESTING=OFF                \\\n"
"      -Wno-dev .. &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=$KF5_PREFIX \\\n"
"      -DCMAKE_BUILD_TYPE=Release         \\\n"
"      -DBUILD_TESTING=OFF                \\\n"
"      -Wno-dev .. &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/kde/kf5-apps/kio-extras.xml:114
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'a pas de suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/kde/kf5-apps/kio-extras.xml:118
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/kde/kf5-apps/kio-extras.xml:121
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

