"          {\n"
"            \"name\": \"domain-search\",\n"
"            \"data\": \"your.domain.tld\"\n"
"          },\n"
"          {\n"
"            \"name\": \"routers\",\n"
"            \"data\": \"192.168.56.2\"\n"
"          }\n"
"        ]\n"
"      }\n"
"    ],\n"
"\n"
"    \"loggers\": [\n"
"      {\n"
"        \"name\": \"kea-dhcp4\",\n"
"        \"output_options\": [\n"
"          {\n"
"            \"output\": \"/var/log/kea-dhcp4.log\",\n"
