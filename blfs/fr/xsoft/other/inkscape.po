#. type: Content of: <sect1><sect2><title>
#: blfs-en/xsoft/other/inkscape.xml:168
msgid "Installation of Inkscape"
msgstr "Installation de Inkscape"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/xsoft/other/inkscape.xml:191
msgid ""
"Install <application>Inkscape</application> by running the following "
"commands:"
msgstr ""
"Installez <application>Inkscape</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. <screen>
#. <userinput remap="pre">patch -Np1 -i ../inkscape-&inkscape-
#. version;-poppler_21.11.0-1.patch &amp;&amp;
#. type: Content of: <sect1><sect2><screen>
#: blfs-en/xsoft/other/inkscape.xml:197
#, no-wrap
msgid ""
"<userinput>mkdir build                       &amp;&amp;\n"
"cd    build                       &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      ..                          &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>mkdir build                       &amp;&amp;\n"
"cd    build                       &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      ..                          &amp;&amp;\n"
"make</userinput>"

