"  for i in $LO_PREFIX/lib/libreoffice/share/xdg/*; do\n"
"    ln -svf $i /usr/share/applications/libreoffice-$(basename $i)\n"
"  done &amp;&amp;\n"
"\n"
"  # Man pages\n"
"  for i in $LO_PREFIX/share/man/man1/*; do\n"
