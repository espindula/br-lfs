#: blfs-en/general/prog/ninja.xml:82
msgid "Required"
msgstr "Requises"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/prog/ninja.xml:84
#| msgid "<xref linkend=\"python2\"/>"
msgid "<xref linkend=\"python3\"/>"
msgstr "<xref linkend=\"python3\"/>"

