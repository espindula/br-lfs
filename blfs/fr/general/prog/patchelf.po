#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/prog/patchelf.xml:75
msgid "Installation of Patchelf"
msgstr "Installation de Patchelf"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/prog/patchelf.xml:78
msgid ""
"Install <application>patchelf</application> by running the following "
"commands:"
msgstr ""
"Installez <application>patchelf</application> en excutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/prog/patchelf.xml:82
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr \\\n"
"            --docdir=/usr/share/doc/patchelf-&patchelf-version; &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr \\\n"
"            --docdir=/usr/share/doc/patchelf-&patchelf-version; &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/prog/patchelf.xml:87
msgid "To test the results, issue: <command>make check</command>."
msgstr "Pour tester les rsultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/prog/patchelf.xml:91
msgid "Now, as the &root; user:"
msgstr "Maintenant, en tant qu'utilisateur &root;&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/prog/patchelf.xml:94
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

