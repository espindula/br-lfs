#. type: Content of: <sect2><sect3><title>
#: blfs-en/general/prog/python-modules/pygtk.xml:122
msgid "Installation of PyGTK"
msgstr "Installation de PyGTK"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-modules/pygtk.xml:125
msgid "First, adapt PyGTK to changes in Pango by removing undefined APIs:"
msgstr ""

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-modules/pygtk.xml:129
#, no-wrap
msgid "<userinput remap=\"pre\">sed -i '1394,1402 d' pango.defs</userinput>"
msgstr "<userinput remap=\"pre\">sed -i '1394,1402 d' pango.defs</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-modules/pygtk.xml:132
msgid ""
"Install <application>PyGTK</application> by running the following commands:"
msgstr ""
"Installez <application>PyGTK</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-modules/pygtk.xml:136
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-modules/pygtk.xml:140
msgid ""
"The tests must be run from an active X display. If this is so, issue: "
"<command>make check</command>."
msgstr ""

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-modules/pygtk.xml:145
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-modules/pygtk.xml:148
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

