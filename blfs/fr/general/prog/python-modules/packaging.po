#. type: Content of: <sect2><sect3><title>
#: blfs-en/general/prog/python-modules/packaging.xml:86
msgid "Installation of Packaging"
msgstr "Installation de Packaging"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-modules/packaging.xml:88
msgid "Build the module:"
msgstr ""

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-modules/packaging.xml:90
#, no-wrap
msgid ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"
msgstr ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-modules/packaging.xml:93
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-modules/packaging.xml:96
#, no-wrap
msgid ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user packaging</userinput>"
msgstr ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user packaging</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-modules/packaging.xml:99
msgid ""
"Assuming <xref linkend=\"pytest\"/> is installed, but the other optional "
"dependency is not, the installation can be tested with the following "
"commands:"
msgstr ""

#.  no && because of a possible error
#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-modules/packaging.xml:104
#, no-wrap
msgid ""
"<userinput>python3 -m venv --system-site-packages testenv &amp;&amp;\n"
"source testenv/bin/activate                    &amp;&amp;\n"
"pip3 install pretend                           &amp;&amp;\n"
"python3 /usr/bin/pytest\n"
"deactivate</userinput>"
msgstr ""
"<userinput>python3 -m venv --system-site-packages testenv &amp;&amp;\n"
"source testenv/bin/activate                    &amp;&amp;\n"
"pip3 install pretend                           &amp;&amp;\n"
"python3 /usr/bin/pytest\n"
"deactivate</userinput>"

