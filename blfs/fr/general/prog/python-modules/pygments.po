#. type: Content of: <sect2><sect3><title>
#: blfs-en/general/prog/python-modules/pygments.xml:76
msgid "Installation of Pygments"
msgstr "Installation de Pygments"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-modules/pygments.xml:79
msgid "Build the <application>Python 3</application> module:"
msgstr "Construisez le module <application>Python 3</application>&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-modules/pygments.xml:82
#, no-wrap
msgid ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"
msgstr ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-modules/pygments.xml:85
msgid "To install the package run the following as the &root; user:"
msgstr ""
"Pour installer le paquet lancez ce qui suit en tant qu'utilisateur "
"&root;&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-modules/pygments.xml:88
#, no-wrap
msgid ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user Pygments</userinput>"
msgstr ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user Pygments</userinput>"

