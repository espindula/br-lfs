#. type: Content of: <sect2><sect3><title>
#: blfs-en/general/prog/python-modules/pytest.xml:99
msgid "Installation of Pytest"
msgstr "Installation de Pytest"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-modules/pytest.xml:101
msgid "Build the module:"
msgstr "Construisez le module&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-modules/pytest.xml:103
#, no-wrap
msgid ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"
msgstr ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-modules/pytest.xml:106
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-modules/pytest.xml:109
#, no-wrap
msgid ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user pytest</userinput>"
msgstr ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user pytest</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-modules/pytest.xml:112
msgid "The installation can be tested with the following commands:"
msgstr "Vous pouvez tester l'installation avec les commandes suivantes&nbsp;:"

#.  no && because of a possible error
#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-modules/pytest.xml:115
#, no-wrap
msgid ""
"<userinput>python3 -m venv --system-site-packages testenv &amp;&amp;\n"
"source testenv/bin/activate                    &amp;&amp;\n"
"pip3 install pytest[testing]                   &amp;&amp;\n"
"python3 /usr/bin/pytest\n"
"deactivate</userinput>"
msgstr ""
"<userinput>python3 -m venv --system-site-packages testenv &amp;&amp;\n"
"source testenv/bin/activate                    &amp;&amp;\n"
"pip3 install pytest[testing]                   &amp;&amp;\n"
"python3 /usr/bin/pytest\n"
"deactivate</userinput>"

