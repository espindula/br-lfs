#: blfs-en/general/prog/python-modules/pycryptodome.xml:76
msgid "Installation of PyCryptodome"
msgstr "Installation de PyCryptodome"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-modules/pycryptodome.xml:79
msgid ""
"To build <application>PyCryptodome</application> as a Python 3 module, run "
"the following command:"
msgstr ""

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-modules/pycryptodome.xml:83
#, no-wrap
msgid ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"
msgstr ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-modules/pycryptodome.xml:86
msgid "To install the module, run the following command as the &root; user:"
msgstr ""

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-modules/pycryptodome.xml:89
#, no-wrap
msgid ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user pycryptodome</userinput>"
msgstr ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user pycryptodome</userinput>"

