#. type: Content of: <sect2><sect3><title>
#: blfs-en/general/prog/python-modules/numpy.xml:100
msgid "Installation of NumPy"
msgstr "Installation de NumPy"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-modules/numpy.xml:102
msgid "Build the module:"
msgstr ""

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-modules/numpy.xml:104
#, no-wrap
msgid ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"
msgstr ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-modules/numpy.xml:107
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-modules/numpy.xml:110
#, no-wrap
msgid ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user numpy</userinput>"
msgstr ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user numpy</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-modules/numpy.xml:113
msgid "The installation can be tested with the following commands:"
msgstr ""

#.  no && because of a possible error
#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-modules/numpy.xml:116
#, no-wrap
msgid ""
"<userinput>python3 -m venv --system-site-packages testenv &amp;&amp;\n"
"source testenv/bin/activate                    &amp;&amp;\n"
"pip3 install hypothesis                        &amp;&amp;\n"
"python3 runtests.py\n"
"deactivate</userinput>"
msgstr ""
"<userinput>python3 -m venv --system-site-packages testenv &amp;&amp;\n"
"source testenv/bin/activate                    &amp;&amp;\n"
"pip3 install hypothesis                        &amp;&amp;\n"
"python3 runtests.py\n"
"deactivate</userinput>"

