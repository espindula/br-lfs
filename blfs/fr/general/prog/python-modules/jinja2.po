#. type: Content of: <sect2><sect3><title>
#: blfs-en/general/prog/python-modules/jinja2.xml:88
msgid "Installation of Jinja2"
msgstr "Installation de Jinja2"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-modules/jinja2.xml:90
msgid "Build the module:"
msgstr "Construisez le module&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-modules/jinja2.xml:93
#, no-wrap
msgid ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"
msgstr ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-modules/jinja2.xml:96
msgid "Install the module as the &root; user:"
msgstr "Installez le module en tant qu'utilisateur &root;&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-modules/jinja2.xml:99
#, no-wrap
msgid ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user Jinja2</userinput>"
msgstr ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user Jinja2</userinput>"

