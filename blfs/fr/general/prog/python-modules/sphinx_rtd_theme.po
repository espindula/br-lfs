#, fuzzy
#| msgid "Fix a wrong version for a dependency list:"
msgid "Fix wrong versions for dependencies:"
msgstr "Corrigez une mauvaise version dans la liste des dépendances&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-modules/sphinx_rtd_theme.xml:95
#, no-wrap
#| msgid ""
#| "<userinput>sed -e s/0.19/0.20/ \\\n"
#| "    -i setup.cfg</userinput>"
msgid ""
"<userinput>sed -e s/0.19/0.21/ \\\n"
"    -e '/sphinx.*7/s/7/8/' \\\n"
"    -i setup.cfg</userinput>"
msgstr ""
