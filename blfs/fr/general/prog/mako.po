#: blfs-en/general/prog/mako.xml:87
msgid "Installation of Mako"
msgstr "Installation de Mako"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/mako.xml:90
msgid "Build the module:"
msgstr "Construisez le module&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/mako.xml:93
#, no-wrap
msgid ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"
msgstr ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/mako.xml:96
msgid "Install the module as the &root; user:"
msgstr "Installez le module en tant qu'utilisateur &root;&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/mako.xml:99
#, no-wrap
msgid ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user Mako</userinput>"
msgstr ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user Mako</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/mako.xml:102
msgid "To test the installation, issue <command>pytest</command>."
