"<userinput>./configure --prefix=/usr      \\\n"
"            --enable-cplusplus \\\n"
"            --disable-static   \\\n"
"            --docdir=/usr/share/doc/gc-&gc-version; &amp;&amp;\n"
"make</userinput>"
msgstr ""
