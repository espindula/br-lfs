#: blfs-en/general/prog/pygobject3.xml:94
msgid "Installation of PyGObject3"
msgstr "Installation de PyGObject3"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/pygobject3.xml:160
#| msgid ""
#| "To build <application>PyGObject3</application> as a <application>Python "
#| "2</application> module, run the following commands:"
msgid ""
"Install <application>pygobject3</application> by running the following "
"commands:"
msgstr ""
"Installez <application>pygobject3</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/pygobject3.xml:164
#, no-wrap
#| msgid ""
#| "<userinput>mkdir python2 &amp;&amp;\n"
#| "pushd python2 &amp;&amp;\n"
#| "meson --prefix=/usr -Dpython=python2 .. &amp;&amp;\n"
#| "ninja &amp;&amp;\n"
#| "popd</userinput>"
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson --prefix=/usr .. &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson --prefix=/usr .. &amp;&amp;\n"
"ninja</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/pygobject3.xml:171
#| msgid ""
#| "To test the results, issue: <command>ninja test</command>.  An already "
#| "active graphical session with bus address is necessary to run the tests."
msgid ""
"To test the results, issue: <command>ninja test</command>.  An already "
"active graphical session with a bus address is necessary to run the tests. "
"One test, <filename>tests/test_gdbus.py</filename>, is known to fail."
msgstr ""
"Pour tester les résultats, tapez&nbsp;: <command>ninja test</command>. Une "
"session graphique déjà active avec une adresse dbus est nécessaire pour "
"lancer les tests. Un test, <filename>tests/test_gdbus.py</filename>, est "
"connu pour échouer."

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/pygobject3.xml:178
#| msgid ""
#| "To install the module, run the following command as the <systemitem "
#| "class=\"username\">root</systemitem> user:"
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/pygobject3.xml:181
#, no-wrap
msgid "<userinput>ninja install</userinput>"
msgstr "<userinput>ninja install</userinput>"

