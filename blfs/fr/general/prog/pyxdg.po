#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/pyxdg.xml:85
msgid "Build the module:"
msgstr ""

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/pyxdg.xml:88
#, no-wrap
msgid ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"
msgstr ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/pyxdg.xml:91
msgid "Install the module as the &root; user:"
msgstr ""

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/pyxdg.xml:94
#, no-wrap
msgid ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir "
"pyxdg</userinput>"
msgstr ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir "
"pyxdg</userinput>"

