"<userinput>rm -rf ./*\n"
"cmake -DLLVM_ENABLE_SPHINX=ON         \\\n"
"      -DSPHINX_WARNINGS_AS_ERRORS=OFF \\\n"
"      -Wno-dev ..                     &amp;&amp;\n"
"make docs-llvm-html  docs-llvm-man</userinput>"
msgstr ""
"<userinput>rm -rf ./*\n"
"cmake -DLLVM_ENABLE_SPHINX=ON         \\\n"
"      -DSPHINX_WARNINGS_AS_ERRORS=OFF \\\n"
"      -Wno-dev ..                     &amp;&amp;\n"
"make docs-llvm-html  docs-llvm-man</userinput>"

