#. type: Content of: <sect2><sect3><title>
#: blfs-en/general/prog/python-dependencies/toml.xml:79
msgid "Installation of Toml"
msgstr "Installation de Toml"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-dependencies/toml.xml:81
msgid "Build the module:"
msgstr "Construisez le module&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-dependencies/toml.xml:83
#, no-wrap
msgid ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"
msgstr ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-dependencies/toml.xml:86
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-dependencies/toml.xml:89
#, no-wrap
msgid ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user toml</userinput>"
msgstr ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user toml</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-dependencies/toml.xml:92
msgid ""
"Assuming <xref linkend=\"pytest\"/> is installed and the other optional "
"dependency is not, the installation can be tested with (some test files need"
" to be downloaded from github):"
msgstr ""

#.  no && because of a possible error
#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-dependencies/toml.xml:97
#, no-wrap
msgid ""
"<userinput>python3 -m venv --system-site-packages testenv        &amp;&amp;\n"
"source testenv/bin/activate                           &amp;&amp;\n"
"pip3 install numpy                                    &amp;&amp;\n"
"git clone https://github.com/BurntSushi/toml-test.git &amp;&amp;\n"
"python3 /usr/bin/pytest\n"
"deactivate</userinput>"
msgstr ""
"<userinput>python3 -m venv --system-site-packages testenv        &amp;&amp;\n"
"source testenv/bin/activate                           &amp;&amp;\n"
"pip3 install numpy                                    &amp;&amp;\n"
"git clone https://github.com/BurntSushi/toml-test.git &amp;&amp;\n"
"python3 /usr/bin/pytest\n"
"deactivate</userinput>"

