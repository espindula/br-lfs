#. type: Content of: <sect2><sect3><title>
#: blfs-en/general/prog/python-dependencies/sphinxcontrib-qthelp.xml:78
msgid "Installation of Sphinxcontrib-qthelp"
msgstr "Installation de Sphinxcontrib-qthelp"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-dependencies/sphinxcontrib-qthelp.xml:80
msgid "Build the module:"
msgstr "Construisez le module&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-dependencies/sphinxcontrib-qthelp.xml:82
#, no-wrap
msgid ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"
msgstr ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-dependencies/sphinxcontrib-qthelp.xml:85
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-dependencies/sphinxcontrib-qthelp.xml:88
#, no-wrap
msgid ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user sphinxcontrib-qthelp</userinput>"
msgstr ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user sphinxcontrib-qthelp</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-dependencies/sphinxcontrib-qthelp.xml:91
msgid "The installation can be tested with the following commands:"
msgstr "Vous pouvez tester l'installation avec les commandes suivantes&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-dependencies/sphinxcontrib-qthelp.xml:94
#, no-wrap
msgid ""
"<userinput>sed -i 's/text()/read_&amp;/' tests/test_qthelp.py &amp;&amp;\n"
"pytest</userinput>"
msgstr ""
"<userinput>sed -i 's/text()/read_&amp;/' tests/test_qthelp.py &amp;&amp;\n"
"pytest</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-dependencies/sphinxcontrib-qthelp.xml:98
msgid ""
"The <command>sed ...</command> command is needed because of a change in "
"<application>Sphinx</application> API for versions greater than 5.0."
msgstr ""
"la commande <command>sed …</command> est requise à cause d'un changement "
"dans l'API de <application>Sphinx</application> dans les versions "
"supérieures à 5.0."

