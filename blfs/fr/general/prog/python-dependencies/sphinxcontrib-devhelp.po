#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-dependencies/sphinxcontrib-devhelp.xml:82
msgid "Build the module:"
msgstr "Construisez le module&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-dependencies/sphinxcontrib-devhelp.xml:84
#, no-wrap
msgid ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"
msgstr ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-dependencies/sphinxcontrib-devhelp.xml:87
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-dependencies/sphinxcontrib-devhelp.xml:90
#, no-wrap
msgid ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user sphinxcontrib-devhelp</userinput>"
msgstr ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user sphinxcontrib-devhelp</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-dependencies/sphinxcontrib-devhelp.xml:93
msgid "To test the installation, run <command>pytest</command>."
msgstr "Pour tester l'installation, exécutez <command>pytest</command>."

