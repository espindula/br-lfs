#. type: Content of: <sect2><sect3><title>
#: blfs-en/general/prog/python-dependencies/commonmark.xml:80
msgid "Installation of Commonmark"
msgstr "Installation de Commonmark"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-dependencies/commonmark.xml:82
msgid "Build the module:"
msgstr "Construisez le module&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-dependencies/commonmark.xml:84
#, no-wrap
msgid ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"
msgstr ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-dependencies/commonmark.xml:87
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-dependencies/commonmark.xml:90
#, no-wrap
msgid ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user commonmark</userinput>"
msgstr ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user commonmark</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-dependencies/commonmark.xml:93
msgid ""
"Assuming <xref linkend=\"pytest\"/> is installed and the other optional "
"dependency is not, the installation can be tested with:"
msgstr ""

#.  no && because of a possible error
#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-dependencies/commonmark.xml:97
#, no-wrap
msgid ""
"<userinput>python3 -m venv --system-site-packages testenv &amp;&amp;\n"
"source testenv/bin/activate                    &amp;&amp;\n"
"pip3 install hypothesis                        &amp;&amp;\n"
"python3 /usr/bin/pytest commonmark/tests/unit_tests.py\n"
"python3 commonmark/tests/run_spec_tests.py\n"
"deactivate</userinput>"
msgstr ""
"<userinput>python3 -m venv --system-site-packages testenv &amp;&amp;\n"
"source testenv/bin/activate                    &amp;&amp;\n"
"pip3 install hypothesis                        &amp;&amp;\n"
"python3 /usr/bin/pytest commonmark/tests/unit_tests.py\n"
"python3 commonmark/tests/run_spec_tests.py\n"
"deactivate</userinput>"

