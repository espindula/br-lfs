#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-dependencies/sphinxcontrib-jsmath.xml:95
#, no-wrap
msgid ""
"<userinput>sed -i 's/text()/read_&amp;/' tests/test_jsmath.py &amp;&amp;\n"
"pytest</userinput>"
msgstr ""
"<userinput>sed -i 's/text()/read_&amp;/' tests/test_jsmath.py &amp;&amp;\n"
"pytest</userinput>"

