#. type: Content of: <sect2><sect3><title>
#: blfs-en/general/prog/python-dependencies/idna.xml:78
msgid "Installation of Idna"
msgstr "Installation de Idna"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-dependencies/idna.xml:80
msgid "Build the module:"
msgstr "Construisez le module&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-dependencies/idna.xml:82
#, no-wrap
msgid ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"
msgstr ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-dependencies/idna.xml:85
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/python-dependencies/idna.xml:88
#, no-wrap
msgid ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user idna</userinput>"
msgstr ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user idna</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/python-dependencies/idna.xml:91
msgid "To test the installation, run <command>pytest</command>."
msgstr "Pour tester l'installation, exécutez <command>pytest</command>."

