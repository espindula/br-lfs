#~ "    exit 1\n"
#~ "fi\n"
#~ "\n"
#~ "if test \"${OPENSSL}x\" == \"x\" ; then\n"
#~ "    echo \"ERROR!  You must provide a valid path to openssl!\"\n"
#~ "    echo \"\\'$(basename ${0}) --help\\' for more info.\"\n"
#~ "    echo \"\"\n"
#~ "    exit 1\n"
#~ "fi\n"
#~ "\n"
#~ "if test \"${OUTFILE}x\" == \"x\" ; then\n"
