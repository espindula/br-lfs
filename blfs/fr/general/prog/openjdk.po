#~ "    echo \"\\'$(basename ${0}) --help\\' for more info.\"\n"
#~ "    echo \"\"\n"
#~ "    exit 1\n"
#~ "fi\n"
#~ "\n"
#~ "if test \"${KEYTOOL}x\" == \"x\" ; then\n"
#~ "    echo \"ERROR!  You must provide a valid keytool program!\"\n"
#~ "    echo \"\\'$(basename ${0}) --help\\' for more info.\"\n"
