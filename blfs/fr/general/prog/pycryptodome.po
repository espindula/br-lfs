#. type: Content of: <sect2><sect3><title>
#: blfs-en/general/prog/pycryptodome.xml:76
msgid "Installation of PyCryptodome"
msgstr "Installation de PyCryptodome"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/pycryptodome.xml:79
msgid ""
"To build <application>PyCryptodome</application> as a Python 3 module, run "
"the following command:"
msgstr ""
"Pour construire <application>PyCryptodome</application> en tant que module "
"Python 3, lancez la commande suivante&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/pycryptodome.xml:83
#, no-wrap
msgid ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"
msgstr ""
"<userinput>pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"

#. type: Content of: <sect2><sect3><para>
#: blfs-en/general/prog/pycryptodome.xml:86
#| msgid ""
#| "To install the module, run the following command as the <systemitem "
#| "class=\"username\">root</systemitem> user:"
msgid "To install the module, run the following command as the &root; user:"
msgstr ""
"Pour installer le module, lancez la commande suivante en tant qu'utilisateur"
" &root;&nbsp;:"

#. type: Content of: <sect2><sect3><screen>
#: blfs-en/general/prog/pycryptodome.xml:89
#, no-wrap
msgid ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir "
"pycryptodome</userinput>"
msgstr ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir "
"pycryptodome</userinput>"

