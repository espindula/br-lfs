#: blfs-en/general/prog/sysprof.xml:95
msgid "Installation of Sysprof"
msgstr "Installation de Sysprof"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/prog/sysprof.xml:98
msgid ""
"Install <application>Sysprof</application> by running the following "
"commands:"
msgstr ""
"Installez <application>Sysprof</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/prog/sysprof.xml:102
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson --prefix=/usr -Dsystemdunitdir=/tmp .. &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson --prefix=/usr -Dsystemdunitdir=/tmp .. &amp;&amp;\n"
"ninja</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/prog/sysprof.xml:108
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson --prefix=/usr .. &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson --prefix=/usr .. &amp;&amp;\n"
"ninja</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/prog/sysprof.xml:115
msgid "To test the results, issue: <command>ninja test</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>ninja test</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/prog/sysprof.xml:119
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/prog/sysprof.xml:122
#, no-wrap
msgid ""
"<userinput>ninja install &amp;&amp;\n"
"rm -v /tmp/*.service</userinput>"
msgstr ""
"<userinput>ninja install &amp;&amp;\n"
"rm -v /tmp/*.service</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/prog/sysprof.xml:125
#, no-wrap
msgid "<userinput>ninja install</userinput>"
msgstr "<userinput>ninja install</userinput>"

#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/prog/sysprof.xml:130
msgid "Command Explanations"
msgstr "Explication des commandes"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/prog/sysprof.xml:133
msgid ""
"<command>rm -v /tmp/*.service</command>: This command removes systemd units "
"which have no use on SysV systems."
msgstr ""

#.  for sd-bus
#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/prog/sysprof.xml:138
msgid ""
"<parameter>-Dsystemdunitdir=/tmp</parameter>: This parameter puts systemd "
"units in /tmp on SysV systems so that they can be removed. It also bypasses "
"a check for systemd in the meson.build file."
msgstr ""

