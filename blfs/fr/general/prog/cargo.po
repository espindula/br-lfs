"<userinput>wget https://github.com/rust-lang/cargo/archive/&cargo-version;.tar.gz \\\n"
"     -O cargo-&cargo-version;.tar.gz</userinput>"
msgstr ""
"<userinput>wget https://github.com/rust-lang/cargo/archive/&cargo-version;.tar.gz \\\n"
"     -O cargo-&cargo-version;.tar.gz</userinput>"

