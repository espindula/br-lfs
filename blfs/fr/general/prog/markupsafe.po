#: blfs-en/general/prog/markupsafe.xml:94
#, no-wrap
#| msgid ""
#| "<userinput>pip3 install --no-index --find-links dist --no-cache-dir "
#| "markupsafe</userinput>"
msgid ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user markupsafe</userinput>"
msgstr ""
"<userinput>pip3 install --no-index --find-links dist --no-cache-dir --no-"
"user markupsafe</userinput>"

