#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/prog/librep.xml:86
msgid "Installation of Librep"
msgstr "Installation de Librep"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/prog/librep.xml:89
msgid ""
"Install <application>librep</application> by running the following commands:"
msgstr ""
"Installez <application>librep</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/prog/librep.xml:93
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/prog/librep.xml:97
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/prog/librep.xml:101
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/prog/librep.xml:104
#, no-wrap
msgid ""
"<userinput>sed -i '5043,5044 d' libtool &amp;&amp;\n"
"make install</userinput>"
msgstr ""
"<userinput>sed -i '5043,5044 d' libtool &amp;&amp;\n"
"make install</userinput>"

