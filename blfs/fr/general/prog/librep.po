#: blfs-en/general/prog/librep.xml:73
msgid "Installation of Librep"
msgstr "Installation de Librep"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/prog/librep.xml:75
msgid ""
"Install <application>librep</application> by running the following commands:"
msgstr ""
"Installez <application>librep</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/prog/librep.xml:78
#, no-wrap
msgid ""
"<userinput>./autogen.sh --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./autogen.sh --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/prog/librep.xml:81
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/prog/librep.xml:83
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/prog/librep.xml:85
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

