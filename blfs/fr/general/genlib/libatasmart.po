#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/genlib/libatasmart.xml:78
msgid "Installation of libatasmart"
msgstr "Installation de libatasmart"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libatasmart.xml:81
msgid ""
"Install <application>libatasmart</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libatasmart</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libatasmart.xml:85
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libatasmart.xml:89
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libatasmart.xml:93
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libatasmart.xml:96
#, no-wrap
msgid ""
"<userinput>make docdir=/usr/share/doc/libatasmart-&libatasmart-version; "
"install</userinput>"
msgstr ""
"<userinput>make docdir=/usr/share/doc/libatasmart-&libatasmart-version; "
"install</userinput>"

