#: blfs-en/general/genlib/libqalculate.xml:91
msgid "Installation of libqalculate"
msgstr "Installation de libqalculate"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libqalculate.xml:94
msgid ""
"Install <application>libqalculate</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libqalculate</application> en lanant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libqalculate.xml:98
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libqalculate.xml:102
msgid "To test the results, issue: <command>make check</command>."
msgstr "Pour tester les rsultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libqalculate.xml:106
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libqalculate.xml:109
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

