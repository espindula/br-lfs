#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/genlib/libqalculate.xml:90
msgid "Installation of libqalculate"
msgstr "Installation de libqalculate"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libqalculate.xml:93
msgid ""
"Install <application>libqalculate</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libqalculate</application> en lanant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libqalculate.xml:97
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr    \\\n"
"            --disable-static \\\n"
"            --docdir=/usr/share/doc/libqalculate-&libqalculate-version; &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr    \\\n"
"            --disable-static \\\n"
"            --docdir=/usr/share/doc/libqalculate-&libqalculate-version; &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libqalculate.xml:103
msgid "To test the results, issue: <command>make check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libqalculate.xml:107
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libqalculate.xml:110
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

