#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/genlib/duktape.xml:75
msgid "Installation of duktape"
msgstr "Installation de duktape"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/duktape.xml:78
msgid ""
"Install <application>duktape</application> by running the following "
"commands:"
msgstr ""
"Installez <application>duktape</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/duktape.xml:82
#, no-wrap
msgid ""
"<userinput>sed -i 's/-Os/-O2/' Makefile.sharedlibrary\n"
"make -f Makefile.sharedlibrary INSTALL_PREFIX=/usr</userinput>"
msgstr ""
"<userinput>sed -i 's/-Os/-O2/' Makefile.sharedlibrary\n"
"make -f Makefile.sharedlibrary INSTALL_PREFIX=/usr</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/duktape.xml:86
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/duktape.xml:89
#, no-wrap
msgid ""
"<userinput>make -f Makefile.sharedlibrary INSTALL_PREFIX=/usr "
"install</userinput>"
msgstr ""
"<userinput>make -f Makefile.sharedlibrary INSTALL_PREFIX=/usr "
"install</userinput>"

