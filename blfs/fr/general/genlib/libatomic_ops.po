"<userinput>./configure --prefix=/usr    \\\n"
"            --enable-shared  \\\n"
"            --disable-static \\\n"
"            --docdir=/usr/share/doc/libatomic_ops-&libatomic_ops-version; &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr    \\\n"
"            --enable-shared  \\\n"
"            --disable-static \\\n"
"            --docdir=/usr/share/doc/libatomic_ops-&libatomic_ops-version; &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libatomic_ops.xml:97
