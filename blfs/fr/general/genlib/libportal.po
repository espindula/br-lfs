#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/genlib/libportal.xml:99
msgid "Installation of libportal"
msgstr "Installation de libportal"

#. type: Content of: <sect1><sect2><warning><para>
#: blfs-en/general/genlib/libportal.xml:103
msgid ""
"If a previous version of libportal is installed, move the headers out of the"
" way so that later packages do not encounter conflicts:"
msgstr ""

#. type: Content of: <sect1><sect2><warning><screen>
#: blfs-en/general/genlib/libportal.xml:107
#, no-wrap
msgid ""
"<userinput>if [ -e /usr/include/libportal ]; then\n"
"    rm -rf /usr/include/libportal.old &amp;&amp;\n"
"    mv -vf /usr/include/libportal{,.old}\n"
"fi</userinput>"
msgstr ""
"<userinput>if [ -e /usr/include/libportal ]; then\n"
"    rm -rf /usr/include/libportal.old &amp;&amp;\n"
"    mv -vf /usr/include/libportal{,.old}\n"
"fi</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libportal.xml:114
msgid ""
"Install <application>libportal</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libportal</application> en lanant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libportal.xml:118
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson --prefix=/usr --buildtype=release -Ddocs=false .. &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson --prefix=/usr --buildtype=release -Ddocs=false .. &amp;&amp;\n"
"ninja</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libportal.xml:125
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libportal.xml:129
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libportal.xml:132
#, no-wrap
msgid "<userinput>ninja install</userinput>"
msgstr "<userinput>ninja install</userinput>"

