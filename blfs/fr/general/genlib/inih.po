"Installez <application>inih</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/inih.xml:82
#, no-wrap
#| msgid ""
#| "<userinput>mkdir build &amp;&amp;\n"
#| "cd    build &amp;&amp;\n"
#| "\n"
#| "meson --prefix=/usr --buildtype=release .. &amp;&amp;\n"
#| "ninja</userinput>"
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson setup --prefix=/usr --buildtype=release .. &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson setup --prefix=/usr --buildtype=release .. &amp;&amp;\n"
"ninja</userinput>"

