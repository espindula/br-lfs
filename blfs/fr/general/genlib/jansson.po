#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/genlib/jansson.xml:79
msgid "Installation of Jansson"
msgstr "Installation de Jansson"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/jansson.xml:82
msgid "First fix one of the tests:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/jansson.xml:85
#, no-wrap
msgid ""
"<userinput>sed -e \"/DT/s;| sort;| sed 's/@@libjansson.*//' &amp;;\" \\\n"
"    -i test/suites/api/check-exports</userinput>"
msgstr ""
"<userinput>sed -e \"/DT/s;| sort;| sed 's/@@libjansson.*//' &amp;;\" \\\n"
"    -i test/suites/api/check-exports</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/jansson.xml:89
msgid ""
"Install <application>jansson</application> by running the following "
"commands:"
msgstr ""
"Installez <application>jansson</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/jansson.xml:93
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/jansson.xml:97
msgid "To test the results, issue: <command>make check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/jansson.xml:101
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/jansson.xml:104
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

