#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/genlib/libssh2.xml:99
msgid "Installation of libssh2"
msgstr "Installation de libssh2"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libssh2.xml:102
msgid ""
"Install <application>libssh2</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libssh2</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libssh2.xml:106
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"

#. One test, using gnupg, may fail for unknown reasons.
#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libssh2.xml:110
msgid "To test the results, issue: <command>make check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libssh2.xml:115
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libssh2.xml:118
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

