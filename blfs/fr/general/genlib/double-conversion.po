#: blfs-en/general/genlib/double-conversion.xml:87
msgid "Installation of Double-conversion"
msgstr "Installation de Double-conversion"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/double-conversion.xml:90
msgid ""
"Install <application>Double-conversion</application> by running the "
"following commands:"
msgstr ""
"Installez <application>Double-conversion</application> en exécutant les "
"commandes suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/double-conversion.xml:95
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DBUILD_SHARED_LIBS=ON      \\\n"
"      -DBUILD_TESTING=ON          \\\n"
"      ..                          &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DBUILD_SHARED_LIBS=ON      \\\n"
"      -DBUILD_TESTING=ON          \\\n"
"      ..                          &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/double-conversion.xml:105
msgid "To test the results, issue: <command>make test</command>."
msgstr "Pour tester les rsultats lancez&nbsp;: <command>make test</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/double-conversion.xml:109
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/double-conversion.xml:112
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

