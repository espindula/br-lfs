#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/genlib/libxkbcommon.xml:93
msgid "Installation of libxkbcommon"
msgstr "Installation de libxkbcommon"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libxkbcommon.xml:96
msgid ""
"Install <application>libxkbcommon</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libxkbcommon</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libxkbcommon.xml:100
#, no-wrap
msgid ""
"<userinput>./configure $XORG_CONFIG     \\\n"
"            --docdir=/usr/share/doc/libxkbcommon-&libxkbcommon-version; &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure $XORG_CONFIG     \\\n"
"            --docdir=/usr/share/doc/libxkbcommon-&libxkbcommon-version; &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libxkbcommon.xml:105
msgid "To test the results, issue: <command>make check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libxkbcommon.xml:109
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libxkbcommon.xml:112
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

#. type: Content of: <sect1><sect2><title>
