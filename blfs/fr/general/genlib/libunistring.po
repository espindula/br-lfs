#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/genlib/libunistring.xml:72
msgid "Installation of libunistring"
msgstr "Installation de libunistring"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libunistring.xml:74
msgid ""
"Install <application>libunistring</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libunistring</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libunistring.xml:77
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr    \\\n"
"            --disable-static \\\n"
"            --docdir=/usr/share/doc/libunistring-&libunistring-version; &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr    \\\n"
"            --disable-static \\\n"
"            --docdir=/usr/share/doc/libunistring-&libunistring-version; &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libunistring.xml:82
msgid "To test the results, issue: <command>make check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libunistring.xml:84
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libunistring.xml:86
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

