#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/genlib/libnvme.xml:91
msgid "Installation of libnvme"
msgstr "Installation de libnvme"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libnvme.xml:94
msgid ""
"Install <application>libnvme</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libnvme</application> en excutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libnvme.xml:98
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson setup --prefix=/usr --buildtype=release -Dlibdbus=auto .. &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson setup --prefix=/usr --buildtype=release -Dlibdbus=auto .. &amp;&amp;\n"
"ninja</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libnvme.xml:105
msgid "To run the tests, issue: <command>ninja test</command>."
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libnvme.xml:109
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libnvme.xml:112
#, no-wrap
msgid "<userinput>ninja install</userinput>"
msgstr "<userinput>ninja install</userinput>"

