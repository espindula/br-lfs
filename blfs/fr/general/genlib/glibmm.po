#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/genlib/glibmm.xml:94
msgid "Installation of GLibmm"
msgstr "Installation de GLibmm"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/glibmm.xml:97
msgid "First, fix the documents directory name:"
msgstr "Pour commencer, corrigez le nom du répertoire de documentation&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/glibmm.xml:100
#, no-wrap
msgid ""
"<userinput>sed -e '/^libdocdir =/ s/$(book_name)/glibmm-&glibmm-version;/' \\\n"
"    -i docs/Makefile.in</userinput>"
msgstr ""
"<userinput>sed -e '/^libdocdir =/ s/$(book_name)/glibmm-&glibmm-version;/' \\\n"
"    -i docs/Makefile.in</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/glibmm.xml:104
msgid ""
"Install <application>GLibmm</application> by running the following commands:"
msgstr ""
"Installez <application>GLibmm</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/glibmm.xml:108
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/glibmm.xml:112
msgid "To test the results, issue: <command>make check</command>."
msgstr "Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/glibmm.xml:116
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem class=\"username\">root</"
"systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/glibmm.xml:119
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

