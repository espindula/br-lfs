#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/genlib/libsigc3.xml:94
msgid "Installation of libsigc++3"
msgstr "Installation de libsigc++3"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libsigc3.xml:97
msgid ""
"Install <application>libsigc++3</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libsigc++3</application> en lanant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libsigc3.xml:101
#, no-wrap
msgid ""
"<userinput>mkdir bld &amp;&amp;\n"
"cd    bld &amp;&amp;\n"
"\n"
"meson --prefix=/usr --buildtype=release .. &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir bld &amp;&amp;\n"
"cd    bld &amp;&amp;\n"
"\n"
"meson --prefix=/usr --buildtype=release .. &amp;&amp;\n"
"ninja</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libsigc3.xml:108
msgid "To test the results, issue: <command>ninja test</command>."
msgstr "Pour tester les rsultats lancez&nbsp;: <command>ninja test</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libsigc3.xml:112
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libsigc3.xml:115
#, no-wrap
msgid "<userinput>ninja install</userinput>"
msgstr "<userinput>ninja install</userinput>"

