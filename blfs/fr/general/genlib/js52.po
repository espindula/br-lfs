#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/genlib/js52.xml:97
msgid "Installation of JS"
msgstr "Installation de JS"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/js52.xml:100
msgid ""
"Install <application>JS</application> by running the following commands:"
msgstr ""
"Installez <application>JS</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/js52.xml:106
#, no-wrap
#| msgid ""
#| "<userinput>cd js/src &amp;&amp;\n"
#| "\n"
#| "./configure --prefix=/usr       \\\n"
#| "            --with-intl-api     \\\n"
#| "            --with-system-zlib  \\\n"
#| "            --with-system-nspr  \\\n"
#| "            --with-system-icu   \\\n"
#| "            --enable-readline   &amp;&amp;\n"
#| "make</userinput>"
msgid ""
"<userinput>cd js/src &amp;&amp;\n"
"\n"
"grep -rl -- '-Werror=format' |\n"
"    xargs sed -i 's/error=format/no-&amp;/' &amp;&amp;\n"
"\n"
"./configure --prefix=/usr       \\\n"
"            --with-intl-api     \\\n"
"            --with-system-zlib  \\\n"
"            --with-system-nspr  \\\n"
"            --with-system-icu   \\\n"
"            --enable-readline   &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>cd js/src &amp;&amp;\n"
"\n"
"grep -rl -- '-Werror=format' |\n"
"    xargs sed -i 's/error=format/no-&amp;/' &amp;&amp;\n"
"\n"
"./configure --prefix=/usr       \\\n"
"            --with-intl-api     \\\n"
"            --with-system-zlib  \\\n"
"            --with-system-nspr  \\\n"
"            --with-system-icu   \\\n"
"            --enable-readline   &amp;&amp;\n"
"make</userinput>"

