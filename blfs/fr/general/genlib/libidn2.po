#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/genlib/libidn2.xml:96
msgid "Installation of libidn2"
msgstr "Installation de libidn2"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libidn2.xml:99
msgid ""
"Install <application>libidn2</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libidn2</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libidn2.xml:103
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libidn2.xml:107
msgid "To test the results, issue: <command>make check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libidn2.xml:111
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libidn2.xml:114
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

