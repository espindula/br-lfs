#: blfs-en/general/genlib/libptytty.xml:86
msgid "Installation of libptytty"
msgstr "Installation de libptytty"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libptytty.xml:89
msgid ""
"Install <application>libptytty</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libptytty</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libptytty.xml:93
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr     \\\n"
"      -DCMAKE_BUILD_TYPE=Release      \\\n"
"      -DPT_UTMP_FILE:STRING=/run/utmp \\\n"
"      .. &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr     \\\n"
"      -DCMAKE_BUILD_TYPE=Release      \\\n"
"      -DPT_UTMP_FILE:STRING=/run/utmp \\\n"
"      .. &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libptytty.xml:103
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libptytty.xml:107
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
