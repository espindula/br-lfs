msgid "Installation of wpebackend-fdo"
msgstr "Installation de wpebackend-fdo"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/wpebackend-fdo.xml:84
msgid ""
"Install <application>wpebackend-fdo</application> by running the following "
"commands:"
msgstr ""
"Installez <application>wpebackend-fdo</application> en exécutant les "
"commandes suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/wpebackend-fdo.xml:88
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson setup --prefix=/usr --buildtype=release ..\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson setup --prefix=/usr --buildtype=release ..\n"
"ninja</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/wpebackend-fdo.xml:95
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/wpebackend-fdo.xml:98
#, no-wrap
msgid "<userinput>ninja install</userinput>"
msgstr "<userinput>ninja install</userinput>"

