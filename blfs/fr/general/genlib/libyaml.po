#: blfs-en/general/genlib/libyaml.xml:86
msgid "Installation of libyaml"
msgstr "Installation de libyaml"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libyaml.xml:89
msgid ""
"Install <application>libyaml</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libyaml</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libyaml.xml:93
#, no-wrap
msgid ""
"<userinput>./bootstrap                                &amp;&amp;\n"
"./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./bootstrap                                &amp;&amp;\n"
"./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libyaml.xml:107
msgid "To test the results, issue: <command>make check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genlib/libyaml.xml:111
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genlib/libyaml.xml:114
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

