#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/sysutils/accountsservice.xml:105
msgid "Installation of AccountsService"
msgstr "Installation de AccountsService"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/sysutils/accountsservice.xml:108
msgid ""
"Install <application>AccountsService</application> by running the following "
"commands:"
msgstr ""
"Installez <application>AccountsService</application> en lançant les "
"commandes suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/sysutils/accountsservice.xml:112
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd build &amp;&amp;\n"
"\n"
"meson --prefix=/usr                   \\\n"
"            -Dadmin_group=adm         \\\n"
"            -Delogind=true            \\\n"
"            -Dsystemdsystemunitdir=no \\\n"
"            .. &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd build &amp;&amp;\n"
"\n"
"meson --prefix=/usr                   \\\n"
"            -Dadmin_group=adm         \\\n"
"            -Delogind=true            \\\n"
"            -Dsystemdsystemunitdir=no \\\n"
"            .. &amp;&amp;\n"
"ninja</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/sysutils/accountsservice.xml:122
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd build &amp;&amp;\n"
"\n"
"meson --prefix=/usr           \\\n"
"            -Dadmin_group=adm \\\n"
"            -Dsystemd=true    \\\n"
"            .. &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd build &amp;&amp;\n"
"\n"
"meson --prefix=/usr           \\\n"
"            -Dadmin_group=adm \\\n"
"            -Dsystemd=true    \\\n"
"            .. &amp;&amp;\n"
"ninja</userinput>"

