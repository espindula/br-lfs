#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/sysutils/accountsservice.xml:114
msgid ""
"Install <application>AccountsService</application> by running the following "
"commands:"
msgstr ""
"Installez <application>AccountsService</application> en exécutant les "
"commandes suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/sysutils/accountsservice.xml:118
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd build &amp;&amp;\n"
"\n"
"meson --prefix=/usr             \\\n"
"      --buildtype=release       \\\n"
"      -Dadmin_group=adm         \\\n"
"      -Delogind=true            \\\n"
"      -Dsystemdsystemunitdir=no \\\n"
"      .. &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd build &amp;&amp;\n"
"\n"
"meson --prefix=/usr             \\\n"
"      --buildtype=release       \\\n"
"      -Dadmin_group=adm         \\\n"
"      -Delogind=true            \\\n"
"      -Dsystemdsystemunitdir=no \\\n"
"      .. &amp;&amp;\n"
"ninja</userinput>"

