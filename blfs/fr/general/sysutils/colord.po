#| msgid ""
#| "<userinput>./configure --prefix=/usr              \\\n"
#| "            --sysconfdir=/etc          \\\n"
#| "            --localstatedir=/var       \\\n"
#| "            --with-daemon-user=colord  \\\n"
#| "            --enable-vala              \\\n"
#| "            --enable-daemon            \\\n"
#| "            --enable-session-helper    \\\n"
#| "            --enable-libcolordcompat   \\\n"
#| "            --disable-rpath            \\\n"
#| "            --disable-argyllcms-sensor \\\n"
#| "            --disable-bash-completion  \\\n"
#| "            --disable-static &amp;&amp;\n"
#| "make</userinput>"
msgid ""
"<userinput>./configure --prefix=/usr              \\\n"
"            --sysconfdir=/etc          \\\n"
"            --localstatedir=/var       \\\n"
"            --with-daemon-user=colord  \\\n"
"            --enable-vala              \\\n"
"            --enable-daemon            \\\n"
"            --enable-session-helper    \\\n"
"            --enable-libcolordcompat   \\\n"
"            --disable-rpath            \\\n"
"            --disable-argyllcms-sensor \\\n"
"            --disable-bash-completion  \\\n"
"            --disable-systemd-login    \\\n"
"            --disable-static &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr              \\\n"
"            --sysconfdir=/etc          \\\n"
"            --localstatedir=/var       \\\n"
"            --with-daemon-user=colord  \\\n"
"            --enable-vala              \\\n"
