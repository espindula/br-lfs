msgid "Configuration Information"
msgstr "Informations sur la configuration"

#. type: Content of: <sect1><sect2><sect3><para>
#: blfs-en/general/sysutils/udisks2.xml:193
msgid ""
"The default configuration of UDisks2 assumes ntfs-3g is used to mount NTFS "
"filesystems.  If the wrapper script for in-kernel NTFS3 driver (recommended "
"in the <xref linkend=\"ntfs-3g\"/> page) is used instead, create a "
"configuration file for UDisks2 compatible with NTFS3 to allow mounting NTFS "
"filesystems:"
msgstr ""

#. type: Content of: <sect1><sect2><sect3><screen>
#: blfs-en/general/sysutils/udisks2.xml:200
#, no-wrap
msgid ""
"<userinput>cat &gt; /etc/udisks2/mount_options.conf &lt;&lt; \"EOF\"\n"
"<literal>[defaults]\n"
"ntfs_defaults=uid=$UID,gid=$GID</literal>\n"
"EOF</userinput>"
msgstr ""
"<userinput>cat &gt; /etc/udisks2/mount_options.conf &lt;&lt; \"EOF\"\n"
"<literal>[defaults]\n"
"ntfs_defaults=uid=$UID,gid=$GID</literal>\n"
"EOF</userinput>"

