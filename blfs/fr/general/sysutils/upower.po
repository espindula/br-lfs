#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/sysutils/upower.xml:127
msgid ""
"Install <application>UPower</application> by running the following commands:"
msgstr ""
"Installez <application>UPower</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/sysutils/upower.xml:131
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson setup ..                  \\\n"
"      --prefix=/usr             \\\n"
"      --buildtype=release       \\\n"
"      -Dgtk-doc=false           \\\n"
"      -Dman=false               \\\n"
"      -Dsystemdsystemunitdir=no \\\n"
"      -Dudevrulesdir=/usr/lib/udev/rules.d &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson setup ..                  \\\n"
"      --prefix=/usr             \\\n"
"      --buildtype=release       \\\n"
"      -Dgtk-doc=false           \\\n"
"      -Dman=false               \\\n"
"      -Dsystemdsystemunitdir=no \\\n"
"      -Dudevrulesdir=/usr/lib/udev/rules.d &amp;&amp;\n"
"ninja</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/sysutils/upower.xml:143
