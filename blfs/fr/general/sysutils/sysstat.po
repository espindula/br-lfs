#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/sysutils/sysstat.xml:89
msgid ""
"Install <application>Sysstat</application> by running the following "
"commands:"
msgstr ""
"Installez <application>Sysstat</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/sysutils/sysstat.xml:93
#, no-wrap
msgid ""
"<userinput>sa_lib_dir=/usr/lib/sa    \\\n"
"sa_dir=/var/log/sa        \\\n"
"conf_dir=/etc/sysconfig   \\\n"
"./configure --prefix=/usr \\\n"
"            --disable-file-attr &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>sa_lib_dir=/usr/lib/sa    \\\n"
"sa_dir=/var/log/sa        \\\n"
"conf_dir=/etc/sysconfig   \\\n"
"./configure --prefix=/usr \\\n"
"            --disable-file-attr &amp;&amp;\n"
"make</userinput>"

