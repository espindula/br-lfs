#: blfs-en/general/sysutils/blocaled.xml:94
msgid "Installation of blocaled"
msgstr "Installation de blocaled"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/sysutils/blocaled.xml:97
msgid ""
"Install <application>blocaled</application> by running the following "
"commands:"
msgstr ""
"Installez <application>blocaled</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/sysutils/blocaled.xml:101
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr --sysconfdir=/etc &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr --sysconfdir=/etc &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/sysutils/blocaled.xml:105
msgid "To test the results, issue: <command>make check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/sysutils/blocaled.xml:109
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/sysutils/blocaled.xml:112
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

