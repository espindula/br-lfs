#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/graphlib/sassc.xml:120
msgid "Installation of sassc"
msgstr "Installation de sassc"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/graphlib/sassc.xml:123
msgid "First, build the library:"
msgstr "Tout d'abord, construisez la bibliothèque :"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/graphlib/sassc.xml:126
#, no-wrap
msgid ""
"<userinput>tar -xf ../libsass-&libsass-version;.tar.gz &amp;&amp;\n"
"pushd libsass-&libsass-version; &amp;&amp;\n"
"\n"
"autoreconf -fi &amp;&amp;\n"
"\n"
"./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>tar -xf ../libsass-&libsass-version;.tar.gz &amp;&amp;\n"
"pushd libsass-&libsass-version; &amp;&amp;\n"
"\n"
"autoreconf -fi &amp;&amp;\n"
"\n"
"./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/graphlib/sassc.xml:135
#: blfs-en/general/graphlib/sassc.xml:155
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/graphlib/sassc.xml:139
#: blfs-en/general/graphlib/sassc.xml:159
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/graphlib/sassc.xml:142
#: blfs-en/general/graphlib/sassc.xml:162
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/graphlib/sassc.xml:145
msgid "Build the command line wrapper:"
msgstr "Construisez l'enveloppe en ligne de commande&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/graphlib/sassc.xml:148
#, no-wrap
msgid ""
"<userinput>popd &amp;&amp;\n"
"autoreconf -fi &amp;&amp;\n"
"\n"
"./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>popd &amp;&amp;\n"
"autoreconf -fi &amp;&amp;\n"
"\n"
"./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"

