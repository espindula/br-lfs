msgid ""
"<userinput>install -v -dm755 \\\n"
"        /usr/share/{man/man{3,5},doc/fontconfig-&fontconfig-version;/fontconfig-devel} &amp;&amp;\n"
"install -v -m644 fc-*/*.1         /usr/share/man/man1 &amp;&amp;\n"
