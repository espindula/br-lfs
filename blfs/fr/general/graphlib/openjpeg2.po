#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/graphlib/openjpeg2.xml:83
msgid "Installation of OpenJPEG"
msgstr "Installation de OpenJPEG"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/graphlib/openjpeg2.xml:86
msgid ""
"Install <application>OpenJPEG</application> by running the following "
"commands:"
msgstr ""
"Installez <application>OpenJPEG</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/graphlib/openjpeg2.xml:90
#, no-wrap
msgid ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_BUILD_TYPE=Release -DCMAKE_INSTALL_PREFIX=/usr .. &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_BUILD_TYPE=Release -DCMAKE_INSTALL_PREFIX=/usr .. &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/graphlib/openjpeg2.xml:100
msgid "This package does not come with a working test suite."
msgstr "Ce paquet n'a pas de suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/graphlib/openjpeg2.xml:102
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem class=\"username\">root</"
"systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/graphlib/openjpeg2.xml:104
#, no-wrap
msgid ""
"<userinput>make install &amp;&amp;\n"
"\n"
"pushd ../doc &amp;&amp;\n"
"for man in man/man?/* ; do\n"
"    install -v -D -m 644 $man /usr/share/$man\n"
"done         &amp;&amp;\n"
"popd</userinput>"
msgstr ""
"<userinput>make install &amp;&amp;\n"
"\n"
"pushd ../doc &amp;&amp;\n"
"for man in man/man?/* ; do\n"
"    install -v -D -m 644 $man /usr/share/$man\n"
"done         &amp;&amp;\n"
"popd</userinput>"

