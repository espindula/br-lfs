#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/graphlib/qpdf.xml:94
msgid "Installation of Qpdf"
msgstr "Installation de Qpdf"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/graphlib/qpdf.xml:97
msgid ""
"Install <application>Qpdf</application> by running the following commands:"
msgstr ""
"Installez <application>Qpdf</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/graphlib/qpdf.xml:101
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr    \\\n"
"            --disable-static \\\n"
"            --docdir=/usr/share/doc/qpdf-&qpdf-version; &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr    \\\n"
"            --disable-static \\\n"
"            --docdir=/usr/share/doc/qpdf-&qpdf-version; &amp;&amp;\n"
"make</userinput>"

#. Three tests are known to fail due to minor differences in
#.       expected output.
#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/graphlib/qpdf.xml:107
msgid "To test the results, issue: <command>make check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/graphlib/qpdf.xml:113
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/graphlib/qpdf.xml:116
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

