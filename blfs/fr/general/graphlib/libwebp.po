#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/graphlib/libwebp.xml:94
msgid "Installation of libwebp"
msgstr "Installation de libwebp"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/graphlib/libwebp.xml:97
msgid ""
"Install <application>libwebp</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libwebp</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/graphlib/libwebp.xml:101
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr           \\\n"
"            --enable-libwebpmux     \\\n"
"            --enable-libwebpdemux   \\\n"
"            --enable-libwebpdecoder \\\n"
"            --enable-libwebpextras  \\\n"
"            --enable-swap-16bit-csp \\\n"
"            --disable-static        &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr           \\\n"
"            --enable-libwebpmux     \\\n"
"            --enable-libwebpdemux   \\\n"
"            --enable-libwebpdecoder \\\n"
"            --enable-libwebpextras  \\\n"
"            --enable-swap-16bit-csp \\\n"
"            --disable-static        &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/graphlib/libwebp.xml:111
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'a pas de suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/graphlib/libwebp.xml:115
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/graphlib/libwebp.xml:118
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

