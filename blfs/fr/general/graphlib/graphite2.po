#| msgid ""
#| "<userinput>mkdir build &amp;&amp;\n"
#| "cd    build &amp;&amp;\n"
#| "\n"
#| "cmake -G \"Unix Makefiles\"         \\\n"
#| "      -DCMAKE_INSTALL_PREFIX=/usr \\\n"
#| "      -Wno-dev .. &amp;&amp;\n"
#| "\n"
#| "make</userinput>"
