#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/graphlib/libspiro.xml:80
msgid "Installation of libspiro"
msgstr "Installation de libspiro"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/graphlib/libspiro.xml:83
msgid ""
"Install <application>libspiro</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libspiro</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/graphlib/libspiro.xml:87
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/graphlib/libspiro.xml:91
msgid "To test the results, issue: <command>make check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/graphlib/libspiro.xml:95
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/graphlib/libspiro.xml:98
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

