msgid ""
"Install <application>newt</application> by running the following command:"
msgstr ""
"Installez <application>newt</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/graphlib/newt.xml:104
#, no-wrap
#| msgid ""
#| "<userinput>sed -e 's/^LIBNEWT =/#&amp;/'                   \\\n"
#| "    -e '/install -m 644 $(LIBNEWT)/ s/^/#/' \\\n"
#| "    -e 's/$(LIBNEWT)/$(LIBNEWTSONAME)/g'    \\\n"
#| "    -i Makefile.in                          &amp;&amp;\n"
#| "\n"
#| "./configure --prefix=/usr           \\\n"
#| "            --with-gpm-support      \\\n"
#| "            --with-python=python&python3-majorver; &amp;&amp;\n"
#| "make</userinput>"
msgid ""
"<userinput>sed -e '/install -m 644 $(LIBNEWT)/ s/^/#/' \\\n"
"    -e '/$(LIBNEWT):/,/rv/ s/^/#/'          \\\n"
"    -e 's/$(LIBNEWT)/$(LIBNEWTSH)/g'        \\\n"
"    -i Makefile.in                          &amp;&amp;\n"
"\n"
"./configure --prefix=/usr           \\\n"
"            --with-gpm-support      \\\n"
"            --with-python=python&python3-majorver; &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>sed -e '/install -m 644 $(LIBNEWT)/ s/^/#/' \\\n"
"    -e '/$(LIBNEWT):/,/rv/ s/^/#/'          \\\n"
"    -e 's/$(LIBNEWT)/$(LIBNEWTSH)/g'        \\\n"
"    -i Makefile.in                          &amp;&amp;\n"
"\n"
"./configure --prefix=/usr           \\\n"
"            --with-gpm-support      \\\n"
"            --with-python=python&python3-majorver; &amp;&amp;\n"
"make</userinput>"

