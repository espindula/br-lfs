#: blfs-en/general/graphlib/openjpeg.xml:84
msgid "Installation of OpenJPEG"
msgstr "Installation de OpenJPEG"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/graphlib/openjpeg.xml:87
msgid ""
"Install <application>OpenJPEG</application> by running the following "
"commands:"
msgstr ""
"Installez <application>OpenJPEG</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/graphlib/openjpeg.xml:91
#, no-wrap
msgid ""
"<userinput>autoreconf -f -i &amp;&amp;\n"
"./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>autoreconf -f -i &amp;&amp;\n"
"./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/graphlib/openjpeg.xml:95
msgid "This package does not come with a testsuite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/graphlib/openjpeg.xml:97
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem class=\"username\">root</"
"systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/graphlib/openjpeg.xml:99
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

