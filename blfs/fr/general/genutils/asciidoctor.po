#. type: Content of: <sect1><sect2><title>
#: blfs-en/general/genutils/asciidoctor.xml:83
msgid "Installation of Asciidoctor"
msgstr "Installation de Asciidoctor"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genutils/asciidoctor.xml:86
msgid "Build the Ruby gem:"
msgstr "Construisez la gem Ruby&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genutils/asciidoctor.xml:89
#, no-wrap
msgid "<userinput>gem build asciidoctor.gemspec</userinput>"
msgstr "<userinput>gem build asciidoctor.gemspec</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genutils/asciidoctor.xml:92
msgid "The test suite needs many Ruby gems beyond the scope of BLFS."
msgstr ""
"La suite de tests demande plusieurs gems Ruby en dehors de la portée de "
"BLFS."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genutils/asciidoctor.xml:96
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genutils/asciidoctor.xml:99
#, no-wrap
msgid ""
"<userinput>gem install asciidoctor-&asciidoctor-version;.gem &amp;&amp;\n"
"install -vm644 man/asciidoctor.1 /usr/share/man/man1</userinput>"
msgstr ""
"<userinput>gem install asciidoctor-&asciidoctor-version;.gem &amp;&amp;\n"
"install -vm644 man/asciidoctor.1 /usr/share/man/man1</userinput>"

