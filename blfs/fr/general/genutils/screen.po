#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genutils/screen.xml:104
msgid ""
"Install <application>Screen</application> by running the following commands:"
msgstr ""
"Installez <application>Screen</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genutils/screen.xml:108
#, no-wrap
msgid ""
"<userinput>sh autogen.sh                                 &amp;&amp;\n"
"./configure --prefix=/usr                     \\\n"
"            --infodir=/usr/share/info         \\\n"
"            --mandir=/usr/share/man           \\\n"
"            --with-socket-dir=/run/screen     \\\n"
"            --with-pty-group=5                \\\n"
"            --with-sys-screenrc=/etc/screenrc &amp;&amp;\n"
"\n"
"sed -i -e \"s%/usr/local/etc/screenrc%/etc/screenrc%\" {etc,doc}/* &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>sh autogen.sh                                 &amp;&amp;\n"
"./configure --prefix=/usr                     \\\n"
"            --infodir=/usr/share/info         \\\n"
"            --mandir=/usr/share/man           \\\n"
"            --with-socket-dir=/run/screen     \\\n"
"            --with-pty-group=5                \\\n"
"            --with-sys-screenrc=/etc/screenrc &amp;&amp;\n"
"\n"
"sed -i -e \"s%/usr/local/etc/screenrc%/etc/screenrc%\" {etc,doc}/* &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genutils/screen.xml:120
