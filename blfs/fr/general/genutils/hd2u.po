#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genutils/hd2u.xml:72
msgid ""
"Install <application>hd2u</application> by running the following commands:"
msgstr ""
"Installez <application>hd2u</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/general/genutils/hd2u.xml:75
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/general/genutils/hd2u.xml:78
