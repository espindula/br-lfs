msgid ""
"<filename>/etc/firewall/applet.conf</filename>, "
"<filename>/etc/firewalld/firewalld.conf</filename>, and "
"<filename>/etc/sysconfig/firewalld</filename>"
msgstr ""
"<filename>/etc/firewall/applet.conf</filename>, "
"<filename>/etc/firewalld/firewalld.conf</filename> et "
"<filename>/etc/sysconfig/firewalld</filename>"

