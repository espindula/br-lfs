#. type: Content of: <sect1><sect2><para>
#: blfs-en/postlfs/security/volume_key.xml:113
msgid ""
"Install <application>volume_key</application> by running the following "
"commands:"
msgstr ""
"Installez <application>volume_key</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/postlfs/security/volume_key.xml:117
#, no-wrap
#| msgid ""
#| "<userinput>sed -i '/config.h/d' lib/libvolume_key.h &amp;&amp;\n"
#| "\n"
#| "autoreconf -fiv           &amp;&amp;\n"
#| "./configure --prefix=/usr &amp;&amp;\n"
#| "make</userinput>"
msgid ""
"<userinput>patch -Np1 -i ../volume_key-&volume_key-version;-cryptsetup2-1.patch &amp;&amp;\n"
"\n"
"./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>patch -Np1 -i ../volume_key-&volume_key-version;-cryptsetup2-1.patch &amp;&amp;\n"
"\n"
"./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"

