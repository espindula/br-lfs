#. type: Content of: <sect1><sect2><para>
#: blfs-en/postlfs/security/gnutls.xml:142
msgid ""
"Install <application>GnuTLS</application> by running the following commands:"
msgstr ""
"Installez <application>GnuTLS</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/postlfs/security/gnutls.xml:146
#, no-wrap
#| msgid ""
#| "<userinput>./configure --prefix=/usr \\\n"
#| "            --docdir=/usr/share/doc/gnutls-&gnutls-version; \\\n"
#| "            --disable-rpath \\\n"
#| "            --with-default-trust-store-pkcs11=\"pkcs11:\" &amp;&amp;\n"
#| "make</userinput>"
msgid ""
"<userinput>./configure --prefix=/usr \\\n"
"            --docdir=/usr/share/doc/gnutls-&gnutls-version; \\\n"
"            --with-default-trust-store-pkcs11=\"pkcs11:\" &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr \\\n"
"            --docdir=/usr/share/doc/gnutls-&gnutls-version; \\\n"
"            --with-default-trust-store-pkcs11=\"pkcs11:\" &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/postlfs/security/gnutls.xml:160
msgid "To test the results, issue: <command>make check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/postlfs/security/gnutls.xml:164
