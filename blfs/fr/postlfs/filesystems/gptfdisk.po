#, no-wrap
msgid ""
"<userinput>patch -Np1 -i ../gptfdisk-&gptfdisk-version;-convenience-1.patch &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>patch -Np1 -i ../gptfdisk-&gptfdisk-version;-convenience-1.patch &amp;&amp;\n"
"make</userinput>"

