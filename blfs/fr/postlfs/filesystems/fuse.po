#: blfs-en/postlfs/filesystems/fuse.xml:125
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr    \\\n"
"            --disable-static \\\n"
"            INIT_D_PATH=/tmp/init.d &amp;&amp;\n"
"\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr    \\\n"
"            --disable-static \\\n"
"            INIT_D_PATH=/tmp/init.d &amp;&amp;\n"
"\n"
"make</userinput>"

