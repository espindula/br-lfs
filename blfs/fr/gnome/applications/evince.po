#. type: Content of: <sect1><sect2><title>
#: blfs-en/gnome/applications/evince.xml:115
msgid "Installation of Evince"
msgstr "Installation de Evince"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/applications/evince.xml:118
msgid ""
"Install <application>Evince</application> by running the following commands:"
msgstr ""
"Installez <application>Evince</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/applications/evince.xml:122
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"CPPFLAGS+=\" -I/opt/texlive/&texlive-year;/include\" \\\n"
"meson setup --prefix=/usr          \\\n"
"            --buildtype=release    \\\n"
"            -Dgtk_doc=false        \\\n"
"            --wrap-mode=nodownload \\\n"
"            -Dsystemduserunitdir=no .. &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"CPPFLAGS+=\" -I/opt/texlive/&texlive-year;/include\" \\\n"
"meson setup --prefix=/usr          \\\n"
"            --buildtype=release    \\\n"
"            -Dgtk_doc=false        \\\n"
"            --wrap-mode=nodownload \\\n"
"            -Dsystemduserunitdir=no .. &amp;&amp;\n"
"ninja</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/applications/evince.xml:133
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"CPPFLAGS+=\" -I/opt/texlive/&texlive-year;/include\" \\\n"
"meson setup --prefix=/usr          \\\n"
"            --buildtype=release    \\\n"
"            -Dgtk_doc=false        \\\n"
"            --wrap-mode=nodownload \\\n"
"            ..                     &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"CPPFLAGS+=\" -I/opt/texlive/&texlive-year;/include\" \\\n"
"meson setup --prefix=/usr          \\\n"
"            --buildtype=release    \\\n"
"            -Dgtk_doc=false        \\\n"
"            --wrap-mode=nodownload \\\n"
"            ..                     &amp;&amp;\n"
"ninja</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/applications/evince.xml:145
msgid ""
"If you have <xref linkend='gi-docgen'/> installed and wish to build the API "
"documentation for this package, issue:"
msgstr ""
"Si vous avez installé <xref linkend='gi-docgen'/> et souhaitez construire la"
" documentation de l'API de ce paquet, exécutez&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/applications/evince.xml:149
#, no-wrap
msgid ""
"<userinput>sed \"/docs_dir/s@\\$@ / 'evince-&evince-version;'@\" -i ../help/meson.build &amp;&amp;\n"
"sed '/fatal-warnings/d' -i ../help/reference/*/meson.build    &amp;&amp;\n"
"meson configure -Dgtk_doc=true                                &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>sed \"/docs_dir/s@\\$@ / 'evince-&evince-version;'@\" -i ../help/meson.build &amp;&amp;\n"
"sed '/fatal-warnings/d' -i ../help/reference/*/meson.build    &amp;&amp;\n"
"meson configure -Dgtk_doc=true                                &amp;&amp;\n"
"ninja</userinput>"

