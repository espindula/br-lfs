#. type: Content of: <sect1><sect2><title>
#: blfs-en/gnome/applications/vino.xml:100
msgid "Installation of Vino"
msgstr "Installation de Vino"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/applications/vino.xml:103
msgid ""
"Install <application>Vino</application> by running the following commands:"
msgstr ""
"Installez <application>Vino</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/applications/vino.xml:107
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr --sysconfdir=/etc &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr --sysconfdir=/etc &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/applications/vino.xml:110
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr                \\\n"
"            --sysconfdir=/etc            \\\n"
"            --with-systemduserunitdir=no &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr                \\\n"
"            --sysconfdir=/etc            \\\n"
"            --with-systemduserunitdir=no &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/applications/vino.xml:116
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'a pas de suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/applications/vino.xml:120
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/applications/vino.xml:123
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

