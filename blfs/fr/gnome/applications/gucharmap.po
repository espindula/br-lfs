msgid "Installation of Gucharmap"
msgstr "Installation de Gucharmap"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/applications/gucharmap.xml:129
msgid ""
"Install <application>Gucharmap</application> by running the following "
"commands:"
msgstr ""
"Installez <application>Gucharmap</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/applications/gucharmap.xml:133
#, no-wrap
msgid ""
"<userinput>mkdir build              &amp;&amp;\n"
"cd build                 &amp;&amp;\n"
"mkdir ucd                &amp;&amp;\n"
"pushdir ucd              &amp;&amp;\n"
"unzip ../../UCD.zip      &amp;&amp;\n"
"cp -v ../../Unihan.zip . &amp;&amp;\n"
"popd                     &amp;&amp;\n"
"meson --prefix=/usr      \\\n"
"      --strip            \\\n"
"      -Ducd_path=./ucd   \\\n"
"      -Ddocs=false ..    &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build              &amp;&amp;\n"
"cd build                 &amp;&amp;\n"
"mkdir ucd                &amp;&amp;\n"
"pushdir ucd              &amp;&amp;\n"
"unzip ../../UCD.zip      &amp;&amp;\n"
"cp -v ../../Unihan.zip . &amp;&amp;\n"
"popd                     &amp;&amp;\n"
"meson --prefix=/usr      \\\n"
"      --strip            \\\n"
"      -Ducd_path=./ucd   \\\n"
"      -Ddocs=false ..    &amp;&amp;\n"
"ninja</userinput>"

