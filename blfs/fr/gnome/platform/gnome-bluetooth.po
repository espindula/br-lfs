#. type: Content of: <sect1><sect2><title>
#: blfs-en/gnome/platform/gnome-bluetooth.xml:109
msgid "Installation of GNOME Bluetooth"
msgstr "Installation de GNOME Bluetooth"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/gnome-bluetooth.xml:112
msgid ""
"Install <application>GNOME Bluetooth</application> by running the following "
"commands:"
msgstr ""
"Installez <application>GNOME Bluetooth</application> en lançant les "
"commandes suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/platform/gnome-bluetooth.xml:116
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson --prefix=/usr .. &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson --prefix=/usr .. &amp;&amp;\n"
"ninja</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/gnome-bluetooth.xml:123
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/gnome-bluetooth.xml:127
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/platform/gnome-bluetooth.xml:130
#, no-wrap
msgid "<userinput>ninja install</userinput>"
msgstr "<userinput>ninja install</userinput>"

