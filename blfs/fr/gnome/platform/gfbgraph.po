msgid "Installation of gfbgraph"
msgstr "Installation de gfbgraph"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/gfbgraph.xml:96
msgid ""
"Install <application>gfbgraph</application> by running the following "
"commands:"
msgstr ""
"Installez <application>gfbgraph</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/platform/gfbgraph.xml:100
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"

