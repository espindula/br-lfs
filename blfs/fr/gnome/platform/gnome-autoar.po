#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/gnome-autoar.xml:99
msgid ""
"Install <application>gnome-autoar</application> by running the following "
"commands:"
msgstr ""
"Installez <application>gnome-autoar</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/platform/gnome-autoar.xml:103
#, no-wrap
#| msgid ""
#| "<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
#| "make</userinput>"
msgid ""
"<userinput>./configure --prefix=/usr    \\\n"
"            --disable-debug  \\\n"
"            --disable-static &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr    \\\n"
"            --disable-debug  \\\n"
"            --disable-static &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/gnome-autoar.xml:109
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/gnome-autoar.xml:113
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/platform/gnome-autoar.xml:116
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

