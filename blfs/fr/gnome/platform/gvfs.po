"      -Dmtp=false       \\\n"
"      -Dsmb=false       \\\n"
"      -Ddnssd=false     \\\n"
"      -Dgoa=false       \\\n"
"      -Dgoogle=false    .. &amp;&amp;\n"
"ninja</userinput>"

