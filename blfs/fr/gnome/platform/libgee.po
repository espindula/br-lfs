#. type: Content of: <sect1><sect2><title>
#: blfs-en/gnome/platform/libgee.xml:92
msgid "Installation of libgee"
msgstr "Installation de libgee"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/libgee.xml:95
msgid ""
"Install <application>libgee</application> by running the following commands:"
msgstr ""
"Installez <application>libgee</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/platform/libgee.xml:99
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/libgee.xml:103
msgid "To test the results, issue: <command>make check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/libgee.xml:109
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/platform/libgee.xml:112
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

