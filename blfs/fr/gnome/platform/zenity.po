#. type: Content of: <sect1><sect2><title>
#: blfs-en/gnome/platform/zenity.xml:100
msgid "Installation of Zenity"
msgstr "Installation de Zenity"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/zenity.xml:103
msgid ""
"Install <application>Zenity</application> by running the following commands:"
msgstr ""
"Installez <application>Zenity</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/platform/zenity.xml:107
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd build    &amp;&amp;\n"
"\n"
"meson --prefix=/usr --buildtype=release -Dlibnotify=true .. &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd build    &amp;&amp;\n"
"\n"
"meson --prefix=/usr --buildtype=release -Dlibnotify=true .. &amp;&amp;\n"
"ninja</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/zenity.xml:114
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'a pas de suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/zenity.xml:118
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/platform/zenity.xml:121
#, no-wrap
msgid "<userinput>ninja install</userinput>"
msgstr "<userinput>ninja install</userinput>"

