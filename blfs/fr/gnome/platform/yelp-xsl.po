#: blfs-en/gnome/platform/yelp-xsl.xml:88
msgid "Installation of Yelp XSL"
msgstr "Installation de Yelp XSL"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/yelp-xsl.xml:91
msgid ""
"Install <application>Yelp XSL</application> by running the following "
"commands:"
msgstr ""
"Installez <application>Yelp XSL</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/platform/yelp-xsl.xml:95
#, no-wrap
msgid "<userinput>./configure --prefix=/usr</userinput>"
msgstr "<userinput>./configure --prefix=/usr</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/yelp-xsl.xml:98
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/yelp-xsl.xml:102
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/platform/yelp-xsl.xml:105
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

