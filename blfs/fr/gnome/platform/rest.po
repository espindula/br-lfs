#: blfs-en/gnome/platform/rest.xml:98
msgid "Installation of rest"
msgstr "Installation de rest"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/rest.xml:101
msgid ""
"Install <application>rest</application> by running the following commands:"
msgstr ""
"Installez <application>rest</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/platform/rest.xml:105
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson --prefix=/usr        \\\n"
"      --buildtype=release  \\\n"
"      -Dexamples=false     \\\n"
"      -Dgtk_doc=false      .. &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson --prefix=/usr        \\\n"
"      --buildtype=release  \\\n"
"      -Dexamples=false     \\\n"
"      -Dgtk_doc=false      .. &amp;&amp;\n"
"ninja</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/rest.xml:115
msgid "To test the results, issue: <command>ninja test</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>ninja test</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/rest.xml:119
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/platform/rest.xml:122
#, no-wrap
msgid "<userinput>ninja install</userinput>"
msgstr "<userinput>ninja install</userinput>"

