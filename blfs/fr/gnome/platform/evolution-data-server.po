#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/evolution-data-server.xml:119
msgid ""
"Install <application>Evolution Data Server</application> by running the "
"following commands:"
msgstr ""
"Installez <application>Evolution Data Server</application> en exécutant les "
"commandes suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/platform/evolution-data-server.xml:123
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr    \\\n"
"      -DSYSCONF_INSTALL_DIR=/etc     \\\n"
"      -DENABLE_VALA_BINDINGS=ON      \\\n"
"      -DENABLE_INSTALLED_TESTS=ON    \\\n"
"      -DWITH_OPENLDAP=OFF            \\\n"
"      -DWITH_KRB5=OFF                \\\n"
"      -DENABLE_INTROSPECTION=ON      \\\n"
"      -DENABLE_GTK_DOC=OFF           \\\n"
"      -DWITH_LIBDB=OFF               \\\n"
"      -DWITH_SYSTEMDUSERUNITDIR=no   \\\n"
"      -Wno-dev -G Ninja ..           &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr    \\\n"
"      -DSYSCONF_INSTALL_DIR=/etc     \\\n"
"      -DENABLE_VALA_BINDINGS=ON      \\\n"
"      -DENABLE_INSTALLED_TESTS=ON    \\\n"
"      -DWITH_OPENLDAP=OFF            \\\n"
"      -DWITH_KRB5=OFF                \\\n"
"      -DENABLE_INTROSPECTION=ON      \\\n"
"      -DENABLE_GTK_DOC=OFF           \\\n"
"      -DWITH_LIBDB=OFF               \\\n"
"      -DWITH_SYSTEMDUSERUNITDIR=no   \\\n"
"      -Wno-dev -G Ninja ..           &amp;&amp;\n"
"ninja</userinput>"

