#. type: Content of: <sect1><sect2><title>
#: blfs-en/gnome/platform/gexiv2.xml:97
msgid "Installation of gexiv2"
msgstr "Installation de gexiv2"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/gexiv2.xml:100
msgid ""
"Install <application>gexiv2</application> by running the following commands:"
msgstr ""
"Installez <application>gexiv2</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/platform/gexiv2.xml:104
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson --prefix=/usr .. &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson --prefix=/usr .. &amp;&amp;\n"
"ninja</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/gexiv2.xml:111
msgid "To test the results, run <command>ninja test</command>."
msgstr "Pour tester les résultats, lancez <command>ninja test</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/gexiv2.xml:115
msgid "As the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"En tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/platform/gexiv2.xml:118
#, no-wrap
msgid "<userinput>ninja install</userinput>"
msgstr "<userinput>ninja install</userinput>"

