#: blfs-en/gnome/platform/gexiv2.xml:105
msgid "Installation of gexiv2"
msgstr "Installation de gexiv2"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/gexiv2.xml:124
msgid ""
"Install <application>gexiv2</application> by running the following commands:"
msgstr ""
"Installez <application>gexiv2</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/platform/gexiv2.xml:128
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson setup --prefix=/usr --buildtype=release .. &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson setup --prefix=/usr --buildtype=release .. &amp;&amp;\n"
"ninja</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/gexiv2.xml:135
msgid "To test the results, run:"
msgstr "Pour tester les résultats, exécutezez&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/platform/gexiv2.xml:138
#, no-wrap
msgid ""
"<userinput>meson configure -Dtests=true &amp;&amp;\n"
"ninja test</userinput>"
msgstr ""
"<userinput>meson configure -Dtests=true &amp;&amp;\n"
"ninja test</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/gnome/platform/gexiv2.xml:142
msgid "As the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"En tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/gnome/platform/gexiv2.xml:145
#, no-wrap
msgid "<userinput>ninja install</userinput>"
msgstr "<userinput>ninja install</userinput>"

