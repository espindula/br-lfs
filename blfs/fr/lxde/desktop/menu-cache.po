#. type: Content of: <sect1><sect2><title>
#: blfs-en/lxde/desktop/menu-cache.xml:92
msgid "Installation of Menu Cache"
msgstr "Installation de Menu Cache"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxde/desktop/menu-cache.xml:95
msgid ""
"Install <application>Menu Cache</application> by running the following "
"commands:"
msgstr ""
"Installez <application>Menu Cache</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxde/desktop/menu-cache.xml:99
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr \\\n"
"            --disable-static &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr \\\n"
"            --disable-static &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxde/desktop/menu-cache.xml:104
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxde/desktop/menu-cache.xml:108
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxde/desktop/menu-cache.xml:111
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

