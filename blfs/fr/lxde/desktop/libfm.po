#. type: Content of: <sect1><sect2><title>
#: blfs-en/lxde/desktop/libfm.xml:104
msgid "Installation of libfm"
msgstr "Installation de libfm"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxde/desktop/libfm.xml:107
msgid ""
"Install <application>libfm</application> by running the following commands:"
msgstr ""
"Installez <application>libfm</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxde/desktop/libfm.xml:111
#, no-wrap
#| msgid ""
#| "<userinput>./configure --prefix=/usr \\\n"
#| "            --sysconfdir=/etc \\\n"
#| "            --disable-static  &amp;&amp;\n"
#| "make</userinput>"
msgid ""
"<userinput>./configure --prefix=/usr     \\\n"
"            --sysconfdir=/etc \\\n"
"            --with-gtk=3      \\\n"
"            --disable-static  &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr     \\\n"
"            --sysconfdir=/etc \\\n"
"            --with-gtk=3      \\\n"
"            --disable-static  &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxde/desktop/libfm.xml:118
msgid "To test the results, issue: <command>make check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxde/desktop/libfm.xml:122
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxde/desktop/libfm.xml:125
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

