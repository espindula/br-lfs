#. type: Content of: <sect1><sect2><title>
#: blfs-en/lxde/desktop/libfm-extra.xml:99
msgid "Installation of libfm-extra"
msgstr "Installation de libfm-extra"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxde/desktop/libfm-extra.xml:102
msgid ""
"Install <application>libfm-extra</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libfm-extra</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxde/desktop/libfm-extra.xml:106
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr     \\\n"
"            --sysconfdir=/etc \\\n"
"            --with-extra-only \\\n"
"            --with-gtk=no     \\\n"
"            --disable-static  &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr     \\\n"
"            --sysconfdir=/etc \\\n"
"            --with-extra-only \\\n"
"            --with-gtk=no     \\\n"
"            --disable-static  &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxde/desktop/libfm-extra.xml:114
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxde/desktop/libfm-extra.xml:118
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxde/desktop/libfm-extra.xml:121
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

