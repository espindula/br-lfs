#. type: Content of: <sect1><sect2><bridgehead>
#: blfs-en/lxde/desktop/lxpanel.xml:73
msgid "LXPanel Dependencies"
msgstr "Dépendances de LXPanel"

#. type: Content of: <sect1><sect2><bridgehead>
#: blfs-en/lxde/desktop/lxpanel.xml:75
msgid "Required"
msgstr "Requises"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxde/desktop/lxpanel.xml:77
msgid ""
"<xref linkend=\"keybinder2\"/>, <xref linkend=\"libwnck2\"/>, <xref "
"linkend=\"lxmenu-data\"/> and <xref linkend=\"menu-cache\"/>"
msgstr ""
"<xref linkend=\"keybinder2\"/>, <xref linkend=\"libwnck2\"/>, <xref "
"linkend=\"lxmenu-data\"/> et <xref linkend=\"menu-cache\"/>"

#. type: Content of: <sect1><sect2><bridgehead>
#: blfs-en/lxde/desktop/lxpanel.xml:83
msgid "Recommended"
msgstr "Recommandées"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxde/desktop/lxpanel.xml:85
msgid ""
"<xref linkend=\"alsa-lib\"/>, <xref linkend=\"gnome-screenshot\"/> (to take "
"screenshots using PrtScr), <xref linkend=\"libxml2\"/> (weather plugin), and"
" <xref linkend=\"wireless_tools\"/>"
msgstr ""
"<xref linkend=\"alsa-lib\"/>, <xref linkend=\"gnome-screenshot\"/> (pour "
"prendre des copies d'écran avec PrtScr), <xref linkend=\"libxml2\"/> (pour "
"le greffon météorologique) et <xref linkend=\"wireless_tools\"/>"

#. type: Content of: <sect1><sect2><bridgehead>
#: blfs-en/lxde/desktop/lxpanel.xml:91
msgid "Optional"
msgstr "Facultatives"

#.       <xref linkend="libxslt"/>
#.  with
#.       <xref linkend="DocBook"/> and
#.       <xref linkend="docbook-xsl"/> (to build man pages)
