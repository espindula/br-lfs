#. type: Content of: <sect1><sect2><title>
#: blfs-en/archive/libosinfo.xml:100
msgid "Installation of libosinfo"
msgstr "Installation de libosinfo"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/libosinfo.xml:103
msgid ""
"Install <application>libosinfo</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libosinfo</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/libosinfo.xml:107
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr \\\n"
"            --enable-vala \\\n"
"            --disable-static &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr \\\n"
"            --enable-vala \\\n"
"            --disable-static &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/libosinfo.xml:113
msgid "To test the results, issue: <command>make check</command>."
msgstr "Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/libosinfo.xml:117
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem class=\"username\">root</"
"systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/libosinfo.xml:120
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

