#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: blfs-en/archive/udftools.xml:157
msgid "<command>cdrwtool</command>"
msgstr "<command>cdrwtool</command>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: blfs-en/archive/udftools.xml:159
msgid ""
"provides facilities to manage CD-RW drives, including formatting new disks, "
"setting the read and write speeds, etc."
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: blfs-en/archive/udftools.xml:162 blfs-en/archive/udftools.xml:191
msgid "Example:"
msgstr "Example:"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><screen>
#: blfs-en/archive/udftools.xml:164
#, no-wrap
msgid "<userinput>cdrwtool -d /dev/scd0 -q</userinput>"
msgstr "<userinput>cdrwtool -d /dev/scd0 -q</userinput>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: blfs-en/archive/udftools.xml:166
msgid "prepares a new CD-RW for use and formats it with a UDF file system."
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><indexterm><primary>
#: blfs-en/archive/udftools.xml:169
msgid "cdrwtool"
msgstr "cdrwtool"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: blfs-en/archive/udftools.xml:175
msgid "<command>mkudffs</command>"
msgstr "<command>mkudffs</command>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: blfs-en/archive/udftools.xml:177
msgid ""
"is used to create new UDF file systems. It can be used on hard disks and CD-"
"Rs as well as CD-RWs."
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><indexterm><primary>
#: blfs-en/archive/udftools.xml:180
msgid "mkudffs"
msgstr "mkudffs"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: blfs-en/archive/udftools.xml:186
msgid "<command>pktsetup</command>"
msgstr "<command>pktsetup</command>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: blfs-en/archive/udftools.xml:188
msgid ""
"is used to establish and break down associations between the kernel packet "
"driver and a physical drive."
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><screen>
#: blfs-en/archive/udftools.xml:193
#, no-wrap
msgid ""
"<userinput>pktsetup /dev/pktcdvd0 /dev/scd0\n"
"mount /dev/pktcdvd0 /mnt/cdrom -t udf -o rw,noatime</userinput>"
msgstr ""
"<userinput>pktsetup /dev/pktcdvd0 /dev/scd0\n"
"mount /dev/pktcdvd0 /mnt/cdrom -t udf -o rw,noatime</userinput>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: blfs-en/archive/udftools.xml:196
msgid ""
"associates the physical device <filename>/dev/scd0</filename> with the "
"kernel packet driver <filename>/dev/pktcdvd0</filename>, then mounts a UDF "
"formatted CD-RW for read/write access."
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><indexterm><primary>
#: blfs-en/archive/udftools.xml:200
msgid "pktsetup"
msgstr "pktsetup"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: blfs-en/archive/udftools.xml:206
msgid "<command>udffsck</command>"
msgstr "<command>udffsck</command>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: blfs-en/archive/udftools.xml:208
msgid "is used to check the integrity and correct errors on UDF filesystems."
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><indexterm><primary>
#: blfs-en/archive/udftools.xml:211
msgid "udffsck"
msgstr "udffsck"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: blfs-en/archive/udftools.xml:217
msgid "<command>wrudf</command>"
msgstr "<command>wrudf</command>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: blfs-en/archive/udftools.xml:219
msgid "is used to maintain a UDF filesystem."
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><indexterm><primary>
#: blfs-en/archive/udftools.xml:221
msgid "wrudf"
msgstr "wrudf"

