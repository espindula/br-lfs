"<userinput>sed -i \"/via_3d.h/i #include &lt;xorg-server.h&gt;\" src/via_3d.c &amp;&amp;\n"
"\n"
"./configure $XORG_CONFIG &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>sed -i \"/via_3d.h/i #include &lt;xorg-server.h&gt;\" src/via_3d.c &amp;&amp;\n"
"\n"
"./configure $XORG_CONFIG &amp;&amp;\n"
"make</userinput>"

