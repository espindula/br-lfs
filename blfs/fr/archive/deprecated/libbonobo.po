#. type: Content of: <sect1><sect2><title>
#: blfs-en/archive/deprecated/libbonobo.xml:84
msgid "Installation of libbonobo"
msgstr "Installation de libbonobo"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/deprecated/libbonobo.xml:86
msgid ""
"Install <application>libbonobo</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libbonobo</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/deprecated/libbonobo.xml:89
#, no-wrap
msgid ""
"<userinput>ORBit_prefix=$(pkg-config --variable=prefix ORBit-2.0) &amp;&amp;\n"
"\n"
"./configure --prefix=$ORBit_prefix                     \\\n"
"            --sysconfdir=&gnome-old-etc-dir;           \\\n"
"            --libexecdir=$ORBit_prefix/lib/bonobo-2.0  \\\n"
"            --mandir=$ORBit_prefix/share/man           &amp;&amp;\n"
"\n"
"unset ORBit_prefix                                     &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>ORBit_prefix=$(pkg-config --variable=prefix ORBit-2.0) &amp;&amp;\n"
"\n"
"./configure --prefix=$ORBit_prefix                     \\\n"
"            --sysconfdir=&gnome-old-etc-dir;           \\\n"
"            --libexecdir=$ORBit_prefix/lib/bonobo-2.0  \\\n"
"            --mandir=$ORBit_prefix/share/man           &amp;&amp;\n"
"\n"
"unset ORBit_prefix                                     &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/deprecated/libbonobo.xml:99
msgid "To test the results, issue: <command>make check</command>."
msgstr "Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/deprecated/libbonobo.xml:101
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem class=\"username\">root</"
"systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/deprecated/libbonobo.xml:103
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

#. type: Content of: <sect1><sect2><title>
