"CFLAGS=\"-fno-delete-null-pointer-checks\"   \\\n"
"CXXFLAGS=\"-fno-delete-null-pointer-checks\" \\\n"
"../configure --prefix=/usr --enable-introspection &amp;&amp;\n"
"make &amp;&amp;\n"
"popd</userinput>"
