#: blfs-en/x/lib/webkitgtk2.xml:169
#, no-wrap
#| msgid ""
#| "<userinput>mkdir -vp build-3           &amp;&amp;\n"
#| "cp -a Documentation build-3 &amp;&amp;\n"
