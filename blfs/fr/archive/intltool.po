#. type: Content of: <sect1><sect2><title>
#: blfs-en/archive/intltool.xml:91
msgid "Installation of Intltool"
msgstr "Installation de Intltool"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/intltool.xml:94
msgid ""
"Install <application>Intltool</application> by running the following "
"commands:"
msgstr ""
"Installez <application>Intltool</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/intltool.xml:98
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/intltool.xml:102
msgid "To test the results, issue: <command>make check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/intltool.xml:106
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/intltool.xml:109
#, no-wrap
msgid ""
"<userinput>make install &amp;&amp;\n"
"install -v -m644 -D doc/I18N-HOWTO \\\n"
"    /usr/share/doc/intltool-&intltool-version;/I18N-HOWTO</userinput>"
msgstr ""
"<userinput>make install &amp;&amp;\n"
"install -v -m644 -D doc/I18N-HOWTO \\\n"
"    /usr/share/doc/intltool-&intltool-version;/I18N-HOWTO</userinput>"

