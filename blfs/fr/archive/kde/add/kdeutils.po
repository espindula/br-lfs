"./configure --prefix=$TRINITY_PREFIX                      \\\n"
"            --with-qt-dir=$QTDIR                          \\\n"
"            --enable-closure                              \\\n"
"            --with-extra-includes=$TRINITY_PREFIX/include \\\n"
"            --with-extra-libs=$TRINITY_PREFIX/lib         &amp;&amp;\n"
"\n"
"make</userinput>"
