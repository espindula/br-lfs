#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/kde/add/kdeartwork.xml:86
#, fuzzy
msgid "Install <application>kdeartwork</application> with:"
msgstr "Installez <application>kdeartwork</application> avec&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/kde/add/kdeartwork.xml:88
#, no-wrap
msgid ""
"<userinput>MAKEDIR=/tmp/buildkdeart &amp;&amp;\n"
"KDEART=`pwd`             &amp;&amp;\n"
"rm -rf   $MAKEDIR        &amp;&amp;\n"
"mkdir -p $MAKEDIR        &amp;&amp;\n"
"cd       $MAKEDIR        &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=$TRINITY_PREFIX \\\n"
"      -DCMAKE_VERBOSE_MAKEFILE=ON            \\\n"
"      -DWITH_XSCREENSAVER=ON                 \\\n"
"      -DWITH_LIBART=ON                       \\\n"
"      -DWITH_OPENGL=ON                       \\\n"
"      -DWITH_ARTS=ON                         \\\n"
"      -DBUILD_ALL=ON                         \\\n"
"      $KDEART            &amp;&amp;\n"
"\n"
"unset KDEART MAKEDIR     &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>MAKEDIR=/tmp/buildkdeart &amp;&amp;\n"
"KDEART=`pwd`             &amp;&amp;\n"
"rm -rf   $MAKEDIR        &amp;&amp;\n"
"mkdir -p $MAKEDIR        &amp;&amp;\n"
"cd       $MAKEDIR        &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=$TRINITY_PREFIX \\\n"
"      -DCMAKE_VERBOSE_MAKEFILE=ON            \\\n"
"      -DWITH_XSCREENSAVER=ON                 \\\n"
"      -DWITH_LIBART=ON                       \\\n"
"      -DWITH_OPENGL=ON                       \\\n"
"      -DWITH_ARTS=ON                         \\\n"
"      -DBUILD_ALL=ON                         \\\n"
"      $KDEART            &amp;&amp;\n"
"\n"
"unset KDEART MAKEDIR     &amp;&amp;\n"
"make</userinput>"

