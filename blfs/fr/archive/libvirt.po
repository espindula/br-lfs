#. type: Content of: <sect1><sect2><title>
#: blfs-en/archive/libvirt.xml:120
msgid "Installation of libvirt"
msgstr "Installation de libvirt"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/libvirt.xml:123
msgid ""
"Install <application>libvirt</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libvirt</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/libvirt.xml:127
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr \\\n"
"            --libexecdir=/usr/lib/libvirt \\\n"
"            --with-storage-lvm \\\n"
"            --without-macvtap \\\n"
"            --disable-static \\\n"
"            CPPFLAGS=\"-I/usr/include/tirpc -I/usr/include/libnl3\" &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr \\\n"
"            --libexecdir=/usr/lib/libvirt \\\n"
"            --with-storage-lvm \\\n"
"            --without-macvtap \\\n"
"            --disable-static \\\n"
"            CPPFLAGS=\"-I/usr/include/tirpc -I/usr/include/libnl3\" &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/libvirt.xml:136
msgid "To test the results, issue: <command>make check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/libvirt.xml:140
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/libvirt.xml:143
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

#. type: Content of: <sect1><sect2><title>
