#. type: Content of: <sect1><sect2><title>
#: blfs-en/archive/spice-gtk.xml:102
msgid "Installation of spice-gtk"
msgstr "Installation de spice-gtk"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/spice-gtk.xml:105
msgid ""
"Install <application>spice-gtk</application> by running the following "
"commands:"
msgstr ""
"Installez <application>spice-gtk</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/spice-gtk.xml:109
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr \\\n"
"            --enable-vala \\\n"
"            --disable-static \\\n"
"            --disable-smartcard &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr \\\n"
"            --enable-vala \\\n"
"            --disable-static \\\n"
"            --disable-smartcard &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/spice-gtk.xml:116
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/spice-gtk.xml:120
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem class=\"username\">root</"
"systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/spice-gtk.xml:123
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

#. type: Content of: <sect1><sect2><title>
#: blfs-en/archive/spice-gtk.xml:128
msgid "Command Explanations"
msgstr "Explication des commandes"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/spice-gtk.xml:131
msgid ""
"<option>--enable-vala</option>: This option enables building of the Vala "
"bindings. Remove if you don't have <xref linkend=\"vala\"/> installed."
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/spice-gtk.xml:137
msgid ""
"<option>--disable-smartcard</option>: This option disables use of libcacard. "
"Remove if you have libcacard installed."
msgstr ""

