#. type: Content of: <sect1><sect2><title>
#: blfs-en/archive/mod_dnssd.xml:95
msgid "Installation of mod_dnssd"
msgstr "Installation de mod_dnssd"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/mod_dnssd.xml:98
msgid ""
"Install <application>mod_dnssd</application> by running the following "
"commands:"
msgstr ""
"Installez <application>mod_dnssd</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/mod_dnssd.xml:102
#, no-wrap
msgid ""
"<userinput>sed -i 's/unixd_setup_child/ap_&amp;/' src/mod_dnssd.c &amp;&amp;\n"
"\n"
"./configure --prefix=/usr \\\n"
"            --disable-lynx &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>sed -i 's/unixd_setup_child/ap_&amp;/' src/mod_dnssd.c &amp;&amp;\n"
"\n"
"./configure --prefix=/usr \\\n"
"            --disable-lynx &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/mod_dnssd.xml:109
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/mod_dnssd.xml:113
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem class=\"username\">root</"
"systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/mod_dnssd.xml:116
#, no-wrap
msgid ""
"<userinput>make install\n"
"sed -i 's| usr| /usr|' /etc/httpd/httpd.conf</userinput>"
msgstr ""
"<userinput>make install\n"
"sed -i 's| usr| /usr|' /etc/httpd/httpd.conf</userinput>"

#. type: Content of: <sect1><sect2><title>
