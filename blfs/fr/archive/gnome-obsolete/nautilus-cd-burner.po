#. type: Content of: <sect1><sect2><title>
#: blfs-en/archive/gnome-obsolete/nautilus-cd-burner.xml:85
msgid "Installation of Nautilus CD Burner"
msgstr "Installation de Nautilus CD Burner"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/gnome-obsolete/nautilus-cd-burner.xml:87
msgid ""
"Install <application>Nautilus CD Burner</application> by running the "
"following commands:"
msgstr ""
"Installez <application>Nautilus CD Burner</application> en lançant les "
"commandes suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/gnome-obsolete/nautilus-cd-burner.xml:90
#, no-wrap
msgid ""
"<userinput>./configure --prefix=$(pkg-config --variable=prefix ORBit-2.0) \\\n"
"            --sysconfdir=&gnome-etc-dir; \\\n"
"            --libexecdir=$(pkg-config \\\n"
"                --variable=prefix ORBit-2.0)/lib/nautilus-cd-burner &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=$(pkg-config --variable=prefix ORBit-2.0) \\\n"
"            --sysconfdir=&gnome-etc-dir; \\\n"
"            --libexecdir=$(pkg-config \\\n"
"                --variable=prefix ORBit-2.0)/lib/nautilus-cd-burner &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/gnome-obsolete/nautilus-cd-burner.xml:96
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/gnome-obsolete/nautilus-cd-burner.xml:98
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/gnome-obsolete/nautilus-cd-burner.xml:100
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

#. type: Content of: <sect1><sect2><title>
#: blfs-en/archive/gnome-obsolete/nautilus-cd-burner.xml:105
msgid "Command Explanations"
msgstr "Explication des commandes"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/gnome-obsolete/nautilus-cd-burner.xml:113
#, fuzzy
msgid ""
"<parameter>--libexecdir=`pkg-config --variable=prefix ORBit-2.0`/lib"
"/nautilus-cd-burner</parameter>: This parameter causes the libexec files to "
"be installed in the preferred location of <filename "
"class=\"directory\">$GNOME_PREFIX/lib/nautilus-cd-burner</filename> instead "
"of <filename class=\"directory\">$GNOME_PREFIX/libexec</filename>."
msgstr ""
"<parameter>--libexecdir=`pkg-config --variable=prefix ORBit-2.0`/lib"
"/nautilus-cd-burner</parameter>&nbsp;: Ce paramètre fait que les fichiers "
"libexec s'installent à l'emplacement préféré par <filename "
"class=\"directory\">$GNOME_PREFIX/lib/nautilus-cd-burner</filename> plutôt "
"que dans <filename class=\"directory\">$GNOME_PREFIX/libexec</filename>."

