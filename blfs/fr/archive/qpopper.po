#. type: Content of: <sect1><sect2><title>
#: blfs-en/archive/qpopper.xml:77
msgid "Installation of Qpopper"
msgstr "Installation de Qpopper"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/qpopper.xml:79
msgid "Install <application>Qpopper</application> with the following commands:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/qpopper.xml:82
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr --enable-standalone &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr --enable-standalone &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/qpopper.xml:85
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/qpopper.xml:87
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem class=\"username\">root</"
"systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/qpopper.xml:89
#, no-wrap
msgid ""
"<userinput>make install &amp;&amp;\n"
"install -D -m644 GUIDE.pdf /usr/share/doc/qpopper-&qpopper-version;/GUIDE.pdf</userinput>"
msgstr ""
"<userinput>make install &amp;&amp;\n"
"install -D -m644 GUIDE.pdf /usr/share/doc/qpopper-&qpopper-version;/GUIDE.pdf</userinput>"

