msgid "Installation of GNOME Online Accounts"
msgstr "Installation de GNOME Online Accounts"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/gnome/gnome-online-accounts.xml:108
msgid ""
"Install <application>GNOME Online Accounts</application> by running the "
"following commands:"
msgstr ""
"Installez <application>GNOME Online Accounts</application> en lançant les "
"commandes suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/gnome/gnome-online-accounts.xml:112
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr \\\n"
"            --libexecdir=/usr/lib/gnome-online-accounts \\\n"
"            --disable-static &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr \\\n"
"            --libexecdir=/usr/lib/gnome-online-accounts \\\n"
"            --disable-static &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/gnome/gnome-online-accounts.xml:118
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/gnome/gnome-online-accounts.xml:122
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/gnome/gnome-online-accounts.xml:125
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

