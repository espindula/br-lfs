msgid "Installation of GNOME Screensaver"
msgstr "Installation de GNOME Screensaver"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/gnome/gnome-screensaver.xml:101
msgid ""
"Install <application>GNOME Screensaver</application> by running the "
"following commands:"
msgstr ""
"Installez <application>GNOME Screensaver</application> en lançant les "
"commandes suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/gnome/gnome-screensaver.xml:105
#, no-wrap
msgid ""
"<userinput>sed -i 's|etc/pam\\.d\"|etc\"|' data/Makefile.in &amp;&amp;\n"
"./configure --prefix=/usr \\\n"
"            --sysconfdir=/etc \\\n"
"            --libexecdir=/usr/lib/gnome-screensaver \\\n"
"            --with-pam-prefix=/etc &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>sed -i 's|etc/pam\\.d\"|etc\"|' data/Makefile.in &amp;&amp;\n"
"./configure --prefix=/usr \\\n"
"            --sysconfdir=/etc \\\n"
"            --libexecdir=/usr/lib/gnome-screensaver \\\n"
"            --with-pam-prefix=/etc &amp;&amp;\n"
"make</userinput>"

