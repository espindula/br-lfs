#. type: Content of: <sect1><sect2><title>
#: blfs-en/archive/gnome/rygel.xml:91
msgid "Installation of Rygel"
msgstr "Installation de Rygel"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/gnome/rygel.xml:94
msgid ""
"Install <application>Rygel</application> by running the following commands:"
msgstr ""
"Installez <application>Rygel</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/gnome/rygel.xml:98
#, fuzzy, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr \\\n"
"            --sysconfdir=/etc &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr \\\n"
"            --sysconfdir=/etc &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/gnome/rygel.xml:103
msgid "To test the results, issue: <command>make check</command>."
msgstr "Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/gnome/rygel.xml:107
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem class=\"username\">root</"
"systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/gnome/rygel.xml:110
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

