#. type: Content of: <sect1><sect2><title>
#: blfs-en/archive/gnome/orca.xml:101
msgid "Installation of Orca"
msgstr "Installation de Orca"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/gnome/orca.xml:104
msgid ""
"Install <application>Orca</application> by running the following commands:"
msgstr ""
"Installez <application>Orca</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/gnome/orca.xml:108
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr --sysconfdir=/etc &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr --sysconfdir=/etc &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/gnome/orca.xml:112
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/gnome/orca.xml:116
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/gnome/orca.xml:119
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

