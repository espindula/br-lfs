#. type: Content of: <sect1><sect2><title>
#: blfs-en/archive/gnome/librest.xml:98
msgid "Installation of librest"
msgstr "Installation de librest"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/gnome/librest.xml:101
msgid ""
"Install <application>librest</application> by running the following commands:"
msgstr ""
"Installez <application>librest</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/gnome/librest.xml:105
#, fuzzy, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr \\\n"
"            --with-ca-certificates=/etc/ssl/ca-bundle.crt &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr \\\n"
"            --with-ca-certificates=/etc/ssl/ca-bundle.crt &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/gnome/librest.xml:110
msgid "To test the results, issue: <command>make check</command>."
msgstr "Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/gnome/librest.xml:114
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem class=\"username\">root</"
"systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/gnome/librest.xml:117
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

#. type: Content of: <sect1><sect2><title>
#: blfs-en/archive/gnome/librest.xml:122
msgid "Command Explanations"
msgstr "Explication des commandes"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/gnome/librest.xml:125
#, fuzzy
msgid ""
"<parameter>--with-ca-certificates=/etc/ssl/ca-bundle.crt</parameter>: The "
