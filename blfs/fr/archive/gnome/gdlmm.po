#: blfs-en/archive/gnome/gdlmm.xml:92
msgid "Installation of Gdlmm"
msgstr "Installation de Gdlmm"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/gnome/gdlmm.xml:95
msgid ""
"Install <application>Gdlmm</application> by running the following commands:"
msgstr ""
"Installez <application>Gdlmm</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/gnome/gdlmm.xml:99
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/gnome/gdlmm.xml:103
msgid "To test the results, issue: <command>make check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/gnome/gdlmm.xml:107
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/gnome/gdlmm.xml:110
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

