msgid "Installation of DeviceKit"
msgstr "Installation de DeviceKit"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/DeviceKit.xml:77
msgid ""
"Install <application>DeviceKit</application> by running the following "
"commands:"
msgstr ""
"Installez <application>DeviceKit</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/archive/DeviceKit.xml:80
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr \\\n"
"    --sysconfdir=/etc \\\n"
"    --localstatedir=/var \\\n"
"    --libexecdir=/usr/lib/DeviceKit &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr \\\n"
"    --sysconfdir=/etc \\\n"
"    --localstatedir=/var \\\n"
"    --libexecdir=/usr/lib/DeviceKit &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/archive/DeviceKit.xml:86
