"PATH=$PWD/pyvenv/bin:$PATH                 \\\n"
"./configure                                \\\n"
"    --prefix=/usr                          \\\n"
"    --sysconfdir=/etc                      \\\n"
"    --localstatedir=/var                   \\\n"
"    --with-piddir=/run/samba               \\\n"
"    --with-pammodulesdir=/usr/lib/security \\\n"
"    --enable-fhs                           \\\n"
"    --without-ad-dc                        \\\n"
"    --without-systemd                      \\\n"
"    --enable-selftest                      &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>PYTHON=$PWD/pyvenv/bin/python3             \\\n"
"CPPFLAGS=\"-I/usr/include/tirpc\"            \\\n"
"LDFLAGS=\"-ltirpc\"                          \\\n"
