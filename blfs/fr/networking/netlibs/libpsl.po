#: blfs-en/networking/netlibs/libpsl.xml:89
msgid "Installation of libpsl"
msgstr "Installation de libpsl"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/networking/netlibs/libpsl.xml:92
msgid ""
"Install <application>libpsl</application> by running the following commands:"
msgstr ""
"Installez <application>libpsl</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/networking/netlibs/libpsl.xml:96
#, no-wrap
msgid ""
"<userinput>sed -i 's/env python/&amp;3/' src/psl-make-dafsa              &amp;&amp;\n"
"./configure --prefix=/usr --disable-static PYTHON=python3 &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>sed -i 's/env python/&amp;3/' src/psl-make-dafsa              &amp;&amp;\n"
"./configure --prefix=/usr --disable-static PYTHON=python3 &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/networking/netlibs/libpsl.xml:101
msgid "To test the results, issue: <command>make check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/networking/netlibs/libpsl.xml:105
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/networking/netlibs/libpsl.xml:108
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

