#: blfs-en/networking/netlibs/uhttpmock.xml:90
msgid "Installation of uhttpmock"
msgstr "Installation de uhttpmock"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/networking/netlibs/uhttpmock.xml:93
msgid ""
"Install <application>uhttpmock</application> by running the following "
"commands:"
msgstr ""
"Installez <application>uhttpmock</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/networking/netlibs/uhttpmock.xml:97
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/networking/netlibs/uhttpmock.xml:101
msgid "To test the results, issue: <command>make check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/networking/netlibs/uhttpmock.xml:105
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/networking/netlibs/uhttpmock.xml:108
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

