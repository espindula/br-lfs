#: blfs-en/pst/sgml/sgml-common.xml:100
#, no-wrap
msgid ""
"<userinput>patch -Np1 -i ../sgml-common-0.6.3-manpage-1.patch &amp;&amp;\n"
"autoreconf -f -i</userinput>"
msgstr ""
"<userinput>patch -Np1 -i ../sgml-common-0.6.3-manpage-1.patch &amp;&amp;\n"
"autoreconf -f -i</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/pst/sgml/sgml-common.xml:104
msgid ""
"Install <application>SGML Common</application> by running the following "
"commands:"
msgstr ""
"Installez <application>SGML Common</application> en exécutant les commandes "
"suivantes&nbsp;:"

