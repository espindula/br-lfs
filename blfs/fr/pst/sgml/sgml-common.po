#: blfs-en/pst/sgml/sgml-common.xml:108
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr --sysconfdir=/etc &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr --sysconfdir=/etc &amp;&amp;\n"
"make</userinput>"

