#. type: Content of: <sect1><sect2><title>
#: blfs-en/pst/sgml/docbook-4.5-dtd.xml:90
msgid "Installation of DocBook-4.5 SGML DTD"
msgstr "Installation de DocBook-4.5 SGML DTD"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/pst/sgml/docbook-4.5-dtd.xml:96
msgid ""
"Install <application>DocBook-4.5 SGML DTD</application> by running the "
"following commands:"
msgstr ""
"Installez <application>DocBook-4.5 SGML DTD</application> en lançant les "
"commandes suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/pst/sgml/docbook-4.5-dtd.xml:100
#, no-wrap
msgid ""
"<userinput>sed -i -e '/ISO 8879/d' \\\n"
"       -e '/gml/d' docbook.cat</userinput>"
msgstr ""
"<userinput>sed -i -e '/ISO 8879/d' \\\n"
"       -e '/gml/d' docbook.cat</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/pst/sgml/docbook-4.5-dtd.xml:104
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/pst/sgml/docbook-4.5-dtd.xml:108
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/pst/sgml/docbook-4.5-dtd.xml:111
#, no-wrap
msgid ""
"<userinput>install -v -d /usr/share/sgml/docbook/sgml-dtd-&docbook-4.5-dtd-version; &amp;&amp;\n"
"chown -R root:root . &amp;&amp;\n"
"\n"
"install -v docbook.cat /usr/share/sgml/docbook/sgml-dtd-&docbook-4.5-dtd-version;/catalog &amp;&amp;\n"
"cp -v -af *.dtd *.mod *.dcl /usr/share/sgml/docbook/sgml-dtd-&docbook-4.5-dtd-version; &amp;&amp;\n"
"\n"
"install-catalog --add /etc/sgml/sgml-docbook-dtd-&docbook-4.5-dtd-version;.cat \\\n"
"    /usr/share/sgml/docbook/sgml-dtd-&docbook-4.5-dtd-version;/catalog &amp;&amp;\n"
"\n"
"install-catalog --add /etc/sgml/sgml-docbook-dtd-&docbook-4.5-dtd-version;.cat \\\n"
"    /etc/sgml/sgml-docbook.cat</userinput>"
msgstr ""
"<userinput>install -v -d /usr/share/sgml/docbook/sgml-dtd-&docbook-4.5-dtd-version; &amp;&amp;\n"
"chown -R root:root . &amp;&amp;\n"
"\n"
"install -v docbook.cat /usr/share/sgml/docbook/sgml-dtd-&docbook-4.5-dtd-version;/catalog &amp;&amp;\n"
"cp -v -af *.dtd *.mod *.dcl /usr/share/sgml/docbook/sgml-dtd-&docbook-4.5-dtd-version; &amp;&amp;\n"
"\n"
"install-catalog --add /etc/sgml/sgml-docbook-dtd-&docbook-4.5-dtd-version;.cat \\\n"
"    /usr/share/sgml/docbook/sgml-dtd-&docbook-4.5-dtd-version;/catalog &amp;&amp;\n"
"\n"
"install-catalog --add /etc/sgml/sgml-docbook-dtd-&docbook-4.5-dtd-version;.cat \\\n"
"    /etc/sgml/sgml-docbook.cat</userinput>"

