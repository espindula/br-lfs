"    /etc/xml/catalog &amp;&amp;\n"
"xmlcatalog --noout --add \"delegateURI\" \\\n"
"    \"http://www.oasis-open.org/docbook/\" \\\n"
"    \"file:///etc/xml/docbook\" \\\n"
"    /etc/xml/catalog</userinput>"
