#. type: Content of: <sect1><sect2><para>
#: blfs-en/pst/typesetting/asymptote.xml:161
msgid ""
"Install <application>asymptote</application> by running the following "
"commands:"
msgstr ""
"Installez <application>asymptote</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/pst/typesetting/asymptote.xml:165
#, no-wrap
msgid ""
"<userinput>export TEXARCH=$(uname -m | sed -e 's/i.86/i386/' -e 's/$/-linux/') &amp;&amp;\n"
"\n"
"./configure --prefix=$TEXLIVE_PREFIX                          \\\n"
"            --bindir=$TEXLIVE_PREFIX/bin/$TEXARCH             \\\n"
"            --datarootdir=$TEXLIVE_PREFIX/texmf-dist          \\\n"
"            --infodir=$TEXLIVE_PREFIX/texmf-dist/doc/info     \\\n"
"            --libdir=$TEXLIVE_PREFIX/texmf-dist               \\\n"
"            --mandir=$TEXLIVE_PREFIX/texmf-dist/doc/man       \\\n"
"            --disable-lsp                                     \\\n"
"            --enable-gc=system                                \\\n"
"            --with-latex=$TEXLIVE_PREFIX/texmf-dist/tex/latex \\\n"
"            --with-context=$TEXLIVE_PREFIX/texmf-dist/tex/context/third &amp;&amp;\n"
"\n"
"make</userinput>"
msgstr ""
"<userinput>export TEXARCH=$(uname -m | sed -e 's/i.86/i386/' -e 's/$/-linux/') &amp;&amp;\n"
"\n"
"./configure --prefix=$TEXLIVE_PREFIX                          \\\n"
"            --bindir=$TEXLIVE_PREFIX/bin/$TEXARCH             \\\n"
"            --datarootdir=$TEXLIVE_PREFIX/texmf-dist          \\\n"
"            --infodir=$TEXLIVE_PREFIX/texmf-dist/doc/info     \\\n"
"            --libdir=$TEXLIVE_PREFIX/texmf-dist               \\\n"
"            --mandir=$TEXLIVE_PREFIX/texmf-dist/doc/man       \\\n"
"            --disable-lsp                                     \\\n"
"            --enable-gc=system                                \\\n"
"            --with-latex=$TEXLIVE_PREFIX/texmf-dist/tex/latex \\\n"
"            --with-context=$TEXLIVE_PREFIX/texmf-dist/tex/context/third &amp;&amp;\n"
"\n"
"make</userinput>"

