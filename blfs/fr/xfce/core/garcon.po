#. type: Content of: <sect1><sect2><title>
#: blfs-en/xfce/core/garcon.xml:95
msgid "Installation of Garcon"
msgstr "Installation de Garcon"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/xfce/core/garcon.xml:98
msgid ""
"Install <application>Garcon</application> by running the following commands:"
msgstr ""
"Installez <application>Garcon</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/xfce/core/garcon.xml:102
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr --sysconfdir=/etc &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr --sysconfdir=/etc &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/xfce/core/garcon.xml:106
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/xfce/core/garcon.xml:110
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/xfce/core/garcon.xml:113
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

