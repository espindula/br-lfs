#. type: Content of: <sect1><sect2><title>
#: blfs-en/xfce/core/xfconf.xml:93
msgid "Installation of Xfconf"
msgstr "Installation de Xfconf"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/xfce/core/xfconf.xml:96
msgid ""
"Install <application>Xfconf</application> by running the following commands:"
msgstr ""
"Installez <application>Xfconf</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/xfce/core/xfconf.xml:100
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/xfce/core/xfconf.xml:104
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/xfce/core/xfconf.xml:108
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/xfce/core/xfconf.xml:111
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

