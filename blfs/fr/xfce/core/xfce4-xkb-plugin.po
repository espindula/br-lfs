#. type: Content of: <sect1><sect2><para>
#: blfs-en/xfce/core/xfce4-xkb-plugin.xml:101
msgid ""
"Install <application>Xfce4 Keyboard Plugin</application> by running the "
"following commands:"
msgstr ""
"Installez <application>Xfce4 Keyboard Plugin</application> en lançant les "
"commandes suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/xfce/core/xfce4-xkb-plugin.xml:105
#, no-wrap
msgid ""
"<userinput>sed -e 's|xfce4/panel-plugins|xfce4/panel/plugins|' \\\n"
"    -i panel-plugin/{Makefile.in,xkb-plugin.desktop.in.in} &amp;&amp;\n"
"\n"
"./configure --prefix=/usr         \\\n"
"            --libexecdir=/usr/lib \\\n"
"            --disable-debug       &amp;&amp;\n"
"\n"
"make</userinput>"
msgstr ""
"<userinput>sed -e 's|xfce4/panel-plugins|xfce4/panel/plugins|' \\\n"
"    -i panel-plugin/{Makefile.in,xkb-plugin.desktop.in.in} &amp;&amp;\n"
"\n"
"./configure --prefix=/usr         \\\n"
"            --libexecdir=/usr/lib \\\n"
"            --disable-debug       &amp;&amp;\n"
"\n"
"make</userinput>"

