#. type: Content of: <sect1><sect2><title>
#: blfs-en/xfce/core/thunar.xml:109
msgid "Installation of Thunar"
msgstr "Installation de Thunar"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/xfce/core/thunar.xml:112
msgid ""
"Install <application>Thunar</application> by running the following commands:"
msgstr ""
"Installez <application>Thunar</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/xfce/core/thunar.xml:116
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr \\\n"
"            --sysconfdir=/etc \\\n"
"            --docdir=/usr/share/doc/Thunar-&thunar-version; &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr \\\n"
"            --sysconfdir=/etc \\\n"
"            --docdir=/usr/share/doc/Thunar-&thunar-version; &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/xfce/core/thunar.xml:122
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/xfce/core/thunar.xml:126
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/xfce/core/thunar.xml:129
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

