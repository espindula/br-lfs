#: blfs-en/xfce/core/thunar-volman.xml:92
msgid "Installation of the Thunar Volume Manager"
msgstr "Installation de Thunar Volume Manager"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/xfce/core/thunar-volman.xml:95
msgid ""
"Install the <application>Thunar Volume Manager</application> by running the "
"following commands:"
msgstr ""
"Installez le <application>Gestionnaire de Volume de Thunar</application> en "
"exécutant les commandes suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/xfce/core/thunar-volman.xml:99
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/xfce/core/thunar-volman.xml:103
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'a pas de suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/xfce/core/thunar-volman.xml:107
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/xfce/core/thunar-volman.xml:110
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

