#. type: Content of: <sect1><sect2><para>
#: blfs-en/xfce/core/xfwm4.xml:96
msgid ""
"Install <application>Xfwm4</application> by running the following commands:"
msgstr ""
"Installez <application>Xfwm4</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/xfce/core/xfwm4.xml:100
#, no-wrap
#| msgid ""
#| "<userinput>./configure --prefix=/usr &amp;&amp;\n"
#| "make</userinput>"
msgid ""
"<userinput>./configure --prefix=/usr --disable-compositor &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr --disable-compositor &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/xfce/core/xfwm4.xml:104
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/xfce/core/xfwm4.xml:108
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/xfce/core/xfwm4.xml:111
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

