#: blfs-en/xfce/apps/ristretto.xml:92
msgid "Installation of Ristretto"
msgstr "Installation de Ristretto"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/xfce/apps/ristretto.xml:95
msgid ""
"Install <application>Ristretto</application> by running the following "
"commands:"
msgstr ""
"Installez <application>Ristretto</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/xfce/apps/ristretto.xml:99
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/xfce/apps/ristretto.xml:103
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/xfce/apps/ristretto.xml:107
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/xfce/apps/ristretto.xml:110
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

