#: blfs-en/xfce/apps/xfce4-dev-tools.xml:80
msgid "Installation of the Xfce4 Development Tools"
msgstr "Installation de the Xfce4 Development Tools"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/xfce/apps/xfce4-dev-tools.xml:83
msgid ""
"Install the <application>Xfce4 Development Tools</application> by running "
"the following commands:"
msgstr ""
"Installez les <application>outils de développement de Xfce4</application> en"
" exécutant les commandes suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/xfce/apps/xfce4-dev-tools.xml:87
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/xfce/apps/xfce4-dev-tools.xml:91
msgid "To test the results, issue: <command>make check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/xfce/apps/xfce4-dev-tools.xml:95
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/xfce/apps/xfce4-dev-tools.xml:98
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

