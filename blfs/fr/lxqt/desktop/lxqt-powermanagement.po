#: blfs-en/lxqt/desktop/lxqt-powermanagement.xml:89
msgid "Installation of lxqt-powermanagement"
msgstr "Installation de lxqt-powermanagement"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/lxqt-powermanagement.xml:92
msgid ""
"Install <application>lxqt-powermanagement</application> by running the "
"following commands:"
msgstr ""
"Installez <application>lxqt-powermanagement</application> en lançant les "
"commandes suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxqt/desktop/lxqt-powermanagement.xml:96
#, no-wrap
msgid ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=$LXQT_PREFIX \\\n"
"      -DCMAKE_BUILD_TYPE=Release          \\\n"
"      ..       &amp;&amp;\n"
"\n"
"make</userinput>"
msgstr ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=$LXQT_PREFIX \\\n"
"      -DCMAKE_BUILD_TYPE=Release          \\\n"
"      ..       &amp;&amp;\n"
"\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/lxqt-powermanagement.xml:106
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/lxqt-powermanagement.xml:110
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxqt/desktop/lxqt-powermanagement.xml:113
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

