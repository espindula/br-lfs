#: blfs-en/lxqt/desktop/obconf-qt.xml:85
msgid ""
"Install <application>obconf-qt</application> by running the following "
"commands:"
msgstr ""
"Installez <application>obconf-qt</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxqt/desktop/obconf-qt.xml:89
#, no-wrap
msgid ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      ..                          &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      ..                          &amp;&amp;\n"
"make</userinput>"

