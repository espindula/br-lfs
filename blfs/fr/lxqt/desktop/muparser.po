#. type: Content of: <sect1><sect2><title>
#: blfs-en/lxqt/desktop/muparser.xml:78
msgid "Installation of muparser"
msgstr "Installation de muparser"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/muparser.xml:81
msgid ""
"Install <application>muparser</application> by running the following "
"commands:"
msgstr ""
"Installez <application>muparser</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxqt/desktop/muparser.xml:85
#, no-wrap
msgid ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      ..                          &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      ..                          &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/muparser.xml:94
msgid "To test the results, issue: <command>make test</command>."
msgstr "Pour tester les résultats lancez&nbsp;: <command>make test</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/muparser.xml:98
msgid "Now, as the &root; user:"
msgstr "Maintenant, en tant qu'utilisateur &root;&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxqt/desktop/muparser.xml:101
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

