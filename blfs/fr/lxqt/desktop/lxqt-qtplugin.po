#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/lxqt-qtplugin.xml:83
msgid ""
"Install <application>lxqt-qtplugin</application> by running the following "
"commands:"
msgstr ""
"Installez <application>lxqt-qtplugin</application> en exécutant les "
"commandes suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxqt/desktop/lxqt-qtplugin.xml:87
#, no-wrap
msgid ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      ..       &amp;&amp;\n"
"\n"
"make</userinput>"
msgstr ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      ..       &amp;&amp;\n"
"\n"
"make</userinput>"

