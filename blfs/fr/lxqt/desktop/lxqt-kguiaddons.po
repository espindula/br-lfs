#. type: Content of: <sect1><sect2><title>
#: blfs-en/lxqt/desktop/lxqt-kguiaddons.xml:92
#| msgid "Introduction to kguiaddons"
msgid "Installation of kguiaddons"
msgstr "Installation de kguiaddons"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/lxqt-kguiaddons.xml:95
#| msgid ""
#| "Install <application>liblxqt</application> by running the following "
#| "commands:"
msgid ""
"Install <application>kguiaddons</application> by running the following "
"commands:"
msgstr ""
"Installez <application>kguiaddons</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxqt/desktop/lxqt-kguiaddons.xml:99
#, no-wrap
msgid ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=$LXQT_PREFIX \\\n"
"      -DCMAKE_BUILD_TYPE=Release          \\\n"
"      -DCMAKE_INSTALL_LIBDIR=lib          \\\n"
"      -DBUILD_TESTING=OFF                 \\\n"
"      -Wno-dev ..                         &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=$LXQT_PREFIX \\\n"
"      -DCMAKE_BUILD_TYPE=Release          \\\n"
"      -DCMAKE_INSTALL_LIBDIR=lib          \\\n"
"      -DBUILD_TESTING=OFF                 \\\n"
"      -Wno-dev ..                         &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/lxqt-kguiaddons.xml:110
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/lxqt-kguiaddons.xml:114
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
