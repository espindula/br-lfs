#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/lxqt-build-tools.xml:87
msgid ""
"Install <application>lxqt-build-tools</application> by running the following"
" commands:"
msgstr ""
"Installez <application>lxqt-build-tools</application> en exécutant les "
"commandes suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxqt/desktop/lxqt-build-tools.xml:91
#, no-wrap
#| msgid ""
#| "<userinput>mkdir -v build &amp;&amp;\n"
#| "cd       build &amp;&amp;\n"
#| "\n"
#| "cmake -DCMAKE_INSTALL_PREFIX=$LXQT_PREFIX \\\n"
#| "      -DCMAKE_BUILD_TYPE=Release  \\\n"
#| "      .. &amp;&amp;\n"
#| "\n"
#| "make</userinput>"
msgid ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      .. &amp;&amp;\n"
"\n"
"make</userinput>"
msgstr ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      .. &amp;&amp;\n"
"\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
