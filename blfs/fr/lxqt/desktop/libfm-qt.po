msgid "Installation of libfm-qt"
msgstr "Installation de libfm-qt"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/libfm-qt.xml:84
msgid ""
"Install <application>libfm-qt</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libfm-qt</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxqt/desktop/libfm-qt.xml:88
#, no-wrap
#| msgid ""
#| "<userinput>mkdir -v build &amp;&amp;\n"
#| "cd       build &amp;&amp;\n"
#| "\n"
#| "cmake -DCMAKE_INSTALL_PREFIX=$LXQT_PREFIX \\\n"
#| "      -DCMAKE_BUILD_TYPE=Release          \\\n"
#| "      -DPULL_TRANSLATIONS=no              \\\n"
#| "      ..                                  &amp;&amp;\n"
#| "make</userinput>"
msgid ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      ..                          &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      ..                          &amp;&amp;\n"
"make</userinput>"

