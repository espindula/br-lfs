#: blfs-en/lxqt/desktop/lxqt-kwindowsystem.xml:90
msgid "Installation of kwindowsystem"
msgstr "Installation de kwindowsystem"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/lxqt-kwindowsystem.xml:93
msgid ""
"Install <application>kwindowsystem</application> by running the following "
"commands:"
msgstr ""
"Installez <application>kwindowsystem</application> en exécutant les "
"commandes suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxqt/desktop/lxqt-kwindowsystem.xml:97
#, no-wrap
#| msgid ""
#| "<userinput>mkdir -v build &amp;&amp;\n"
#| "cd       build &amp;&amp;\n"
#| "\n"
#| "cmake -DCMAKE_INSTALL_PREFIX=$LXQT_PREFIX \\\n"
#| "      -DCMAKE_BUILD_TYPE=Release          \\\n"
#| "      -DBUILD_TESTING=OFF                 \\\n"
#| "      -Wno-dev ..                         &amp;&amp;\n"
#| "make</userinput>"
msgid ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      -DBUILD_TESTING=OFF         \\\n"
"      -Wno-dev ..                 &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      -DBUILD_TESTING=OFF         \\\n"
"      -Wno-dev ..                 &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/lxqt-kwindowsystem.xml:107
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'a pas de suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/lxqt-kwindowsystem.xml:111
msgid "Now, as the &root; user:"
msgstr "Maintenant, en tant qu'utilisateur &root;&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxqt/desktop/lxqt-kwindowsystem.xml:114
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/lxqt-kwindowsystem.xml:117
msgid "Finally, remove some unneeded files as the &root; user:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxqt/desktop/lxqt-kwindowsystem.xml:120
#, no-wrap
#| msgid "<userinput>make install</userinput>"
msgid "<userinput>rm -rf /usr/mkspecs</userinput>"
msgstr "<userinput>rm -rf /usr/mkspecs</userinput>"

