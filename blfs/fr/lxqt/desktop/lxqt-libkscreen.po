msgid "Installation of libkscreen"
msgstr "Installation de libkscreen"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/lxqt-libkscreen.xml:90
msgid ""
"Install <application>libkscreen</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libkscreen</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxqt/desktop/lxqt-libkscreen.xml:94
#, no-wrap
#| msgid ""
#| "<userinput>mkdir -v build &amp;&amp;\n"
#| "cd       build &amp;&amp;\n"
#| "\n"
#| "cmake -DCMAKE_INSTALL_PREFIX=$LXQT_PREFIX \\\n"
#| "      -DCMAKE_BUILD_TYPE=Release          \\\n"
#| "      -DCMAKE_INSTALL_LIBDIR=lib          \\\n"
#| "      -DBUILD_TESTING=OFF                 \\\n"
#| "      -Wno-dev ..                         &amp;&amp;\n"
#| "make</userinput>"
msgid ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      -DCMAKE_INSTALL_LIBDIR=lib  \\\n"
"      -DBUILD_TESTING=OFF         \\\n"
"      -Wno-dev ..                 &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      -DCMAKE_INSTALL_LIBDIR=lib  \\\n"
"      -DBUILD_TESTING=OFF         \\\n"
"      -Wno-dev ..                 &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/lxqt-libkscreen.xml:105
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'a pas de suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/lxqt-libkscreen.xml:109
msgid "Now, as the &root; user:"
msgstr "Maintenant, en tant qu'utilisateur &root;&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxqt/desktop/lxqt-libkscreen.xml:112
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/lxqt-libkscreen.xml:115
msgid "Finally, remove some unneeded files as the &root; user:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxqt/desktop/lxqt-libkscreen.xml:118
#, no-wrap
msgid ""
"<userinput>rm -rf /usr/mkspecs &amp;&amp;\n"
"rm -rf /usr/lib/libexec</userinput>"
msgstr ""
"<userinput>rm -rf /usr/mkspecs &amp;&amp;\n"
"rm -rf /usr/lib/libexec</userinput>"

