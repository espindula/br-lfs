#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/lxqt-about.xml:82
msgid ""
"Install <application>lxqt-about</application> by running the following "
"commands:"
msgstr ""
"Installez <application>lxqt-about</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxqt/desktop/lxqt-about.xml:86
#, no-wrap
msgid ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      ..       &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      ..       &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/lxqt-about.xml:95
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'a pas de suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/desktop/lxqt-about.xml:99
msgid "Now, as the &root; user:"
msgstr "Maintenant, en tant qu'utilisateur &root;&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxqt/desktop/lxqt-about.xml:102
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

