#. type: Content of: <sect1><sect2><para>
#: blfs-en/lxqt/apps/pavucontrol-qt.xml:84
msgid ""
"Install <application>pavucontrol-qt</application> by running the following "
"commands:"
msgstr ""
"Installez <application>pavucontrol-qt</application> en exécutant les "
"commandes suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/lxqt/apps/pavucontrol-qt.xml:88
#, no-wrap
#| msgid ""
#| "<userinput>mkdir -v build &amp;&amp;\n"
#| "cd       build &amp;&amp;\n"
#| "\n"
#| "cmake -DCMAKE_BUILD_TYPE=Release          \\\n"
#| "      -DCMAKE_INSTALL_PREFIX=$LXQT_PREFIX \\\n"
#| "      -DPULL_TRANSLATIONS=no              \\\n"
#| "      ..       &amp;&amp;\n"
#| "make</userinput>"
msgid ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_BUILD_TYPE=Release  \\\n"
"      -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      ..       &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>mkdir -v build &amp;&amp;\n"
"cd       build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_BUILD_TYPE=Release  \\\n"
"      -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      ..       &amp;&amp;\n"
"make</userinput>"

