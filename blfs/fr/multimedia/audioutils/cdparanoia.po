#. type: Content of: <sect1><sect2><title>
#: blfs-en/multimedia/audioutils/cdparanoia.xml:87
msgid "Installation of CDParanoia"
msgstr "Installation de CDParanoia"

#. type: Content of: <sect1><sect2><note><para>
#: blfs-en/multimedia/audioutils/cdparanoia.xml:91
msgid "This package does not support parallel build."
msgstr "Ce paquet ne supporte pas la construction parallèle."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/multimedia/audioutils/cdparanoia.xml:96
msgid ""
"Install <application>CDParanoia</application> by running the following "
"commands:"
msgstr ""
"Installez <application>CDParanoia</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/multimedia/audioutils/cdparanoia.xml:100
#, no-wrap
msgid ""
"<userinput>patch -Np1 -i ../cdparanoia-&CDParanoia-version;-gcc_fixes-1.patch &amp;&amp;\n"
"./configure --prefix=/usr --mandir=/usr/share/man &amp;&amp;\n"
"make -j1</userinput>"
msgstr ""
"<userinput>patch -Np1 -i ../cdparanoia-&CDParanoia-version;-gcc_fixes-1.patch &amp;&amp;\n"
"./configure --prefix=/usr --mandir=/usr/share/man &amp;&amp;\n"
"make -j1</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/multimedia/audioutils/cdparanoia.xml:105
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'a pas de suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/multimedia/audioutils/cdparanoia.xml:109
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/multimedia/audioutils/cdparanoia.xml:112
#, no-wrap
msgid ""
"<userinput>make install &amp;&amp;\n"
"chmod -v 755 /usr/lib/libcdda_*.so.0.10.2 &amp;&amp;\n"
"rm -fv /usr/lib/libcdda_*.a</userinput>"
msgstr ""
"<userinput>make install &amp;&amp;\n"
"chmod -v 755 /usr/lib/libcdda_*.so.0.10.2 &amp;&amp;\n"
"rm -fv /usr/lib/libcdda_*.a</userinput>"

