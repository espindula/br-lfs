#. type: Content of: <sect1><sect2><title>
#: blfs-en/multimedia/libdriv/libvorbis.xml:94
msgid "Installation of libvorbis"
msgstr "Installation de libvorbis"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/multimedia/libdriv/libvorbis.xml:104
msgid ""
"Install <application>libvorbis</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libvorbis</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/multimedia/libdriv/libvorbis.xml:108
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr --disable-static &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/multimedia/libdriv/libvorbis.xml:112
#| msgid "To test the results, issue: <command>make check</command>."
msgid "To test the results, issue: <command>make -j1 check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make -j1 check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/multimedia/libdriv/libvorbis.xml:116
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/multimedia/libdriv/libvorbis.xml:119
#, no-wrap
msgid ""
"<userinput>make install &amp;&amp;\n"
"install -v -m644 doc/Vorbis* /usr/share/doc/libvorbis-&libvorbis-version;</userinput>"
msgstr ""
"<userinput>make install &amp;&amp;\n"
"install -v -m644 doc/Vorbis* /usr/share/doc/libvorbis-&libvorbis-version;</userinput>"

#. type: Content of: <sect1><sect2><title>
