#: blfs-en/multimedia/libdriv/libdvdcss.xml:87
msgid "Installation of libdvdcss"
msgstr "Installation de libdvdcss"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/multimedia/libdriv/libdvdcss.xml:90
msgid ""
"Install <application>libdvdcss</application> by running the following "
"commands:"
msgstr ""
"Installez <application>libdvdcss</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/multimedia/libdriv/libdvdcss.xml:94
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr    \\\n"
"            --disable-static \\\n"
"            --docdir=/usr/share/doc/libdvdcss-&libdvdcss-version; &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr    \\\n"
"            --disable-static \\\n"
"            --docdir=/usr/share/doc/libdvdcss-&libdvdcss-version; &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/multimedia/libdriv/libdvdcss.xml:100
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/multimedia/libdriv/libdvdcss.xml:104
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/multimedia/libdriv/libdvdcss.xml:107
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

