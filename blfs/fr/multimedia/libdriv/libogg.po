#. type: Content of: <sect1><sect2><title>
#: blfs-en/multimedia/libdriv/libogg.xml:79
msgid "Installation of libogg"
msgstr "Installation de libogg"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/multimedia/libdriv/libogg.xml:82
msgid ""
"Install <application>libogg</application> by running the following commands:"
msgstr ""
"Installez <application>libogg</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/multimedia/libdriv/libogg.xml:86
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr    \\\n"
"            --disable-static \\\n"
"            --docdir=/usr/share/doc/libogg-&libogg-version; &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr    \\\n"
"            --disable-static \\\n"
"            --docdir=/usr/share/doc/libogg-&libogg-version; &amp;&amp;\n"
"make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/multimedia/libdriv/libogg.xml:92
msgid "To test the results, issue: <command>make check</command>."
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/multimedia/libdriv/libogg.xml:96
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/multimedia/libdriv/libogg.xml:99
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

