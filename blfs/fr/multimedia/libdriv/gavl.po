#. type: Content of: <sect1><sect2><para>
#: blfs-en/multimedia/libdriv/gavl.xml:94
msgid ""
"Install <application>Gavl</application> by running the following commands:"
msgstr ""
"Installez <application>Gavl</application> en exécutant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/multimedia/libdriv/gavl.xml:98
#, no-wrap
msgid ""
"<userinput>LIBS=-lm                      \\\n"
"./configure --prefix=/usr     \\\n"
"            --without-doxygen \\\n"
"            --docdir=/usr/share/doc/gavl-&gavl-version; &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>LIBS=-lm                      \\\n"
"./configure --prefix=/usr     \\\n"
"            --without-doxygen \\\n"
"            --docdir=/usr/share/doc/gavl-&gavl-version; &amp;&amp;\n"
"make</userinput>"

