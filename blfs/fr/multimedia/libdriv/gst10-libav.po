msgid "Installation of GStreamer Libav"
msgstr "Installation de GStreamer Libav"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/multimedia/libdriv/gst10-libav.xml:108
msgid ""
"Install <application>GStreamer Libav</application> by running the following "
"commands:"
msgstr ""
"Installez <application>GStreamer Libav</application> en lançant les "
"commandes suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/multimedia/libdriv/gst10-libav.xml:112
#, no-wrap
msgid ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson  --prefix=/usr       \\\n"
"       --buildtype=release \\\n"
"       -Dpackage-origin=https://www.linuxfromscratch.org/blfs/view/&blfs-version;/ \\\n"
"       -Dpackage-name=\"GStreamer &gst10-libav-version; BLFS\" &amp;&amp;\n"
"ninja</userinput>"
msgstr ""
"<userinput>mkdir build &amp;&amp;\n"
"cd    build &amp;&amp;\n"
"\n"
"meson  --prefix=/usr       \\\n"
"       --buildtype=release \\\n"
"       -Dpackage-origin=https://www.linuxfromscratch.org/blfs/view/&blfs-version;/ \\\n"
"       -Dpackage-name=\"GStreamer &gst10-libav-version; BLFS\" &amp;&amp;\n"
"ninja</userinput>"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/multimedia/libdriv/gst10-libav.xml:123
msgid "To test the results, issue: <command>ninja test</command>"
msgstr ""
"Pour tester les résultats lancez&nbsp;: <command>ninja test</command>."

#. type: Content of: <sect1><sect2><para>
#: blfs-en/multimedia/libdriv/gst10-libav.xml:127
msgid "Now, as the <systemitem class=\"username\">root</systemitem> user:"
msgstr ""
"Maintenant, en tant qu'utilisateur <systemitem "
"class=\"username\">root</systemitem>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/multimedia/libdriv/gst10-libav.xml:130
#, no-wrap
msgid "<userinput>ninja install</userinput>"
msgstr "<userinput>ninja install</userinput>"

