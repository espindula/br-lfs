"<userinput>./configure --prefix=/usr    \\\n"
"            --disable-static \\\n"
"            --docdir=/usr/share/doc/libdvdread-&libdvdread-version; &amp;&amp;\n"
"make</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr    \\\n"
"            --disable-static \\\n"
"            --docdir=/usr/share/doc/libdvdread-&libdvdread-version; &amp;&amp;\n"
"make</userinput>"

