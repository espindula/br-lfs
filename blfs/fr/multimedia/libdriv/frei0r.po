msgstr "Installation de Frei0r-plugins"

#. type: Content of: <sect1><sect2><para>
#: blfs-en/multimedia/libdriv/frei0r.xml:82
msgid ""
"Install <application>Frei0r</application> by running the following commands:"
msgstr ""
"Installez <application>Frei0r</application> en lançant les commandes "
"suivantes&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: blfs-en/multimedia/libdriv/frei0r.xml:85
#, no-wrap
#| msgid ""
#| "<userinput>sed -i 's/CV_RGB/cv::Scalar/' src/filter/facebl0r/facebl0r.cpp &amp;&amp;\n"
#| "\n"
#| "mkdir -vp build &amp;&amp;\n"
#| "cd        build &amp;&amp;\n"
#| "\n"
#| "cmake -DCMAKE_INSTALL_PREFIX=/usr    \\\n"
#| "      -DCMAKE_BUILD_TYPE=Release     \\\n"
#| "      -Wno-dev ..                    &amp;&amp;\n"
#| "\n"
#| "make</userinput>"
msgid ""
"<userinput>mkdir -vp build &amp;&amp;\n"
"cd        build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      -DWITHOUT_OPENCV=TRUE       \\\n"
"      -Wno-dev ..                 &amp;&amp;\n"
"\n"
"make</userinput>"
msgstr ""
"<userinput>mkdir -vp build &amp;&amp;\n"
"cd        build &amp;&amp;\n"
"\n"
"cmake -DCMAKE_INSTALL_PREFIX=/usr \\\n"
"      -DCMAKE_BUILD_TYPE=Release  \\\n"
"      -DWITHOUT_OPENCV=TRUE       \\\n"
"      -Wno-dev ..                 &amp;&amp;\n"
"\n"
"make</userinput>"

