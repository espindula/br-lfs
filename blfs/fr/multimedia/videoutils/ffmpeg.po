#: blfs-en/multimedia/videoutils/ffmpeg.xml:187
#, no-wrap
msgid ""
"<userinput>./configure --prefix=/usr        \\\n"
"            --enable-gpl         \\\n"
"            --enable-version3    \\\n"
"            --enable-nonfree     \\\n"
"            --disable-static     \\\n"
"            --enable-shared      \\\n"
"            --disable-debug      \\\n"
"            --enable-libaom      \\\n"
"            --enable-libass      \\\n"
"            --enable-libfdk-aac  \\\n"
"            --enable-libfreetype \\\n"
"            --enable-libmp3lame  \\\n"
"            --enable-libopus     \\\n"
"            --enable-libtheora   \\\n"
"            --enable-libvorbis   \\\n"
"            --enable-libvpx      \\\n"
"            --enable-libx264     \\\n"
"            --enable-libx265     \\\n"
"            --enable-openssl     \\\n"
"            --docdir=/usr/share/doc/ffmpeg-&ffmpeg-version; &amp;&amp;\n"
"\n"
"make &amp;&amp;\n"
"\n"
"gcc tools/qt-faststart.c -o tools/qt-faststart</userinput>"
msgstr ""
"<userinput>./configure --prefix=/usr        \\\n"
"            --enable-gpl         \\\n"
"            --enable-version3    \\\n"
"            --enable-nonfree     \\\n"
"            --disable-static     \\\n"
"            --enable-shared      \\\n"
"            --disable-debug      \\\n"
"            --enable-libaom      \\\n"
"            --enable-libass      \\\n"
"            --enable-libfdk-aac  \\\n"
"            --enable-libfreetype \\\n"
"            --enable-libmp3lame  \\\n"
"            --enable-libopus     \\\n"
"            --enable-libtheora   \\\n"
"            --enable-libvorbis   \\\n"
"            --enable-libvpx      \\\n"
"            --enable-libx264     \\\n"
"            --enable-libx265     \\\n"
"            --enable-openssl     \\\n"
