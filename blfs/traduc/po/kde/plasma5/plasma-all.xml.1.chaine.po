#: ./test3/kde/plasma5/plasma-all.xml.1.chaine:150-1008
#, 
msgid "n"
msgstr "n"

#. type: Content of: 
#: ./test3/kde/plasma5/plasma-all.xml.1.chaine:150-1009
#, 
msgid "simultaneously pressing the keys Ctrl-Alt-F"
msgstr "en appuyant simultanément les touches Ctrl-Alt-F"

#. type: Content of: 
#: ./test3/kde/plasma5/plasma-all.xml.1.chaine:150-1010
#, 
msgid "n"
msgstr "n"

