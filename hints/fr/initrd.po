#. type: Plain text
#: origin/initrd.txt:138
#, no-wrap
msgid ""
"\tThis script will create the initial RAM Disk image file.\n"
"\tBy default this script creates /boot/initrd-{kernelversion}.img\n"
msgstr ""

#. type: Plain text
#: origin/initrd.txt:142
#, no-wrap
msgid ""
"\tI have sent a mkinitrd file to be placed\n"
"\tin the hints attachments at \n"
"\thttp://www.linuxfromscratch.org/hints/downloads/attachments. \n"
msgstr ""

#. type: Plain text
#: origin/initrd.txt:144
#, no-wrap
msgid "\tThe default location for this file is /sbin\n"
msgstr ""

#. type: Plain text
#: origin/initrd.txt:148
#, no-wrap
msgid ""
"\tThere are options on the new script, it will not just create\n"
"\ta initrd anymore. The kernel_version= is the only mandatory\n"
"\toption\n"
msgstr ""

#. type: Plain text
#: origin/initrd.txt:150
#, no-wrap
msgid "\tOptions are the following:\n"
msgstr ""

#. type: Plain text
#: origin/initrd.txt:153
#, no-wrap
msgid ""
"\tRequired\n"
"        kernel_version={version} = use kernel version\n"
msgstr ""

#. type: Plain text
#: origin/initrd.txt:164
#, no-wrap
msgid ""
"\tOptional\n"
"\t-ide = remove ide modules [-scsi = remove scsi modules]\n"
"        -usb = remove usb modules [-cdrom = remove cdrom modules]\n"
" \t-ide-scsi = remove ide-scsi modules\n"
"\t-devfs = remove devfs settings from initrd\n"
"\tinitrdfs={ext2|ext3} = use either ext2 or ext3\n"
"\t-module={module} = remove module\n"
"\t+module={module} - add module\n"
"        root-device={device} - skips auto detection of the root device\n"
"\torder={scsi,ide,usb,added} - sets the order of modules to be loaded\n"
msgstr ""

#. type: Plain text
#: origin/initrd.txt:167
msgid "initrd script"
msgstr ""

#. type: Plain text
#: origin/initrd.txt:169
#, no-wrap
msgid "\tThe following script needs to placed in /etc/rc.d/init.d.\n"
msgstr ""

#. type: Plain text
#: origin/initrd.txt:171
#, no-wrap
msgid "\tYou will then need to link it to rcsysinit.d.\n"
msgstr ""

#. type: Plain text
#: origin/initrd.txt:174
#, no-wrap
msgid ""
"\tIt is recommended that this script be run right after\n"
"\tmountfs.\n"
msgstr ""

#. type: Plain text
#: origin/initrd.txt:177
#, no-wrap
msgid ""
"\tTo link the script change to the /etc/rc.d/rcsysinit.d\n"
"\tdirectory and issue the following command.\n"
msgstr ""

#. type: Plain text
#: origin/initrd.txt:179
#, no-wrap
msgid "\tln -sf ../init.d/initrd S41initrd\n"
msgstr ""

#. type: Plain text
#: origin/initrd.txt:182
#, no-wrap
msgid ""
"\t#!/bin/sh\n"
"\t# Begin $rc_base/init.d/initrd\n"
msgstr ""

#. type: Plain text
#: origin/initrd.txt:185
#, no-wrap
msgid ""
"\t# Based on sysklogd script from LFS-3.1 and earlier.\n"
"\t# Rewritten by Gerard Beekmans  - gerard@linuxfromscratch.org\n"
msgstr ""

#. type: Plain text
#: origin/initrd.txt:188
#, no-wrap
msgid ""
"\tsource /etc/sysconfig/rc\n"
"\tsource $rc_functions\n"
msgstr ""

#. type: Plain text
#: origin/initrd.txt:195
#, no-wrap
msgid ""
"\tcase \"$1\" in\n"
"\t\tstart)\n"
"\t\t\tif [ -e /initrd/dev/.devfsd ]\n"
"\t\t\t\tthen\n"
"\t\t\t\t\tumount /initrd/dev\n"
"\t\t\tfi\n"
msgstr ""

#. type: Plain text
#: origin/initrd.txt:200
#, no-wrap
msgid ""
"\t\t\tif [ -e /initrd/proc ]\n"
"\t\t\t\tthen\n"
"\t\t\t\t\tumount /initrd/proc\n"
"\t\t\tfi\n"
msgstr ""

#. type: Plain text
#: origin/initrd.txt:205
#, no-wrap
msgid ""
"\t\t\tif [ -e /initrd/sys ]\n"
"\t\t\t\tthen\n"
"\t\t\t\t\tumount /initrd/sys\n"
"\t\t\tfi\n"
msgstr ""

#. type: Plain text
#: origin/initrd.txt:219
#, no-wrap
msgid ""
"\t\t\tif [ -e /initrd/linuxrc ]\n"
"\t\t\t\tthen\n"
"\t\t\t\t\tumount /initrd\n"
"\t\t\t\t\techo \"Clearing Initial RAM Disk...\"\n"
"\t\t\t\t\tblockdev --flushbufs /dev/ram0\n"
"\t\t\t\t\tevaluate_retval\n"
"\t\t\tfi\n"
"\t\t\t;;\n"
"\t\t*)\n"
"\t\t\techo \"Usage: $0 {start}\"\n"
"\t\t\texit 1\n"
"\t\t\t;;\n"
"\tesac\n"
msgstr ""

#. type: Plain text
#: origin/initrd.txt:221
msgid "# End $rc_base/init.d/initrd"
msgstr ""

