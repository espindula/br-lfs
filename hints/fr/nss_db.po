#: origin/nss_db.txt:250
#, no-wrap
msgid ""
"Update /etc/ld.so.cache:\n"
"------------------------\n"
msgstr ""

#. type: Plain text
#: origin/nss_db.txt:253
msgid "Update the linker's library cache file by issuing the \"ldconfig\" command:"
msgstr ""

#. type: Plain text
#: origin/nss_db.txt:255
msgid "ldconfig -v"
msgstr ""

