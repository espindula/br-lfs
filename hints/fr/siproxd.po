#. type: Plain text
#: origin/siproxd.txt:75
#, no-wrap
msgid ""
"\tTo configure siproxd use the following commands to make a siproxd.conf "
"file\n"
msgstr ""

#. type: Plain text
#: origin/siproxd.txt:78
#, no-wrap
msgid ""
"\tcd /etc/siproxd\n"
"\tcp siproxd.conf.example siproxd.conf\n"
msgstr ""

#. type: Plain text
#: origin/siproxd.txt:80
#, no-wrap
msgid "\tNow you will need to change the following entries in the file\n"
msgstr ""

#. type: Plain text
#: origin/siproxd.txt:83
#, no-wrap
msgid ""
"\tif_inbound  = eth0\tto\tif_inbound = {Your LAN Interface}\n"
"\tif_outbound = ppp0\tto\tif_outbound = {Your Internet Interface}\n"
msgstr ""

#. type: Plain text
#: origin/siproxd.txt:85
#, no-wrap
msgid ""
"\t#hosts_allow_reg = 192.168.1.8/24 to hosts_allow_reg = {Your LAN "
"Network)\n"
msgstr ""

#. type: Plain text
#: origin/siproxd.txt:87
msgid "Bootup Script"
msgstr ""

#. type: Plain text
#: origin/siproxd.txt:90
#, no-wrap
msgid ""
"\t#!/bin/bash\n"
"\t# Begin $rc_base/init.d/siproxd\n"
msgstr ""

#. type: Plain text
#: origin/siproxd.txt:93
#, no-wrap
msgid ""
"\t# Based on sysklogd script from LFS-3.1 and earlier.\n"
"\t# Rewritten by Gerard Beekmans  - gerard@linuxfromscratch.org\n"
msgstr ""

#. type: Plain text
#: origin/siproxd.txt:96
#, no-wrap
msgid ""
"\tsource /etc/sysconfig/rc\n"
"\tsource $rc_functions\n"
msgstr ""

#. type: Plain text
#: origin/siproxd.txt:105
#, no-wrap
msgid ""
"\tcase \"$1\" in\n"
"\t\tstart)\n"
"\t\t\techo \"Creating IPTables Rules for Siproxd...\"\n"
"\t\t\tiptables -A INPUT -i {if_inbound from siproxd} -p udp --dport 5006 -j "
"ACCEPT\n"
"\t\t\tiptables -A INPUT -i {if_inbound from siproxd} -p udp --dport "
"7070:7080 -j ACCEPT\n"
"\t\t\techo \"Starting Siproxd...\"\n"
"\t\t\tloadproc /usr/sbin/siproxd\n"
"\t\t\t;;\n"
msgstr ""

#. type: Plain text
#: origin/siproxd.txt:112
#, no-wrap
msgid ""
"\t\tstop)\n"
"\t\t\tiptables -D INPUT -i {if_inbound from siproxd} -p udp --dport 5006 -j "
"ACCEPT\n"
"\t\t\tiptables -D INPUT -i {if_inbound from siproxd} -p udp --dport "
"7070:7080 -j ACCEPT\n"
"\t\t\tkillproc /usr/sbin/siproxd\n"
"\t\t\techo \"Stopping Siproxd...\"\n"
"\t\t\t;;\n"
msgstr ""

#. type: Plain text
#: origin/siproxd.txt:123
#, no-wrap
msgid ""
"\t\trestart)\n"
"\t\t\t$0 stop\n"
"\t\t\tsleep 1\n"
"\t\t\t$0 start\n"
"\t\t\t;;\n"
"\t\t*)\n"
"\t\t\techo \"Usage: $0 {start|stop|restart}\"\n"
"\t\t\texit 1\n"
"\t\t\t;;\n"
"\tesac\n"
msgstr ""

#. type: Plain text
#: origin/siproxd.txt:125
#, no-wrap
msgid ""
"\t# End $rc_base/init.d/siproxd\n"
"-----\n"
msgstr ""

