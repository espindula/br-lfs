#. type: Plain text
#: origin/blowfish-passwords.txt:8
msgid "SYNOPSIS: Blowfish passwords."
msgstr ""

#. type: Plain text
#: origin/blowfish-passwords.txt:11
msgid "DESCRIPTION: How to install a blowfish crypt library and use it."
msgstr ""

#. type: Plain text
#: origin/blowfish-passwords.txt:13
msgid "PREREQUISITES: None"
msgstr ""

#. type: Plain text
#: origin/blowfish-passwords.txt:15
msgid "HINT:"
msgstr ""

#. type: Plain text
#: origin/blowfish-passwords.txt:18
msgid ""
"A paper on the blowfish algorithm is available here: "
"http://www.usenix.org/events/usenix99/provos.html"
msgstr ""

#. type: Plain text
#: origin/blowfish-passwords.txt:20
msgid "DOWNLOAD:"
msgstr ""

#. type: Plain text
#: origin/blowfish-passwords.txt:26
#, no-wrap
msgid ""
"http://ca.lfs-matrix.net/patches/downloads/glibc/glibc-2.5-blowfish.patch\n"
"http://ca.lfs-matrix.net/patches/downloads/shadow/\\\n"
"\tshadow-4.0.18.1-owl_blowfish-1.patch\n"
"http://ca.lfs-matrix.net/patches/downloads/sysvinit/\\\n"
"\tsysvinit-2.86-owl_blowfish.patch\n"
msgstr ""

#. type: Plain text
#: origin/blowfish-passwords.txt:28
msgid "INSTALLATION:"
msgstr ""

#. type: Plain text
#: origin/blowfish-passwords.txt:30
msgid "# With Glibc chapter 6:"
msgstr ""

#. type: Plain text
#: origin/blowfish-passwords.txt:32
msgid "patch -Np1 -i ../glibc-2.5-blowfish.patch"
msgstr ""

#. type: Plain text
#: origin/blowfish-passwords.txt:34
msgid "# With Shadow chapter 6:"
msgstr ""

#. type: Plain text
#: origin/blowfish-passwords.txt:39
msgid ""
"patch -Np1 -i ../shadow-4.0.18.1-owl_blowfish-1.patch && aclocal && autoconf "
"&& autoheader"
msgstr ""

#. type: Plain text
#: origin/blowfish-passwords.txt:44
msgid ""
"# This patch for Shadow must be regenerated with autotools so that this # "
"patch can be used by many versions of Shadow. As long as the patch applies # "
"without error then it should work on whichever version of Shadow you are # "
"using."
msgstr ""

#. type: Plain text
#: origin/blowfish-passwords.txt:48
msgid ""
"# This patch will instruct blowfish to use /dev/random for entropy. If you # "
"want to use /dev/urandom or something else then use the --with-random= # "
"configure option."
msgstr ""

#. type: Plain text
#: origin/blowfish-passwords.txt:52
msgid ""
"# The Sed command for MD5_CRYPT_ENAB, on the Shadow page, won't make any # "
"difference. If you get an error from 'make install' because of funny # "
"business from Autoconf, then use:"
msgstr ""

#. type: Plain text
#: origin/blowfish-passwords.txt:54
msgid "make MKINSTALLDIRS=$(pwd)/mkinstalldirs install"
msgstr ""

#. type: Plain text
#: origin/blowfish-passwords.txt:58
msgid ""
"# Shadow will still be able to use MD5 and DES passwords if you add them to "
"# /etc/shadow manually, or if you reset \"CRYPT_PREFIX\" to \"$1$\" in # "
"/etc/login.defs."
msgstr ""

#. type: Plain text
#: origin/blowfish-passwords.txt:61
msgid ""
"# After running 'passwd' you should find your passwords in /etc/shadow # "
"begin with \"$2a$\"."
msgstr ""

#. type: Plain text
#: origin/blowfish-passwords.txt:63
msgid "# With Sysvinit chapter 6:"
msgstr ""

#. type: Plain text
#: origin/blowfish-passwords.txt:65
msgid "patch -Np1 -i ../sysvinit-2.86-owl_blowfish.patch"
msgstr ""

#. type: Plain text
#: origin/blowfish-passwords.txt:68
msgid ""
"# OpenSSH can be installed normally, and using \"--with-md5-passwords\" is # "
"optional (it will still be able to use blowfish passwords too)."
msgstr ""

