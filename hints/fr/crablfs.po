"python-2.4.1.tar.bz2 # cd python-2.4.1 # patch -Np1 -i "
"../python-2.4.1-gdbm-1.patch # ./configure --prefix=/opt --enable-shared # "
"make && make install # cd ..  # rm -rf python-2.4.1 // U'd better delete it, "
"otherwise may be problems"
msgstr ""

#. type: Plain text
#: origin/crablfs.txt:410
msgid "# export PYTHONPATH=/opt/lib/python2.4/site-packages"
msgstr ""

#. type: Plain text
#: origin/crablfs.txt:417
#, no-wrap
msgid ""
"*** Now pexpect is optional ***\n"
"# tar xfz pexpect-2.1.tar.gz\n"
"# cd pexpect-2.1\n"
"# /opt/bin/python setup.py install --prefix=/opt\n"
"# cd ..\n"
"# rm -rf pexpect-2.1\n"
msgstr ""

#. type: Plain text
#: origin/crablfs.txt:425
msgid ""
"# tar xfz crablfs-0.1.tar.gz # cd crablfs-0.1 # cp userpack.dirs.blfs "
"userpack.dirs // Adjust for you own need and system // Whenever U meet "
"pemission problem, just adjust /etc/userpack.dirs // and run 'userpack init' "
"# /opt/bin/python setup.py install --prefix=/opt"
msgstr ""

#. type: Plain text
#: origin/crablfs.txt:437
msgid ""
"# /opt/bin/userpack init # /opt/bin/userpack install -f python-2.4.1.tar.bz2 "
"-p # python-2.4.1-gdbm-1.patch python-2.4.1 crablfs> cmd tar xfj "
"python-2.4.1.tar.bz2 crablfs> cmd cd python-2.4.1 crablfs> cmd ./configure "
"--prefix=/usr --enable-shared crablfs> cmd make crablfs> cmd make install "
"crablfs> cmd cd ..  crablfs> cmd rm -rf python-2.4.1 crablfs> commit"
msgstr ""

#. type: Plain text
#: origin/crablfs.txt:445
msgid ""
"# /opt/bin/userpack install -f crablfs-0.1.tar.gz crablfs-0.1 crablfs> cmd "
"tar xfz crablfs-0.1.tar.gz crablfs> cmd cd crablfs-0.1 crablfs> cmd python "
"setup.py install --install-scripts=/usr/local/bin crablfs> cmd cd ..  "
"crablfs> cmd rm -rf crablfs-0.1 crablfs> commit"
msgstr ""

#. type: Plain text
#: origin/crablfs.txt:454
#, no-wrap
msgid ""
"*** Now the pexpect is optional ***\n"
"# userpack install -f pexpect-2.1.tar.gz pexpect-2.1\n"
"crablfs> cmd tar xfz pexpect-2.1.tar.gz\n"
"crablfs> cmd cd pexpect-2.1\n"
"crablfs> cmd python setup.py install\n"
"crablfs> cmd cd ..\n"
"crablfs> cmd rm -rf pexpect-2.1\n"
"crablfs> commit\n"
msgstr ""

#. type: Plain text
#: origin/crablfs.txt:460
msgid "# unset PYTHONPATH # userpack packs python-2.4.1 crablfs-0.1 pexpect-2.1"
msgstr ""

#. type: Plain text
#: origin/crablfs.txt:464
#, no-wrap
msgid ""
