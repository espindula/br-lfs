#. type: Plain text
#: origin/initrd-diskcheck.txt:182
#, no-wrap
msgid ""
"\tThe following script needs to placed in /etc/rc.d/init.d.\n"
"\tIf you already have this script added, you do not need to\n"
"\tadd it again. This is the same one from the initrd hint.\n"
msgstr ""

#. type: Plain text
#: origin/initrd-diskcheck.txt:184
#, no-wrap
msgid "\tYou will then need to link it to rcsysinit.d.\n"
msgstr ""

#. type: Plain text
#: origin/initrd-diskcheck.txt:187
#, no-wrap
msgid ""
"\tIt is recommended that this script be run right after\n"
"\tmountfs.\n"
msgstr ""

#. type: Plain text
#: origin/initrd-diskcheck.txt:190
#, no-wrap
msgid ""
"\tTo link the script change to the /etc/rc.d/rcsysinit.d\n"
"\tdirectory and issue the following command.\n"
msgstr ""

#. type: Plain text
#: origin/initrd-diskcheck.txt:192
#, no-wrap
msgid "\tln -sf ../init.d/initrd S41initrd\n"
msgstr ""

#. type: Plain text
#: origin/initrd-diskcheck.txt:195
msgid "#!/bin/bash # Begin $rc_base/init.d/initrd"
msgstr ""

#. type: Plain text
#: origin/initrd-diskcheck.txt:198
msgid ""
"# Based on sysklogd script from LFS-3.1 and earlier.  # Rewritten by Gerard "
"Beekmans - gerard@linuxfromscratch.org"
msgstr ""

#. type: Plain text
#: origin/initrd-diskcheck.txt:201
msgid "source /etc/sysconfig/rc source $rc_functions"
msgstr ""

#. type: Plain text
#: origin/initrd-diskcheck.txt:210
#, no-wrap
msgid ""
"echo \"Clearing Initial RAM Disk...\"\n"
"\tif [ -e /initrd/dev/.devfsd ]\n"
"\t\tthen\n"
"      \t\tumount /initrd/dev\n"
"   \tfi\n"
"\tumount /initrd/proc\n"
"   \tumount /initrd\n"
"   \tblockdev --flushbufs /dev/ram0\n"
msgstr ""

#. type: Plain text
#: origin/initrd-diskcheck.txt:212
msgid "# End $rc_base/init.d/initrd"
msgstr ""

