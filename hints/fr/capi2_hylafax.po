#. type: Plain text
#: origin/capi2_hylafax.txt:470
msgid "STEP 11: Installing a boot script for the capi"
msgstr ""

#. type: Plain text
#: origin/capi2_hylafax.txt:472
#, no-wrap
msgid "     The following installs a simple script to start CAPI2.0 at boot time:\n"
msgstr ""

#. type: Plain text
#: origin/capi2_hylafax.txt:476
#, no-wrap
msgid ""
"          cat > /etc/rc.d/init.d/capi <<\"EOF\"\n"
"          #!/bin/bash\n"
"          # Begin $rc_base/init.d/capi\n"
msgstr ""

#. type: Plain text
#: origin/capi2_hylafax.txt:479
#, no-wrap
msgid ""
"          # Based on sysklogd script from LFS-3.1 and earlier.\n"
"          # Rewritten by Gerard Beekmans  - gerard@linuxfromscratch.org\n"
msgstr ""

#. type: Plain text
#: origin/capi2_hylafax.txt:482
#, no-wrap
msgid ""
"          source /etc/sysconfig/rc\n"
"          source $rc_functions\n"
msgstr ""

#. type: Plain text
#: origin/capi2_hylafax.txt:488
#, no-wrap
msgid ""
"          case \"$1\" in\n"
"                  start)\n"
"                          echo \"Starting capi 2.0...\"\n"
"                          loadproc capiinit start\n"
"                          ;;\n"
msgstr ""

#. type: Plain text
#: origin/capi2_hylafax.txt:494
#, no-wrap
msgid ""
"                  stop)\n"
"                          echo \"Stopping capi 2.0...\"\n"
"                          capiinit stop\n"
"                          evaluate_retval\n"
"                          ;;\n"
msgstr ""

#. type: Plain text
#: origin/capi2_hylafax.txt:500
#, no-wrap
msgid ""
"                  restart)\n"
"                          $0 stop\n"
"                          sleep 1\n"
"                          $0 start\n"
"                          ;;\n"
msgstr ""

#. type: Plain text
#: origin/capi2_hylafax.txt:504
#, no-wrap
msgid ""
"                  status)\n"
"                          capiinfo\n"
"                          ;;\n"
msgstr ""

#. type: Plain text
#: origin/capi2_hylafax.txt:510
#, no-wrap
msgid ""
"                  *)\n"
"                          echo \"Usage: $0 {start|stop|restart|status}\"\n"
"                          exit 1\n"
"                          ;;\n"
"          esac\n"
msgstr ""

#. type: Plain text
#: origin/capi2_hylafax.txt:513
#, no-wrap
msgid ""
"          # End $rc_base/init.d/capi\n"
"          EOF\n"
msgstr ""

#. type: Plain text
#: origin/capi2_hylafax.txt:515
#, no-wrap
msgid "          chmod 755 /etc/rc.d/init.d/capi\n"
msgstr ""

#. type: Plain text
#: origin/capi2_hylafax.txt:524
#, no-wrap
msgid ""
"          cd /etc/rc.d/init.d &&\n"
"          ln -sf ../init.d/capi ../rc0.d/K10capi &&\n"
"          ln -sf ../init.d/capi ../rc1.d/K10capi &&\n"
"          ln -sf ../init.d/capi ../rc2.d/S90capi &&\n"
"          ln -sf ../init.d/capi ../rc3.d/S90capi &&\n"
"          ln -sf ../init.d/capi ../rc4.d/S90capi &&\n"
"          ln -sf ../init.d/capi ../rc5.d/S90capi &&\n"
"          ln -sf ../init.d/capi ../rc6.d/K10capi\n"
msgstr ""

