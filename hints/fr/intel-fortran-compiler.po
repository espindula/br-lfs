#. type: Plain text
#: origin/intel-fortran-compiler.txt:118
#, no-wrap
msgid ""
"2.3. Uninstall script\n"
"=====================\n"
msgstr ""

#. type: Plain text
#: origin/intel-fortran-compiler.txt:160
#, no-wrap
msgid ""
"   "
"__________________________________________________________________________\n"
"  |                                                                          "
"|\n"
"  |  cat > /opt/bin/uninstall_intel-fortran-compiler.sh << \"EOF\"             "
"|\n"
"  |  #!/bin/sh                                                               "
"|\n"
"  |                                                                          "
"|\n"
"  |  echo -n \"Uninstalling Intel Fortran Compiler: \"                         "
"|\n"
"  |  rm -f /opt/bin/{codecov,fortcom,fpp,ifc,ifc.cfg,ifcbin,ifort,\\          "
"|\n"
"  |\tifort.cfg,ifortbin,ifortvars.csh,ifortvars.sh,profmerge,proforder,\\  "
"|\n"
"  |\tprofrun,pronto_tool, tselect,xiar,xild}                              "
"|\n"
"  |  rm -rf /opt/doc/ifc                                                     "
"|\n"
"  |  rm -f /opt/include/{for_fpclass.for,for_fpclass.h,for_fpeflags.for,\\    "
"|\n"
"  |\tfor_fpeflags.h,for_iosdef.for,for_iosdef.h,fordef.for,fordef.h,\\     "
"|\n"
"  |\tforreent.for,forreent.h,ifcore.f90,ifcore.mod,iflport.f90,\\          "
"|\n"
"  |\tiflport.mod,iflposix.f90,iflposix.mod,ifport.f90,ifport.mod,\\        "
"|\n"
"  |\tifport_types.mod,ifposix.f90,ifposix.mod,omp_lib.f,omp_lib.h,\\       "
"|\n"
"  |\tomp_lib.mod,omp_lib_kinds.mod,tbk_traceback.h}                       "
"|\n"
"  |  rm -f /opt/lib/{crtxi.o,crtxn.o,for_main.o,icrt.internal.map,\\          "
"|\n"
"  |\ticrt.link,ifcore_msg.cat,libcprts.a,libcprts.so,libcprts.so.5,\\      "
"|\n"
"  |\tlibcxa.a,libcxa.so,libcxa.so.5,libcxaguard.a,libcxaguard.so,\\        "
"|\n"
"  |\tlibcxaguard.so.5,libguide.a,libguide.so,libguide_stats.a,\\           "
"|\n"
"  |\tlibguide_stats.so,libifcore.a,libifcore.so,libifcore.so.5,\\          "
"|\n"
"  |\tlibifcore_pic.a,libifcoremt.a,libifcoremt.so,libifcoremt.so.5,\\      "
"|\n"
"  |\tlibifcoremt_pic.a,libifport.a,libifport.so,libifport.so.5,libimf.a,\\ "
"|\n"
"  |\tlibimf.so,libirc.a,libirc_s.a,libompstub.a,libsvml.a,libsvml.so,\\    "
"|\n"
"  |\tlibunwind.a,libunwind.so,libunwind.so.5}                             "
"|\n"
"  |  rm -f /opt/man/man1{ifc.1,ifort.1}                                      "
"|\n"
"  |  sed -i -e 's/source \\/opt\\/bin\\/ifortvars.sh//' /etc/profile            "
"|\n"
"  |  echo \"done.\"                                                            "
"|\n"
"  |  echo -n \"Uninstalling Intel Application Debugger: \"                     "
"|\n"
"  |  rm -f /opt/bin/{idb,idb.el,idbvars.csh,idbvars.sh,iidb,mpirun_dbg.idb}  "
"|\n"
"  |  rm -rf /opt/doc/idb                                                     "
"|\n"
"  |  rm -f /opt/man/man1/idb.1                                               "
"|\n"
"  |  sed -i -e 's/source \\/opt\\/bin\\/idbvars.sh//' /etc/profile              "
"|\n"
"  |  echo \"done.\"                                                            "
"|\n"
"  |                                                                          "
"|\n"
"  |  rm -f /opt/bin/uninstall_intel-fortran-compiler.sh                      "
"|\n"
"  |  EOF                                                                     "
"|\n"
"  |                                                                          "
"|\n"
"  |  chmod 755 /opt/bin/uninstall_intel-fortran-compiler.sh                  "
"|\n"
"  "
"|__________________________________________________________________________|\n"
msgstr ""

