#. type: Plain text
#: origin/icewm.txt:47
#, no-wrap
msgid ""
"libXpm\n"
"------\n"
msgstr ""

#. type: Plain text
#: origin/icewm.txt:50
#, no-wrap
msgid ""
"  XPM is a format for storing/retrieving X Pixmaps\n"
"  from/to files\n"
msgstr ""

#. type: Plain text
#: origin/icewm.txt:55
#, no-wrap
msgid ""
"  current version : 3.4k\n"
"  where to get it : http://koala.inria.fr/ftp/pub/xpm/\n"
"                    ftp://ftp.x.org/contrib/libraries\n"
"  how to build it :\n"
msgstr ""

#. type: Plain text
#: origin/icewm.txt:61
#, no-wrap
msgid ""
"       tar xzvf xpm-3.4k.tar.gz /usr/src\n"
"       cd /usr/src/xpm-3.4k\n"
"       xmkmf -a\n"
"       make\n"
"       make install\n"
msgstr ""

#. type: Plain text
#: origin/icewm.txt:63
#, no-wrap
msgid "  note : by default, Xpm is installed in the X11 tree\n"
msgstr ""

