#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:45
#, no-wrap
msgid ""
"1.1 Installation of FUSE and FuseSMB.\n"
"-------------------------------------\n"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:48
msgid "Get FUSE from the projectsite:"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:50
msgid "http://fuse.sourceforge.net"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:52
msgid "Installing FUSE:"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:57
msgid ""
"cd fuse-2.3.0 ./configure --prefix=/usr --sysconfdir=/etc "
"--localstatedir=/var --enable-kernel-module --enable-lib --enable-util make "
"make install"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:59
msgid "A module is installed, fuse."
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:61
msgid "To load it:"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:63
msgid "modprobe fuse"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:65
msgid "and add it to /etc/sysconfig/modules."
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:67
msgid "Note:"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:70
msgid ""
"In the newest kernels (>=2.6.14) the kernelmodule is included in the "
"kernel. You still need the package above, because of the library and the "
"utilities."
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:72
msgid "Configuration of fuse goes via the fuse.conf file in the /etc directory:"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:74
msgid "cat >> /etc/fuse.conf << \"EOF\""
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:76
msgid "mount_max = 999"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:79
msgid "user_allow_other EOF"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:81
msgid "Get fusesmb:"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:83
msgid "Look for a link at :"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:85
msgid "http://freshmeat.net/projects/fusesmb/"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:87
msgid "Installing fusesmb:"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:92
msgid ""
"cd fusesmb-0.8.3 ./configure --prefix=/usr --sysconfdir=/etc "
"--localstatedir=/var make make install"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:94
msgid "It requires samba-3.0.*."
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:97
#, no-wrap
msgid "---------------------\n"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:99
#, no-wrap
msgid ""
"1.2 Starting fusesmb.\n"
"---------------------\n"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:103
msgid "Now the actual scripts:"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:105
msgid "cd /etc/session.d/kdm/startup"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:108 origin/starting-and-stopping-fusesmb-with-kdm.txt:161
msgid "cat >> fusesmb.sh << \"EOF\" #!/bin/bash"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:110 origin/starting-and-stopping-fusesmb-with-kdm.txt:163
msgid "retcode=0;"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:116
msgid ""
"userid=$1 userproperties=$(getent passwd | grep -m 1-E \"^$userid\")  "
"homedir=$(echo $userproperties | cut -d \":\" -f 6); gidnr=$(echo "
"$userproperties | cut -d \":\" -f 4); uidnr=$(echo $userproperties | cut -d "
"\":\" -f 3);"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:118 origin/starting-and-stopping-fusesmb-with-kdm.txt:171
msgid "if [ -d $homedir ]; then"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:123
#, no-wrap
msgid ""
"    if [ ! -d $homedir/network ]; then\n"
"\tmkdir -p $homedir/network\n"
"\tchown $uidnr:$gidnr $homedir/network\n"
"    fi\n"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:131
#, no-wrap
msgid ""
"    if [ $(id -u) -eq 0 ]; then\n"
" \tsudo -H -u $userid /bin/sh -c \"fusesmb $homedir/network -o "
"fsname=fusesmb,default_permissions,allow_other\"\n"
"\tretcode=$?\n"
"    elif [ $(id -u) -eq $uidnr ]; then \n"
"    \tfusesmb $homedir/network -o "
"fsname=fusesmb,default_permissions,allow_other\n"
"\tretcode=$?\n"
"    fi\t\n"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:133 origin/starting-and-stopping-fusesmb-with-kdm.txt:179
msgid "fi;"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:137 origin/starting-and-stopping-fusesmb-with-kdm.txt:184
#, no-wrap
msgid ""
"if [ $retcode -ne 0 ]; then\n"
"    echo \"An error with fusesmb ($retcode).\"\n"
"fi;\n"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:140 origin/starting-and-stopping-fusesmb-with-kdm.txt:187
msgid "exit $retcode EOF"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:146
msgid ""
"Now with fusesmb running you can access your SMB(Windows)  network "
"environment via a filesystem in userspace, with **any** (not only KDE apps "
"with kio's or GNOME with vfs)  application, like MC or vi."
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:149
msgid ""
"This configuration allows other users to enter the fusesmb filesystem. If "
"you don't want this, you should remove the \"allow_other\" flag."
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:151
#, no-wrap
msgid "------------------------\n"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:153
#, no-wrap
msgid ""
"1.3 Stopping of fusesmb.\n"
"------------------------\n"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:156
msgid "And the logout script:"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:158
msgid "cd /etc/session.d/kdm/reset"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:169
msgid ""
"userid=$1 userproperties=$(getent passwd | grep -m 1 -E \"^$userid\")  "
"homedir=$(echo $userproperties | cut -d \":\" -f 6); gidnr=$(echo "
"$userproperties | cut -d \":\" -f 4); uidnr=$(echo $userproperties | cut -d "
"\":\" -f 3);"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:173
#, no-wrap
msgid "    if [ -n \"$(mount | grep $homedir/network)\" ]; then\n"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:175
#, no-wrap
msgid "\tfusermount -u $homedir/network\n"
msgstr ""

#. type: Plain text
#: origin/starting-and-stopping-fusesmb-with-kdm.txt:177
#, no-wrap
msgid "    fi;\n"
msgstr ""

