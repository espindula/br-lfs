#: origin/at.txt:54
#, no-wrap
msgid ""
"PACKAGE INSTALLATION:\n"
"=====================\n"
msgstr ""

#. type: Plain text
#: origin/at.txt:56
#, no-wrap
msgid "---------------------\n"
msgstr ""

#. type: Plain text
#: origin/at.txt:58
#, no-wrap
msgid ""
"Download the package:\n"
"---------------------\n"
msgstr ""

#. type: Plain text
#: origin/at.txt:61
msgid "At seems to be maintained by debian now, so download the package from:"
msgstr ""

#. type: Plain text
#: origin/at.txt:63
msgid "ftp://ftp.debian.org/debian/pool/main/a/at/at_3.1.8-11.tar.gz"
msgstr ""

#. type: Plain text
#: origin/at.txt:64
#, no-wrap
msgid "-----------------\n"
msgstr ""

#. type: Plain text
#: origin/at.txt:66
#, no-wrap
msgid ""
"Patch the package\n"
"-----------------\n"
msgstr ""

#. type: Plain text
#: origin/at.txt:70
msgid ""
"patch -Np1 -i ../at-3.1.8-fixes-1.patch patch -Np1 -i "
"../at-3.1.8-fcron.patch"
msgstr ""

#. type: Plain text
#: origin/at.txt:71
#, no-wrap
msgid "--------------------\n"
msgstr ""

#. type: Plain text
#: origin/at.txt:73
#, no-wrap
msgid ""
"Configure the build:\n"
"--------------------\n"
msgstr ""

#. type: Plain text
#: origin/at.txt:76
msgid ""
"The values presented here are based on a relatively clean LFS/BLFS build, "
"and should thus be appropriate for most. However, you may want to consult "
"./configure --help."
msgstr ""

#. type: Plain text
#: origin/at.txt:78
msgid ""
"./configure --prefix=/usr --with-daemon_username=nobody "
"--with-daemon_groupname=nogroup"
msgstr ""

#. type: Plain text
#: origin/at.txt:79
#, no-wrap
msgid "------------------\n"
msgstr ""

#. type: Plain text
#: origin/at.txt:81
#, no-wrap
msgid ""
"Build the package:\n"
"------------------\n"
msgstr ""

#. type: Plain text
#: origin/at.txt:84
msgid "make"
msgstr ""

#. type: Plain text
#: origin/at.txt:86
msgid "make install"
msgstr ""

#. type: Plain text
#: origin/at.txt:88 origin/at.txt:136
msgid "chmod 755 /var/spool/fcron"
msgstr ""

#. type: Plain text
#: origin/at.txt:89
#, no-wrap
msgid "------------------------\n"
msgstr ""

#. type: Plain text
#: origin/at.txt:91
#, no-wrap
msgid ""
"Install the init script:\n"
"------------------------\n"
msgstr ""

#. type: Plain text
#: origin/at.txt:94
msgid "cp debian/rc /etc/rc.d/init.d/atd"
msgstr ""

#. type: Plain text
#: origin/at.txt:98
msgid "cat > /etc/rc.d/init.d/atd << \"EOF\" #!/bin/sh # Begin $rc_base/init.d/atd"
msgstr ""

#. type: Plain text
#: origin/at.txt:101
msgid ""
"# Based on various other init scripts.  # Rewritten by Sune Molgaard - "
"sune@molgaard.org"
msgstr ""

#. type: Plain text
#: origin/at.txt:104
msgid ". /etc/sysconfig/rc . $rc_functions"
msgstr ""

#. type: Plain text
#: origin/at.txt:106
msgid "case \"$1\" in"
msgstr ""

#. type: Plain text
#: origin/at.txt:115
#, no-wrap
msgid ""
"\tstart)\n"
"\t\techo \"Starting atd...\"\n"
"\t\tloadproc /usr/sbin/atd\n"
"\t\t;;\n"
"        stop)\n"
"                echo \"Stopping atd...\"\n"
"                killproc atd\n"
"                ;;\n"
msgstr ""

#. type: Plain text
#: origin/at.txt:121
#, no-wrap
msgid ""
"        restart)\n"
"                $0 stop\n"
"                sleep 1\n"
"                $0 start\n"
"                ;;\n"
msgstr ""

#. type: Plain text
#: origin/at.txt:125
#, no-wrap
msgid ""
"        status)\n"
"                statusproc atd\n"
"                ;;\n"
msgstr ""

#. type: Plain text
#: origin/at.txt:131
#, no-wrap
msgid ""
"        *)\n"
"                echo \"Usage: $0 {start|stop|restart|status}\"\n"
"                exit 1\n"
"                ;;\n"
"esac\n"
msgstr ""

#. type: Plain text
#: origin/at.txt:134
msgid "# End $rc_base/init.d/atd EOF"
msgstr ""

#. type: Plain text
#: origin/at.txt:143
msgid ""
"ln -s /etc/rc.d/init.d/atd /etc/rc.d/rc0.d/K07atd ln -s /etc/rc.d/init.d/atd "
"/etc/rc.d/rc2.d/S41atd ln -s /etc/rc.d/init.d/atd /etc/rc.d/rc3.d/S41atd ln "
"-s /etc/rc.d/init.d/atd /etc/rc.d/rc4.d/S41atd ln -s /etc/rc.d/init.d/atd "
"/etc/rc.d/rc5.d/S41atd ln -s /etc/rc.d/init.d/atd /etc/rc.d/rc6.d/K07atd"
msgstr ""

