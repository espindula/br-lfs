#. type: Plain text
#: origin/unreal_tournament.txt:60
msgid ""
"In the Maps directory are a lot of .unr.uz files. In order to get the game "
"to run they have to be decompressed. This script will do the work for us:"
msgstr ""

#. type: Plain text
#: origin/unreal_tournament.txt:66
#, no-wrap
msgid ""
"cd /usr/local/games/ut/System &&\n"
"for FILE in ../Maps/*.uz; do ./ucc-bin decompress ../Maps/$FILE -nohomedir "
"\\\n"
"  && rm -f $FILE;\n"
"done\n"
"mv *.unr ../Maps\n"
msgstr ""

