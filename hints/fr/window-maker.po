#. type: Plain text
#: origin/window-maker.txt:126
#, no-wrap
msgid ""
"2. Installation of Window Maker\n"
"===============================\n"
msgstr ""

#. type: Plain text
#: origin/window-maker.txt:129
msgid "Install Window Maker by running the following commands:"
msgstr ""

#. type: Plain text
#: origin/window-maker.txt:141
#, no-wrap
msgid ""
"   "
"__________________________________________________________________________\n"
"  |                                                                          "
"|\n"
"  |  patch -Np1 -i ../WindowMaker-0.91.0-clip_captions-1.patch &&            "
"|\n"
"  |  patch -Np1 -i ../WindowMaker-0.91.0-wmsetbg_workspace_number-1.patch && "
"|\n"
"  |  patch -Np1 -i ../WindowMaker-0.91.0-preference_directory-1.patch &&     "
"|\n"
"  |                                                                          "
"|\n"
"  |  ./configure --prefix=/usr --sysconfdir=/etc &&                          "
"|\n"
"  |  make &&                                                                 "
"|\n"
"  |  make install &&                                                         "
"|\n"
"  |  /sbin/ldconfig                                                          "
"|\n"
"  "
"|__________________________________________________________________________|\n"
msgstr ""

#. type: Plain text
#: origin/window-maker.txt:145
msgid ""
"If you want to strip the debugging symbols from the binaries and make them "
"smaller, you can use 'make install-strip' instead of 'make install'."
msgstr ""

#. type: Plain text
#: origin/window-maker.txt:147
msgid "If you have downloaded the extra package, install it with:"
msgstr ""

#. type: Plain text
#: origin/window-maker.txt:154
#, no-wrap
msgid ""
"   "
"__________________________________________________________________________\n"
"  |                                                                          "
"|\n"
"  |  ./configure --prefix=/usr                                               "
"|\n"
"  |  make &&                                                                 "
"|\n"
"  |  make install &&                                                         "
"|\n"
"  "
"|__________________________________________________________________________|\n"
msgstr ""

