msgid "Optional preference-directory patch"
msgstr ""

#. type: Plain text
#: origin/window-maker.txt:88
#, no-wrap
msgid "    Changes the preference directory from ~/GNUstep to ~/.GNUstep.\n"
msgstr ""

#. type: Plain text
#: origin/window-maker.txt:92
#, no-wrap
