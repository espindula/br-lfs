               <primary sortas="b-profiles"> profiles </primary>
            </indexterm>
         </listitem>
      </varlistentry>
      
      <varlistentry id="rpcclient">
         <term><command>rpcclient</command> </term>
         <listitem>
            <para>
               <!-- is used to execute MS-RPC client side functions.= --> \n --non_traduit-- 
