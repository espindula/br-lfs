            <indexterm zone="samba3 nmbd">
               <primary sortas="b-nmbd"> nmbd </primary>
            </indexterm>
         </listitem>
      </varlistentry>
      
      <varlistentry id="nmblookup">
         <term><command>nmblookup</command> </term>
         <listitem>
            <para>
               <!-- is used to query NetBIOS names and map them to IP addresses.= --> \n
