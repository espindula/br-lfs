"<userinput remap=\"install\">install -vm755 ninja /usr/bin/\n"
"install -vDm644 misc/bash-completion /usr/share/bash-completion/completions/"
"ninja\n"
"install -vDm644 misc/zsh-completion  /usr/share/zsh/site-functions/"
"_ninja</userinput>"

