#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter08/expat.xml:42
msgid "Installation of Expat"
msgstr "Instalação do Expat"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/expat.xml:44
msgid "Prepare Expat for compilation:"
msgstr "Prepare Expat para compilação:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/expat.xml:46
#, no-wrap
msgid ""
"<userinput remap=\"configure\">./configure --prefix=/usr    \\\n"
"            --disable-static \\\n"
"            --docdir=/usr/share/doc/expat-&expat-version;</userinput>"
msgstr ""
"<userinput remap=\"configure\">./configure --prefix=/usr    \\\n"
"            --disable-static \\\n"
"            --docdir=/usr/share/doc/expat-&expat-version;</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/expat.xml:50
msgid "Compile the package:"
msgstr "Compile o pacote:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/expat.xml:52
#, no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr "<userinput remap=\"make\">make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/expat.xml:54
msgid "To test the results, issue:"
msgstr "Para testar os resultados, emita:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/expat.xml:56
#, no-wrap
msgid "<userinput remap=\"test\">make check</userinput>"
msgstr "<userinput remap=\"test\">make check</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/expat.xml:58
msgid "Install the package:"
msgstr "Instale o pacote:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/expat.xml:60
#, no-wrap
msgid "<userinput remap=\"install\">make install</userinput>"
msgstr "<userinput remap=\"install\">make install</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/expat.xml:62
msgid "If desired, install the documentation:"
msgstr "Se desejado, instale a documentação:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/expat.xml:64
#, no-wrap
msgid ""
"<userinput remap=\"install\">install -v -m644 doc/*.{html,css} "
"/usr/share/doc/expat-&expat-version;</userinput>"
msgstr ""
"<userinput remap=\"install\">install -v -m644 doc/*.{html,css} /usr/share/"
"doc/expat-&expat-version;</userinput>"

