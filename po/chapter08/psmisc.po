#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter08/psmisc.xml:42
msgid "Installation of Psmisc"
msgstr "Instalação do Psmisc"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/psmisc.xml:44
msgid "Prepare Psmisc for compilation:"
msgstr "Prepare Psmisc para compilação:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/psmisc.xml:46
#, no-wrap
msgid "<userinput remap=\"configure\">./configure --prefix=/usr</userinput>"
msgstr "<userinput remap=\"configure\">./configure --prefix=/usr</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/psmisc.xml:48
msgid "Compile the package:"
msgstr "Compile o pacote:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/psmisc.xml:50
#, no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr "<userinput remap=\"make\">make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/psmisc.xml:52
msgid "This package does not come with a test suite."
msgstr "Esse pacote não vem com uma suíte de teste."

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/psmisc.xml:54
msgid "Install the package:"
msgstr "Instale o pacote:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/psmisc.xml:56
#, no-wrap
msgid "<userinput remap=\"install\">make install</userinput>"
msgstr "<userinput remap=\"install\">make install</userinput>"

#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter08/psmisc.xml:61
msgid "Contents of Psmisc"
msgstr "Conteúdo do Psmisc"

#. type: Content of: <sect1><sect2><segmentedlist><segtitle>
#: lfs-en/chapter08/psmisc.xml:64
msgid "Installed programs"
msgstr "Aplicativos instalados"

#. type: Content of: <sect1><sect2><segmentedlist><seglistitem><seg>
#: lfs-en/chapter08/psmisc.xml:67
msgid ""
"fuser, killall, peekfd, prtstat, pslog, pstree, and pstree.x11 (link to "
"pstree)"
msgstr ""
"fuser, killall, peekfd, prtstat, pslog, pstree e pstree.x11 (link para "
"pstree)"

