#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter07/bison.xml:44
msgid "Installation of Bison"
msgstr "Instalação do Bison"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter07/bison.xml:46
msgid "Prepare Bison for compilation:"
msgstr "Prepare o Bison para compilação:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter07/bison.xml:48
#, no-wrap
msgid ""
"<userinput remap=\"configure\">./configure --prefix=/usr \\\n"
"            --docdir=/usr/share/doc/bison-&bison-version;</userinput>"
msgstr ""
"<userinput remap=\"configure\">./configure --prefix=/usr \\\n"
"            --docdir=/usr/share/doc/bison-&bison-version;</userinput>"

#. type: Content of: <sect1><sect2><variablelist><title>
#: lfs-en/chapter07/bison.xml:52
msgid "The meaning of the new configure option:"
msgstr "O significado da nova opção do configure:"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter07/bison.xml:55
msgid "<parameter>--docdir=/usr/share/doc/bison-&bison-version;</parameter>"
msgstr "<parameter>--docdir=/usr/share/doc/bison-&bison-version;</parameter>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter07/bison.xml:57
msgid ""
"This tells the build system to install bison documentation into a versioned "
"directory."
msgstr ""
"Isso diz ao sistema de construção para instalar a documentação do bison em "
"um diretório versionado."

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter07/bison.xml:64
