#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter06/xz.xml:44
msgid "Installation of Xz"
msgstr "Instalação do Xz"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/xz.xml:46
msgid "Prepare Xz for compilation:"
msgstr "Prepare o Xz para compilação:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/xz.xml:48
#, no-wrap
msgid ""
"<userinput remap=\"configure\">./configure --prefix=/usr                     "
"\\\n"
"            --host=$LFS_TGT                   \\\n"
"            --build=$(build-aux/config.guess) \\\n"
"            --disable-static                  \\\n"
"            --docdir=/usr/share/doc/xz-&xz-version;</userinput>"
msgstr ""
"<userinput remap=\"configure\">./configure --prefix=/usr                     "
"\\\n"
"            --host=$LFS_TGT                   \\\n"
"            --build=$(build-aux/config.guess) \\\n"
"            --disable-static                  \\\n"
"            --docdir=/usr/share/doc/xz-&xz-version;</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/xz.xml:54
msgid "Compile the package:"
msgstr "Compile o pacote:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/xz.xml:56
#, no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr "<userinput remap=\"make\">make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/xz.xml:58
msgid "Install the package:"
msgstr "Instale o pacote:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/xz.xml:60
#, no-wrap
msgid "<userinput remap=\"install\">make DESTDIR=$LFS install</userinput>"
msgstr "<userinput remap=\"install\">make DESTDIR=$LFS install</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/xz.xml:62
msgid "Remove the libtool archive file because it is harmful for cross compilation:"
msgstr ""
"Remova o arquivo de arquivamento do libtool pois ele é danoso para "
"compilação cruzada:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/xz.xml:65
#, no-wrap
msgid "<userinput remap=\"install\">rm -v $LFS/usr/lib/liblzma.la</userinput>"
msgstr "<userinput remap=\"install\">rm -v $LFS/usr/lib/liblzma.la</userinput>"

