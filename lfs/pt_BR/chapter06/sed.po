#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/sed.xml:46
msgid "Prepare Sed for compilation:"
msgstr "Prepare o Sed para compilação:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/sed.xml:48
#, no-wrap
msgid ""
"<userinput remap=\"configure\">./configure --prefix=/usr   \\\n"
"            --host=$LFS_TGT \\\n"
"            --build=$(./build-aux/config.guess)</userinput>"
msgstr ""
"<userinput remap=\"configure\">./configure --prefix=/usr  \\\n"
"            --host=$LFS_TGT \\\n"
"            --build=$(./build-aux/config.guess)</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/sed.xml:52
msgid "Compile the package:"
msgstr "Compile o pacote:"

