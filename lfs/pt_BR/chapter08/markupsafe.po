#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter08/markupsafe.xml:41
msgid "Installation of MarkupSafe"
msgstr "Instalação do MarkupSafe"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/markupsafe.xml:43
msgid "Compile MarkupSafe with the following command:"
msgstr "Compile MarkupSafe com o seguinte comando:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/markupsafe.xml:45
#, no-wrap
msgid "<userinput remap=\"make\">pip3 wheel -w dist --no-build-isolation --no-deps $PWD</userinput>"
msgstr ""
"<userinput remap=\"make\">pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/markupsafe.xml:47
msgid "This package does not come with a test suite."
msgstr "Esse pacote não vem com uma suíte de teste."

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/markupsafe.xml:49
msgid "Install the package:"
msgstr "Instale o pacote:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/markupsafe.xml:51
#, no-wrap
msgid "<userinput remap=\"install\">pip3 install --no-index --no-user --find-links dist Markupsafe</userinput>"
msgstr ""
"<userinput remap=\"install\">pip3 install --no-index --no-user --find-links "
"dist Markupsafe</userinput>"

