#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter08/attr.xml:42
msgid "Installation of Attr"
msgstr "Instalação do Attr"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/attr.xml:44
msgid "Prepare Attr for compilation:"
msgstr "Prepare Attr para compilação:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/attr.xml:46
#, no-wrap
msgid ""
"<userinput remap=\"configure\">./configure --prefix=/usr     \\\n"
"            --disable-static  \\\n"
"            --sysconfdir=/etc \\\n"
"            --docdir=/usr/share/doc/attr-&attr-version;</userinput>"
msgstr ""
"<userinput remap=\"configure\">./configure --prefix=/usr     \\\n"
"            --disable-static  \\\n"
"            --sysconfdir=/etc \\\n"
"            --docdir=/usr/share/doc/attr-&attr-version;</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/attr.xml:51
msgid "Compile the package:"
msgstr "Compile o pacote:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/attr.xml:53
#, no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr "<userinput remap=\"make\">make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/attr.xml:55
msgid ""
"The tests must be run on a filesystem that supports extended attributes such "
"as the ext2, ext3, or ext4 filesystems.  To test the results, issue:"
msgstr ""
"Os testes precisam ser executados sobre um sistema de arquivos que suporte "
"atributos estendidos, tais como os sistemas de arquivos ext2, ext3 ou ext4. "
"Para testar os resultados, emita:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/attr.xml:59
#, no-wrap
msgid "<userinput remap=\"test\">make check</userinput>"
msgstr "<userinput remap=\"test\">make check</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/attr.xml:61
msgid "Install the package:"
msgstr "Instale o pacote:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/attr.xml:63
#, no-wrap
msgid "<userinput remap=\"install\">make install</userinput>"
msgstr "<userinput remap=\"install\">make install</userinput>"

