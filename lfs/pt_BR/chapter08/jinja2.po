#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter08/jinja2.xml:41
msgid "Installation of Jinja2"
msgstr "Instalação do Jinja2"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/jinja2.xml:43
msgid "Build the package:"
msgstr "Construa o pacote:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/jinja2.xml:45
#, no-wrap
msgid ""
"<userinput remap=\"install\">pip3 wheel -w dist --no-build-isolation "
"--no-deps $PWD</userinput>"
msgstr ""
"<userinput remap=\"install\">pip3 wheel -w dist --no-build-isolation --no-"
"deps $PWD</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/jinja2.xml:47
msgid "Install the package:"
msgstr "Instale o pacote:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/jinja2.xml:49
#, no-wrap
msgid ""
"<userinput remap=\"install\">pip3 install --no-index --no-user --find-links "
"dist Jinja2</userinput>"
msgstr ""
"<userinput remap=\"install\">pip3 install --no-index --no-user --find-links "
"dist Jinja2</userinput>"

