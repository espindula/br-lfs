#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter08/gperf.xml:41
msgid "Installation of Gperf"
msgstr "Instalação do Gperf"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/gperf.xml:43
msgid "Prepare Gperf for compilation:"
msgstr "Prepare Gperf para compilação:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/gperf.xml:45
#, no-wrap
msgid ""
"<userinput remap=\"configure\">./configure --prefix=/usr "
"--docdir=/usr/share/doc/gperf-&gperf-version;</userinput>"
msgstr ""
"<userinput remap=\"configure\">./configure --prefix=/usr --docdir=/usr/share/"
"doc/gperf-&gperf-version;</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/gperf.xml:47
msgid "Compile the package:"
msgstr "Compile o pacote:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/gperf.xml:49
#, no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr "<userinput remap=\"make\">make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/gperf.xml:51
msgid ""
"The tests are known to fail if running multiple simultaneous tests (-j "
"option greater than 1). To test the results, issue:"
msgstr ""
"Os testes são conhecidos por falharem se executar múltiplos testes "
"simultâneos (opção -j maior que 1). Para testar os resultados, emita:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/gperf.xml:55
#, no-wrap
msgid "<userinput remap=\"test\">make -j1 check</userinput>"
msgstr "<userinput remap=\"test\">make -j1 check</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/gperf.xml:57
msgid "Install the package:"
msgstr "Instale o pacote:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/gperf.xml:59
#, no-wrap
msgid "<userinput remap=\"install\">make install</userinput>"
msgstr "<userinput remap=\"install\">make install</userinput>"

