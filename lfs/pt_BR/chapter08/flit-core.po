#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter08/flit-core.xml:41
msgid "Installation of Flit-Core"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/flit-core.xml:43
msgid "Build the package:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/flit-core.xml:45
#, no-wrap
msgid ""
"<userinput remap=\"install\">pip3 wheel -w dist --no-build-isolation "
"--no-deps $PWD</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/flit-core.xml:47
msgid "Install the package:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/flit-core.xml:49
#, no-wrap
msgid ""
"<userinput remap=\"install\">pip3 install --no-index --no-user --find-links "
"dist flit_core</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><title>
#: lfs-en/chapter08/flit-core.xml:52
msgid "The meaning of the pip3 configuration options and commands:"
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/flit-core.xml:55
msgid "<command>wheel</command>"
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/flit-core.xml:57
msgid "This command builds the wheel archive for this package."
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/flit-core.xml:62
msgid "<parameter>-w dist</parameter>"
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/flit-core.xml:64
msgid ""
"Instructs pip to put the created wheel into the <filename "
"class='directory'>dist</filename> directory."
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/flit-core.xml:70
msgid "<command>install</command>"
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/flit-core.xml:72
msgid "This command installs the package."
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/flit-core.xml:77
msgid ""
"<parameter>--no-build-isolation</parameter>, "
"<parameter>--no-deps</parameter>, and <parameter>--no-index</parameter>"
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/flit-core.xml:81
msgid ""
"These options prevent fetching files from the online package repository "
"(PyPI). If packages are installed in the correct order, pip won't need to "
"fetch any files in the first place; these options add some safety in case of "
"user error."
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/flit-core.xml:89
msgid "<parameter>--find-links dist</parameter>"
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/flit-core.xml:91
msgid ""
"Instructs pip to search for wheel archives in the <filename "
"class='directory'>dist</filename> directory."
msgstr ""

