#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter08/dejagnu.xml:43
msgid "Installation of DejaGNU"
msgstr "Instalação do DejaGNU"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/dejagnu.xml:45
msgid "The upstream recommends building DejaGNU in a dedicated build directory:"
msgstr ""
"A(O) desenvolvedora(r) recomenda construir DejaGNU em um diretório dedicado "
"à construção:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/dejagnu.xml:48
#, no-wrap
msgid ""
"<userinput remap=\"pre\">mkdir -v build\n"
"cd       build</userinput>"
msgstr ""
"<userinput remap=\"pre\">mkdir -v build\n"
"cd      build</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/dejagnu.xml:51
msgid "Prepare DejaGNU for compilation:"
msgstr "Prepare DejaGNU para compilação:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/dejagnu.xml:53
#, no-wrap
msgid ""
"<userinput remap=\"configure\">../configure --prefix=/usr\n"
"makeinfo --html --no-split -o doc/dejagnu.html ../doc/dejagnu.texi\n"
"makeinfo --plaintext       -o doc/dejagnu.txt  "
"../doc/dejagnu.texi</userinput>"
msgstr ""
"<userinput remap=\"configure\">../configure --prefix=/usr\n"
"makeinfo --html --no-split -o doc/dejagnu.html ../doc/dejagnu.texi\n"
"makeinfo --plaintext       -o doc/dejagnu.txt  ../doc/dejagnu."
"texi</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/dejagnu.xml:57
msgid "Build and install the package:"
msgstr "Construa e instale o pacote:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/dejagnu.xml:59
#, no-wrap
msgid ""
"<userinput remap=\"install\">make install\n"
"install -v -dm755  /usr/share/doc/dejagnu-&dejagnu-version;\n"
"install -v -m644   doc/dejagnu.{html,txt} "
"/usr/share/doc/dejagnu-&dejagnu-version;</userinput>"
msgstr ""
"<userinput remap=\"install\">make install\n"
"install -v -dm755  /usr/share/doc/dejagnu-&dejagnu-version;\n"
"install -v -m644   doc/dejagnu.{html,txt} /usr/share/doc/dejagnu-&dejagnu-"
"version;</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/dejagnu.xml:63
msgid "To test the results, issue:"
msgstr "Para testar os resultados, emita:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/dejagnu.xml:65
#, no-wrap
msgid "<userinput remap=\"test\">make check</userinput>"
msgstr "<userinput remap=\"test\">make check</userinput>"

