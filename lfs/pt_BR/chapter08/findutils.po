#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter08/findutils.xml:46
msgid "Installation of Findutils"
msgstr "Instalação do Findutils"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/findutils.xml:48
msgid "Prepare Findutils for compilation:"
msgstr "Prepare o Findutils para compilação:"

#
#.  no longer needed with gcc13
#. <screen>
#. <userinput remap="configure">case $(uname -m) in
#.     i?86)   TIME_T_32_BIT_OK=yes ./configure - -prefix=/usr - -localstatedir=/var/lib/locate ;;
#.     x86_64) ./configure - -prefix=/usr - -localstatedir=/var/lib/locate ;;
#. esac</userinput></screen>
#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/findutils.xml:56
#, no-wrap
msgid "<userinput remap=\"configure\">./configure --prefix=/usr --localstatedir=/var/lib/locate</userinput>"
msgstr ""
"<userinput remap=\"configure\">./configure --prefix=/usr --localstatedir=/"
"var/lib/locate</userinput>"

#. type: Content of: <sect1><sect2><variablelist><title>
#: lfs-en/chapter08/findutils.xml:59
msgid "The meaning of the configure options:"
msgstr "O significado das opções do configure:"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/findutils.xml:69
msgid "<parameter>--localstatedir</parameter>"
msgstr "<parameter>--localstatedir</parameter>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/findutils.xml:71
msgid ""
"This option moves the <command>locate</command> database to <filename "
"class=\"directory\">/var/lib/locate</filename>, which is the FHS-compliant "
"location."
msgstr ""
"Essa opção move a base de dados <command>locate</command> para <filename "
"class=\"directory\">/var/lib/locate</filename>, o qual é o local conforme "
"com FHS."

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/findutils.xml:79
msgid "Compile the package:"
msgstr "Compile o pacote:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/findutils.xml:81
#, no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr "<userinput remap=\"make\">make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/findutils.xml:83
msgid "To test the results, issue:"
msgstr "Para testar os resultados, emita:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/findutils.xml:85
#, no-wrap
msgid ""
"<userinput remap=\"test\">chown -Rv tester .\n"
"su tester -c \"PATH=$PATH make check\"</userinput>"
msgstr ""
"<userinput remap=\"test\">chown -Rv tester .\n"
"su tester -c \"PATH=$PATH make check\"</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/findutils.xml:88
msgid "Install the package:"
msgstr "Instale o pacote:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/findutils.xml:90
#, no-wrap
msgid "<userinput remap=\"install\">make install</userinput>"
msgstr "<userinput remap=\"install\">make install</userinput>"

