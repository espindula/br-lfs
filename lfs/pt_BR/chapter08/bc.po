#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter08/bc.xml:42
msgid "Installation of Bc"
msgstr "Instalação do Bc"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/bc.xml:44
msgid "Prepare Bc for compilation:"
msgstr "Prepare Bc para compilação:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/bc.xml:46
#, no-wrap
msgid "<userinput remap=\"configure\">CC=gcc ./configure --prefix=/usr -G -O3 -r</userinput>"
msgstr ""
"<userinput remap=\"configure\">CC=gcc ./configure --prefix=/usr -G -O3 "
"-r</userinput>"

#. type: Content of: <sect1><sect2><variablelist><title>
#: lfs-en/chapter08/bc.xml:49
msgid "The meaning of the configure options:"
msgstr "O significado das opções do configure:"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/bc.xml:52
msgid "<parameter>CC=gcc</parameter>"
msgstr "<parameter>CC=gcc</parameter>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/bc.xml:54
msgid "This parameter specifies the compiler to use."
msgstr "Esse parâmetro especifica o compilador a usar."

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/bc.xml:59
msgid "<parameter>-G</parameter>"
msgstr "<parameter>-G</parameter>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/bc.xml:61
msgid ""
"Omit parts of the test suite that won't work until the bc program has been "
"installed."
msgstr ""
"Omite partes da suíte de teste que não funcionariam até que o aplicativo bc "
"tenha sido instalado."

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/bc.xml:67
msgid "<parameter>-O3</parameter>"
msgstr "<parameter>-O3</parameter>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/bc.xml:69
msgid "Specify the optimization to use."
msgstr "Especifica a optimização a usar."

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/bc.xml:74
msgid "<parameter>-r</parameter>"
msgstr "<parameter>-r</parameter>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/bc.xml:76
msgid ""
"Enable the use of <application>Readline</application> to improve the line "
"editing feature of bc."
msgstr ""
"Habilita o uso de <application>Readline</application> para melhorar o "
"recurso de edição de linha do bc."

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/bc.xml:82
msgid "Compile the package:"
msgstr "Compile o pacote:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/bc.xml:84
#, no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr "<userinput remap=\"make\">make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/bc.xml:86
msgid "To test bc, run:"
msgstr "Para testar bc, execute:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/bc.xml:88
#, no-wrap
msgid "<userinput remap=\"test\">make test</userinput>"
msgstr "<userinput remap=\"test\">make test</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/bc.xml:91
msgid "Install the package:"
msgstr "Instale o pacote:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/bc.xml:93
#, no-wrap
msgid "<userinput remap=\"install\">make install</userinput>"
msgstr "<userinput remap=\"install\">make install</userinput>"

