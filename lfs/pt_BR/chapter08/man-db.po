#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/man-db.xml:116
#, no-wrap
msgid "<userinput remap=\"test\">make -k check</userinput>"
msgstr "<userinput remap=\"test\">make -k check</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/man-db.xml:119
msgid "One test named <filename>man1/lexgrog.1</filename> is known to fail."
msgstr ""
"Um teste chamado <filename>man1/lexgrog.1</filename> é conhecido por falhar."

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/man-db.xml:122
