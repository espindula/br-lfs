#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter08/bison.xml:41
msgid "Installation of Bison"
msgstr "Instalação do Bison"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/bison.xml:43
msgid "Prepare Bison for compilation:"
msgstr "Prepare o Bison para compilação:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/bison.xml:45
#, no-wrap
msgid ""
"<userinput remap=\"configure\">./configure --prefix=/usr "
"--docdir=/usr/share/doc/bison-&bison-version;</userinput>"
msgstr ""
"<userinput remap=\"configure\">./configure --prefix=/usr --docdir=/usr/share/"
"doc/bison-&bison-version;</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/bison.xml:47
msgid "Compile the package:"
msgstr "Compile o pacote:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/bison.xml:49
#, no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr "<userinput remap=\"make\">make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/bison.xml:51
msgid "To test the results (about 5.5 SBU), issue:"
msgstr "Para testar os resultados (cerca de 5,5 UPCs), emita:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/bison.xml:53
#, no-wrap
msgid "<userinput remap=\"test\">make check</userinput>"
msgstr "<userinput remap=\"test\">make check</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/bison.xml:55
msgid "Install the package:"
msgstr "Instale o pacote:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/bison.xml:57
#, no-wrap
msgid "<userinput remap=\"install\">make install</userinput>"
msgstr "<userinput remap=\"install\">make install</userinput>"

