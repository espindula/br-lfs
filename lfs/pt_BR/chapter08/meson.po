#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter08/meson.xml:41
msgid "Installation of Meson"
msgstr "Instalação do Meson"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/meson.xml:47
msgid "Compile Meson with the following command:"
msgstr "Compile Meson com o seguinte comando:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/meson.xml:49
#, no-wrap
msgid "<userinput remap=\"make\">pip3 wheel -w dist --no-build-isolation --no-deps $PWD</userinput>"
msgstr "<userinput remap=\"make\">pip3 wheel -w dist --no-build-isolation --no-deps $PWD</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/meson.xml:51
msgid "The test suite requires some packages outside the scope of LFS."
msgstr "A suíte de teste exige alguns pacotes fora do escopo do LFS."

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/meson.xml:53
msgid "Install the package:"
msgstr "Instale o pacote:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/meson.xml:55
#, no-wrap
msgid ""
"<userinput remap=\"install\">pip3 install --no-index --find-links dist meson\n"
"install -vDm644 data/shell-completions/bash/meson /usr/share/bash-completion/completions/meson\n"
"install -vDm644 data/shell-completions/zsh/_meson /usr/share/zsh/site-functions/_meson</userinput>"
msgstr ""
"<userinput remap=\"install\">pip3 install --no-index --find-links dist meson\n"
"install -vDm644 data/shell-completions/bash/meson /usr/share/bash-completion/completions/meson\n"
"install -vDm644 data/shell-completions/zsh/_meson /usr/share/zsh/site-functions/_meson</userinput>"

#. type: Content of: <sect1><sect2><variablelist><title>
#: lfs-en/chapter08/meson.xml:60
msgid "The meaning of the install parameters:"
msgstr "O significado dos parâmetros do install:"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/meson.xml:63
msgid "<parameter>-w dist</parameter>"
msgstr "<parameter>-w dist</parameter>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/meson.xml:65
msgid ""
"Puts the created wheels into the <filename class='directory'>dist</filename> "
"directory."
msgstr ""
"Coloca as rodas criadas no diretório <filename class='directory'>dist</"
"filename>."

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/meson.xml:71
msgid "<parameter>--find-links dist</parameter>"
msgstr "<parameter>--find-links dist</parameter>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/meson.xml:73
msgid ""
"Installs wheels from the <filename class='directory'>dist</filename> "
"directory."
msgstr ""
"Instala as rodas a partir do diretório <filename class='directory'>dist</"
"filename>."

