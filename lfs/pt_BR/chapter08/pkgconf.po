#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter08/pkgconf.xml:44
msgid "Installation of Pkgconf"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/pkgconf.xml:46
msgid "Prepare Pkgconf for compilation:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/pkgconf.xml:48
#, no-wrap
msgid ""
"<userinput remap=\"configure\">./configure --prefix=/usr              \\\n"
"            --disable-static           \\\n"
"            --docdir=/usr/share/doc/pkgconf-&pkgconf-version;</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/pkgconf.xml:52
msgid "Compile the package:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/pkgconf.xml:54
#, no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/pkgconf.xml:56
msgid "Install the package:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/pkgconf.xml:58
#, no-wrap
msgid "<userinput remap=\"install\">make install</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/pkgconf.xml:60
msgid "To maintain compatibility with the original Pkg-config create a symlink:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/pkgconf.xml:63
#, no-wrap
msgid "<userinput remap=\"install\">ln -sfv pkgconf /usr/bin/pkg-config</userinput>"
msgstr ""

