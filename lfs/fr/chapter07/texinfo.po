#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter07/texinfo.xml:44
msgid "Installation of Texinfo"
msgstr "Installation de Texinfo"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter07/texinfo.xml:46
msgid "First, fix an issue building the package with Glibc-2.34 or later:"
msgstr ""
"Tout d'abord, corrigez un problème de construction du paquet avec Glibc-2.34 "
"et ultérieur&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter07/texinfo.xml:48
#, no-wrap
msgid ""
"<userinput remap=\"pre\">sed -e 's/__attribute_nonnull__/__nonnull/' \\\n"
"    -i gnulib/lib/malloc/dynarray-skeleton.c</userinput>"
msgstr ""
"<userinput remap=\"pre\">sed -e 's/__attribute_nonnull__/__nonnull/' \\\n"
"    -i gnulib/lib/malloc/dynarray-skeleton.c</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter07/texinfo.xml:51
msgid "Prepare Texinfo for compilation:"
msgstr "Préparez la compilation de Texinfo&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter07/texinfo.xml:53
#, no-wrap
msgid "<userinput remap=\"configure\">./configure --prefix=/usr</userinput>"
msgstr "<userinput remap=\"configure\">./configure --prefix=/usr</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter07/texinfo.xml:55
msgid "Compile the package:"
msgstr "Compilez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter07/texinfo.xml:57
#, no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr "<userinput remap=\"make\">make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter07/texinfo.xml:59
msgid "Install the package:"
msgstr "Installez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter07/texinfo.xml:61
#, no-wrap
msgid "<userinput remap=\"install\">make install</userinput>"
msgstr "<userinput remap=\"install\">make install</userinput>"

