#: lfs-en/chapter07/bison.xml:44
msgid "Installation of Bison"
msgstr "Installation de Bison"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter07/bison.xml:46
msgid "Prepare Bison for compilation:"
msgstr "Préparez la compilation de Bison&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter07/bison.xml:48
#, no-wrap
msgid ""
"<userinput remap=\"configure\">./configure --prefix=/usr \\\n"
"            --docdir=/usr/share/doc/bison-&bison-version;</userinput>"
msgstr ""
"<userinput remap=\"configure\">./configure --prefix=/usr \\\n"
"            --docdir=/usr/share/doc/bison-&bison-version;</userinput>"

