#~ "<userinput remap=\"pre\">sed -i 's/if \\((code.*))\\)/if (\\1 \\&amp;\\&amp;"
#~ " \\!DEBUG_INSN_P (insn))/' gcc/sched-deps.c</userinput>"
#~ msgstr ""
