#~ msgstr ""
#~ "<userinput remap=\"pre\">sed -i '/k prot/agcc_cv_libc_provides_ssp=yes' "
#~ "gcc/configure</userinput>"

#~ msgid "Also fix a problem identified upstream:"
#~ msgstr "Corrigez aussi un problème identifié en amont&nbsp;:"

