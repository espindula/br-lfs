#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter05/util-linux.xml:46
#, no-wrap
msgid ""
"<userinput remap=\"configure\">./configure --prefix=/tools                \\\n"
"            --without-python               \\\n"
"            --disable-makeinstall-chown    \\\n"
"            --without-systemdsystemunitdir \\\n"
"            PKG_CONFIG=\"\"</userinput>"
msgstr ""
"<userinput remap=\"configure\">./configure --prefix=/tools                \\\n"
"            --without-python               \\\n"
"            --disable-makeinstall-chown    \\\n"
"            --without-systemdsystemunitdir \\\n"
"            PKG_CONFIG=\"\"</userinput>"

#. type: Content of: <sect1><sect2><screen>
