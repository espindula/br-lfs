#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter06/psmisc.xml:42
msgid "Installation of Psmisc"
msgstr "Installation de Psmisc"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/psmisc.xml:44
msgid "Prepare Psmisc for compilation:"
msgstr "Préparez la compilation de Psmisc pour&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/psmisc.xml:46
#, , no-wrap
msgid "<userinput remap=\"configure\">./configure --prefix=/usr</userinput>"
msgstr "<userinput remap=\"configure\">./configure --prefix=/usr</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/psmisc.xml:48
msgid "Compile the package:"
msgstr "Compilez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/psmisc.xml:50
#, , no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr "<userinput remap=\"make\">make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/psmisc.xml:52
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/psmisc.xml:54
msgid "Install the package:"
msgstr "Installez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/psmisc.xml:56
#, , no-wrap
msgid "<userinput remap=\"install\">make install</userinput>"
msgstr "<userinput remap=\"install\">make install</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/psmisc.xml:58
msgid ""
"Finally, move the <command>killall</command> and <command>fuser</command> "
"programs to the location specified by the FHS:"
msgstr ""
"Enfin, déplacez les programmes <command>killall</command> et "
"<command>fuser</command> à l'endroit spécifié par la FHS&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/psmisc.xml:61
#, , no-wrap
msgid ""
"<userinput remap=\"install\">mv -v /usr/bin/fuser   /bin\n"
"mv -v /usr/bin/killall /bin</userinput>"
msgstr ""
"<userinput remap=\"install\">mv -v /usr/bin/fuser   /bin\n"
"mv -v /usr/bin/killall /bin</userinput>"

#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter06/psmisc.xml:67
