"<userinput remap=\"pre\">sed -i 's/IO_ftrylockfile/IO_EOF_SEEN/' lib/*.c\n"
"echo \"#define _IO_IN_BACKUP 0x100\" >> lib/stdio-impl.h</userinput>"
msgstr ""

