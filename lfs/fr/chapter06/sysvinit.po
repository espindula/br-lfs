#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter06/sysvinit.xml:42
msgid "Installation of Sysvinit"
msgstr "Installation de Sysvinit"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/sysvinit.xml:70
msgid ""
"First, apply a patch that removes several programs installed by other "
"packages, clarifies a message, and fixes a compiler warning:"
msgstr ""
"Tout d'abord, appliquez un correctif qui supprime plusieurs programmes "
"installés par d'autres paquets, qui clarifie un message et qui corrige un "
"avertissement du compilateur&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/sysvinit.xml:73
#, no-wrap
msgid "<userinput remap=\"pre\">patch -Np1 -i ../&sysvinit-consolidated-patch;</userinput>"
msgstr "<userinput remap=\"pre\">patch -Np1 -i ../&sysvinit-consolidated-patch;</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/sysvinit.xml:75
msgid "Compile the package:"
msgstr "Compilez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/sysvinit.xml:77
#, no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr "<userinput remap=\"make\">make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/sysvinit.xml:79
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'est pas fourni avec une suite de tests."

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/sysvinit.xml:81
msgid "Install the package:"
msgstr "Installez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/sysvinit.xml:83
#, no-wrap
msgid "<userinput remap=\"install\">make install</userinput>"
msgstr "<userinput remap=\"install\">make install</userinput>"

#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter06/sysvinit.xml:88
msgid "Contents of Sysvinit"
msgstr "Contenu de Sysvinit"

#. type: Content of: <sect1><sect2><segmentedlist><segtitle>
#: lfs-en/chapter06/sysvinit.xml:91
msgid "Installed programs"
msgstr "Programmes installés"

#. type: Content of: <sect1><sect2><segmentedlist><seglistitem><seg>
#: lfs-en/chapter06/sysvinit.xml:94
msgid ""
"bootlogd, fstab-decode, halt, init, killall5, poweroff (link to halt), "
"reboot (link to halt), runlevel, shutdown, and telinit (link to init)"
msgstr ""
"bootlogd, fstab-decode, halt, init, killall5, poweroff (liien vers halt), "
"reboot (lien vers halt), runlevel, shutdown et telinit (link to init)"

