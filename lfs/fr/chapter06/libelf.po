#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter06/libelf.xml:42
msgid "Installation of Libelf"
msgstr "Installation de Libelf"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/libelf.xml:44
msgid ""
"Libelf is part of elfutils-&elfutils-version; package. Use the elfutils-"
"&elfutils-version;.tar.xz as the source tarball."
msgstr ""
"Libelf fait partit du paquet elfutils-&elfutils-version;. Utilisez elfutils-"
"&elfutils-version;.tar.xz comme archive des sources."

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/libelf.xml:47
#| msgid "Fiirst, fix a file to be compatible with gcc-8\""
msgid "First, fix a file to be compatible with gcc-8\""
msgstr ""
"Tout d'abord, corrigez un fichier pour qu'il soit compatible avec gcc-8"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/libelf.xml:49
#, no-wrap
msgid ""
"<userinput remap=\"pre\">sed -e '/ALIGN_PRSTATUS)/{ \n"
"        s/__attribute/attribute_packed &amp;/\n"
"        s/packed, //}' \\\n"
"    -i backends/linux-core-note.c</userinput>"
msgstr ""
"<userinput remap=\"pre\">sed -e '/ALIGN_PRSTATUS)/{ \n"
"        s/__attribute/attribute_packed &amp;/\n"
"        s/packed, //}' \\\n"
"    -i backends/linux-core-note.c</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/libelf.xml:54
msgid "Prepare Libelf for compilation:"
msgstr "Préparez Libelf pour la compilation&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/libelf.xml:56
#, no-wrap
msgid "<userinput remap=\"configure\">./configure --prefix=/usr</userinput>"
msgstr "<userinput remap=\"configure\">./configure --prefix=/usr</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/libelf.xml:58
msgid "Compile the package:"
msgstr "Compilez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/libelf.xml:60
#, no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr "<userinput remap=\"make\">make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/libelf.xml:62
msgid "To test the results, issue <command>make check</command>."
msgstr "Pour tester les résultats, lancez <command>make check</command>."

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/libelf.xml:64
msgid "Install only Libelf:"
msgstr "Installez seulement Libelf&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/libelf.xml:66
#, no-wrap
msgid ""
"<userinput remap=\"install\">make -C libelf install\n"
"install -vm644 config/libelf.pc /usr/lib/pkgconfig</userinput>"
msgstr ""
"<userinput remap=\"install\">make -C libelf install\n"
"install -vm644 config/libelf.pc /usr/lib/pkgconfig</userinput>"

