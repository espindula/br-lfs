#. type: Content of: <sect1><sect2><segmentedlist><seglistitem><seg>
#: lfs-en/chapter08/gawk.xml:79
msgid "awk (link to gawk), gawk, and awk-&gawk-version;"
msgstr "awk (lien vers gawk), gawk et awk-&gawk-version;"

#. type: Content of: <sect1><sect2><segmentedlist><seglistitem><seg>
#: lfs-en/chapter08/gawk.xml:80
msgid ""
"filefuncs.so, fnmatch.so, fork.so, inplace.so, intdiv.so, ordchr.so, readdir."
"so, readfile.so, revoutput.so, revtwoway.so, rwarray.so, and time.so (all "
"in /usr/lib/gawk)"
msgstr ""
"filefuncs.so, fnmatch.so, fork.so, inplace.so, intdiv.so, ordchr.so, readdir."
"so, readfile.so, revoutput.so, revtwoway.so, rwarray.so, et time.so (toutes "
"dans /usr/lib/gawk)"

#. type: Content of: <sect1><sect2><segmentedlist><seglistitem><seg>
#: lfs-en/chapter08/gawk.xml:83
msgid ""
"/usr/lib/gawk, /usr/libexec/awk, /usr/share/awk, and /usr/share/doc/gawk-"
"&gawk-version;"
msgstr ""
"/usr/lib/gawk, /usr/libexec/awk, /usr/share/awk, et /usr/share/doc/gawk-"
"&gawk-version;"

