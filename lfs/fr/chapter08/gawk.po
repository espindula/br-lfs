#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter08/gawk.xml:41
msgid "Installation of Gawk"
msgstr "Installation de Gawk"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/gawk.xml:43
msgid "First, ensure some unneeded files are not installed:"
msgstr ""
"Tout d'abord, assurez-vous que certains fichiers inutiles ne sont pas installés&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/gawk.xml:45
#, no-wrap
msgid "<userinput remap=\"pre\">sed -i 's/extras//' Makefile.in</userinput>"
msgstr "<userinput remap=\"pre\">sed -i 's/extras//' Makefile.in</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/gawk.xml:47
msgid "Prepare Gawk for compilation:"
msgstr "Préparez la compilation de Gawk&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/gawk.xml:49
#, no-wrap
msgid "<userinput remap=\"configure\">./configure --prefix=/usr</userinput>"
msgstr "<userinput remap=\"configure\">./configure --prefix=/usr</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/gawk.xml:51
msgid "Compile the package:"
msgstr "Compilez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/gawk.xml:53
#, no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr "<userinput remap=\"make\">make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/gawk.xml:55
msgid "To test the results, issue:"
msgstr "Pour tester les résultats, lancez&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/gawk.xml:57
#, no-wrap
msgid "<userinput remap=\"test\">make check</userinput>"
msgstr "<userinput remap=\"test\">make check</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/gawk.xml:59
msgid "Install the package:"
msgstr "Installez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/gawk.xml:61
#, no-wrap
msgid "<userinput remap=\"install\">make LN='ln -f' install</userinput>"
msgstr "<userinput remap=\"install\">make LN='ln -f' install</userinput>"

#. type: Content of: <sect1><sect2><variablelist><title>
#: lfs-en/chapter08/gawk.xml:64
msgid "The meaning of the overridden make variable:"
msgstr "Signification de la variable make du override :"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/gawk.xml:67
msgid "<envar>LN='ln -f'</envar>"
msgstr "<envar>LN= \"ln -f</envar>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/gawk.xml:69
msgid ""
"This variable ensures that the previous hard link installed in <xref linkend='ch-tools-"
"gawk'/> is updated here."
msgstr ""
"Cette variable garantit que le précédent hard link installé sur <xref linkend=‘ch-tools-"
"gawk’/> soit actualisé ici."

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/gawk.xml:75
msgid "If desired, install the documentation:"
msgstr "Si vous le souhaitez, installez la documentation&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/gawk.xml:77
#, no-wrap
msgid ""
"<userinput remap=\"install\">mkdir -pv                                   /usr/share/doc/gawk-&gawk-version;\n"
"cp    -v doc/{awkforai.txt,*.{eps,pdf,jpg}} /usr/share/doc/gawk-&gawk-version;</userinput>"
msgstr ""
"<userinput remap=\"install\">mkdir -pv                                   /usr/share/doc/gawk-&gawk-version;\n"
"cp    -v doc/{awkforai.txt,*.{eps,pdf,jpg}} /usr/share/doc/gawk-&gawk-version;</userinput>"

