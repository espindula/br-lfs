#: lfs-en/chapter08/sysklogd.xml:42
msgid "Installation of Sysklogd"
msgstr "Installation de Sysklogd"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/sysklogd.xml:44
msgid ""
"First, fix problems that causes a segmentation fault under some conditions "
"in klogd and fix an obsolete program construct:"
msgstr ""
"Tout d'abord, corrigez un problème qui crée une erreur de segmentation dans "
"certaines conditions dans klogd et corrigez une conception obsolète du "
"programme&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/sysklogd.xml:47
#, no-wrap
msgid ""
"<userinput remap=\"pre\">sed -i '/Error loading kernel symbols/{n;n;d}' ksym_mod.c\n"
"sed -i 's/union wait/int/' syslogd.c</userinput>"
msgstr ""
"<userinput remap=\"pre\">sed -i '/Error loading kernel symbols/{n;n;d}' ksym_mod.c\n"
"sed -i 's/union wait/int/' syslogd.c</userinput>"

