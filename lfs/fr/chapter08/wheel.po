#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter08/wheel.xml:41
msgid "Installation of Wheel"
msgstr "Installation de Wheel"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/wheel.xml:43
msgid "Compile Wheel with the following command:"
msgstr "Compilez wheel avec la commande suivante&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/wheel.xml:45
#, no-wrap
msgid "<userinput remap=\"make\">pip3 wheel -w dist --no-build-isolation --no-deps $PWD</userinput>"
msgstr ""
"<userinput remap=\"make\">pip3 wheel -w dist --no-build-isolation --no-deps "
"$PWD</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/wheel.xml:47
msgid "Install Wheel with the following command:"
msgstr "Installez wheel avec la commande suivante&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/wheel.xml:49
#, no-wrap
msgid "<userinput remap=\"install\">pip3 install --no-index --find-links=dist wheel</userinput>"
msgstr "<userinput remap=\"install\">pip3 install --no-index --find-links=dist wheel</userinput>"

