#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter08/mpfr.xml:42
msgid "Installation of MPFR"
msgstr "Installation de MPFR"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/mpfr.xml:44
msgid "Prepare MPFR for compilation:"
msgstr "Préparez la compilation de MPFR&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/mpfr.xml:46
#, no-wrap
msgid ""
"<userinput remap=\"configure\">./configure --prefix=/usr        \\\n"
"            --disable-static     \\\n"
"            --enable-thread-safe \\\n"
"            --docdir=/usr/share/doc/mpfr-&mpfr-version;</userinput>"
msgstr ""
"<userinput remap=\"configure\">./configure --prefix=/usr        \\\n"
"            --disable-static     \\\n"
"            --enable-thread-safe \\\n"
"            --docdir=/usr/share/doc/mpfr-&mpfr-version;</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/mpfr.xml:51
msgid "Compile the package and generate the HTML documentation:"
msgstr "Compilez le paquet et générez la documentation HTML&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/mpfr.xml:53
#, no-wrap
msgid ""
"<userinput remap=\"make\">make\n"
"make html</userinput>"
msgstr ""
"<userinput remap=\"make\">make\n"
"make html</userinput>"

#. type: Content of: <sect1><sect2><important><para>
#: lfs-en/chapter08/mpfr.xml:57
msgid ""
"The test suite for MPFR in this section is considered critical.  Do not skip "
"it under any circumstances."
msgstr ""
"La suite de tests de MPFR dans cette section est considérée comme critique. "
"Ne la sautez en aucun cas."

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/mpfr.xml:61
msgid "Test the results and ensure that all tests passed:"
msgstr ""
"Testez les résultats et assurez-vous que tous les tests ont réussi&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/mpfr.xml:63
#, no-wrap
msgid "<userinput remap=\"test\">make check</userinput>"
msgstr "<userinput remap=\"test\">make check</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/mpfr.xml:65
msgid "Install the package and its documentation:"
msgstr "Installez le paquet et sa documentation&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/mpfr.xml:67
#, no-wrap
msgid ""
"<userinput remap=\"install\">make install\n"
"make install-html</userinput>"
msgstr ""
"<userinput remap=\"install\">make install\n"
"make install-html</userinput>"

