"<userinput remap=\"configure\">./configure --prefix=/usr                           \\\n"
"            --docdir=/usr/share/doc/procps-ng-&procps-ng-version; \\\n"
"            --disable-static                        \\\n"
"            --disable-kill                          \\\n"
"            --with-systemd</userinput>"
msgstr ""
"<userinput remap=\"configure\">./configure --prefix=/usr                           \\\n"
"            --docdir=/usr/share/doc/procps-ng-&procps-ng-version; \\\n"
