#: lfs-en/chapter08/markupsafe.xml:41
msgid "Installation of MarkupSafe"
msgstr "Installation de MarkupSafe"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/markupsafe.xml:43
msgid "Compile MarkupSafe with the following command:"
msgstr "Compilez MarkupSafe avec la commande suivante&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/markupsafe.xml:45
#, fuzzy, no-wrap
#| msgid "<userinput remap=\"make\">python3 setup.py build</userinput>"
msgid "<userinput remap=\"make\">pip3 wheel -w dist --no-build-isolation --no-deps $PWD</userinput>"
msgstr "<userinput remap=\"make\">python3 setup.py build</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/markupsafe.xml:47
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'a pas de suite de tests."

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/markupsafe.xml:49
msgid "Install the package:"
msgstr "Installez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/markupsafe.xml:51
#, fuzzy, no-wrap
#| msgid "<userinput remap=\"install\">python3 setup.py install --optimize=1</userinput>"
msgid "<userinput remap=\"install\">pip3 install --no-index --find-links dist Markupsafe</userinput>"
msgstr "<userinput remap=\"install\">python3 setup.py install --optimize=1</userinput>"

