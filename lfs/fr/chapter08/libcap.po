#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter08/libcap.xml:44
msgid "Installation of Libcap"
msgstr "Installation de Libcap"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/libcap.xml:46
msgid "First, fix an upstream oversight in the latest release:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/libcap.xml:48
#, fuzzy, no-wrap
#| msgid "<userinput remap=\"pre\">sed -i '/install -m.*STA/d' libcap/Makefile</userinput>"
msgid "<userinput remap=\"pre\">sed -i 's/ $(PROGS)/&amp; capsh/' progs/Makefile</userinput>"
msgstr "<userinput remap=\"pre\">sed -i '/install -m.*STA/d' libcap/Makefile</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/libcap.xml:50
msgid "Prevent static libraries from being installed:"
msgstr "Évitez que des bibliothèques statiques ne soient installées&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/libcap.xml:52
#, no-wrap
msgid "<userinput remap=\"pre\">sed -i '/install -m.*STA/d' libcap/Makefile</userinput>"
msgstr "<userinput remap=\"pre\">sed -i '/install -m.*STA/d' libcap/Makefile</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/libcap.xml:54
msgid "Compile the package:"
msgstr "Compilez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/libcap.xml:56
#, no-wrap
msgid "<userinput remap=\"make\">make prefix=/usr lib=lib</userinput>"
msgstr "<userinput remap=\"make\">make prefix=/usr lib=lib</userinput>"

#. type: Content of: <sect1><sect2><variablelist><title>
#: lfs-en/chapter08/libcap.xml:59
msgid "The meaning of the make option:"
msgstr "Voici la signification de l'option make&nbsp;:"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/libcap.xml:62
msgid "<parameter>lib=lib</parameter>"
msgstr "<parameter>lib=lib</parameter>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/libcap.xml:64
msgid ""
"This parameter sets the library directory to <filename>/usr/lib</filename> "
"rather than <filename>/usr/lib64</filename> on x86_64. It has no effect on "
"x86."
msgstr ""
"Ce paramètre fait en sorte que la bibliothèque soit installée dans "
"<filename>/usr/lib</filename> plutôt que dans <filename>/usr/lib64</"
"filename> sur x86_64. Il n'a aucun effet sur x86."

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/libcap.xml:72
msgid "To test the results, issue:"
msgstr "Pour tester les résultats, lancez&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/libcap.xml:74
#, no-wrap
msgid "<userinput remap=\"test\">make test</userinput>"
msgstr "<userinput remap=\"test\">make test</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/libcap.xml:76
msgid "Install the package:"
msgstr "Installez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/libcap.xml:78
#, no-wrap
msgid "<userinput remap=\"install\">make prefix=/usr lib=lib install</userinput>"
msgstr "<userinput remap=\"install\">make prefix=/usr lib=lib install</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/libcap.xml:80
msgid "Adjust the permission of the shared libraries:"
msgstr "Ajustez les permissions des bibliothèques partagées&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/libcap.xml:82
#, no-wrap
msgid "<userinput remap=\"install\">chmod -v 755 /usr/lib/lib{cap,psx}.so.&libcap-version;</userinput>"
msgstr "<userinput remap=\"install\">chmod -v 755 /usr/lib/lib{cap,psx}.so.&libcap-version;</userinput>"

