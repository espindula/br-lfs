#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/man-db.xml:46
#, no-wrap
msgid ""
"<userinput remap=\"configure\">sed -i '/find/s@/usr@@' init/systemd/man-db.service.in\n"
"\n"
