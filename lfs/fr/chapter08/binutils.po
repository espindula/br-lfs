msgid "<userinput remap=\"install\">rm -fv /usr/lib/lib{bfd,ctf,ctf-nobfd,sframe,opcodes}.a</userinput>"
msgstr ""
"<userinput remap=\"install\">rm -fv /usr/lib/lib{bfd,ctf,ctf-nobfd,sframe,opcodes}.a\n"
