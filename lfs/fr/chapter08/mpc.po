#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter08/mpc.xml:43
msgid "Installation of MPC"
msgstr "Installation de MPC"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/mpc.xml:45
msgid "First, make a fix identified upstream:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/mpc.xml:47
#, no-wrap
msgid "<userinput remap=\"pre\">sed -i '273{ h; d; }; 274,286 { H; d; }; 287 G' src/mpc.h</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><note><para>
#: lfs-en/chapter08/mpc.xml:50
msgid ""
"This sed is a little complex and needs some explanation.  It holds the line "
"273 in a buffer, adds lines 274-286 to it. It then writes the buffer out "
"after line 287 is read. The result is that the line 287 is moved upwards "
"from an incorrect location to its correct location in an #ifdef block."
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/mpc.xml:57
msgid "Prepare MPC for compilation:"
msgstr "Préparez la compilation de MPC&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/mpc.xml:59
#, no-wrap
msgid ""
"<userinput remap=\"configure\">./configure --prefix=/usr    \\\n"
"            --disable-static \\\n"
"            --docdir=/usr/share/doc/mpc-&mpc-version;</userinput>"
msgstr ""
"<userinput remap=\"configure\">./configure --prefix=/usr    \\\n"
"            --disable-static \\\n"
"            --docdir=/usr/share/doc/mpc-&mpc-version;</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/mpc.xml:63
msgid "Compile the package and generate the HTML documentation:"
msgstr "Compilez le paquet et générez la documentation HTML&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/mpc.xml:65
#, no-wrap
msgid ""
"<userinput remap=\"make\">make\n"
"make html</userinput>"
msgstr ""
"<userinput remap=\"make\">make\n"
"make html</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/mpc.xml:68
msgid "To test the results, issue:"
msgstr "Pour tester les résultats, lancez&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/mpc.xml:70
#, no-wrap
msgid "<userinput remap=\"test\">make check</userinput>"
msgstr "<userinput remap=\"test\">make check</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/mpc.xml:72
msgid "Install the package and its documentation:"
msgstr "Installez le paquet ainsi que sa documentation&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/mpc.xml:74
#, no-wrap
msgid ""
"<userinput remap=\"install\">make install\n"
"make install-html</userinput>"
msgstr ""
"<userinput remap=\"install\">make install\n"
"make install-html</userinput>"

