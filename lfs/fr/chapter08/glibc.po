"(lien symbolique vers ld-linux-x86-64.so.2 ou ld-linux.so.2), locale, "
"localedef, makedb, mtrace, nscd, pcprofiledump, pldd, sln, sotruss, sprof, "
"tzselect, xtrace, zdump et zic"

#. type: Content of: <sect1><sect2><segmentedlist><seglistitem><seg>
#: lfs-en/chapter08/glibc.xml:489
msgid ""
"ld-linux-x86-64.so.2, ld-linux.so.2, libBrokenLocale.{a,so}, libanl.{a,so}, "
"libc.{a,so}, libc_nonshared.a, libc_malloc_debug.so, libcrypt.{a,so}, libdl."
"{a,so.2}, libg.a, libm.{a,so}, libmcheck.a, libmemusage.so, libmvec.{a,so}, "
"libnsl.so.1, libnss_compat.so, libnss_dns.so, libnss_files.so, libnss_hesiod."
"so, libpcprofile.so, libpthread.{a,so.0}, libresolv.{a,so}, librt.{a,so.1}, "
"libthread_db.so, and libutil.{a,so.1}"
msgstr ""
"ld-linux-x86-64.so.2, ld-linux.so.2, libBrokenLocale.{a,so}, libanl.{a,so}, "
"libc.{a,so}, libc_nonshared.a, libc_malloc_debug.so, libcrypt.{a,so}, libdl."
"{a,so.2}, libg.a, libm.{a,so}, libmcheck.a, libmemusage.so, libmvec.{a,so}, "
"libnsl.so.1, libnss_compat.so, libnss_dns.so, libnss_files.so, libnss_hesiod."
"so, libpcprofile.so, libpthread.{a,so.0}, libresolv.{a,so}, librt.{a,so.1}, "
"libthread_db.so et libutil.{a,so.1}"

#. type: Content of: <sect1><sect2><segmentedlist><seglistitem><seg>
