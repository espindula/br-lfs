#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter05/findutils.xml:44
msgid "Installation of Findutils"
msgstr "安装 Findutils"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter05/findutils.xml:52
msgid "Prepare Findutils for compilation:"
msgstr "准备编译 Findutils："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter05/findutils.xml:54
#, no-wrap
msgid "<userinput remap=\"configure\">./configure --prefix=/tools</userinput>"
msgstr "<userinput remap=\"configure\">./configure --prefix=/tools</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter05/findutils.xml:56
msgid "Compile the package:"
msgstr "编译该软件包："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter05/findutils.xml:58
#, no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr "<userinput remap=\"make\">make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter05/findutils.xml:60
msgid ""
"Compilation is now complete. As discussed earlier, running the test suite is "
"not mandatory for the temporary tools here in this chapter. To run the "
"Findutils test suite anyway, issue the following command:"
msgstr "现在编译已经完成，正如前文所述， 没有必要在本章中运行临时工具的测试套件。如果一定要运行 Findutils 的测试套件，执行以下命令："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter05/findutils.xml:64
#, no-wrap
msgid "<userinput remap=\"test\">make check</userinput>"
msgstr "<userinput remap=\"test\">make check</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter05/findutils.xml:66
msgid "Install the package:"
msgstr "安装该软件包："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter05/findutils.xml:68
#, no-wrap
msgid "<userinput remap=\"install\">make install</userinput>"
msgstr "<userinput remap=\"install\">make install</userinput>"

