#: lfs-en/chapter05/bison.xml:44
msgid "Installation of Bison"
msgstr "安装 Bison"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter05/bison.xml:46
msgid "Prepare Bison for compilation:"
msgstr "准备编译 Bison："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter05/bison.xml:48
#, no-wrap
msgid "<userinput remap=\"configure\">./configure --prefix=/tools</userinput>"
msgstr "<userinput remap=\"configure\">./configure --prefix=/tools</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter05/bison.xml:50
msgid "Compile the package:"
msgstr "编译该软件包："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter05/bison.xml:52
#, no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr "<userinput remap=\"make\">make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter05/bison.xml:54
msgid "To test the results, issue:"
msgstr "如果希望测试编译结果，运行："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter05/bison.xml:56
#, no-wrap
msgid "<userinput remap=\"test\">make check</userinput>"
msgstr "<userinput remap=\"test\">make check</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter05/bison.xml:58
msgid "Install the package:"
msgstr "安装该软件包："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter05/bison.xml:60
#, no-wrap
msgid "<userinput remap=\"install\">make install</userinput>"
msgstr "<userinput remap=\"install\">make install</userinput>"

