#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter06/mpc.xml:43
msgid "Installation of MPC"
msgstr "安装 MPC"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/mpc.xml:45
msgid "Prepare MPC for compilation:"
msgstr "准备编译 MPC："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/mpc.xml:47
#, no-wrap
msgid ""
"<userinput remap=\"configure\">./configure --prefix=/usr    \\\n"
"            --disable-static \\\n"
"            --docdir=/usr/share/doc/mpc-&mpc-version;</userinput>"
msgstr ""
"<userinput remap=\"configure\">./configure --prefix=/usr    \\\n"
"            --disable-static \\\n"
"            --docdir=/usr/share/doc/mpc-&mpc-version;</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/mpc.xml:51
msgid "Compile the package and generate the HTML documentation:"
msgstr "编译该软件包，并生成 HTML 文档："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/mpc.xml:53
#, no-wrap
msgid ""
"<userinput remap=\"make\">make\n"
"make html</userinput>"
msgstr ""
"<userinput remap=\"make\">make\n"
"make html</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/mpc.xml:56
msgid "To test the results, issue:"
msgstr "运行以下命令以测试编译结果："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/mpc.xml:58
#, no-wrap
msgid "<userinput remap=\"test\">make check</userinput>"
msgstr "<userinput remap=\"test\">make check</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/mpc.xml:60
msgid "Install the package and its documentation:"
msgstr "安装该软件包"

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/mpc.xml:62
#, no-wrap
msgid ""
"<userinput remap=\"install\">make install\n"
"make install-html</userinput>"
msgstr ""
"<userinput remap=\"install\">make install\n"
"make install-html</userinput>"

