#~ "./configure --prefix=/usr    \\\n"
#~ "            --with-shared    \\\n"
#~ "            --without-normal \\\n"
#~ "            --without-debug  \\\n"
#~ "            --without-cxx-binding \\\n"
#~ "            --with-abi-version=5 \n"
#~ "make sources libs\n"
#~ "cp -av lib/lib*.so.5* /usr/lib</userinput>"
#~ msgstr ""
#~ "<userinput>make distclean\n"
#~ "./configure --prefix=/usr    \\\n"
#~ "            --with-shared    \\\n"
#~ "            --without-normal \\\n"
#~ "            --without-debug  \\\n"
#~ "            --without-cxx-binding \\\n"
#~ "            --with-abi-version=5 \n"
#~ "make sources libs\n"
