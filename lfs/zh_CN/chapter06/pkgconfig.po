#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter06/pkgconfig.xml:43
msgid "Installation of Pkg-config"
msgstr "安装 Pkg-config"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/pkgconfig.xml:45
msgid "Prepare Pkg-config for compilation:"
msgstr "准备编译 Pkg-config："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/pkgconfig.xml:47
#, no-wrap
msgid ""
"<userinput remap=\"configure\">./configure --prefix=/usr              \\\n"
"            --with-internal-glib       \\\n"
"            --disable-host-tool        \\\n"
"            "
"--docdir=/usr/share/doc/pkg-config-&pkgconfig-version;</userinput>"
msgstr ""
"<userinput remap=\"configure\">./configure --prefix=/usr              \\\n"
"            --with-internal-glib       \\\n"
"            --disable-host-tool        \\\n"
"            "
"--docdir=/usr/share/doc/pkg-config-&pkgconfig-version;</userinput>"

#. type: Content of: <sect1><sect2><variablelist><title>
#: lfs-en/chapter06/pkgconfig.xml:53
msgid "The meaning of the new configure options:"
msgstr "新的配置选项的含义："

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter06/pkgconfig.xml:56
msgid "<parameter>--with-internal-glib</parameter>"
msgstr "<parameter>--with-internal-glib</parameter>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter06/pkgconfig.xml:58
msgid ""
"This will allow pkg-config to use its internal version of Glib because an "
"external version is not available in LFS."
msgstr "该选项允许 pkg-config 使用它内部的 Glib 版本，因为 LFS 不提供外部的 Glib。"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter06/pkgconfig.xml:64
msgid "<parameter>--disable-host-tool</parameter>"
msgstr "<parameter>--disable-host-tool</parameter>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter06/pkgconfig.xml:66
msgid ""
"This option disables the creation of an undesired hard link to the "
"pkg-config program."
msgstr "该选项防止创建一个指向 pkg-config 程序的不需要的硬链接。"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/pkgconfig.xml:73
msgid "Compile the package:"
msgstr "编译该软件包："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/pkgconfig.xml:75
#, no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr "<userinput remap=\"make\">make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/pkgconfig.xml:77
msgid "To test the results, issue:"
msgstr "运行以下命令以测试编译结果："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/pkgconfig.xml:79
#, no-wrap
msgid "<userinput remap=\"test\">make check</userinput>"
msgstr "<userinput remap=\"test\">make check</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter06/pkgconfig.xml:81
msgid "Install the package:"
msgstr "安装该软件包："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter06/pkgconfig.xml:83
#, no-wrap
msgid "<userinput remap=\"install\">make install</userinput>"
msgstr "<userinput remap=\"install\">make install</userinput>"

