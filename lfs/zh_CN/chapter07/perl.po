#| "<userinput remap=\"configure\">sh Configure -des                                        \\\n"
#| "             -Dprefix=/usr                               \\\n"
#| "             -Dvendorprefix=/usr                         \\\n"
#| "             -Dprivlib=/usr/lib/perl5/&perl-version-min;/core_perl     \\\n"
