#: lfs-en/chapter08/expect.xml:49
msgid "Installation of Expect"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/expect.xml:51
msgid "Prepare Expect for compilation:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/expect.xml:53
#, no-wrap
msgid ""
"<userinput remap=\"configure\">./configure --prefix=/usr           \\\n"
"            --with-tcl=/usr/lib     \\\n"
"            --enable-shared         \\\n"
"            --mandir=/usr/share/man \\\n"
"            --with-tclinclude=/usr/include</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><title>
#: lfs-en/chapter08/expect.xml:60
msgid "The meaning of the configure options:"
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/expect.xml:63
msgid "<parameter>--with-tcl=/usr/lib</parameter>"
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/expect.xml:65
msgid ""
"This parameter is needed to tell <command>configure</command> where the "
"<command>tclConfig.sh</command> script is located."
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/expect.xml:72
msgid "<parameter>--with-tclinclude=/usr/include</parameter>"
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/expect.xml:74
msgid "This explicitly tells Expect where to find Tcl's internal headers."
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/expect.xml:81
msgid "Build the package:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/expect.xml:83
#, no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/expect.xml:85
msgid "To test the results, issue:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/expect.xml:87
#, no-wrap
msgid "<userinput remap=\"test\">make test</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/expect.xml:89
msgid "Install the package:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/expect.xml:91
#, no-wrap
msgid ""
"<userinput remap=\"install\">make install\n"
"ln -svf expect&expect-version;/libexpect&expect-version;.so "
"/usr/lib</userinput>"
msgstr ""

