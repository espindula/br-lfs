#: lfs-en/chapter08/libpipeline.xml:42
msgid "Installation of Libpipeline"
msgstr "安装 Libpipeline"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/libpipeline.xml:44
msgid "Prepare Libpipeline for compilation:"
msgstr "准备编译 Libpipeline："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/libpipeline.xml:46
#, no-wrap
msgid "<userinput remap=\"configure\">./configure --prefix=/usr</userinput>"
msgstr "<userinput remap=\"configure\">./configure --prefix=/usr</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/libpipeline.xml:48
msgid "Compile the package:"
msgstr "编译该软件包："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/libpipeline.xml:50
#, no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr "<userinput remap=\"make\">make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/libpipeline.xml:52
msgid "To test the results, issue:"
msgstr "运行以下命令以测试编译结果："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/libpipeline.xml:54
#, no-wrap
msgid "<userinput remap=\"test\">make check</userinput>"
msgstr "<userinput remap=\"test\">make check</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/libpipeline.xml:56
msgid "Install the package:"
msgstr "安装该软件包："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/libpipeline.xml:58
#, no-wrap
msgid "<userinput remap=\"install\">make install</userinput>"
msgstr "<userinput remap=\"install\">make install</userinput>"

