#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter08/psmisc.xml:42
msgid "Installation of Psmisc"
msgstr "安装 Psmisc"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/psmisc.xml:44
msgid "Prepare Psmisc for compilation:"
msgstr "准备编译 Psmisc："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/psmisc.xml:46
#, no-wrap
msgid "<userinput remap=\"configure\">./configure --prefix=/usr</userinput>"
msgstr "<userinput remap=\"configure\">./configure --prefix=/usr</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/psmisc.xml:48
msgid "Compile the package:"
msgstr "编译该软件包："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/psmisc.xml:50
#, no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr "<userinput remap=\"make\">make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/psmisc.xml:52
msgid "This package does not come with a test suite."
msgstr "该软件包不包含测试套件。"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/psmisc.xml:54
msgid "Install the package:"
msgstr "安装该软件包："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/psmisc.xml:56
#, no-wrap
msgid "<userinput remap=\"install\">make install</userinput>"
msgstr "<userinput remap=\"install\">make install</userinput>"

#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter08/psmisc.xml:61
msgid "Contents of Psmisc"
msgstr "Psmisc 的内容"

#. type: Content of: <sect1><sect2><segmentedlist><segtitle>
#: lfs-en/chapter08/psmisc.xml:64
msgid "Installed programs"
msgstr "安装的程序"

#. type: Content of: <sect1><sect2><segmentedlist><seglistitem><seg>
#: lfs-en/chapter08/psmisc.xml:67
msgid ""
"fuser, killall, peekfd, prtstat, pslog, pstree, and pstree.x11 (link to "
"pstree)"
msgstr ""
"fuser, killall, peekfd, prtstat, pslog, pstree, 以及 pstree.x11 (到 pstree 的"
"链接)"

#. type: Content of: <sect1><sect2><variablelist><bridgehead>
#: lfs-en/chapter08/psmisc.xml:72
msgid "Short Descriptions"
msgstr "简要描述"

#. type: Content of: <sect1><sect2><variablelist>
#: lfs-en/chapter08/psmisc.xml:73
msgid ""
"<?dbfo list-presentation=\"list\"?> <?dbhtml list-presentation=\"table\"?>"
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/psmisc.xml:77
msgid "<command>fuser</command>"
msgstr "<command>fuser</command>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/psmisc.xml:79
msgid ""
"Reports the Process IDs (PIDs) of processes that use the given files or file "
"systems"
msgstr "报告使用给定文件或文件系统的进程 ID (PID)"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><indexterm><primary>
#: lfs-en/chapter08/psmisc.xml:82
msgid "fuser"
msgstr "fuser"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/psmisc.xml:88
msgid "<command>killall</command>"
msgstr "<command>killall</command>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/psmisc.xml:90
msgid ""
"Kills processes by name; it sends a signal to all processes running any of "
"the given commands"
msgstr "根据名称杀死进程；它向所有运行给定命令的进程发送信号"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><indexterm><primary>
#: lfs-en/chapter08/psmisc.xml:93
msgid "killall"
msgstr "killall"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/psmisc.xml:99
msgid "<command>peekfd</command>"
msgstr "<command>peekfd</command>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/psmisc.xml:101
msgid "Peek at file descriptors of a running process, given its PID"
msgstr "根据给定 PID，查看正在运行进程的文件描述符"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><indexterm><primary>
#: lfs-en/chapter08/psmisc.xml:104
msgid "peekfd"
msgstr "peekfd"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/psmisc.xml:110
msgid "<command>prtstat</command>"
msgstr "<command>prtstat</command>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/psmisc.xml:112
msgid "Prints information about a process"
msgstr "打印某个进程的信息"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><indexterm><primary>
#: lfs-en/chapter08/psmisc.xml:114
msgid "prtstat"
msgstr "prtstat"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/psmisc.xml:120
msgid "<command>pslog</command>"
msgstr "<command>pslog</command>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/psmisc.xml:122
msgid "Reports current logs path of a process"
msgstr "报告某个进程当前使用的日志路径"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><indexterm><primary>
#: lfs-en/chapter08/psmisc.xml:124
msgid "pslog"
msgstr "pslog"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/psmisc.xml:130
msgid "<command>pstree</command>"
msgstr "<command>pstree</command>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/psmisc.xml:132
msgid "Displays running processes as a tree"
msgstr "以树形格式列出正在运行的进程"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><indexterm><primary>
#: lfs-en/chapter08/psmisc.xml:134
msgid "pstree"
msgstr "pstree"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/psmisc.xml:140
msgid "<command>pstree.x11</command>"
msgstr "<command>pstree.x11</command>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/psmisc.xml:142
msgid ""
"Same as <command>pstree</command>, except that it waits for confirmation "
"before exiting"
msgstr "除了在退出前等待用户确认外，和 <command>pstree</command>相同"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><indexterm><primary>
#: lfs-en/chapter08/psmisc.xml:145
msgid "pstree.x11"
msgstr "pstree.x11"

