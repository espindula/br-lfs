#. type: Content of: <sect1><sect2><title>
#: lfs-en/chapter08/findutils.xml:44
msgid "Installation of Findutils"
msgstr "安装 Findutils"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/findutils.xml:46
msgid "Prepare Findutils for compilation:"
msgstr "准备编译 Findutils："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/findutils.xml:48
#, no-wrap
msgid ""
"<userinput remap=\"configure\">case $(uname -m) in\n"
"    i?86)   TIME_T_32_BIT_OK=yes ./configure --prefix=/usr --localstatedir=/var/lib/locate ;;\n"
"    x86_64) ./configure --prefix=/usr --localstatedir=/var/lib/locate ;;\n"
"esac</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><title>
#: lfs-en/chapter08/findutils.xml:54
msgid "The meaning of the configure options:"
msgstr "配置选项的含义："

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/findutils.xml:57
#, fuzzy
#| msgid "<command>xargs</command>"
msgid "<command>TIME_32_BIT_OK=yes</command>"
msgstr "<command>xargs</command>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/findutils.xml:59
msgid "This setting is needed for building on a 32 bit system."
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/findutils.xml:64
msgid "<parameter>--localstatedir</parameter>"
msgstr "<parameter>--localstatedir</parameter>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/findutils.xml:66
msgid ""
"This option changes the location of the <command>locate</command> database "
"to be in <filename class=\"directory\">/var/lib/locate</filename>, which is "
"FHS-compliant."
msgstr ""
"该选项将 <command>locate</command> 数据库的位置改为 <filename "
"class=\"directory\">/var/lib/locate</filename>，以与 FHS 兼容。"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/findutils.xml:74
msgid "Compile the package:"
msgstr "编译该软件包："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/findutils.xml:76
#, no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr "<userinput remap=\"make\">make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/findutils.xml:78
msgid "To test the results, issue:"
msgstr "运行以下命令以测试编译结果："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/findutils.xml:80
#, no-wrap
msgid ""
"<userinput remap=\"test\">chown -Rv tester .\n"
"su tester -c \"PATH=$PATH make check\"</userinput>"
msgstr ""
"<userinput remap=\"test\">chown -Rv tester .\n"
"su tester -c \"PATH=$PATH make check\"</userinput>"

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/findutils.xml:83
msgid "Install the package:"
msgstr "安装该软件包："

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/findutils.xml:85
#, no-wrap
msgid "<userinput remap=\"install\">make install</userinput>"
msgstr "<userinput remap=\"install\">make install</userinput>"

