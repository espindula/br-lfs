#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/zlib.xml:44
msgid "Prepare Zlib for compilation:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/zlib.xml:46
#, no-wrap
msgid "<userinput remap=\"configure\">./configure --prefix=/usr</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/zlib.xml:48
msgid "Compile the package:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/zlib.xml:50
#, no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/zlib.xml:52
msgid "To test the results, issue:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/zlib.xml:54
#, no-wrap
msgid "<userinput remap=\"test\">make check</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/zlib.xml:56
msgid "Install the package:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/zlib.xml:58
#, no-wrap
msgid "<userinput remap=\"install\">make install</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/zlib.xml:60
msgid ""
"The shared library needs to be moved to <filename "
"class=\"directory\">/lib</filename>, and as a result the <filename "
"class=\"extension\">.so</filename> file in <filename "
"class=\"directory\">/usr/lib</filename> will need to be recreated:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/zlib.xml:65
#, no-wrap
msgid ""
"<userinput remap=\"install\">mv -v /usr/lib/libz.so.* /lib\n"
"ln -sfv ../../lib/$(readlink /usr/lib/libz.so) /usr/lib/libz.so</userinput>"
msgstr ""

