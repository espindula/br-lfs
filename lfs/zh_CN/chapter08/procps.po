#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/procps.xml:43
msgid "Prepare procps-ng for compilation:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/procps.xml:45
#, no-wrap
msgid ""
"<userinput remap=\"configure\">./configure --prefix=/usr                            \\\n"
"            --exec-prefix=                           \\\n"
"            --libdir=/usr/lib                        \\\n"
"            --docdir=/usr/share/doc/procps-ng-&procps-ng-version; \\\n"
"            --disable-static                         \\\n"
"            --disable-kill</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/procps.xml:52
#, no-wrap
msgid ""
"<userinput remap=\"configure\">./configure --prefix=/usr                            \\\n"
"            --exec-prefix=                           \\\n"
"            --libdir=/usr/lib                        \\\n"
"            --docdir=/usr/share/doc/procps-ng-&procps-ng-version; \\\n"
"            --disable-static                         \\\n"
"            --disable-kill                           \\\n"
"            --with-systemd</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><title>
#: lfs-en/chapter08/procps.xml:61
msgid "The meaning of the configure option:"
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: lfs-en/chapter08/procps.xml:64
msgid "<parameter>--disable-kill</parameter>"
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: lfs-en/chapter08/procps.xml:66
msgid ""
"This switch disables building the <command>kill</command> command that will "
"be installed by the Util-linux package."
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/procps.xml:72
msgid "Compile the package:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/procps.xml:74
#, no-wrap
msgid "<userinput remap=\"make\">make</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/procps.xml:86
msgid "To run the test suite, run:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/procps.xml:88
#, no-wrap
msgid "<userinput remap=\"test\">make check</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/procps.xml:90
msgid "Install the package:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/procps.xml:92
#, no-wrap
msgid "<userinput remap=\"install\">make install</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: lfs-en/chapter08/procps.xml:94
msgid ""
"Finally, move essential libraries to a location that can be found if "
"<filename class=\"directory\">/usr</filename> is not mounted."
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: lfs-en/chapter08/procps.xml:97
#, no-wrap
msgid ""
"<userinput remap=\"install\">mv -v /usr/lib/libprocps.so.* /lib\n"
"ln -sfv ../../lib/$(readlink /usr/lib/libprocps.so) /usr/lib/libprocps.so</userinput>"
msgstr ""

