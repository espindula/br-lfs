      <segtitle>Bibliothèques installées</segtitle>

      <seglistitem>
        <seg>convert</seg>
        <seg>libee.[a,so]</seg>
      </seglistitem>
    </segmentedlist>

    <variablelist>
      <bridgehead renderas="sect3">Descriptions courtes</bridgehead>
      <?dbfo list-presentation="list"?>
