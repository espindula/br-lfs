<sect1 id="ch-cross-tools-file">
  <?dbhtml filename="file.html"?>

  <title>File-&file-version;</title>

  <indexterm zone="ch-cross-tools-file">
    <primary sortas="a-File">File</primary>
    <secondary>cross-tools</secondary>
  </indexterm>

  <xi:include xmlns:xi="http://www.w3.org/2001/XInclude"
  href="../../final-system/common/file.xml"
  xpointer="xpointer(//*[@role='package'])"/>

  <sect2 role="installation">
    <title>Installation de File</title>

    <para>Préparez la compilation de File&nbsp;:</para>

<screen><userinput>./configure --prefix=/cross-tools</userinput></screen>

    <para>Compilez le paquet&nbsp;:</para>

<screen><userinput>make</userinput></screen>

    <para>Installez le paquet&nbsp;:</para>

<screen><userinput>make install</userinput></screen>

  </sect2>

  <sect2 role="content">
    <title/>

    <para>Les détails de ce paquet sont situés dans
    <xref linkend="contents-file" role="."/></para>

  </sect2>

