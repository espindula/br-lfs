"<userinput>cat &gt; ${CLFS}/boot/default.colo &lt;&lt; \"EOF\"\n"
"#:CoLo:#\n"
"#\n"
"# load linux\n"
"#\n"
"lcd 'Booting &linux-version2;...'\n"
