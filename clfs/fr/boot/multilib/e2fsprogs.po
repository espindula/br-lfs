msgid "Installation of E2fsprogs"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/boot/multilib/e2fsprogs.xml:25
msgid ""
"Make sure the libraries get installed to <filename "
"class=\"directory\">/tools/lib64</filename>:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/boot/multilib/e2fsprogs.xml:28
#, no-wrap
msgid ""
"<userinput>cp -v configure{,.orig}\n"
"sed -e \"/libdir=.*\\/lib/s@/lib@/lib64@g\" configure.orig &gt; "
"configure</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/boot/multilib/e2fsprogs.xml:43
#, no-wrap
msgid ""
"<userinput>../configure \\\n"
"    --prefix=/tools \\\n"
"    --enable-elf-shlibs \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --disable-libblkid \\\n"
"    --disable-libuuid \\\n"
"    --disable-fsck \\\n"
"    --disable-uuidd</userinput>"
msgstr ""

