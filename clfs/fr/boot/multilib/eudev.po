msgid ""
"<userinput>./configure \\\n"
"    --prefix=/tools \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --libdir=/tools/lib64 \\\n"
"    --with-rootlibdir=/tools/lib64 \\\n"
"    --disable-introspection \\\n"
"    --disable-gtk-doc-html \\\n"
"    --disable-gudev \\\n"
"    --disable-keymap \\\n"
"    --with-firmware-path=/lib/firmware \\\n"
"    --enable-libkmod</userinput>"
