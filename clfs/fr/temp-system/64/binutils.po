#, fuzzy, no-wrap
msgid ""
"<userinput>../binutils-&binutils-dir;/configure \\\n"
"    --prefix=/tools \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --target=${CLFS_TARGET} \\\n"
"    --with-lib-path=/tools/lib \\\n"
"    --disable-nls \\\n"
"    --enable-shared \\\n"
"    --enable-64-bit-bfd \\\n"
"    --disable-multilib \\\n"
"    --enable-gold=yes \\\n"
"    --enable-plugins \\\n"
"    --with-system-zlib \\\n"
"    --enable-threads</userinput>"
msgstr ""
"<userinput>CC=\"${CC} ${BUILD64}\" ../binutils-&binutils-dir;/configure \\\n"
"    --prefix=/tools --build=${CLFS_HOST} --host=${CLFS_TARGET} \\\n"
"    --target=${CLFS_TARGET} --with-lib-path=/tools/lib --disable-nls \\\n"
"    --enable-shared --enable-64-bit-bfd --disable-multilib</userinput>"

