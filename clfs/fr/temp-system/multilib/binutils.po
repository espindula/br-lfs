#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/temp-system/multilib/binutils.xml:46
#, no-wrap
msgid ""
"<userinput>../binutils-&binutils-dir;/configure \\\n"
"    --prefix=/tools \\\n"
"    --libdir=/tools/lib64 \\\n"
"    --with-lib-path=/tools/lib64:/tools/lib \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --target=${CLFS_TARGET} \\\n"
"    --disable-nls \\\n"
"    --enable-shared \\\n"
"    --enable-64-bit-bfd \\\n"
"    --enable-gold=yes \\\n"
"    --enable-plugins \\\n"
"    --with-system-zlib \\\n"
"    --enable-threads</userinput>"
msgstr ""
"<userinput>../binutils-&binutils-dir;/configure \\\n"
"    --prefix=/tools \\\n"
"    --libdir=/tools/lib64 \\\n"
"    --with-lib-path=/tools/lib64:/tools/lib \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --target=${CLFS_TARGET} \\\n"
"    --disable-nls \\\n"
"    --enable-shared \\\n"
"    --enable-64-bit-bfd \\\n"
"    --enable-gold=yes \\\n"
"    --enable-plugins \\\n"
"    --with-system-zlib \\\n"
"    --enable-threads</userinput>"

