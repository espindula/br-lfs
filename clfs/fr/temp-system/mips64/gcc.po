#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/temp-system/mips64/gcc.xml:75
#, fuzzy, no-wrap
msgid ""
"<userinput>../gcc-&gcc-version;/configure \\\n"
"    --prefix=/tools \\\n"
"    --libdir=/tools/lib64 \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --target=${CLFS_TARGET} \\\n"
"    --with-local-prefix=/tools \\\n"
"    --enable-languages=c,c++ \\\n"
"    --with-abi=64 \\\n"
"    --with-system-zlib \\\n"
"    --with-native-system-header-dir=/tools/include \\\n"
"    --enable-install-libiberty</userinput>"
msgstr ""
"<userinput>CC=\"${CC} ${BUILD64}\" CXX=\"${CXX} ${BUILD64}\" \\\n"
"  ../gcc-&gcc-version;/configure --prefix=/tools \\\n"
"  --libdir=/tools/lib64 --build=${CLFS_HOST} --host=${CLFS_TARGET} \\\n"
"  --target=${CLFS_TARGET} --with-local-prefix=/tools --enable-long-long \\\n"
"  --enable-c99 --enable-shared --enable-threads=posix \\\n"
"  --enable-__cxa_atexit --disable-nls --enable-languages=c,c++ \\\n"
"  --disable-libstdcxx-pch --with-abi=64 --enable-cloog-backend=isl \\\n"
"  --with-gmp=/tools --with-mpfr=/tools --with-mpc=/tools --with-isl=/tools \\\n"
"  --disable-isl-version-check --with cloog=/tools --with-system-zlib \\\n"
"  --enable-checking=release --enable-libstdcxx-time \\\n"
"  --with-native-system-header-dir=/tools/include</userinput>"

