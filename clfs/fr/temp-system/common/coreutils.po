#. type: Content of: <sect1><title>
#: clfs-en/BOOK/temp-system/common/coreutils.xml:11
msgid "Coreutils-&coreutils-version;"
msgstr "Coreutils-&coreutils-version;"

#. type: Content of: <sect1><indexterm><primary>
#: clfs-en/BOOK/temp-system/common/coreutils.xml:14
msgid "Coreutils"
msgstr "Coreutils"

#. type: Content of: <sect1><indexterm><secondary>
#: clfs-en/BOOK/temp-system/common/coreutils.xml:15
msgid "temporary system"
msgstr "système temporaire"

#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/temp-system/common/coreutils.xml:23
msgid "Installation of Coreutils"
msgstr "Installation de Coreutils"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/temp-system/common/coreutils.xml:25
msgid "Prepare Coreutils for compilation:"
msgstr "Préparez la compilation de Coreutils&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/temp-system/common/coreutils.xml:27
#, no-wrap
msgid ""
"<userinput>./configure \\\n"
"    --prefix=/tools \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --enable-install-program=hostname \\\n"
"    --cache-file=config.cache</userinput>"
msgstr ""
"<userinput>./configure \\\n"
"    --prefix=/tools \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --enable-install-program=hostname \\\n"
"    --cache-file=config.cache</userinput>"

#. type: Content of: <sect1><sect2><variablelist><title>
#: clfs-en/BOOK/temp-system/common/coreutils.xml:35
msgid "The meaning of the new configure option:"
msgstr "Voici la signification de la nouvelle option de configure&nbsp;:"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: clfs-en/BOOK/temp-system/common/coreutils.xml:38
msgid "<parameter>--enable-install-program=hostname</parameter>"
msgstr "<parameter>--enable-install-program=hostname</parameter>"

#. type: Content of:
#. <sect1><sect2><variablelist><varlistentry><listitem><para>
#: clfs-en/BOOK/temp-system/common/coreutils.xml:40
msgid ""
"Tells Coreutils to install <command>hostname</command>, which is needed for "
"the Perl test suite."
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/temp-system/common/coreutils.xml:47
msgid "Adjust the Makefile so man pages aren't generated:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/temp-system/common/coreutils.xml:49
#, no-wrap
msgid "<userinput>sed -i -e 's/^man1_MANS/#man1_MANS/' Makefile</userinput>"
msgstr "<userinput>sed -i -e 's/^man1_MANS/#man1_MANS/' Makefile</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/temp-system/common/coreutils.xml:51
msgid "Compile the package:"
msgstr "Compilez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/temp-system/common/coreutils.xml:53
#, no-wrap
msgid "<userinput>make</userinput>"
msgstr "<userinput>make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/temp-system/common/coreutils.xml:55
msgid "Install the package:"
msgstr "Installez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/temp-system/common/coreutils.xml:57
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

