msgid "Installation of Findutils"
msgstr "Installation de Findutils"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/temp-system/common/findutils.xml:25
msgid ""
"The following cache entries set the values for tests that do not run while "
"cross-compiling:"
msgstr ""
"Les entrées de cache suivantes règlent les valeurs des tests qui ne se "
"lançaient pas lors de la compilation croisée&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/temp-system/common/findutils.xml:28
#, no-wrap
msgid ""
"<userinput>echo \"gl_cv_func_wcwidth_works=yes\" &gt; config.cache\n"
"echo \"ac_cv_func_fnmatch_gnu=yes\" &gt;&gt; config.cache</userinput>"
msgstr ""
"<userinput>echo \"gl_cv_func_wcwidth_works=yes\" &gt; config.cache\n"
"echo \"ac_cv_func_fnmatch_gnu=yes\" &gt;&gt; config.cache</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/temp-system/common/findutils.xml:35
#, no-wrap
msgid ""
"<userinput>./configure \\\n"
"    --prefix=/tools \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --cache-file=config.cache</userinput>"
msgstr ""
"<userinput>./configure \\\n"
"    --prefix=/tools \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --cache-file=config.cache</userinput>"

