msgid "Installation of Binutils"
msgstr "Installation de Binutils"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/temp-system/common/binutils.xml:47
#, no-wrap
msgid ""
"<userinput>../binutils-&binutils-dir;/configure \\\n"
"    --prefix=/tools \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --target=${CLFS_TARGET} \\\n"
"    --with-lib-path=/tools/lib \\\n"
"    --disable-nls \\\n"
"    --enable-shared \\\n"
"    --disable-multilib \\\n"
"    --enable-gold=yes \\\n"
"    --enable-plugins \\\n"
"    --with-system-zlib \\\n"
"    --enable-threads</userinput>"
msgstr ""
"<userinput>../binutils-&binutils-dir;/configure \\\n"
"    --prefix=/tools \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --target=${CLFS_TARGET} \\\n"
"    --with-lib-path=/tools/lib \\\n"
"    --disable-nls \\\n"
"    --enable-shared \\\n"
"    --disable-multilib \\\n"
"    --enable-gold=yes \\\n"
"    --enable-plugins \\\n"
"    --with-system-zlib \\\n"
"    --enable-threads</userinput>"

