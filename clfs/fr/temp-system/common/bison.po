#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/temp-system/common/bison.xml:25
#, fuzzy
msgid ""
"Apply a <command>sed</command> which disables the building of "
"<filename>bison.help</filename> when cross-compiling."
msgstr ""
"Appliquez un <command>sed</command> qui désactive la construction de "
"<filename>bison.help</filename> lors de la compilation croisée."

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/temp-system/common/bison.xml:27
#, no-wrap
msgid ""
"<userinput>cp -v Makefile.in{,.orig}\n"
"sed '/bison.help:/s/^/# /' Makefile.in.orig &gt; Makefile.in</userinput>"
msgstr ""
"<userinput>cp -v Makefile.in{,.orig}\n"
"sed '/bison.help:/s/^/# /' Makefile.in.orig &gt; Makefile.in</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/temp-system/common/bison.xml:34
#, no-wrap
msgid ""
"<userinput>M4=m4 \\\n"
"./configure \\\n"
"    --prefix=/tools \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --cache-file=config.cache</userinput>"
msgstr ""
"<userinput>M4=m4 \\\n"
"./configure \\\n"
"    --prefix=/tools \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --cache-file=config.cache</userinput>"

