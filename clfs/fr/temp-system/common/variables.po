#. type: Content of: <sect1><title>
#: clfs-en/BOOK/temp-system/common/variables.xml:11
msgid "Build Variables"
msgstr "Variables de construction"

#. type: Content of: <sect1><para>
#: clfs-en/BOOK/temp-system/common/variables.xml:13
msgid "Setup target-specific variables for the compiler and linkers:"
msgstr ""
"Initialisez les variables spécifiques à la cible pour le compilateur et les "
"éditeurs de liens&nbsp;:"

#. type: Content of: <sect1><screen>
#: clfs-en/BOOK/temp-system/common/variables.xml:16
#, no-wrap
msgid ""
"<userinput>export CC=\"${CLFS_TARGET}-gcc\"\n"
"export CXX=\"${CLFS_TARGET}-g++\"\n"
"export AR=\"${CLFS_TARGET}-ar\"\n"
"export AS=\"${CLFS_TARGET}-as\"\n"
"export RANLIB=\"${CLFS_TARGET}-ranlib\"\n"
"export LD=\"${CLFS_TARGET}-ld\"\n"
"export STRIP=\"${CLFS_TARGET}-strip\"</userinput>"
msgstr ""
"<userinput>export CC=\"${CLFS_TARGET}-gcc\"\n"
"export CXX=\"${CLFS_TARGET}-g++\"\n"
"export AR=\"${CLFS_TARGET}-ar\"\n"
"export AS=\"${CLFS_TARGET}-as\"\n"
"export RANLIB=\"${CLFS_TARGET}-ranlib\"\n"
"export LD=\"${CLFS_TARGET}-ld\"\n"
"export STRIP=\"${CLFS_TARGET}-strip\"</userinput>"

#. type: Content of: <sect1><para>
#: clfs-en/BOOK/temp-system/common/variables.xml:24
msgid ""
"Then add the build variables to <filename>~/.bashrc</filename> to prevent "
"issues if you stop and come back later:"
msgstr ""
"Puis ajoutez les variables de construction à <filename>~/.bashrc</filename> "
"pour éviter les problèmes si vous vous arrêtez et reprenez plus tard&nbsp;:"

#. type: Content of: <sect1><screen>
#: clfs-en/BOOK/temp-system/common/variables.xml:27
#, no-wrap
msgid ""
"<userinput>echo export CC=\\\"\"${CC}\\\"\" &gt;&gt; ~/.bashrc\n"
"echo export CXX=\\\"\"${CXX}\\\"\" &gt;&gt; ~/.bashrc\n"
"echo export AR=\\\"\"${AR}\\\"\" &gt;&gt; ~/.bashrc\n"
"echo export AS=\\\"\"${AS}\\\"\" &gt;&gt; ~/.bashrc\n"
"echo export RANLIB=\\\"\"${RANLIB}\\\"\" &gt;&gt; ~/.bashrc\n"
"echo export LD=\\\"\"${LD}\\\"\" &gt;&gt; ~/.bashrc\n"
"echo export STRIP=\\\"\"${STRIP}\\\"\" &gt;&gt; ~/.bashrc</userinput>"
msgstr ""
"<userinput>echo export CC=\\\"\"${CC}\\\"\" &gt;&gt; ~/.bashrc\n"
"echo export CXX=\\\"\"${CXX}\\\"\" &gt;&gt; ~/.bashrc\n"
"echo export AR=\\\"\"${AR}\\\"\" &gt;&gt; ~/.bashrc\n"
"echo export AS=\\\"\"${AS}\\\"\" &gt;&gt; ~/.bashrc\n"
"echo export RANLIB=\\\"\"${RANLIB}\\\"\" &gt;&gt; ~/.bashrc\n"
"echo export LD=\\\"\"${LD}\\\"\" &gt;&gt; ~/.bashrc\n"
"echo export STRIP=\\\"\"${STRIP}\\\"\" &gt;&gt; ~/.bashrc</userinput>"
