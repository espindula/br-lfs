msgid ""
"Configure can not properly determine the results of a few tests. Set them "
"manually:"
msgstr ""
"Configure ne peut déterminer le résultat de quelques tests.  Réglez-les "
"manuellement&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/./temp-system/common/tar.xml:28
#, fuzzy, no-wrap
msgid ""
"<userinput>cat &gt; config.cache &lt;&lt; EOF\n"
"gl_cv_func_wcwidth_works=yes\n"
"gl_cv_func_btowc_eof=yes\n"
"ac_cv_func_malloc_0_nonnull=yes\n"
"gl_cv_func_mbrtowc_incomplete_state=yes\n"
"gl_cv_func_mbrtowc_nul_retval=yes\n"
"gl_cv_func_mbrtowc_null_arg1=yes\n"
"gl_cv_func_mbrtowc_null_arg2=yes\n"
"gl_cv_func_mbrtowc_retval=yes\n"
"gl_cv_func_wcrtomb_retval=yes\n"
"EOF</userinput>"
msgstr ""
"<userinput>cat &gt; config.cache &lt;&lt; EOF\n"
"gl_cv_func_wcwidth_works=yes\n"
"gl_cv_func_btowc_eof=yes\n"
"ac_cv_func_malloc_0_nonnull=yes\n"
"ac_cv_func_realloc_0_nonnull=yes\n"
"gl_cv_func_mbrtowc_incomplete_state=yes\n"
"gl_cv_func_mbrtowc_nul_retval=yes\n"
"gl_cv_func_mbrtowc_null_arg=yes\n"
"gl_cv_func_mbrtowc_retval=yes\n"
"gl_cv_func_wcrtomb_retval=yes\n"
"EOF</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/./temp-system/common/tar.xml:44
#, fuzzy, no-wrap
msgid ""
"<userinput>./configure \\\n"
"    --prefix=/tools \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --cache-file=config.cache</userinput>"
msgstr ""
"<userinput>./configure --prefix=/tools \\\n"
"    --build=${CLFS_HOST} --host=${CLFS_TARGET} \\\n"
"    --cache-file=config.cache</userinput>"

