#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/temp-system/common/gcc.xml:55
#, no-wrap
msgid ""
"<userinput>cp -v gcc/Makefile.in{,.orig}\n"
"sed 's@\\./fixinc\\.sh@-c true@' gcc/Makefile.in.orig &gt; gcc/Makefile.in</userinput>"
msgstr ""
"<userinput>cp -v gcc/Makefile.in{,.orig}\n"
"sed 's@\\./fixinc\\.sh@-c true@' gcc/Makefile.in.orig &gt; gcc/Makefile.in</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/temp-system/common/gcc.xml:66
msgid ""
"Before starting to build GCC, remember to unset any environment variables "
"that override the default optimization flags."
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/temp-system/common/gcc.xml:73
#, no-wrap
msgid ""
"<userinput>../gcc-&gcc-version;/configure \\\n"
"    --prefix=/tools \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --target=${CLFS_TARGET} \\\n"
"    --with-local-prefix=/tools \\\n"
"    --disable-multilib \\\n"
"    --enable-languages=c,c++ \\\n"
"    --with-system-zlib \\\n"
"    --with-native-system-header-dir=/tools/include \\\n"
"    --disable-libssp \\\n"
"    --enable-install-libiberty</userinput>"
msgstr ""
"<userinput>../gcc-&gcc-version;/configure \\\n"
"    --prefix=/tools \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --target=${CLFS_TARGET} \\\n"
"    --with-local-prefix=/tools \\\n"
"    --disable-multilib \\\n"
"    --enable-languages=c,c++ \\\n"
"    --with-system-zlib \\\n"
"    --with-native-system-header-dir=/tools/include \\\n"
"    --disable-libssp \\\n"
"    --enable-install-libiberty</userinput>"

