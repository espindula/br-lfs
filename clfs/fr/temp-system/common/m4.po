msgstr "Installation de M4"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/temp-system/common/m4.xml:25
#, fuzzy
msgid ""
"Configure can not properly determine the results of the following tests:"
msgstr ""
"Configure ne peut pas déterminer correctement les résultats des tests "
"suivants&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/temp-system/common/m4.xml:28
#, no-wrap
msgid ""
"<userinput>cat &gt; config.cache &lt;&lt; EOF\n"
"gl_cv_func_btowc_eof=yes\n"
"gl_cv_func_mbrtowc_incomplete_state=yes\n"
"gl_cv_func_mbrtowc_sanitycheck=yes\n"
"gl_cv_func_mbrtowc_null_arg1=yes\n"
"gl_cv_func_mbrtowc_null_arg2=yes\n"
"gl_cv_func_mbrtowc_retval=yes\n"
"gl_cv_func_mbrtowc_nul_retval=yes\n"
"gl_cv_func_wcrtomb_retval=yes\n"
"gl_cv_func_wctob_works=yes\n"
"EOF</userinput>"
msgstr ""
"<userinput>cat &gt; config.cache &lt;&lt; EOF\n"
"gl_cv_func_btowc_eof=yes\n"
"gl_cv_func_mbrtowc_incomplete_state=yes\n"
"gl_cv_func_mbrtowc_sanitycheck=yes\n"
"gl_cv_func_mbrtowc_null_arg1=yes\n"
"gl_cv_func_mbrtowc_null_arg2=yes\n"
"gl_cv_func_mbrtowc_retval=yes\n"
"gl_cv_func_mbrtowc_nul_retval=yes\n"
"gl_cv_func_wcrtomb_retval=yes\n"
"gl_cv_func_wctob_works=yes\n"
"EOF</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/temp-system/common/m4.xml:40
msgid "Prepare M4 for compilation:"
msgstr "Préparez la compilation de M4&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/temp-system/common/m4.xml:42
#, no-wrap
msgid ""
"<userinput>./configure \\\n"
"    --prefix=/tools \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --cache-file=config.cache</userinput>"
msgstr ""
"<userinput>./configure \\\n"
"    --prefix=/tools \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --cache-file=config.cache</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/temp-system/common/m4.xml:48
msgid "Compile the package:"
msgstr "Compilez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/temp-system/common/m4.xml:50
#, no-wrap
msgid "<userinput>make</userinput>"
msgstr "<userinput>make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/temp-system/common/m4.xml:52
msgid "Install the package:"
msgstr "Installez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/temp-system/common/m4.xml:54
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

