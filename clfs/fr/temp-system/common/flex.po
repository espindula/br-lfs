#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/temp-system/common/flex.xml:25
msgid ""
"When cross compiling, the <command>configure</command> script does not "
"determine the correct values for the following.  Set the values manually:"
msgstr ""
"Lors de la compilation croisée, le script <command>configure</command> ne "
"détermine pas la bonne valeur pour ce qui suit. Réglez les valeurs "
"manuellement&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/temp-system/common/flex.xml:29
#, no-wrap
msgid ""
"<userinput>cat &gt; config.cache &lt;&lt; EOF\n"
"ac_cv_func_malloc_0_nonnull=yes\n"
"ac_cv_func_realloc_0_nonnull=yes\n"
"EOF</userinput>"
msgstr ""
"<userinput>cat &gt; config.cache &lt;&lt; EOF\n"
"ac_cv_func_malloc_0_nonnull=yes\n"
"ac_cv_func_realloc_0_nonnull=yes\n"
"EOF</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/temp-system/common/flex.xml:38
#, no-wrap
msgid ""
"<userinput>M4=m4 ./configure \\\n"
"    --prefix=/tools \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --cache-file=config.cache</userinput>"
msgstr ""
"<userinput>M4=m4 ./configure \\\n"
"    --prefix=/tools \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --cache-file=config.cache</userinput>"

