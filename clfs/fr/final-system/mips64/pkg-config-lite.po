#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/mips64/pkg-config-lite.xml:22
msgid "Installation of Pkg-config-lite"
msgstr "Installation de Pkg-config-lite"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/mips64/pkg-config-lite.xml:60
#, no-wrap
msgid ""
"<userinput>export PKG_CONFIG_PATH32=\"/usr/lib/pkgconfig\"\n"
"export PKG_CONFIG_PATHN32=\"/usr/lib32/pkgconfig\"\n"
"export PKG_CONFIG_PATH64=\"/usr/lib64/pkgconfig\"</userinput>"
msgstr ""
"<userinput>export PKG_CONFIG_PATH32=\"/usr/lib/pkgconfig\"\n"
"export PKG_CONFIG_PATHN32=\"/usr/lib32/pkgconfig\"\n"
"export PKG_CONFIG_PATH64=\"/usr/lib64/pkgconfig\"</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/mips64/pkg-config-lite.xml:68
#, no-wrap
msgid ""
"<userinput>cat &gt;&gt; ${CLFS}/root/.bash_profile &lt;&lt; EOF\n"
"export PKG_CONFIG_PATH32=\"${PKG_CONFIG_PATH32}\"\n"
"export PKG_CONFIG_PATHN32=\"${PKG_CONFIG_PATHN32}\"\n"
"export PKG_CONFIG_PATH64=\"${PKG_CONFIG_PATH64}\"\n"
"EOF</userinput>"
msgstr ""
"<userinput>cat &gt;&gt; ${CLFS}/root/.bash_profile &lt;&lt; EOF\n"
"export PKG_CONFIG_PATH32=\"${PKG_CONFIG_PATH32}\"\n"
"export PKG_CONFIG_PATHN32=\"${PKG_CONFIG_PATHN32}\"\n"
"export PKG_CONFIG_PATH64=\"${PKG_CONFIG_PATH64}\"\n"
"EOF</userinput>"
