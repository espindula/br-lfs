#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/mips64/glibc-n32.xml:22
msgid "Installation of Glibc"
msgstr "Installation de Glibc"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/mips64/glibc-n32.xml:28
#, no-wrap
msgid ""
"<userinput>LINKER=$(readelf -l /tools/bin/bash | sed -n 's@.*interpret.*/tools\\(.*\\)]$@\\1@p')\n"
"sed -i \"s|libs -o|libs -L/usr/lib32 -Wl,-dynamic-linker=${LINKER} -o|\" \\\n"
"  scripts/test-installation.pl\n"
"unset LINKER</userinput>"
msgstr ""
"<userinput>LINKER=$(readelf -l /tools/bin/bash | sed -n 's@.*interpret.*/tools\\(.*\\)]$@\\1@p')\n"
"sed -i \"s|libs -o|libs -L/usr/lib32 -Wl,-dynamic-linker=${LINKER} -o|\" \\\n"
"  scripts/test-installation.pl\n"
"unset LINKER</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/mips64/glibc-n32.xml:45
msgid ""
"Configure Glibc to install its 32-bit libraries into <filename "
"class=\"directory\">/lib32</filename>:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/mips64/glibc-n32.xml:48
#, no-wrap
msgid ""
"<userinput>echo \"libc_cv_slibdir=/lib32\" &gt;&gt; config.cache</userinput>"
msgstr ""
"<userinput>echo \"libc_cv_slibdir=/lib32\" &gt;&gt; config.cache</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/mips64/glibc-n32.xml:54
#, no-wrap
msgid ""
"<userinput>CC=\"gcc ${BUILDN32}\" CXX=\"g++ ${BUILDN32}\" \\\n"
"../glibc-&glibc-version;/configure \\\n"
"    --prefix=/usr \\\n"
"    --enable-kernel=&glibc-kernel-version; \\\n"
"    --libexecdir=/usr/lib32/glibc \\\n"
"    --libdir=/usr/lib32 \\\n"
"    --enable-obsolete-rpc \\\n"
"    --enable-stack-protector=strong \\\n"
"    --cache-file=config.cache</userinput>"
msgstr ""
"<userinput>CC=\"gcc ${BUILDN32}\" CXX=\"g++ ${BUILDN32}\" \\\n"
"../glibc-&glibc-version;/configure \\\n"
"    --prefix=/usr \\\n"
"    --enable-kernel=&glibc-kernel-version; \\\n"
"    --libexecdir=/usr/lib32/glibc \\\n"
"    --libdir=/usr/lib32 \\\n"
"    --enable-obsolete-rpc \\\n"
"    --enable-stack-protector=strong \\\n"
"    --cache-file=config.cache</userinput>"

