msgid "Adjusting the Toolchain"
msgstr ""

#. type: Content of: <sect1><screen>
#: clfs-en/BOOK/final-system/mips64-64/adjusting.xml:17
#, no-wrap
msgid ""
"<userinput>gcc -dumpspecs | \\\n"
"perl -p -e 's@/tools/lib/ld@/lib/ld@g;' \\\n"
"     -e 's@/tools/lib32/ld@/lib32/ld@g;' \\\n"
"     -e 's@/tools/lib64/ld@/lib64/ld@g;' \\\n"
"     -e 's@\\*startfile_prefix_spec:\\n@$_/usr/lib/ @g;' &gt; \\\n"
"     $(dirname $(gcc --print-libgcc-file-name))/specs</userinput>"
msgstr ""

