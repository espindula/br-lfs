#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/64/binutils.xml:65
#, no-wrap
msgid ""
"<userinput>CC=\"gcc -isystem /usr/include\" \\\n"
"LDFLAGS=\"-Wl,-rpath-link,/usr/lib:/lib\" \\\n"
"../binutils-&binutils-dir;/configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/usr/lib \\\n"
"    --enable-shared \\\n"
"    --disable-multilib \\\n"
"    --enable-64-bit-bfd \\\n"
"    --enable-gold=yes \\\n"
"    --enable-plugins \\\n"
"    --with-system-zlib \\\n"
"    --enable-threads</userinput>"
msgstr ""
