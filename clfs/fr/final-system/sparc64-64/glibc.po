#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/sparc64-64/glibc.xml:76
#, no-wrap
msgid ""
"<userinput>CC=\"gcc ${BUILD64}\" \\\n"
"../glibc-&glibc-version;/configure \\\n"
"    --prefix=/usr \\\n"
"    --enable-kernel=&glibc-kernel-version; \\\n"
"    --libexecdir=/usr/lib/glibc \\\n"
"    --libdir=/usr/lib \\\n"
"    --enable-obsolete-rpc \\\n"
"    --enable-stack-protector=strong \\\n"
"    --cache-file=config.cache</userinput>"
msgstr ""
"<userinput>CC=\"gcc ${BUILD64}\" \\\n"
