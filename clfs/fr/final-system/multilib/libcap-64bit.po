msgid "Libcap-&libcap-version; 64 Bit"
msgstr "Libcap-&libcap-version; 64 Bit"

#. type: Content of: <sect1><indexterm><primary>
#: clfs-en/BOOK/final-system/multilib/libcap-64bit.xml:14
msgid "Libcap"
msgstr "Libcap"

#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/multilib/libcap-64bit.xml:22
msgid "Installation of Libcap"
msgstr "Installation de Libcap"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/libcap-64bit.xml:28
#, no-wrap
msgid "<userinput>make CC=\"gcc ${BUILD64}\"</userinput>"
msgstr "<userinput>make CC=\"gcc ${BUILD64}\"</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/libcap-64bit.xml:38
#, no-wrap
msgid ""
"<userinput>make lib=lib64 install\n"
"chmod -v 755 /lib64/libcap.so.&libcap-version;\n"
"ln -sfv ../../lib64/$(readlink /lib64/libcap.so) /usr/lib64/libcap.so\n"
"rm -v /lib64/libcap.so\n"
"mv -v /lib64/libcap.a /usr/lib64</userinput>"
msgstr ""
"<userinput>make lib=lib64 install\n"
"chmod -v 755 /lib64/libcap.so.&libcap-version;\n"
"ln -sfv ../../lib64/$(readlink /lib64/libcap.so) /usr/lib64/libcap.so\n"
"rm -v /lib64/libcap.so\n"
"mv -v /lib64/libcap.a /usr/lib64</userinput>"
