#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/multilib/procps-ng-n32.xml:23
msgid "Installation of Procps-ng"
msgstr "Installation de Procps-ng"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/procps-ng-n32.xml:29
#, no-wrap
msgid ""
"<userinput>CC=\"gcc ${BUILDN32}\" ./configure \\\n"
"    --prefix=/usr \\\n"
"    --exec-prefix= \\\n"
"    --libdir=/usr/lib32 \\\n"
"    --docdir=/usr/share/doc/procps-ng-&procps-ng-version; \\\n"
"    --disable-kill</userinput>"
msgstr ""
"<userinput>CC=\"gcc ${BUILDN32}\" ./configure \\\n"
"    --prefix=/usr \\\n"
"    --exec-prefix= \\\n"
"    --libdir=/usr/lib32 \\\n"
"    --docdir=/usr/share/doc/procps-ng-&procps-ng-version; \\\n"
"    --disable-kill</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/procps-ng-n32.xml:68
#, no-wrap
msgid ""
"<userinput>mv -v /usr/lib32/libprocps.so.* /lib32\n"
"ln -sfv ../../lib32/$(readlink /usr/lib32/libprocps.so) /usr/lib32/libprocps.so</userinput>"
msgstr ""
"<userinput>mv -v /usr/lib32/libprocps.so.* /lib32\n"
"ln -sfv ../../lib32/$(readlink /usr/lib32/libprocps.so) /usr/lib32/libprocps.so</userinput>"

