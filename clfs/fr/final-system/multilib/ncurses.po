#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/ncurses.xml:39
#, fuzzy, no-wrap
msgid ""
"<userinput>CC=\"gcc ${BUILD32}\" CXX=\"g++ ${BUILD32}\" \\\n"
"./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/usr/lib \\\n"
"    --with-shared \\\n"
"    --without-debug \\\n"
"    --enable-widec \\\n"
"    --enable-pc-files</userinput>"
msgstr ""
"<userinput>CC=\"gcc ${BUILD32}\" CXX=\"g++ ${BUILD32}\" \\\n"
"   ./configure --prefix=/usr --libdir=/lib \\\n"
"   --with-shared --without-debug --enable-widec \\\n"
"   --with-manpage-format=normal \\\n"
"   --with-default-terminfo-dir=/usr/share/terminfo</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/multilib/ncurses.xml:72
#, fuzzy
msgid ""
"Prepare <filename>ncursesw6-config</filename> to be wrapped by the multiarch "
"wrapper:"
msgstr ""
"Préparez <filename>ncursesw5-config</filename> à être enballé par le "
"multiarch wrapper&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/ncurses.xml:75
#, fuzzy, no-wrap
msgid "<userinput>mv -v /usr/bin/ncursesw6-config{,-32}</userinput>"
msgstr "<userinput>mv -v /usr/bin/ncursesw5-config{,-32}</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/ncurses.xml:89
#, fuzzy, no-wrap
msgid ""
"<userinput>for lib in ncurses form panel menu ; do\n"
"        echo \"INPUT(-l${lib}w)\" &gt; /usr/lib/lib${lib}.so\n"
"        ln -sfv lib${lib}w.a /usr/lib/lib${lib}.a\n"
"done\n"
"ln -sfv libncurses++w.a /usr/lib/libncurses++.a\n"
"ln -sfv ncursesw6-config-32 /usr/bin/ncurses6-config-32</userinput>"
msgstr ""
"<userinput>for lib in curses ncurses form panel menu ; do\n"
"        echo \"INPUT(-l${lib}w)\" > /usr/lib/lib${lib}.so\n"
"        ln -sfv lib${lib}w.a /usr/lib/lib${lib}.a\n"
"done\n"
"ln -sfv libcurses.so /usr/lib/libcursesw.so\n"
"ln -sfv libncurses.so /usr/lib/libcurses.so\n"
"ln -sfv libncursesw.a /usr/lib/libcursesw.a\n"
"ln -sfv libncurses.a /usr/lib/libcurses.a\n"
"ln -sfv libncurses++w.a /usr/lib/libncurses++.a\n"
"ln -sfv ncursesw5-config-32 /usr/bin/ncurses5-config-32</userinput>"

