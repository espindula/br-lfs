#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/acl.xml:45
#, no-wrap
msgid ""
"<userinput>CC=\"gcc ${BUILD32}\" \\\n"
"./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/lib \\\n"
"    --libexecdir=/usr/lib</userinput>"
msgstr ""
"<userinput>CC=\"gcc ${BUILD32}\" \\\n"
"./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/lib \\\n"
"    --libexecdir=/usr/lib</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/multilib/acl.xml:75
msgid ""
"Recreate the symlink in <filename class=\"directory\">/usr/lib </filename> "
"and remove the shared link from <filename class=\"directory\"> "
"/lib</filename>:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/acl.xml:79
#, no-wrap
msgid ""
"<userinput>ln -sfv ../../lib/$(readlink /lib/libacl.so) /usr/lib/libacl.so\n"
"rm -v /lib/libacl.so</userinput>"
msgstr ""
"<userinput>ln -sfv ../../lib/$(readlink /lib/libacl.so) /usr/lib/libacl.so\n"
"rm -v /lib/libacl.so</userinput>"

