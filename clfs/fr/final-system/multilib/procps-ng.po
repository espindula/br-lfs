#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/multilib/procps-ng.xml:23
msgid "Installation of Procps-ng"
msgstr "Installation de Procps-ng"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/procps-ng.xml:29
#, no-wrap
msgid ""
"<userinput>CC=\"gcc ${BUILD32}\" ./configure \\\n"
"    --prefix=/usr \\\n"
"    --exec-prefix= \\\n"
"    --libdir=/usr/lib \\\n"
"    --docdir=/usr/share/doc/procps-ng-&procps-ng-version; \\\n"
"    --disable-kill</userinput>"
msgstr ""
"<userinput>CC=\"gcc ${BUILD32}\" ./configure \\\n"
"    --prefix=/usr \\\n"
"    --exec-prefix= \\\n"
"    --libdir=/usr/lib \\\n"
"    --docdir=/usr/share/doc/procps-ng-&procps-ng-version; \\\n"
"    --disable-kill</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/procps-ng.xml:72
#, no-wrap
msgid ""
"<userinput>mv -v /usr/lib/libprocps.so.* /lib\n"
"ln -sfv ../../lib/$(readlink /usr/lib/libprocps.so) /usr/lib/libprocps.so</userinput>"
msgstr ""
"<userinput>mv -v /usr/lib/libprocps.so.* /lib\n"
"ln -sfv ../../lib/$(readlink /usr/lib/libprocps.so) /usr/lib/libprocps.so</userinput>"

