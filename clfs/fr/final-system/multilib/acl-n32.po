#. type: Content of: <sect1><title>
#: clfs-en/BOOK/final-system/multilib/acl-n32.xml:11
msgid "Acl-&acl-version; N32 Libraries"
msgstr "Bibliothèques Acl-&acl-version; N32"

#. type: Content of: <sect1><indexterm><primary>
#: clfs-en/BOOK/final-system/multilib/acl-n32.xml:14
msgid "Acl"
msgstr "Acl"

#. type: Content of: <sect1><indexterm><secondary>
#: clfs-en/BOOK/final-system/multilib/acl-n32.xml:15
msgid "N32"
msgstr "N32"

#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/multilib/acl-n32.xml:23
msgid "Installation of Acl"
msgstr "Installation de Acl"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/acl-n32.xml:45
#, no-wrap
msgid ""
"<userinput>CC=\"gcc ${BUILDN32}\" \\\n"
"./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/lib32 \\\n"
"    --libexecdir=/usr/lib32</userinput>"
msgstr ""
"<userinput>CC=\"gcc ${BUILDN32}\" \\\n"
"./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/lib32 \\\n"
"    --libexecdir=/usr/lib32</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/multilib/acl-n32.xml:75
msgid ""
"Recreate the symlink in <filename class=\"directory\">/usr/lib32 </filename>"
" and remove the shared link from <filename class=\"directory\"> "
"/lib32</filename>:"
msgstr ""
"Recréez le lien symbolique dans <filename class=\"directory\""
">/usr/lib32</filename> et supprimez le lien partagé depuis <filename class=\""
"directory\">/lib32</filename>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/acl-n32.xml:79
#, no-wrap
msgid ""
"<userinput>ln -sfv ../../lib32/$(readlink /lib32/libacl.so) /usr/lib32/libacl.so\n"
"rm -v /lib32/libacl.so</userinput>"
msgstr ""
"<userinput>ln -sfv ../../lib32/$(readlink /lib32/libacl.so) /usr/lib32/libacl.so\n"
"rm -v /lib32/libacl.so</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/acl-n32.xml:86
#, no-wrap
msgid "<userinput>chmod 755 -v /lib32/libacl.so.1.1.0</userinput>"
msgstr "<userinput>chmod 755 -v /lib32/libacl.so.1.1.0</userinput>"

