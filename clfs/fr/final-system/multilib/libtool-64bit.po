msgid "Libtool"
msgstr "Libtool"

#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/multilib/libtool-64bit.xml:22
msgid "Installation of Libtool"
msgstr "Installation de Libtool"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/libtool-64bit.xml:28
#, no-wrap
msgid ""
"<userinput>echo \"lt_cv_sys_dlsearch_path='/lib64 /usr/lib64 "
"/usr/local/lib64 /opt/lib64'\" &gt; config.cache</userinput>"
msgstr ""
"<userinput>echo \"lt_cv_sys_dlsearch_path='/lib64 /usr/lib64 "
"/usr/local/lib64 /opt/lib64'\" &gt; config.cache</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/libtool-64bit.xml:34
#, no-wrap
msgid ""
"<userinput>CC=\"gcc ${BUILD64}\" ./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/usr/lib64 \\\n"
"    --cache-file=config.cache</userinput>"
msgstr ""
"<userinput>CC=\"gcc ${BUILD64}\" ./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/usr/lib64 \\\n"
"    --cache-file=config.cache</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/libtool-64bit.xml:67
#, no-wrap
msgid ""
"<userinput>mv -v /usr/bin/libtool{,-64}\n"
"ln -sv multiarch_wrapper /usr/bin/libtool</userinput>"
msgstr ""
"<userinput>mv -v /usr/bin/libtool{,-64}\n"
"ln -sv multiarch_wrapper /usr/bin/libtool</userinput>"
