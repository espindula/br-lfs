#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/zlib-n32.xml:29
#, fuzzy, no-wrap
msgid ""
"<userinput>CC=\"gcc -isystem /usr/include ${BUILDN32}\" \\\n"
"CXX=\"g++ -isystem /usr/include ${BUILDN32}\" \\\n"
"LDFLAGS=\"-Wl,-rpath-link,/usr/lib32:/lib32 ${BUILDN32}\" \\\n"
"./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/usr/lib32</userinput>"
msgstr ""
"<userinput>CC=\"gcc -isystem /usr/include ${BUILDN32}\" \\\n"
"CXX=\"g++ -isystem /usr/include ${BUILDN32}\" \\\n"
"LDFLAGS=\"-Wl,-rpath-link,/usr/lib32:/lib32 ${BUILDN32}\" \\\n"
" ./configure --prefix=/usr --libdir=/usr/lib32</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/multilib/zlib-n32.xml:60
#, fuzzy
msgid ""
"The previous command installed two <filename class=\"extension\">.so</"
"filename> files into <filename class=\"directory\">/usr/lib32</filename>. We "
"will move them into <filename class=\"directory\">/lib32</filename> and then "
"recreate a link in <filename class=\"directory\">/usr/lib32</filename>:"
msgstr ""
"The previous command installed two <filename class=\"extension\">.so</"
"filename> files into <filename class=\"directory\">/usr/lib32</filename>. We "
"will remove itinto <filename class=\"directory\">/lib32</filename> and then "
"relink it to <filename class=\"directory\">/usr/lib32</filename>:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/zlib-n32.xml:66
#, fuzzy, no-wrap
msgid ""
"<userinput>mv -v /usr/lib32/libz.so.* /lib32\n"
"ln -sfv ../../lib32/$(readlink /usr/lib32/libz.so) /usr/lib32/libz.so</userinput>"
msgstr ""
"<userinput>mv -v /usr/lib32/libz.so.* /lib32\n"
"ln -svf ../../lib32/libz.so.1 /usr/lib32/libz.so</userinput>"

