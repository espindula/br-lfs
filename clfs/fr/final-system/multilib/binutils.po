#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/binutils.xml:65
#, no-wrap
msgid ""
"<userinput>CC=\"gcc -isystem /usr/include ${BUILD64}\" \\\n"
"LDFLAGS=\"-Wl,-rpath-link,/usr/lib64:/lib64:/usr/lib:/lib ${BUILD64}\" \\\n"
"../binutils-&binutils-dir;/configure \\\n"
"    --prefix=/usr \\\n"
"    --enable-shared \\\n"
"    --enable-64-bit-bfd \\\n"
"    --libdir=/usr/lib64 \\\n"
"    --enable-gold=yes \\\n"
"    --enable-plugins \\\n"
"    --with-system-zlib \\\n"
"    --enable-threads</userinput>"
msgstr ""
