#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/multilib/libcap.xml:23
msgid "Installation of Libcap"
msgstr "Installation de Libcap"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/libcap.xml:29
#, no-wrap
msgid "<userinput>make CC=\"gcc ${BUILD32}\"</userinput>"
msgstr "<userinput>make CC=\"gcc ${BUILD32}\"</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/libcap.xml:39
#, no-wrap
msgid ""
"<userinput>make RAISE_SETFCAP=no lib=lib install\n"
"chmod -v 755 /lib/libcap.so.&libcap-version;\n"
"ln -sfv ../../lib/$(readlink /lib/libcap.so) /usr/lib/libcap.so\n"
"rm -v /lib/libcap.so\n"
"mv -v /lib/libcap.a /usr/lib</userinput>"
msgstr ""
"<userinput>make RAISE_SETFCAP=no lib=lib install\n"
"chmod -v 755 /lib/libcap.so.&libcap-version;\n"
"ln -sfv ../../lib/$(readlink /lib/libcap.so) /usr/lib/libcap.so\n"
"rm -v /lib/libcap.so\n"
"mv -v /lib/libcap.a /usr/lib</userinput>"

