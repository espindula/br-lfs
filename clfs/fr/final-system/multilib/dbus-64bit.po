#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/multilib/dbus-64bit.xml:22
msgid "Installation of D-Bus"
msgstr "Installation de D-Bus"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/dbus-64bit.xml:28
#, no-wrap
msgid ""
"<userinput>CC=\"gcc ${BUILD64}\" USE_ARCH=64 PKG_CONFIG_PATH=${PKG_CONFIG_PATH64} \\\n"
"./configure \\\n"
"    --prefix=/usr \\\n"
"    --sysconfdir=/etc \\\n"
"    --libdir=/usr/lib64 \\\n"
"    --libexecdir=/usr/lib64/dbus-1.0 \\\n"
"    --localstatedir=/var \\\n"
"    --with-systemdsystemunitdir=/lib/systemd/system \\\n"
"    --docdir=/usr/share/doc/dbus-&dbus-version; \\\n"
"    SYSTEMD_LIBS=\"-L/lib64 -lsystemd\"</userinput>"
msgstr ""
"<userinput>CC=\"gcc ${BUILD64}\" USE_ARCH=64 PKG_CONFIG_PATH=${PKG_CONFIG_PATH64} \\\n"
"./configure \\\n"
"    --prefix=/usr \\\n"
"    --sysconfdir=/etc \\\n"
"    --libdir=/usr/lib64 \\\n"
"    --libexecdir=/usr/lib64/dbus-1.0 \\\n"
"    --localstatedir=/var \\\n"
"    --with-systemdsystemunitdir=/lib/systemd/system \\\n"
"    --docdir=/usr/share/doc/dbus-&dbus-version; \\\n"
"    SYSTEMD_LIBS=\"-L/lib64 -lsystemd\"</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/multilib/dbus-64bit.xml:59
msgid ""
"Move the shared library to <filename class=\"directory\">/lib64 </filename> "
"and recreate the symbolic link."
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/dbus-64bit.xml:62
#, no-wrap
msgid ""
"<userinput>mv -v /usr/lib64/libdbus-1.so.* /lib64\n"
"ln -sfv ../../lib64/$(readlink /usr/lib64/libdbus-1.so) /usr/lib64/libdbus-1.so\n"
"</userinput>"
msgstr ""
"<userinput>mv -v /usr/lib64/libdbus-1.so.* /lib64\n"
"ln -sfv ../../lib64/$(readlink /usr/lib64/libdbus-1.so) /usr/lib64/libdbus-1.so\n"
"</userinput>"
