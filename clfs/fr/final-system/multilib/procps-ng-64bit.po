#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/multilib/procps-ng-64bit.xml:22
msgid "Installation of Procps-ng"
msgstr "Installation de Procps-ng"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/procps-ng-64bit.xml:28
#, no-wrap
msgid ""
"<userinput>CC=\"gcc ${BUILD64}\" ./configure \\\n"
"    --prefix=/usr \\\n"
"    --exec-prefix= \\\n"
"    --libdir=/usr/lib64 \\\n"
"    --docdir=/usr/share/doc/procps-ng-&procps-ng-version; \\\n"
"    --disable-kill</userinput>"
msgstr ""
"<userinput>CC=\"gcc ${BUILD64}\" ./configure \\\n"
"    --prefix=/usr \\\n"
"    --exec-prefix= \\\n"
"    --libdir=/usr/lib64 \\\n"
"    --docdir=/usr/share/doc/procps-ng-&procps-ng-version; \\\n"
"    --disable-kill</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/procps-ng-64bit.xml:67
#, no-wrap
msgid ""
"<userinput>mv -v /usr/lib64/libprocps.so.* /lib64\n"
"ln -sfv ../../lib64/$(readlink /usr/lib64/libprocps.so) /usr/lib64/libprocps.so</userinput>"
msgstr ""
"<userinput>mv -v /usr/lib64/libprocps.so.* /lib64\n"
"ln -sfv ../../lib64/$(readlink /usr/lib64/libprocps.so) /usr/lib64/libprocps.so</userinput>"
