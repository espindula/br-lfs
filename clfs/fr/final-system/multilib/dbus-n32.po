msgid "Installation of D-Bus"
msgstr "Installation de D-Bus"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/dbus-n32.xml:28
#, no-wrap
msgid ""
"<userinput>CC=\"gcc ${BUILDN32}\" USE_ARCH=n32 PKG_CONFIG_PATH=${PKG_CONFIG_PATHN32} \\\n"
"./configure \\\n"
"    --prefix=/usr \\\n"
"    --sysconfdir=/etc \\\n"
"    --libdir=/usr/lib32 \\\n"
"    --libexecdir=/usr/lib32/dbus-1.0 \\\n"
"    --localstatedir=/var \\\n"
"    --with-systemdsystemunitdir=/lib/systemd/system \\\n"
"    --docdir=/usr/share/doc/dbus-&dbus-version; \\\n"
"    SYSTEMD_LIBS=\"-L/lib32 -lsystemd\"</userinput>"
msgstr ""
"<userinput>CC=\"gcc ${BUILDN32}\" USE_ARCH=n32 PKG_CONFIG_PATH=${PKG_CONFIG_PATHN32} \\\n"
"./configure \\\n"
"    --prefix=/usr \\\n"
"    --sysconfdir=/etc \\\n"
"    --libdir=/usr/lib32 \\\n"
"    --libexecdir=/usr/lib32/dbus-1.0 \\\n"
"    --localstatedir=/var \\\n"
"    --with-systemdsystemunitdir=/lib/systemd/system \\\n"
"    --docdir=/usr/share/doc/dbus-&dbus-version; \\\n"
"    SYSTEMD_LIBS=\"-L/lib32 -lsystemd\"</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/multilib/dbus-n32.xml:59
msgid ""
"Move the shared library to <filename class=\"directory\">/lib32 </filename> "
"and recreate the symbolic link."
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/dbus-n32.xml:62
#, no-wrap
msgid ""
"<userinput>mv -v /usr/lib32/libdbus-1.so.* /lib32\n"
"ln -sfv ../../lib32/$(readlink /usr/lib32/libdbus-1.so) /usr/lib32/libdbus-1.so\n"
"</userinput>"
msgstr ""
"<userinput>mv -v /usr/lib32/libdbus-1.so.* /lib32\n"
"ln -sfv ../../lib32/$(readlink /usr/lib32/libdbus-1.so) /usr/lib32/libdbus-1.so\n"
"</userinput>"

