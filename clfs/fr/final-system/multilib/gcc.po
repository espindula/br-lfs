#. type: Content of: <sect1><title>
#: clfs-en/BOOK/final-system/multilib/gcc.xml:11
msgid "GCC-&gcc-version;"
msgstr "GCC-&gcc-version;"

#. type: Content of: <sect1><indexterm><primary>
#: clfs-en/BOOK/final-system/multilib/gcc.xml:14
msgid "GCC"
msgstr "GCC"

#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/multilib/gcc.xml:22
msgid "Installation of GCC"
msgstr "Installation de GCC"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/gcc.xml:54
#, no-wrap
msgid ""
"<userinput>SED=sed CC=\"gcc -isystem /usr/include ${BUILD64}\" \\\n"
"CXX=\"g++ -isystem /usr/include ${BUILD64}\" \\\n"
"LDFLAGS=\"-Wl,-rpath-link,/usr/lib64:/lib64:/usr/lib:/lib\" \\\n"
"../gcc-&gcc-version;/configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/usr/lib64 \\\n"
"    --libexecdir=/usr/lib64 \\\n"
"    --enable-languages=c,c++ \\\n"
"    --with-system-zlib \\\n"
"    --enable-install-libiberty \\\n"
"    --disable-bootstrap</userinput>"
msgstr ""
"<userinput>SED=sed CC=\"gcc -isystem /usr/include ${BUILD64}\" \\\n"
"CXX=\"g++ -isystem /usr/include ${BUILD64}\" \\\n"
"LDFLAGS=\"-Wl,-rpath-link,/usr/lib64:/lib64:/usr/lib:/lib\" \\\n"
"../gcc-&gcc-version;/configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/usr/lib64 \\\n"
"    --libexecdir=/usr/lib64 \\\n"
"    --enable-languages=c,c++ \\\n"
"    --with-system-zlib \\\n"
"    --enable-install-libiberty \\\n"
"    --disable-bootstrap</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/multilib/gcc.xml:130
msgid "Finally, move some misplaced files:"
msgstr "Enfin, déplacez des fichiers mal placés&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/gcc.xml:133
#, no-wrap
msgid ""
"<userinput>mv -v /usr/lib/libstdc++*gdb.py /usr/share/gdb/auto-load/usr/lib\n"
"mv -v /usr/lib64/libstdc++*gdb.py /usr/share/gdb/auto-load/usr/lib64</userinput>"
msgstr ""
"<userinput>mv -v /usr/lib/libstdc++*gdb.py /usr/share/gdb/auto-load/usr/lib\n"
"mv -v /usr/lib64/libstdc++*gdb.py /usr/share/gdb/auto-load/usr/lib64</userinput>"
