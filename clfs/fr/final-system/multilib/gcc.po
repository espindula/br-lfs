msgid "Installation of GCC"
msgstr "Installation de GCC"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/./final-system/multilib/gcc.xml:54
#, fuzzy, no-wrap
msgid ""
"<userinput>SED=sed CC=\"gcc -isystem /usr/include ${BUILD64}\" \\\n"
"CXX=\"g++ -isystem /usr/include ${BUILD64}\" \\\n"
"LDFLAGS=\"-Wl,-rpath-link,/usr/lib64:/lib64:/usr/lib:/lib\" \\\n"
"../gcc-&gcc-version;/configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/usr/lib64 \\\n"
"    --libexecdir=/usr/lib64 \\\n"
"    --enable-languages=c,c++ \\\n"
"    --with-system-zlib \\\n"
"    --enable-install-libiberty \\\n"
"    --disable-bootstrap</userinput>"
msgstr ""
"<userinput>CC=\"gcc -isystem /usr/include ${BUILD64}\" \\\n"
"CXX=\"g++ -isystem /usr/include ${BUILD64}\" \\\n"
"LDFLAGS=\"-Wl,-rpath-link,/usr/lib64:/lib64:/usr/lib:/lib\" \\\n"
"  ../gcc-&gcc-version;/configure --prefix=/usr --libdir=/usr/lib64 \\\n"
"    --libexecdir=/usr/lib64 --enable-shared --enable-threads=posix \\\n"
"    --enable-__cxa_atexit --enable-c99 --enable-long-long \\\n"
"    --enable-clocale=gnu --enable-languages=c,c++ --disable-libstdcxx-pch "
"\\\n"
"    --enable-cloog-backend=isl --disable-isl-version-check "
"--with-system-zlib \\\n"
"    --enable-checking=release --enable-libstdcxx-time \\\n"
"    --disable-install-libiberty</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/./final-system/multilib/gcc.xml:130
#, fuzzy
msgid "Finally, move some misplaced files:"
msgstr "Enfin, déplacez des fichiers mal placés&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/./final-system/multilib/gcc.xml:133
#, fuzzy, no-wrap
msgid ""
"<userinput>mv -v /usr/lib/libstdc++*gdb.py "
"/usr/share/gdb/auto-load/usr/lib\n"
"mv -v /usr/lib64/libstdc++*gdb.py "
"/usr/share/gdb/auto-load/usr/lib64</userinput>"
msgstr ""
"<userinput>mv -v /usr/lib/*gdb.py /usr/share/gdb/auto-load/usr/lib\n"
"mv -v /usr/lib64/*gdb.py /usr/share/gdb/auto-load/usr/lib64</userinput>"
