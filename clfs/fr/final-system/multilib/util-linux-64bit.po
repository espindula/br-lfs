msgid ""
"<userinput>CC=\"gcc ${BUILD64}\" ./configure \\\n"
"    ADJTIME_PATH=/var/lib/hwclock/adjtime \\\n"
"    --libdir=/lib64 \\\n"
"    --enable-write \\\n"
"    --disable-chfn-chsh \\\n"
"    --disable-login \\\n"
"    --disable-nologin \\\n"
"    --disable-su \\\n"
"    --disable-setpriv \\\n"
"    --disable-runuser \\\n"
"    --docdir=/usr/share/doc/util-linux-&util-linux-version;</userinput>"
