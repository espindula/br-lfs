#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/systemd-n32.xml:36
#, no-wrap
msgid ""
"<userinput>CC=\"gcc ${BUILDN32}\" PKG_CONFIG_PATH=\"${PKG_CONFIG_PATHN32}\" \\\n"
"./configure \\\n"
"    --prefix=/usr \\\n"
"    --sysconfdir=/etc \\\n"
"    --localstatedir=/var \\\n"
"    --libexecdir=/usr/lib32 \\\n"
"    --docdir=/usr/share/doc/systemd-&systemd-version; \\\n"
"    --with-rootprefix=\"\" \\\n"
"    --with-rootlibdir=/lib32 \\\n"
"    --enable-split-usr \\\n"
"    --disable-firstboot \\\n"
"    --disable-ldconfig \\\n"
"    --disable-lto \\\n"
"    --disable-sysusers \\\n"
"    --with-default-dnssec=no \\\n"
"    --with-kbd-loadkeys=/bin/loadkeys \\\n"
"    --with-kbd-setfont=/bin/setfont \\\n"
"    --with-dbuspolicydir=/etc/dbus-1/system.d \\\n"
"    --with-dbussessionservicedir=/usr/share/dbus-1/services \\\n"
"    --with-dbussystemservicedir=/usr/share/dbus-1/system-services \\\n"
"    --config-cache</userinput>"
