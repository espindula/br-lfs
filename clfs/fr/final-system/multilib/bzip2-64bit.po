#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/multilib/bzip2-64bit.xml:22
msgid "Installation of Bzip2"
msgstr "Installation de Bzip2"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/multilib/bzip2-64bit.xml:40
#, fuzzy
msgid ""
"We need to change the default lib path to <filename "
"class=\"directory\">lib64</filename>:"
msgstr ""
"Il nous faut modifier le chemin par défaut des bibliothèques en <filename "
"class=\"directory\">lib64</filename>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/bzip2-64bit.xml:43
#, no-wrap
msgid "<userinput>sed -i 's@/lib\\(/\\| \\|$\\)@/lib64\\1@g' Makefile</userinput>"
msgstr "<userinput>sed -i 's@/lib\\(/\\| \\|$\\)@/lib64\\1@g' Makefile</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/bzip2-64bit.xml:49
#, no-wrap
msgid ""
"<userinput>make -f Makefile-libbz2_so CC=\"gcc ${BUILD64}\" CXX=\"g++ ${BUILD64}\"\n"
"make clean</userinput>"
msgstr ""
"<userinput>make -f Makefile-libbz2_so CC=\"gcc ${BUILD64}\" CXX=\"g++ ${BUILD64}\"\n"
"make clean</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/bzip2-64bit.xml:60
#, no-wrap
msgid "<userinput>make CC=\"gcc ${BUILD64}\" CXX=\"g++ ${BUILD64}\"</userinput>"
msgstr "<userinput>make CC=\"gcc ${BUILD64}\" CXX=\"g++ ${BUILD64}\"</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/bzip2-64bit.xml:66
#, no-wrap
msgid ""
"<userinput>make CC=\"gcc ${BUILD64}\" CXX=\"g++ ${BUILD64}\" PREFIX=/usr "
"install</userinput>"
msgstr ""
"<userinput>make CC=\"gcc ${BUILD64}\" CXX=\"g++ ${BUILD64}\" PREFIX=/usr "
"install</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/bzip2-64bit.xml:72
#, no-wrap
msgid ""
"<userinput>cp -v bzip2-shared /bin/bzip2\n"
"cp -av libbz2.so* /lib64\n"
"ln -sv ../../lib64/libbz2.so.1.0 /usr/lib64/libbz2.so\n"
"rm -v /usr/bin/{bunzip2,bzcat,bzip2}\n"
"ln -sv bzip2 /bin/bunzip2\n"
"ln -sv bzip2 /bin/bzcat</userinput>"
msgstr ""
"<userinput>cp -v bzip2-shared /bin/bzip2\n"
"cp -av libbz2.so* /lib64\n"
"ln -sv ../../lib64/libbz2.so.1.0 /usr/lib64/libbz2.so\n"
"rm -v /usr/bin/{bunzip2,bzcat,bzip2}\n"
"ln -sv bzip2 /bin/bunzip2\n"
"ln -sv bzip2 /bin/bzcat</userinput>"
