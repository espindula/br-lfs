#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/multilib/xz-n32.xml:23
msgid "Installation of XZ Utils"
msgstr "Installation de XZ Utils"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/xz-n32.xml:29
#, no-wrap
msgid ""
"<userinput>CC=\"gcc ${BUILDN32}\" ./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/usr/lib32 \\\n"
"    --docdir=/usr/share/doc/xz-&xz-version;</userinput>"
msgstr ""
"<userinput>CC=\"gcc ${BUILDN32}\" ./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/usr/lib32 \\\n"
"    --docdir=/usr/share/doc/xz-&xz-version;</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/xz-n32.xml:54
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/xz-n32.xml:60
#, no-wrap
msgid ""
"<userinput>mv -v /usr/lib32/liblzma.so.* /lib32\n"
"ln -sfv ../../lib32/$(readlink /usr/lib32/liblzma.so) /usr/lib32/liblzma.so</userinput>"
msgstr ""
"<userinput>mv -v /usr/lib32/liblzma.so.* /lib32\n"
"ln -sfv ../../lib32/$(readlink /usr/lib32/liblzma.so) /usr/lib32/liblzma.so</userinput>"

