#. type: Content of: <sect1><title>
#: clfs-en/BOOK/final-system/multilib/attr.xml:11
msgid "Attr-&attr-version; 32 Bit Libraries"
msgstr "Bibliothèques 32 Bit Attr-&attr-version;"

#. type: Content of: <sect1><indexterm><primary>
#: clfs-en/BOOK/final-system/multilib/attr.xml:14
msgid "Attr"
msgstr "Attr"

#. type: Content of: <sect1><indexterm><secondary>
#: clfs-en/BOOK/final-system/multilib/attr.xml:15
msgid "32 Bit"
msgstr "32 Bit"

#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/multilib/attr.xml:23
msgid "Installation of Attr"
msgstr "Installation de Attr"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/attr.xml:45
#, no-wrap
msgid ""
"<userinput>CC=\"gcc ${BUILD32}\" \\\n"
"./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/lib \\\n"
"    --libexecdir=/usr/lib</userinput>"
msgstr ""
"<userinput>CC=\"gcc ${BUILD32}\" \\\n"
"./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/lib \\\n"
"    --libexecdir=/usr/lib</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/multilib/attr.xml:75
msgid ""
"Recreate the symlink in <filename class=\"directory\">/usr/lib </filename> "
"and remove the shared link from <filename class=\"directory\"> "
"/lib</filename>:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/attr.xml:79
#, no-wrap
msgid ""
"<userinput>ln -sfv ../../lib/$(readlink /lib/libattr.so) /usr/lib/libattr.so\n"
"rm -v /lib/libattr.so</userinput>"
msgstr ""
"<userinput>ln -sfv ../../lib/$(readlink /lib/libattr.so) /usr/lib/libattr.so\n"
"rm -v /lib/libattr.so</userinput>"

