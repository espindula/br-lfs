#. type: Content of: <sect1><title>
#: clfs-en/BOOK/final-system/multilib/gmp-64bit.xml:11
msgid "GMP-&gmp-version; 64 Bit"
msgstr "GMP-&gmp-version; 64 Bit"

#. type: Content of: <sect1><indexterm><primary>
#: clfs-en/BOOK/final-system/multilib/gmp-64bit.xml:14
msgid "GMP"
msgstr "GMP"

#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/multilib/gmp-64bit.xml:22
msgid "Installation of GMP"
msgstr "Installation de GMP"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/gmp-64bit.xml:42
#, no-wrap
msgid ""
"<userinput>CC=\"gcc -isystem /usr/include ${BUILD64}\" \\\n"
"CXX=\"g++ -isystem /usr/include ${BUILD64}\" \\\n"
"LDFLAGS=\"-Wl,-rpath-link,/usr/lib64:/lib64 ${BUILD64}\" \\\n"
"./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/usr/lib64 \\\n"
"    --enable-cxx \\\n"
"    --docdir=/usr/share/doc/gmp-&gmp-version;</userinput>"
msgstr ""
"<userinput>CC=\"gcc -isystem /usr/include ${BUILD64}\" \\\n"
"CXX=\"g++ -isystem /usr/include ${BUILD64}\" \\\n"
"LDFLAGS=\"-Wl,-rpath-link,/usr/lib64:/lib64 ${BUILD64}\" \\\n"
"./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/usr/lib64 \\\n"
"    --enable-cxx \\\n"
"    --docdir=/usr/share/doc/gmp-&gmp-version;</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/multilib/gmp-64bit.xml:91
msgid "Create the 64bit header file:"
msgstr "Créez le fichier d'entêtes 64 bits&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/gmp-64bit.xml:93
#, no-wrap
msgid "<userinput>mv -v /usr/include/gmp{,-64}.h</userinput>"
msgstr "<userinput>mv -v /usr/include/gmp{,-64}.h</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/multilib/gmp-64bit.xml:95
msgid "Finally, create a stub header in the place of the originals:"
msgstr "Finalement, créez un morceau d'entête à la place des originaux&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/gmp-64bit.xml:97
#, no-wrap
msgid ""
"<userinput>cat &gt; /usr/include/gmp.h &lt;&lt; \"EOF\"\n"
"/* gmp.h - Stub Header  */\n"
"#ifndef __STUB__GMP_H__\n"
"#define __STUB__GMP_H__\n"
"\n"
"#if defined(__x86_64__) || \\\n"
"    defined(__sparc64__) || \\\n"
"    defined(__arch64__) || \\\n"
"    defined(__powerpc64__) || \\\n"
"    defined (__s390x__)\n"
"# include \"gmp-64.h\"\n"
"#else\n"
"# include \"gmp-32.h\"\n"
"#endif\n"
"\n"
"#endif /* __STUB__GMP_H__ */\n"
"EOF</userinput>"
msgstr ""
"<userinput>cat &gt; /usr/include/gmp.h &lt;&lt; \"EOF\"\n"
"/* gmp.h - Stub Header  */\n"
"#ifndef __STUB__GMP_H__\n"
"#define __STUB__GMP_H__\n"
"\n"
"#if defined(__x86_64__) || \\\n"
"    defined(__sparc64__) || \\\n"
"    defined(__arch64__) || \\\n"
"    defined(__powerpc64__) || \\\n"
"    defined (__s390x__)\n"
"# include \"gmp-64.h\"\n"
"#else\n"
"# include \"gmp-32.h\"\n"
"#endif\n"
"\n"
"#endif /* __STUB__GMP_H__ */\n"
"EOF</userinput>"
