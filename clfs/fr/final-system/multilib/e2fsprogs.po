#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/e2fsprogs.xml:37
#, no-wrap
msgid ""
"<userinput>PKG_CONFIG_PATH=\"${PKG_CONFIG_PATH32}\" \\\n"
"CC=\"gcc ${BUILD32}\" \\\n"
"../configure \\\n"
"    --prefix=/usr \\\n"
"    --bindir=/bin \\\n"
"    --with-root-prefix=\"\" \\\n"
"    --enable-elf-shlibs \\\n"
"    --disable-libblkid \\\n"
"    --disable-libuuid \\\n"
"    --disable-fsck \\\n"
"    --disable-uuidd</userinput>"
msgstr ""
