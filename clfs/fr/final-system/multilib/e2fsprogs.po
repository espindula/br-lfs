#, fuzzy, no-wrap
msgid ""
"<userinput>PKG_CONFIG_PATH=\"${PKG_CONFIG_PATH32}\" \\\n"
"CC=\"gcc ${BUILD32}\" \\\n"
"../configure \\\n"
"    --prefix=/usr \\\n"
"    --bindir=/bin \\\n"
"    --with-root-prefix=\"\" \\\n"
"    --enable-elf-shlibs \\\n"
"    --disable-libblkid \\\n"
"    --disable-libuuid \\\n"
"    --disable-fsck \\\n"
"    --disable-uuidd</userinput>"
msgstr ""
"<userinput>CC=\"gcc ${BUILD32}\" PKG_CONFIG_PATH=\"${PKG_CONFIG_PATH32}\" \\\n"
"  ../configure --prefix=/usr --with-root-prefix=\"\" \\\n"
"    --enable-elf-shlibs --disable-libblkid \\\n"
"    --disable-libuuid --disable-fsck \\\n"
"    --disable-uuidd</userinput>"

