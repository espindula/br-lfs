msgid "Installation of E2fsprogs"
msgstr "Installation de E2fsprogs"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/multilib/e2fsprogs-n32.xml:25
msgid ""
"Change the library directory to <filename class=\"directory\">lib32</"
"filename>:"
msgstr ""
"Modifiez le chemin du répertoire des bibliothèques en <filename class="
"\"directory\">lib32</filename>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/e2fsprogs-n32.xml:28
#, no-wrap
msgid "<userinput>sed -i '/libdir.*=.*\\/lib/s@/lib@/lib32@g' configure</userinput>"
msgstr "<userinput>sed -i '/libdir.*=.*\\/lib/s@/lib@/lib32@g' configure</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/e2fsprogs-n32.xml:42
#, no-wrap
msgid ""
"<userinput>PKG_CONFIG_PATH=\"${PKG_CONFIG_PATHN32}\" \\\n"
"CC=\"gcc ${BUILDN32}\" \\\n"
"../configure \\\n"
"    --prefix=/usr \\\n"
"    --bindir=/bin \\\n"
"    --with-root-prefix=\"\" \\\n"
"    --enable-elf-shlibs \\\n"
"    --disable-libblkid \\\n"
"    --disable-libuuid \\\n"
"    --disable-fsck \\\n"
"    --disable-uuidd</userinput>"
msgstr ""
"<userinput>PKG_CONFIG_PATH=\"${PKG_CONFIG_PATHN32}\" \\\n"
"CC=\"gcc ${BUILDN32}\" \\\n"
"../configure \\\n"
"    --prefix=/usr \\\n"
"    --bindir=/bin \\\n"
"    --with-root-prefix=\"\" \\\n"
"    --enable-elf-shlibs \\\n"
"    --disable-libblkid \\\n"
"    --disable-libuuid \\\n"
"    --disable-fsck \\\n"
"    --disable-uuidd</userinput>"

