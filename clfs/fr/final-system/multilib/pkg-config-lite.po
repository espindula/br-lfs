#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/pkg-config-lite.xml:28
#, no-wrap
msgid ""
"<userinput>CC=\"gcc ${BUILD64}\" ./configure \\\n"
"    --prefix=/usr \\\n"
"    --docdir=/usr/share/doc/pkg-config-&pkg-config-lite-version; \\\n"
"    --with-pc-path=/usr/share/pkgconfig</userinput>"
msgstr ""
"<userinput>CC=\"gcc ${BUILD64}\" ./configure \\\n"
"    --prefix=/usr \\\n"
"    --docdir=/usr/share/doc/pkg-config-&pkg-config-lite-version; \\\n"
"    --with-pc-path=/usr/share/pkgconfig</userinput>"

