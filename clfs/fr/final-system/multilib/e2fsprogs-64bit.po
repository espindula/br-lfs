#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/multilib/e2fsprogs-64bit.xml:22
msgid "Installation of E2fsprogs"
msgstr "Installation de E2fsprogs"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/multilib/e2fsprogs-64bit.xml:24
msgid ""
"Change the library directory to <filename class=\"directory\">lib64</"
"filename>:"
msgstr ""
"Modifiez le chemin du répertoire des bibliothèques en <filename class="
"\"directory\">lib64</filename>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/e2fsprogs-64bit.xml:27
#, no-wrap
msgid "<userinput>sed -i '/libdir.*=.*\\/lib/s@/lib@/lib64@g' configure</userinput>"
msgstr "<userinput>sed -i '/libdir.*=.*\\/lib/s@/lib@/lib64@g' configure</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/e2fsprogs-64bit.xml:41
#, no-wrap
msgid ""
"<userinput>PKG_CONFIG_PATH=\"${PKG_CONFIG_PATH64}\" \\\n"
"CC=\"gcc ${BUILD64}\" \\\n"
"../configure \\\n"
"    --prefix=/usr \\\n"
"    --bindir=/bin \\\n"
"    --with-root-prefix=\"\" \\\n"
"    --enable-elf-shlibs \\\n"
"    --disable-libblkid \\\n"
"    --disable-libuuid \\\n"
"    --disable-fsck \\\n"
"    --disable-uuidd</userinput>"
msgstr ""
"<userinput>PKG_CONFIG_PATH=\"${PKG_CONFIG_PATH64}\" \\\n"
"CC=\"gcc ${BUILD64}\" \\\n"
"../configure \\\n"
"    --prefix=/usr \\\n"
"    --bindir=/bin \\\n"
"    --with-root-prefix=\"\" \\\n"
"    --enable-elf-shlibs \\\n"
"    --disable-libblkid \\\n"
"    --disable-libuuid \\\n"
"    --disable-fsck \\\n"
"    --disable-uuidd</userinput>"
