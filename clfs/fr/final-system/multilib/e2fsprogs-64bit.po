#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/e2fsprogs-64bit.xml:41
#, fuzzy, no-wrap
msgid ""
"<userinput>PKG_CONFIG_PATH=\"${PKG_CONFIG_PATH64}\" \\\n"
"CC=\"gcc ${BUILD64}\" \\\n"
"../configure \\\n"
"    --prefix=/usr \\\n"
"    --bindir=/bin \\\n"
"    --with-root-prefix=\"\" \\\n"
"    --enable-elf-shlibs \\\n"
"    --disable-libblkid \\\n"
"    --disable-libuuid \\\n"
"    --disable-fsck \\\n"
"    --disable-uuidd</userinput>"
msgstr ""
"<userinput>CC=\"gcc ${BUILD64}\" PKG_CONFIG_PATH=\"${PKG_CONFIG_PATH64}\" \\\n"
"  ../configure --prefix=/usr --with-root-prefix=\"\" \\\n"
"    --enable-elf-shlibs --disable-libblkid \\\n"
"    --disable-libuuid --disable-fsck \\\n"
"    --disable-uuidd</userinput>"
