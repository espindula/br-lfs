#, no-wrap
msgid ""
"<userinput>PKG_CONFIG_PATH=${PKG_CONFIG_PATH32} CC=\"gcc ${BUILD32}\" \\\n"
"./configure \\\n"
"    --prefix=/usr \\\n"
"    --bindir=/bin \\\n"
"    --sysconfdir=/etc \\\n"
"    --with-rootlibdir=/lib \\\n"
"    --libdir=/usr/lib \\\n"
"    --with-zlib \\\n"
"    --with-xz</userinput>"
