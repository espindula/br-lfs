#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/multilib/perl-n32.xml:23
msgid "Installation of Perl"
msgstr "Installation de Perl"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/perl-n32.xml:29
#, no-wrap
msgid ""
"<userinput>sed -i -e '/^BUILD_ZLIB/s/True/False/' \\\n"
"       -e '/^INCLUDE/s,\\./zlib-src,/usr/include,' \\\n"
"       -e '/^LIB/s,\\./zlib-src,/usr/lib32,' \\\n"
"       cpan/Compress-Raw-Zlib/config.in</userinput>"
msgstr ""
"<userinput>sed -i -e '/^BUILD_ZLIB/s/True/False/' \\\n"
"       -e '/^INCLUDE/s,\\./zlib-src,/usr/include,' \\\n"
"       -e '/^LIB/s,\\./zlib-src,/usr/lib32,' \\\n"
"       cpan/Compress-Raw-Zlib/config.in</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/multilib/perl-n32.xml:42
msgid ""
"We still need to tell perl to actually use <filename "
"class=\"directory\">lib32</filename>:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/perl-n32.xml:45
#, no-wrap
msgid ""
"<userinput>echo 'installstyle=\"lib32/perl5\"' &gt;&gt; "
"hints/linux.sh</userinput>"
msgstr ""
"<userinput>echo 'installstyle=\"lib32/perl5\"' &gt;&gt; "
"hints/linux.sh</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/perl-n32.xml:51
#, no-wrap
msgid ""
"<userinput>./configure.gnu \\\n"
"    --prefix=/usr \\\n"
"    -Dvendorprefix=/usr \\\n"
"    -Dman1dir=/usr/share/man/man1 \\\n"
"    -Dman3dir=/usr/share/man/man3 \\\n"
"    -Dpager=\"/bin/less -isR\" \\\n"
"    -Dlibpth=\"/usr/local/lib32 /lib32 /usr/lib32\" \\\n"
"    -Dcc=\"gcc ${BUILDN32}\" \\\n"
"    -Dusethreads \\\n"
"    -Duseshrplib</userinput>"
msgstr ""
"<userinput>./configure.gnu \\\n"
"    --prefix=/usr \\\n"
"    -Dvendorprefix=/usr \\\n"
"    -Dman1dir=/usr/share/man/man1 \\\n"
"    -Dman3dir=/usr/share/man/man3 \\\n"
"    -Dpager=\"/bin/less -isR\" \\\n"
"    -Dlibpth=\"/usr/local/lib32 /lib32 /usr/lib32\" \\\n"
"    -Dcc=\"gcc ${BUILDN32}\" \\\n"
"    -Dusethreads \\\n"
"    -Duseshrplib</userinput>"

#. type: Content of: <sect1><sect2><variablelist><title>
#: clfs-en/BOOK/final-system/multilib/perl-n32.xml:63
msgid "The meaning of the new configure option:"
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: clfs-en/BOOK/final-system/multilib/perl-n32.xml:66
msgid "<parameter>-Dlibpth=\"/usr/local/lib32 /lib32 /usr/lib32\"</parameter>"
msgstr ""

#. type: Content of:
#. <sect1><sect2><variablelist><varlistentry><listitem><para>
#: clfs-en/BOOK/final-system/multilib/perl-n32.xml:68
msgid "This tells Perl to link against the N32 libraries."
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/perl-n32.xml:118
#, no-wrap
msgid ""
"<userinput>mv -v /usr/bin/perl{,-n32}\n"
"mv -v /usr/bin/perl&perl-version;{,-n32}</userinput>"
msgstr ""
"<userinput>mv -v /usr/bin/perl{,-n32}\n"
"mv -v /usr/bin/perl&perl-version;{,-n32}</userinput>"

