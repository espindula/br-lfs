#: clfs-en/BOOK/final-system/multilib/isl-n32.xml:23
msgid "Installation of ISL"
msgstr "Installation de ISL"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/isl-n32.xml:29
#, no-wrap
msgid ""
"<userinput>CC=\"gcc -isystem /usr/include ${BUILDN32}\" \\\n"
"LDFLAGS=\"-Wl,-rpath-link,/usr/lib32:/lib32 ${BUILDN32}\" \\\n"
"./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/usr/lib32</userinput>"
msgstr ""
"<userinput>CC=\"gcc -isystem /usr/include ${BUILDN32}\" \\\n"
"LDFLAGS=\"-Wl,-rpath-link,/usr/lib32:/lib32 ${BUILDN32}\" \\\n"
"./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/usr/lib32</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/isl-n32.xml:63
#, no-wrap
msgid ""
"<userinput>mkdir -pv /usr/share/gdb/auto-load/usr/lib32\n"
"mv -v /usr/lib32/*gdb.py /usr/share/gdb/auto-load/usr/lib32</userinput>"
msgstr ""
"<userinput>mkdir -pv /usr/share/gdb/auto-load/usr/lib32\n"
"mv -v /usr/lib32/*gdb.py /usr/share/gdb/auto-load/usr/lib32</userinput>"

