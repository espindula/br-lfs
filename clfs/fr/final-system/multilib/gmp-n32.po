msgid "Installation of GMP"
msgstr "Installation de GMP"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/gmp-n32.xml:44
#, no-wrap
msgid ""
"<userinput>CC=\"gcc -isystem /usr/include ${BUILDN32}\" \\\n"
"CXX=\"g++ -isystem /usr/include ${BUILDN32}\" \\\n"
"LDFLAGS=\"-Wl,-rpath-link,/usr/lib32:/lib32 ${BUILDN32}\" \\\n"
"  ABI=n32 ./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/usr/lib32 \\\n"
"    --enable-cxx</userinput>"
msgstr ""
"<userinput>CC=\"gcc -isystem /usr/include ${BUILDN32}\" \\\n"
"CXX=\"g++ -isystem /usr/include ${BUILDN32}\" \\\n"
"LDFLAGS=\"-Wl,-rpath-link,/usr/lib32:/lib32 ${BUILDN32}\" \\\n"
"  ABI=n32 ./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/usr/lib32 \\\n"
"    --enable-cxx</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/multilib/gmp-n32.xml:76
#, fuzzy
msgid "Create the N32 header file:"
msgstr "Création du fichier d'entête N32:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/gmp-n32.xml:78
#, no-wrap
msgid "<userinput>mv -v /usr/include/gmp{,-n32}.h</userinput>"
msgstr "<userinput>mv -v /usr/include/gmp{,-n32}.h</userinput>"

