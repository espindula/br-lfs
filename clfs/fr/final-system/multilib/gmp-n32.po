#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/gmp-n32.xml:44
#, fuzzy, no-wrap
msgid ""
"<userinput>CC=\"gcc -isystem /usr/include ${BUILDN32}\" \\\n"
"CXX=\"g++ -isystem /usr/include ${BUILDN32}\" \\\n"
"LDFLAGS=\"-Wl,-rpath-link,/usr/lib32:/lib32 ${BUILDN32}\" \\\n"
"  ABI=n32 ./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/usr/lib32 \\\n"
"    --enable-cxx</userinput>"
msgstr ""
"<userinput>CC=\"gcc -isystem /usr/include ${BUILDN32}\" \\\n"
"CXX=\"g++ -isystem /usr/include ${BUILDN32}\" \\\n"
"LDFLAGS=\"-Wl,-rpath-link,/usr/lib32:/lib32 ${BUILDN32}\" \\\n"
"  ABI=n32 ./configure --prefix=/usr \\\n"
"    --libdir=/usr/lib32 --enable-cxx</userinput>"

