#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/multilib/xz-64bit.xml:22
msgid "Installation of XZ Utils"
msgstr "Installation de XZ Utils"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/xz-64bit.xml:28
#, no-wrap
msgid ""
"<userinput>CC=\"gcc ${BUILD64}\" ./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/usr/lib64 \\\n"
"    --docdir=/usr/share/doc/xz-&xz-version;</userinput>"
msgstr ""
"<userinput>CC=\"gcc ${BUILD64}\" ./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/usr/lib64 \\\n"
"    --docdir=/usr/share/doc/xz-&xz-version;</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/xz-64bit.xml:69
#, no-wrap
msgid ""
"<userinput>mv -v /usr/lib64/liblzma.so.* /lib64\n"
"ln -sfv ../../lib64/$(readlink /usr/lib64/liblzma.so) /usr/lib64/liblzma.so</userinput>"
msgstr ""
"<userinput>mv -v /usr/lib64/liblzma.so.* /lib64\n"
"ln -sfv ../../lib64/$(readlink /usr/lib64/liblzma.so) /usr/lib64/liblzma.so</userinput>"
