msgid ""
"<userinput>PKG_CONFIG_PATH=${PKG_CONFIG_PATH64} CC=\"gcc ${BUILD64}\" \\\n"
"./configure \\\n"
"    --prefix=/usr \\\n"
"    --bindir=/bin \\\n"
"    --sysconfdir=/etc \\\n"
"    --with-rootlibdir=/lib64 \\\n"
"    --libdir=/usr/lib64 \\\n"
"    --with-zlib \\\n"
"    --with-xz</userinput>"
