#. type: Content of: <sect1><title>
#: clfs-en/BOOK/final-system/multilib/acl-64bit.xml:11
msgid "Acl-&acl-version; 64 Bit"
msgstr "Acl-&acl-version; 64 Bit"

#. type: Content of: <sect1><indexterm><primary>
#: clfs-en/BOOK/final-system/multilib/acl-64bit.xml:14
msgid "Acl"
msgstr "Acl"

#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/multilib/acl-64bit.xml:22
msgid "Installation of Acl"
msgstr "Installation de Acl"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/acl-64bit.xml:44
#, no-wrap
msgid ""
"<userinput>CC=\"gcc ${BUILD64}\" \\\n"
"./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/lib64 \\\n"
"    --libexecdir=/usr/lib64</userinput>"
msgstr ""
"<userinput>CC=\"gcc ${BUILD64}\" \\\n"
"./configure \\\n"
"    --prefix=/usr \\\n"
"    --libdir=/lib64 \\\n"
"    --libexecdir=/usr/lib64</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/multilib/acl-64bit.xml:74
msgid ""
"Recreate the symlink in <filename class=\"directory\">/usr/lib64 </filename>"
" and remove the shared link from <filename class=\"directory\"> "
"/lib64</filename>:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/acl-64bit.xml:78
#, no-wrap
msgid ""
"<userinput>ln -sfv ../../lib64/$(readlink /lib64/libacl.so) /usr/lib64/libacl.so\n"
"rm -v /lib64/libacl.so</userinput>"
msgstr ""
"<userinput>ln -sfv ../../lib64/$(readlink /lib64/libacl.so) /usr/lib64/libacl.so\n"
"rm -v /lib64/libacl.so</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/multilib/acl-64bit.xml:85
#, no-wrap
msgid "<userinput>chmod 755 -v /lib64/libacl.so.1.1.0</userinput>"
msgstr "<userinput>chmod 755 -v /lib64/libacl.so.1.1.0</userinput>"
