#, no-wrap
msgid ""
"<userinput>make lib=lib32 install\n"
"chmod -v 755 /lib32/libcap.so.&libcap-version;\n"
"ln -sfv ../../lib32/$(readlink /lib32/libcap.so) /usr/lib32/libcap.so\n"
"rm -v /lib32/libcap.so\n"
"mv -v /lib32/libcap.a /usr/lib32</userinput>"
