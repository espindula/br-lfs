#. type: Content of: <sect1><title>
#: clfs-en/BOOK/final-system/multilib/adjusting.xml:11
msgid "Adjusting the Toolchain"
msgstr "Ajuster la chaîne d'outils"

#. type: Content of: <sect1><screen>
#: clfs-en/BOOK/final-system/multilib/adjusting.xml:17
#, no-wrap
msgid ""
"<userinput>gcc -dumpspecs | \\\n"
"perl -p -e 's@/tools/lib/ld@/lib/ld@g;' \\\n"
"     -e 's@/tools/lib64/ld@/lib64/ld@g;' \\\n"
"     -e 's@\\*startfile_prefix_spec:\\n@$_/usr/lib/ @g;' &gt; \\\n"
"     $(dirname $(gcc --print-libgcc-file-name))/specs</userinput>"
msgstr ""
"<userinput>gcc -dumpspecs | \\\n"
"perl -p -e 's@/tools/lib/ld@/lib/ld@g;' \\\n"
"     -e 's@/tools/lib64/ld@/lib64/ld@g;' \\\n"
"     -e 's@\\*startfile_prefix_spec:\\n@$_/usr/lib/ @g;' &gt; \\\n"
"     $(dirname $(gcc --print-libgcc-file-name))/specs</userinput>"

#. type: Content of: <sect1><caution><para>
#: clfs-en/BOOK/final-system/multilib/adjusting.xml:33
msgid "For 32 bit ABI:"
msgstr "Pour l'ABI 32 bits&nbsp;:"

#. type: Content of: <sect1><caution><screen>
#: clfs-en/BOOK/final-system/multilib/adjusting.xml:35
#, no-wrap
msgid ""
"<userinput>echo 'int main(){}' &gt; dummy.c\n"
"gcc ${BUILD32} dummy.c\n"
"readelf -l a.out | grep ': /lib'</userinput>"
msgstr ""
"<userinput>echo 'int main(){}' &gt; dummy.c\n"
"gcc ${BUILD32} dummy.c\n"
"readelf -l a.out | grep ': /lib'</userinput>"

#. type: Content of: <sect1><caution><para>
#: clfs-en/BOOK/final-system/multilib/adjusting.xml:47
msgid "For 64 bit ABI:"
msgstr "Pour l'ABI 64 bits&nbsp;:"

#. type: Content of: <sect1><caution><screen>
#: clfs-en/BOOK/final-system/multilib/adjusting.xml:49
#, no-wrap
msgid ""
"<userinput>echo 'main(){}' &gt; dummy.c\n"
"gcc ${BUILD64} dummy.c\n"
"readelf -l a.out | grep ': /lib'</userinput>"
msgstr ""
"<userinput>echo 'main(){}' &gt; dummy.c\n"
"gcc ${BUILD64} dummy.c\n"
"readelf -l a.out | grep ': /lib'</userinput>"

#.  specific to sparc64 
#. type: Content of: <sect1><caution><screen>
#: clfs-en/BOOK/final-system/multilib/adjusting.xml:58
#, no-wrap
msgid "<computeroutput>[Requesting program interpreter: /lib64/ld-linux.so.2]</computeroutput>"
msgstr "<computeroutput>[Requesting program interpreter: /lib64/ld-linux.so.2]</computeroutput>"

#. type: Content of: <sect1><caution><para>
#: clfs-en/BOOK/final-system/multilib/adjusting.xml:60
msgid ""
"Note that <filename class=\"directory\">/lib</filename> or <filename class="
"\"directory\">/lib64</filename> is now the prefix of our dynamic linker."
msgstr ""
"Notez que <filename class=\"directory\">/lib</filename> ou <filename class="
"\"directory\">/lib64</filename> est maintenant le préfixe de notre éditeur "
"de lien dynamique."
