#. type: Content of: <sect1><title>
#: clfs-en/BOOK/final-system/common/tar.xml:11
msgid "Tar-&tar-version;"
msgstr "Tar-&tar-version;"

#. type: Content of: <sect1><indexterm><primary>
#: clfs-en/BOOK/final-system/common/tar.xml:14
msgid "Tar"
msgstr "Tar"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/tar.xml:20
msgid "The Tar package contains an archiving program."
msgstr "Le paquet Tar contient un programme d'archivage."

#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/common/tar.xml:26
msgid "Installation of Tar"
msgstr "Installation de Tar"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/tar.xml:28
msgid "Prepare Tar for compilation:"
msgstr "Préparez la compilation de Tar&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/tar.xml:30
#, no-wrap
msgid ""
"<userinput>FORCE_UNSAFE_CONFIGURE=1 ./configure \\\n"
"    --prefix=/usr \\\n"
"    --bindir=/bin \\\n"
"    --libexecdir=/usr/sbin</userinput>"
msgstr ""
"<userinput>FORCE_UNSAFE_CONFIGURE=1 ./configure \\\n"
"    --prefix=/usr \\\n"
"    --bindir=/bin \\\n"
"    --libexecdir=/usr/sbin</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/tar.xml:35
msgid "Compile the package:"
msgstr "Compilez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/tar.xml:37
#, no-wrap
msgid "<userinput>make</userinput>"
msgstr "<userinput>make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/tar.xml:39
msgid "To test the results, issue:"
msgstr "Pour tester les résultats, lancez&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/tar.xml:41
#, no-wrap
msgid "<userinput remap=\"test\">make check</userinput>"
msgstr "<userinput remap=\"test\">make check</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/tar.xml:43
msgid "Install the package:"
msgstr "Installez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/tar.xml:45
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/tar.xml:47
msgid "Install the documentation:"
msgstr "Installez la documentation&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/tar.xml:49
#, no-wrap
msgid "<userinput>make -C doc install-html docdir=/usr/share/doc/tar-&tar-version;</userinput>"
msgstr "<userinput>make -C doc install-html docdir=/usr/share/doc/tar-&tar-version;</userinput>"

