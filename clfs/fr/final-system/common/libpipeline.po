#. type: Content of: <sect1><indexterm><primary>
#: clfs-en/BOOK/final-system/common/libpipeline.xml:14
msgid "Libpipeline"
msgstr "Libpipeline"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/libpipeline.xml:20
msgid ""
"The Libpipeline package contains a library for manipulating pipelines of "
"subprocesses in a flexible and convenient way."
msgstr ""
"Le paquet Libpipeline contient une bibliothèque pour manipuler des pipelines "
"de sous-processus d'une manière flexible et pratique."

#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/common/libpipeline.xml:26
msgid "Installation of Libpipeline"
msgstr "Installation de Libpipeline"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/libpipeline.xml:28
msgid "Prepare Libpipeline for compilation:"
msgstr "Préparez la compilation de Libpipeline&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/libpipeline.xml:30
#, no-wrap
msgid ""
"<userinput>PKG_CONFIG_PATH=/tools/lib/pkgconfig \\\n"
"./configure \\\n"
"    --prefix=/usr</userinput>"
msgstr ""
"<userinput>PKG_CONFIG_PATH=/tools/lib/pkgconfig \\\n"
"./configure \\\n"
"    --prefix=/usr</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/libpipeline.xml:34
msgid "Compile the package:"
msgstr "Compilez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/libpipeline.xml:36
#, no-wrap
msgid "<userinput>make</userinput>"
msgstr "<userinput>make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/libpipeline.xml:38
msgid "To test the results, issue:"
msgstr "Pour tester les résultats, lancez&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/libpipeline.xml:40
#, no-wrap
msgid "<userinput remap=\"test\">make check</userinput>"
msgstr "<userinput remap=\"test\">make check</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/libpipeline.xml:42
msgid "Install the package:"
msgstr "Installez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/libpipeline.xml:44
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

