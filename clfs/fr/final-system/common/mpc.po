#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/mpc.xml:28
msgid "Prepare MPC for compilation:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/mpc.xml:30
#, no-wrap
msgid ""
"<userinput>CC=\"gcc -isystem /usr/include\" \\\n"
"LDFLAGS=\"-Wl,-rpath-link,/usr/lib:/lib\" \\\n"
"./configure \\\n"
"    --prefix=/usr \\\n"
"    --docdir=/usr/share/doc/mpc-&mpc-version;</userinput>"
