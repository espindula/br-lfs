#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/common/automake.xml:26
msgid "Installation of Automake"
msgstr "Installation de Automake"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/automake.xml:28
msgid ""
"Apply the following patch to fix outdated syntax that is no longer "
"recognized by Perl-&perl-version;."
msgstr ""
"Appliquez le correctif suivant pour corriger la syntaxe obsolète qui n'est "
"plus reconnue par Perl-&perl-version;."

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/automake.xml:31
#, no-wrap
msgid "<userinput>patch -Np1 -i ../&automake-perl-patch;</userinput>"
msgstr "<userinput>patch -Np1 -i ../&automake-perl-patch;</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/automake.xml:33
msgid "Prepare Automake for compilation:"
msgstr "Préparez la compilation de Automake&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/automake.xml:35
#, no-wrap
msgid ""
"<userinput>./configure \\\n"
"    --prefix=/usr \\\n"
"    --docdir=/usr/share/doc/automake-&automake-version;</userinput>"
msgstr ""
"<userinput>./configure \\\n"
"    --prefix=/usr \\\n"
"    --docdir=/usr/share/doc/automake-&automake-version;</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/automake.xml:39
msgid "Compile the package:"
msgstr "Compilez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/automake.xml:41
#, no-wrap
msgid "<userinput>make</userinput>"
msgstr "<userinput>make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/automake.xml:43
msgid "To test the results, issue:"
msgstr "Pour tester les résultats, lancez&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/automake.xml:45
#, no-wrap
msgid "<userinput remap=\"test\">make check</userinput>"
msgstr "<userinput remap=\"test\">make check</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/automake.xml:47
msgid "Install the package:"
