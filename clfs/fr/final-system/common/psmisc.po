#: clfs-en/BOOK/final-system/common/psmisc.xml:92
msgid "<command>peekfd</command>"
msgstr "<command>peekfd</command>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: clfs-en/BOOK/final-system/common/psmisc.xml:94
msgid "Peeks at file descriptors of running processes"
msgstr ""
"Recherche les descripteurs de fichiers des processus en cours d'exécution"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><indexterm><primary>
#: clfs-en/BOOK/final-system/common/psmisc.xml:96
msgid "peekfd"
msgstr "peekfd"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
#: clfs-en/BOOK/final-system/common/psmisc.xml:102
msgid "<command>prtstat</command>"
msgstr "<command>prtstat</command>"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><para>
#: clfs-en/BOOK/final-system/common/psmisc.xml:104
msgid "Prints information about a process"
msgstr "Affiche des informations sur un processus"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><indexterm><primary>
#: clfs-en/BOOK/final-system/common/psmisc.xml:106
