#. type: Content of: <sect1><title>
#: clfs-en/BOOK/final-system/common/pkg-config-lite.xml:11
msgid "Pkg-config-lite-&pkg-config-lite-version;"
msgstr ""

#. type: Content of: <sect1><indexterm><primary>
#: clfs-en/BOOK/final-system/common/pkg-config-lite.xml:14
msgid "Pkg-config-lite"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/pkg-config-lite.xml:20
msgid ""
"Pkg-config-lite is a tool to help you insert the correct compiler options on "
"the command line when compiling applications and libraries."
msgstr ""

#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/common/pkg-config-lite.xml:26
msgid "Installation of Pkg-config-lite"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/pkg-config-lite.xml:28
msgid "Prepare Pkg-config-lite for compilation:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/pkg-config-lite.xml:30
#, no-wrap
msgid ""
"<userinput>./configure \\\n"
"    --prefix=/usr \\\n"
"    --docdir=/usr/share/doc/pkg-config-&pkg-config-lite-version;</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/pkg-config-lite.xml:34
msgid "Compile the package:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/pkg-config-lite.xml:36
#, no-wrap
msgid "<userinput>make</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/pkg-config-lite.xml:38
msgid "To test the results, issue:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/pkg-config-lite.xml:40
#, no-wrap
msgid "<userinput remap=\"test\">make check</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/pkg-config-lite.xml:42
msgid "Install the package:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/pkg-config-lite.xml:44
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr ""

