#: clfs-en/BOOK/final-system/common/gawk.xml:26
msgid "Installation of Gawk"
msgstr "Installation de Gawk"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/gawk.xml:28
msgid "Prepare Gawk for compilation:"
msgstr "Préparez la compilation de Gawk&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/gawk.xml:30
#, no-wrap
msgid ""
"<userinput>./configure \\\n"
"    --prefix=/usr \\\n"
"    --libexecdir=/usr/lib</userinput>"
msgstr ""
"<userinput>./configure \\\n"
"    --prefix=/usr \\\n"
"    --libexecdir=/usr/lib</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/gawk.xml:34
msgid "Compile the package:"
msgstr "Compilez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/gawk.xml:36
#, no-wrap
msgid "<userinput>make</userinput>"
msgstr "<userinput>make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/gawk.xml:38
msgid "To test the results, issue:"
msgstr "Pour tester les résultats, lancez&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/gawk.xml:40
#, no-wrap
msgid "<userinput remap=\"test\">make check</userinput>"
msgstr "<userinput remap=\"test\">make check</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/gawk.xml:42
msgid "Install the package:"
msgstr "Installez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/gawk.xml:44
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/gawk.xml:46
msgid "Install the documentation:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/gawk.xml:48
#, no-wrap
msgid ""
"<userinput>mkdir -v /usr/share/doc/gawk-&gawk-version;\n"
"cp -v doc/{awkforai.txt,*.{eps,pdf,jpg}} /usr/share/doc/gawk-&gawk-version;</userinput>"
msgstr ""
"<userinput>mkdir -v /usr/share/doc/gawk-&gawk-version;\n"
"cp -v doc/{awkforai.txt,*.{eps,pdf,jpg}} /usr/share/doc/gawk-&gawk-version;</userinput>"

