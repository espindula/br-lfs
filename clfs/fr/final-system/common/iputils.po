#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/iputils.xml:20
msgid "The IPutils package contains programs for basic networking."
msgstr "Le paquet IPutils contient des programmes de base pour le réseau."

#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/common/iputils.xml:25
msgid "Installation of IPutils"
msgstr "Installation de IPutils"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/iputils.xml:27
msgid "Apply the following patch for man-pages and build edits:"
msgstr ""
"Appliquer le correctif suivant pour modifier les pages de manuel et la "
"construction&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/iputils.xml:29
#, no-wrap
msgid "<userinput>patch -Np1 -i ../&iputils-build-patch;</userinput>"
msgstr "<userinput>patch -Np1 -i ../&iputils-build-patch;</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/iputils.xml:31
msgid "Compile the package:"
msgstr "Compilez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/iputils.xml:33
#, no-wrap
msgid "<userinput>make TARGETS=\"clockdiff ping rdisc tracepath tracepath6 traceroute6\"</userinput>"
msgstr "<userinput>make TARGETS=\"clockdiff ping rdisc tracepath tracepath6 traceroute6\"</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/iputils.xml:35
msgid "This package does not come with a test suite."
msgstr "Ce paquet n'a pas de suite de tests."

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/iputils.xml:37
msgid "Install the package:"
msgstr "Installez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/iputils.xml:39
#, no-wrap
msgid ""
"<userinput>install -v -m755 ping /bin\n"
"install -v -m755 clockdiff /usr/bin\n"
"install -v -m755 rdisc /usr/bin\n"
"install -v -m755 tracepath /usr/bin\n"
"install -v -m755 trace{path,route}6 /usr/bin\n"
"install -v -m644 doc/*.8 /usr/share/man/man8\n"
"ln -sv ping /bin/ping4\n"
"ln -sv ping /bin/ping6</userinput>"
msgstr ""
"<userinput>install -v -m755 ping /bin\n"
"install -v -m755 clockdiff /usr/bin\n"
"install -v -m755 rdisc /usr/bin\n"
"install -v -m755 tracepath /usr/bin\n"
"install -v -m755 trace{path,route}6 /usr/bin\n"
"install -v -m644 doc/*.8 /usr/share/man/man8\n"
"ln -sv ping /bin/ping4\n"
"ln -sv ping /bin/ping6</userinput>"

