msgid "This package does not come with a test suite."
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/libcap.xml:34
msgid "Install the package:"
msgstr "Installez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/libcap.xml:36
#, no-wrap
msgid ""
"<userinput>make RAISE_SETFCAP=no install\n"
"chmod -v 755 /lib/libcap.so.&libcap-version;\n"
"ln -sfv ../../lib/$(readlink /lib/libcap.so) /usr/lib/libcap.so\n"
"rm -v /lib/libcap.so\n"
"mv -v /lib/libcap.a /usr/lib</userinput>"
msgstr ""
"<userinput>make RAISE_SETFCAP=no install\n"
"chmod -v 755 /lib/libcap.so.&libcap-version;\n"
"ln -sfv ../../lib/$(readlink /lib/libcap.so) /usr/lib/libcap.so\n"
"rm -v /lib/libcap.so\n"
"mv -v /lib/libcap.a /usr/lib</userinput>"

