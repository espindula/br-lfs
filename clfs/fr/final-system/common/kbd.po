#: clfs-en/BOOK/final-system/common/kbd.xml:74
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/kbd.xml:76
msgid ""
"Some of the programs from Kbd are used by systemd to initialize the system, "
"so those binaries need to be on the root partition:"
msgstr ""
"Certains programmes de Kbd sont utilisés par systemd pour initialiser le "
"système, donc ces binaires doivent être sur la partition racine&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/kbd.xml:80
#, no-wrap
msgid "<userinput>mv -v /usr/bin/{loadkeys,setfont} /bin</userinput>"
msgstr "<userinput>mv -v /usr/bin/{loadkeys,setfont} /bin</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/kbd.xml:82
msgid "Install the documentation:"
msgstr "Installez la documentation&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/kbd.xml:84
#, no-wrap
msgid ""
"<userinput>mkdir -v /usr/share/doc/kbd-&kbd-version;\n"
"cp -R -v docs/doc/* /usr/share/doc/kbd-&kbd-version;</userinput>"
msgstr ""
"<userinput>mkdir -v /usr/share/doc/kbd-&kbd-version;\n"
"cp -R -v docs/doc/* /usr/share/doc/kbd-&kbd-version;</userinput>"

