#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/gdbm.xml:30
msgid "Prepare GDBM for compilation:"
msgstr "Préparez la compilation de GDBM&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/gdbm.xml:32
#, no-wrap
msgid ""
"<userinput>./configure \\\n"
"    --prefix=/usr \\\n"
"    --enable-libgdbm-compat</userinput>"
