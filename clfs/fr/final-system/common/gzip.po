#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/common/gzip.xml:26
msgid "Installation of Gzip"
msgstr "Installation de Gzip"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/gzip.xml:28
msgid "Prepare Gzip for compilation:"
msgstr "Préparez la compilation de Gzip&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/gzip.xml:30
#, no-wrap
msgid ""
"<userinput>./configure \\\n"
"    --prefix=/usr \\\n"
"    --bindir=/bin</userinput>"
msgstr ""
"<userinput>./configure \\\n"
"    --prefix=/usr \\\n"
"    --bindir=/bin</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/gzip.xml:34
msgid "Compile the package:"
msgstr "Compilez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/gzip.xml:36
#, no-wrap
msgid "<userinput>make</userinput>"
msgstr "<userinput>make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/gzip.xml:38
msgid "To test the results, issue:"
msgstr "Pour tester les résultats, lancez&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/gzip.xml:40
#, no-wrap
msgid "<userinput remap=\"test\">make check</userinput>"
msgstr "<userinput remap=\"test\">make check</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/gzip.xml:42
msgid "Install the package:"
msgstr "Installez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/gzip.xml:44
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr "<userinput>make install</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/gzip.xml:46
msgid ""
"Now we will move some of the utilities to <filename "
"class=\"directory\">/usr/bin</filename> to meet FHS compliance:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/gzip.xml:49
#, no-wrap
msgid ""
"<userinput>mv -v /bin/{gzexe,uncompress} /usr/bin\n"
"mv -v /bin/z{egrep,cmp,diff,fgrep,force,grep,less,more,new} /usr/bin\n"
"\t</userinput>"
msgstr ""
"<userinput>mv -v /bin/{gzexe,uncompress} /usr/bin\n"
"mv -v /bin/z{egrep,cmp,diff,fgrep,force,grep,less,more,new} /usr/bin\n"
"\t</userinput>"

