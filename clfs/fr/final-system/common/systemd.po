#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/systemd.xml:174
#, no-wrap
msgid "<userinput>make install</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/systemd.xml:176
msgid "Install documentation files that are not installed by default:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/systemd.xml:178
#, no-wrap
msgid ""
"<userinput>install -v -m644 man/*.html "
"/usr/share/doc/systemd-&systemd-version;</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/systemd.xml:180
msgid "Remove an unnecessary directory:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/systemd.xml:182
#, no-wrap
msgid "<userinput>rm -rfv /usr/lib/rpm</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/systemd.xml:184
msgid "Create symlinks for backwards-compatibility with Sysvinit:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/systemd.xml:186
#, no-wrap
msgid ""
"<userinput>for tool in runlevel reboot shutdown poweroff halt telinit; do\n"
"  ln -sfv ../bin/systemctl /sbin/$tool\n"
"done\n"
"ln -sfv ../lib/systemd/systemd /sbin/init</userinput>"
msgstr ""

