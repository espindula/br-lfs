#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/systemd.xml:27
msgid ""
"Prevent the <filename>Makefile</filename> from trying to run "
"<command>setcap</command> on <command>systemd-detect-virt</command>, which "
"will fail if the kernel or file system does not support extended "
"capabilities:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/systemd.xml:32
#, no-wrap
msgid "<userinput>sed -i '/virt-install-hook /d' Makefile.in</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/systemd.xml:34
msgid ""
"The <filename>timesyncd.conf</filename> file contains a reference to a "
"non-existent <filename>timesyncd.conf(5)</filename> man page. Remove that "
"reference to avoid possible confusion:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/systemd.xml:38
#, no-wrap
msgid ""
"<userinput>sed -i '/timesyncd.conf/d' "
"src/timesync/timesyncd.conf.in</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/systemd.xml:40
msgid "Create a file to declare some variables"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/systemd.xml:42
#, no-wrap
msgid ""
"<userinput>cat &gt; config.cache &lt;&lt; \"EOF\"\n"
"<literal>KILL=\"/bin/kill\"\n"
"MOUNT_PATH=\"/bin/mount\"\n"
"UMOUNT_PATH=\"/bin/umount\"\n"
"SULOGIN=\"/sbin/sulogin\"\n"
"XSLTPROC=\"/usr/bin/xsltproc\"</literal>\n"
"EOF</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/systemd.xml:50
msgid "Prepare systemd for compilation:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/systemd.xml:52
#, no-wrap
msgid ""
"<userinput>./configure \\\n"
"    --prefix=/usr \\\n"
"    --sysconfdir=/etc \\\n"
"    --localstatedir=/var \\\n"
"    --libexecdir=/usr/lib \\\n"
"    --docdir=/usr/share/doc/systemd-&systemd-version; \\\n"
"    --with-rootprefix=\"\" \\\n"
"    --with-rootlibdir=/lib \\\n"
"    --enable-split-usr \\\n"
"    --disable-firstboot \\\n"
"    --disable-ldconfig \\\n"
"    --disable-lto \\\n"
"    --disable-sysusers \\\n"
"    --with-default-dnssec=no \\\n"
"    --with-kbd-loadkeys=/bin/loadkeys \\\n"
"    --with-kbd-setfont=/bin/setfont \\\n"
"    --with-dbuspolicydir=/etc/dbus-1/system.d \\\n"
"    --with-dbussessionservicedir=/usr/share/dbus-1/services \\\n"
"    --with-dbussystemservicedir=/usr/share/dbus-1/system-services \\\n"
"    --config-cache</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><title>
#: clfs-en/BOOK/final-system/common/systemd.xml:74
msgid "The meaning of the configure options:"
msgstr ""

#. type: Content of: <sect1><sect2><variablelist><varlistentry><term>
