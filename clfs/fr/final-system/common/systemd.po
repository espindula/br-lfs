#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/systemd.xml:160
msgid "Prevent a broken test case from running:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/systemd.xml:162
#, no-wrap
msgid ""
"<userinput remap=\"test\">sed -e 's@test/udev-test.pl @@'  \\\n"
"        -e 's@test-copy$(EXEEXT) @@' \\\n"
"        -i Makefile.in\n"
"</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/systemd.xml:167
msgid "To test the results, issue:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/common/systemd.xml:169
#, no-wrap
msgid ""
"<userinput remap=\"test\">sed -i \"s:minix:ext4:g\" "
"src/test/test-path-util.c\n"
"make check</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/common/systemd.xml:172
msgid "Install the package:"
