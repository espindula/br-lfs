msgid ""
"This bootloader is for the MIPS based cobalt servers RaQ, RaQ2, Qube, or the"
" Qube2."
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/mips/colo.xml:32
msgid "This patch fixes a relocation error when linking with Binutils:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/mips/colo.xml:35
#, no-wrap
msgid "<userinput>patch -Np1 -i ../&colo-relocation-patch;</userinput>"
msgstr "<userinput>patch -Np1 -i ../&colo-relocation-patch;</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/mips/colo.xml:37
msgid ""
"Disable -Werror in one of the <filename>Makefile</filename>s to prevent a "
"compile failure:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/mips/colo.xml:40
#, no-wrap
msgid "<userinput>sed -i '/^CFLAGS/s/-Werror //' stage2/Makefile</userinput>"
msgstr "<userinput>sed -i '/^CFLAGS/s/-Werror //' stage2/Makefile</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/mips/colo.xml:42
msgid "Compile the Colo package:"
msgstr "Compilez le paquet Colo&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/mips/colo.xml:44
#, no-wrap
msgid "<userinput>make</userinput>"
msgstr "<userinput>make</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/mips/colo.xml:46
msgid "Install the package:"
msgstr "Installez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/mips/colo.xml:48
#, no-wrap
msgid ""
"<userinput>install -dv  /usr/lib/colo/examples\n"
"install -v chain/colo-chain.elf /usr/lib/colo\n"
"install -v tools/lcdtools/e2fsck-lcd/e2fsck-lcd /sbin\n"
"install -v tools/lcdtools/e2fsck-lcd/e2fsck-lcd.8 /usr/man/man8\n"
"install -v tools/lcdtools/paneld/paneld /sbin\n"
"install -v tools/lcdtools/paneld/paneld.8 /usr/man/man8\n"
"install -v tools/lcdtools/putlcd/putlcd /sbin\n"
"install -v tools/lcdtools/putlcd/putlcd.8 /usr/man/man8\n"
"install -v examples/menu.colo /usr/lib/colo/examples\n"
"install -v examples/simple.colo /usr/lib/colo/examples\n"
"cp -v chain/colo-chain.elf /boot/vmlinux\n"
"gzip -9 /boot/vmlinux</userinput>"
msgstr ""
"<userinput>install -dv  /usr/lib/colo/examples\n"
"install -v chain/colo-chain.elf /usr/lib/colo\n"
"install -v tools/lcdtools/e2fsck-lcd/e2fsck-lcd /sbin\n"
"install -v tools/lcdtools/e2fsck-lcd/e2fsck-lcd.8 /usr/man/man8\n"
"install -v tools/lcdtools/paneld/paneld /sbin\n"
"install -v tools/lcdtools/paneld/paneld.8 /usr/man/man8\n"
"install -v tools/lcdtools/putlcd/putlcd /sbin\n"
"install -v tools/lcdtools/putlcd/putlcd.8 /usr/man/man8\n"
"install -v examples/menu.colo /usr/lib/colo/examples\n"
"install -v examples/simple.colo /usr/lib/colo/examples\n"
"cp -v chain/colo-chain.elf /boot/vmlinux\n"
"gzip -9 /boot/vmlinux</userinput>"

