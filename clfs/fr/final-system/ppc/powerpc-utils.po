msgid "Installation of Powerpc-Utils"
msgstr "Installation de Powerpc-Utils"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/./final-system/ppc/powerpc-utils.xml:30
#, fuzzy
msgid ""
"This package, originally pmac-utils, has issues with NewWorld "
"Macintoshes. The following patch fixes these issues and generally updates "
"the package:"
msgstr ""
"This package, originally pmac-utils, has issues with NewWorld "
"Macintoshes. The following patch fixes these issues and generally updates "
"the package:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/./final-system/ppc/powerpc-utils.xml:34
#, fuzzy, no-wrap
msgid "<userinput>patch -Np1 -i ../&powerpcutils-patch;</userinput>"
msgstr "<userinput>patch -Np1 -i ../&powerpcutils-patch;</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/./final-system/ppc/powerpc-utils.xml:36
#, fuzzy
msgid "Compile the needed programs:"
msgstr "Compile the needed programs:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/./final-system/ppc/powerpc-utils.xml:38
#, fuzzy, no-wrap
msgid "<userinput>make nvsetenv nvsetvol</userinput>"
msgstr "<userinput>make nvsetenv nvsetvol</userinput>"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/./final-system/ppc/powerpc-utils.xml:40
#, fuzzy
msgid "Install the package:"
msgstr "Installez le paquet&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/./final-system/ppc/powerpc-utils.xml:42
#, fuzzy, no-wrap
msgid ""
"<userinput>install -v -m755 nvsetenv nvsetvol /usr/sbin\n"
"install -v -m644 nvsetenv.8 nvsetvol.8 /usr/share/man/man8</userinput>"
msgstr ""
"<userinput>install -v -m755 nvsetenv nvsetvol /usr/sbin\n"
"install -v -m644 nvsetenv.8 nvsetvol.8 /usr/share/man/man8</userinput>"

