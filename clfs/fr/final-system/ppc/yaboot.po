#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/ppc/yaboot.xml:26
msgid "Installation of Yaboot"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/ppc/yaboot.xml:28
msgid "The following patch adds stub functions for newer e2fsprogs releases:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/ppc/yaboot.xml:30
#, no-wrap
msgid "<userinput>patch -Np1 -i ../&yaboot-stubfuncs-patch;</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/ppc/yaboot.xml:32
msgid "The following patch adds Parted support to yabootconfig:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/ppc/yaboot.xml:34
#, no-wrap
msgid "<userinput>patch -Np1 -i ../&yaboot-parted-patch;</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/ppc/yaboot.xml:36
msgid ""
"The supplied man pages have <filename "
"class=\"directory\">/usr/local</filename> in the text. This sed will correct "
"that:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/ppc/yaboot.xml:40
#, no-wrap
msgid "<userinput>sed -i 's%/usr/local%/usr%' man/*</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/ppc/yaboot.xml:42
msgid "Prevent the build from failing due to warnings with the following sed:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/ppc/yaboot.xml:45
#, no-wrap
msgid "<userinput>sed -i 's%-Werror%%' Makefile</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/ppc/yaboot.xml:47
msgid "Compile the package:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/ppc/yaboot.xml:49
#, no-wrap
msgid "<userinput>make PREFIX=/usr</userinput>"
msgstr ""

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/ppc/yaboot.xml:51
msgid "Install the package:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/ppc/yaboot.xml:53
#, no-wrap
msgid "<userinput>make PREFIX=/usr install</userinput>"
msgstr ""

