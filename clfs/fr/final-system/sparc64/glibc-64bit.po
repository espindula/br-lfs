#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/final-system/sparc64/glibc-64bit.xml:22
msgid "Installation of Glibc"
msgstr "Installation de Glibc"

#. type: Content of: <sect1><sect2><para>
#: clfs-en/BOOK/final-system/sparc64/glibc-64bit.xml:52
msgid ""
"For TLS support on the Sparc64 we will need to add the following line to "
"<filename>config.cache</filename>:"
msgstr ""
"Pour le support de TLS sar Sparc64, nous devrons ajouter la ligne suivante "
"dans <filename>config.cache</filename>&nbsp;:"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/sparc64/glibc-64bit.xml:55
#, no-wrap
msgid ""
"<userinput>echo \"libc_cv_sparc64_tls=yes\" &gt;&gt; "
"config.cache</userinput>"
msgstr ""
"<userinput>echo \"libc_cv_sparc64_tls=yes\" &gt;&gt; "
"config.cache</userinput>"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/final-system/sparc64/glibc-64bit.xml:61
#, no-wrap
msgid ""
"<userinput>CC=\"gcc ${BUILD64}\" CXX=\"g++ ${BUILD64}\" \\\n"
"../glibc-&glibc-version;/configure \\\n"
"    --prefix=/usr \\\n"
"    --enable-kernel=&glibc-kernel-version; \\\n"
"    --libexecdir=/usr/lib64/glibc \\\n"
"    --libdir=/usr/lib64 \\\n"
"    --enable-obsolete-rpc \\\n"
"    --enable-stack-protector=strong \\\n"
"    --cache-file=config.cache</userinput>"
msgstr ""
"<userinput>CC=\"gcc ${BUILD64}\" CXX=\"g++ ${BUILD64}\" \\\n"
"../glibc-&glibc-version;/configure \\\n"
"    --prefix=/usr \\\n"
"    --enable-kernel=&glibc-kernel-version; \\\n"
"    --libexecdir=/usr/lib64/glibc \\\n"
"    --libdir=/usr/lib64 \\\n"
"    --enable-obsolete-rpc \\\n"
"    --enable-stack-protector=strong \\\n"
"    --cache-file=config.cache</userinput>"
