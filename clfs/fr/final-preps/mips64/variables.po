#. type: Content of: <sect1><title>
#: clfs-en/BOOK/final-preps/mips64/variables.xml:11
msgid "Build Variables"
msgstr "Variables de construction"

#. type: Content of: <sect1><bridgehead>
#: clfs-en/BOOK/final-preps/mips64/variables.xml:13
msgid "Setting Host and Target"
msgstr "Initialisation de l'hôte et de la cible"

#. type: Content of: <sect1><screen>
#: clfs-en/BOOK/final-preps/mips64/variables.xml:31
#, no-wrap
msgid ""
"<userinput>export CLFS_TARGET=\"<replaceable>mips64el-unknown-linux-"
"gnu</replaceable>\"</userinput>"
msgstr ""
"<userinput>export CLFS_TARGET=\"<replaceable>mips64el-unknown-linux-"
"gnu</replaceable>\"</userinput>"

#. type: Content of: <sect1><screen>
#: clfs-en/BOOK/final-preps/mips64/variables.xml:37
#, no-wrap
msgid ""
"<userinput>export CLFS_TARGET=\"<replaceable>mips64-unknown-linux-"
"gnu</replaceable>\"</userinput>"
msgstr ""
"<userinput>export CLFS_TARGET=\"<replaceable>mips64-unknown-linux-"
"gnu</replaceable>\"</userinput>"

#. type: Content of: <sect1><screen>
#: clfs-en/BOOK/final-preps/mips64/variables.xml:43
#, no-wrap
msgid ""
"<userinput>export CLFS_TARGET32=\"$(echo ${CLFS_TARGET}| sed -e "
"'s/64//g')\"</userinput>"
msgstr ""
"<userinput>export CLFS_TARGET32=\"$(echo ${CLFS_TARGET}| sed -e "
"'s/64//g')\"</userinput>"

#. type: Content of: <sect1><screen>
#: clfs-en/BOOK/final-preps/mips64/variables.xml:49
#, no-wrap
msgid ""
"<userinput>export BUILD32=\"-mabi=32\"\n"
"export BUILDN32=\"-mabi=n32\"\n"
"export BUILD64=\"-mabi=64\"</userinput>"
msgstr ""
"<userinput>export BUILD32=\"-mabi=32\"\n"
"export BUILDN32=\"-mabi=n32\"\n"
"export BUILD64=\"-mabi=64\"</userinput>"

#. type: Content of: <sect1><bridgehead>
#: clfs-en/BOOK/final-preps/mips64/variables.xml:53
msgid "Copy settings to Environment"
msgstr "Copie des paramètres vers l'environnement"

#. type: Content of: <sect1><screen>
#: clfs-en/BOOK/final-preps/mips64/variables.xml:59
#, no-wrap
msgid ""
"<userinput>cat &gt;&gt; ~/.bashrc &lt;&lt; EOF\n"
"export CLFS_HOST=\"${CLFS_HOST}\"\n"
"export CLFS_TARGET=\"${CLFS_TARGET}\"\n"
"export CLFS_TARGET32=\"${CLFS_TARGET32}\"\n"
"export BUILD32=\"${BUILD32}\"\n"
"export BUILDN32=\"${BUILDN32}\"\n"
"export BUILD64=\"${BUILD64}\"\n"
"EOF</userinput>"
msgstr ""
"<userinput>cat &gt;&gt; ~/.bashrc &lt;&lt; EOF\n"
"export CLFS_HOST=\"${CLFS_HOST}\"\n"
"export CLFS_TARGET=\"${CLFS_TARGET}\"\n"
"export CLFS_TARGET32=\"${CLFS_TARGET32}\"\n"
"export BUILD32=\"${BUILD32}\"\n"
"export BUILDN32=\"${BUILDN32}\"\n"
"export BUILD64=\"${BUILD64}\"\n"
"EOF</userinput>"
