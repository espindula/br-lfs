#. type: Content of: <sect1><itemizedlist><listitem><screen>
#: clfs-en/BOOK/./the-end/downloadclient.xml:40
#, fuzzy, no-wrap
msgid ""
"<userinput>cat &gt; download.sh &lt;&lt; \"EOF\"\n"
"#!/bin/bash\n"
"\n"
"WGET_VERSION='1.14'\n"
"WGET_HOSTNAME='ftp.gnu.org'\n"
"exec {HTTP_FD}&lt;&gt;/dev/tcp/${WGET_HOSTNAME}/80\n"
"echo -ne \"GET /gnu/wget/wget-${WGET_VERSION}.tar.xz HTTP/1.1\\r\\nHost: "
"\"\\\n"
"    ${WGET_HOSTNAME}'\\r\\nUser-Agent: '\\\n"
"    'bash/'${BASH_VERSION}'\\r\\n\\r\\n'  &gt;&amp;${HTTP_FD}\n"
"sed -e '1,/^.$/d' &lt;&amp;${HTTP_FD} &gt;wget-${WGET_VERSION}.tar.xz\n"
"EOF</userinput>"
msgstr ""
"<userinput>cat &gt; download.sh &lt;&lt; \"EOF\"\n"
"#!/bin/bash\n"
"\n"
"WGET_VERSION='1.14'\n"
"WGET_HOSTNAME='ftp.gnu.org'\n"
"exec {HTTP_FD}&lt;&gt;/dev/tcp/${WGET_HOSTNAME}/80\n"
"echo -ne \"GET /gnu/wget/wget-${WGET_VERSION}.tar.xz HTTP/1.1\\r\\nHost: "
"\"\\\n"
"    ${WGET_HOSTNAME}'\\r\\nUser-Agent: '\\\n"
"    'bash/'${BASH_VERSION}'\\r\\n\\r\\n'  &gt;&amp;${HTTP_FD}\n"
"sed -e '1,/^.$/d' &lt;&amp;${HTTP_FD} &gt;wget-${WGET_VERSION}.tar.xz\n"
"EOF</userinput>"

#. type: Content of: <sect1><itemizedlist><listitem><para>
#: clfs-en/BOOK/./the-end/downloadclient.xml:53
#, fuzzy
msgid "GAWK"
msgstr "GAWK"

#. type: Content of: <sect1><itemizedlist><listitem><screen>
#: clfs-en/BOOK/./the-end/downloadclient.xml:54
#, fuzzy, no-wrap
msgid ""
"<userinput>cat &gt; gawkdl.sh &lt;&lt; \"EOF\"\n"
"#!/bin/bash\n"
"\n"
"gawk 'BEGIN {\n"
"  NetService = \"/inet/tcp/0/mirror.anl.gov/80\"\n"
"  print \"GET /pub/gnu/wget/wget-1.14.tar.xz\" |&amp; NetService\n"
"  while ((NetService |&amp; getline) &gt; 0)\n"
"    print $0\n"
"  close(NetService)\n"
"}' &gt; binary\n"
"\n"
"gawk '{q=p;p=$0}NR&gt;1{print q}END{ORS = \"\"; print p}' binary &gt; "
"wget-1.14.tar.xz\n"
"\n"
"rm binary\n"
"EOF</userinput>"
msgstr ""
"<userinput>cat &gt; gawkdl.sh &lt;&lt; \"EOF\"\n"
"#!/bin/bash\n"
"\n"
"gawk 'BEGIN {\n"
"  NetService = \"/inet/tcp/0/mirror.anl.gov/80\"\n"
"  print \"GET /pub/gnu/wget/wget-1.14.tar.xz\" |&amp; NetService\n"
"  while ((NetService |&amp; getline) > 0)\n"
"    print $0\n"
"  close(NetService)\n"
"}' > binary\n"
"\n"
"gawk '{q=p;p=$0}NR&gt;1{print q}END{ORS = \"\"; print p}' binary &gt; "
"wget-1.14.tar.xz \n"
"\n"
"rm binary\n"
"EOF</userinput>"

#. type: Content of: <sect1><itemizedlist><listitem><para>
#: clfs-en/BOOK/./the-end/downloadclient.xml:71
#, fuzzy
msgid "PERL with HTTP::Tiny (Included with final system PERL install)."
msgstr "PERL avec HTTP::Tiny (Inclus dans l'installation de PERL du système final)."

#. type: Content of: <sect1><itemizedlist><listitem><screen>
#: clfs-en/BOOK/./the-end/downloadclient.xml:72
#, fuzzy, no-wrap
msgid ""
"<userinput>cat &gt; download.pl &lt;&lt; \"EOF\"\n"
"#!/usr/bin/perl\n"
"\n"
"use HTTP::Tiny;\n"
"my $http = HTTP::Tiny->new;\n"
"my $response;\n"
"\n"
"$response = $http->mirror('http://ftp.gnu.org/gnu/wget/wget-1.14.tar.xz', "
"'wget-1.14.tar.xz');\n"
"die \"Failed!\\n\" unless $response->{success};\n"
"print \"Unchanged!\\n\" if $response->{status} eq '304';\n"
"EOF</userinput>"
msgstr ""
"<userinput>cat &gt; download.pl &lt;&lt; \"EOF\"\n"
"#!/usr/bin/perl\n"
"\n"
"use HTTP::Tiny;\n"
"my $http = HTTP::Tiny->new;\n"
"my $response;\n"
"\n"
"$response = $http->mirror('http://ftp.gnu.org/gnu/wget/wget-1.14.tar.xz', "
"'wget-1.14.tar.xz');\n"
"die \"Failed!\\n\" unless $response->{success};\n"
"print \"Unchanged!\\n\" if $response->{status} eq '304';      \n"
"EOF</userinput>"

