#. type: Content of: <sect1><title>
#: clfs-en/BOOK/system-config/sparc/fstab.xml:11
msgid "Creating the /etc/fstab File"
msgstr "Création du fichier /etc/fstab"

#. type: Content of: <sect1><indexterm><primary>
#: clfs-en/BOOK/system-config/sparc/fstab.xml:14
msgid "/etc/fstab"
msgstr "/etc/fstab"

#. type: Content of: <sect1><indexterm><secondary>
#: clfs-en/BOOK/system-config/sparc/fstab.xml:15
msgid "system config"
msgstr "configuration du système"

#. type: Content of: <sect1><screen>
#: clfs-en/BOOK/system-config/sparc/fstab.xml:22
#, no-wrap
msgid ""
"<userinput>cat &gt; /etc/fstab &lt;&lt; \"EOF\"\n"
"<literal># Begin /etc/fstab\n"
"\n"
"# file system  mount-point     type        options          dump  fsck\n"
"#                                                                 order\n"
"\n"
"/dev/<replaceable>[xxx]</replaceable>     /               <replaceable>[fff]</replaceable>       defaults         1     1\n"
"/dev/<replaceable>[yyy]</replaceable>     swap            swap        pri=1            0     0\n"
"proc           /proc           proc        defaults         0     0\n"
"sysfs          /sys            sysfs       defaults         0     0\n"
"devpts         /dev/pts        devpts      gid=&gid-tty;,mode=620  0     0\n"
"shm            /dev/shm        tmpfs       defaults         0     0\n"
"tmpfs          /run            tmpfs       defaults         0     0\n"
"devtmpfs       /dev            devtmpfs    mode=0755,nosuid 0     0\n"
"none           /proc/openprom  openpromfs  defaults         0     0\n"
"# End /etc/fstab</literal>\n"
"EOF</userinput>"
msgstr ""
