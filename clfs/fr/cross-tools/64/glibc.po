msgid "Installation of Glibc"
msgstr "Installation de Glibc"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/cross-tools/64/glibc.xml:41
#, no-wrap
msgid ""
"<userinput>BUILD_CC=\"gcc\" \\\n"
"CC=\"${CLFS_TARGET}-gcc ${BUILD64}\" \\\n"
"AR=\"${CLFS_TARGET}-ar\" \\\n"
"RANLIB=\"${CLFS_TARGET}-ranlib\" \\\n"
"../glibc-&glibc-version;/configure \\\n"
"    --prefix=/tools \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --enable-kernel=&glibc-kernel-version; \\\n"
"    --with-binutils=/cross-tools/bin \\\n"
"    --with-headers=/tools/include \\\n"
"    --enable-obsolete-rpc</userinput>"
msgstr ""
"<userinput>BUILD_CC=\"gcc\" \\\n"
"CC=\"${CLFS_TARGET}-gcc ${BUILD64}\" \\\n"
"AR=\"${CLFS_TARGET}-ar\" \\\n"
"RANLIB=\"${CLFS_TARGET}-ranlib\" \\\n"
"../glibc-&glibc-version;/configure \\\n"
"    --prefix=/tools \\\n"
"    --host=${CLFS_TARGET} \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --enable-kernel=&glibc-kernel-version; \\\n"
"    --with-binutils=/cross-tools/bin \\\n"
"    --with-headers=/tools/include \\\n"
"    --enable-obsolete-rpc</userinput>"

