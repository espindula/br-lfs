msgid ""
"Add the following variables to <filename>config.cache</filename> so they are "
"set properly:"
msgstr ""

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/cross-tools/sparc64/glibc-64bit.xml:39
#, no-wrap
msgid ""
"<userinput>cat &gt; config.cache &lt;&lt; \"EOF\"\n"
"libc_cv_forced_unwind=yes\n"
"libc_cv_c_cleanup=yes\n"
"libc_cv_sparc64_tls=yes\n"
"libc_cv_gnu89_inline=yes\n"
"EOF</userinput>"
msgstr ""

