msgid "Installation of Cross GCC Compiler with Static libgcc and no Threads"
msgstr "Installation of Cross GCC Compiler with Static libgcc and no Threads"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/./cross-tools/mips64-64/gcc-static.xml:76
#, fuzzy, no-wrap
msgid ""
"<userinput>AR=ar \\\n"
"LDFLAGS=\"-Wl,-rpath,/cross-tools/lib\" \\\n"
"../gcc-&gcc-version;/configure \\\n"
"    --prefix=/cross-tools \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_HOST} \\\n"
"    --target=${CLFS_TARGET} \\\n"
"    --with-sysroot=${CLFS} \\\n"
"    --with-local-prefix=/tools \\\n"
"    --with-native-system-header-dir=/tools/include \\\n"
"    --disable-shared \\\n"
"    --with-mpfr=/cross-tools \\\n"
"    --with-gmp=/cross-tools \\\n"
"    --with-isl=/cross-tools \\\n"
"    --with-mpc=/cross-tools \\\n"
"    --without-headers \\\n"
"    --with-newlib \\\n"
"    --disable-decimal-float \\\n"
"    --disable-libgomp \\\n"
"    --disable-libssp \\\n"
"    --disable-libatomic \\\n"
"    --disable-libitm \\\n"
"    --disable-libsanitizer \\\n"
"    --disable-libquadmath \\\n"
"    --disable-libvtv \\\n"
"    --disable-libcilkrts \\\n"
"    --disable-libcilkrts \\\n"
"    --disable-threads \\\n"
"    --disable-multilib \\\n"
"    --enable-languages=c \\\n"
"    --with-abi=64 \\\n"
"    --with-glibc-version=&glibc-version;</userinput>"
msgstr ""
"<userinput>AR=ar LDFLAGS=\"-Wl,-rpath,/cross-tools/lib\" \\\n"
"  ../gcc-&gcc-version;/configure --prefix=/cross-tools \\\n"
"  --build=${CLFS_HOST} --host=${CLFS_HOST} --target=${CLFS_TARGET} \\\n"
"  --with-sysroot=${CLFS} --with-local-prefix=/tools \\\n"
"  --with-native-system-header-dir=/tools/include --disable-nls \\\n"
"  --enable-shared --disable-static --enable-languages=c,c++ \\\n"
"  --enable-__cxa_atexit --enable-c99 --enable-long-long "
"--enable-threads=posix \\\n"
"  --disable-multilib --with-mpc=/cross-tools --with-mpfr=/cross-tools \\\n"
"  --with-gmp=/cross-tools --with-cloog=/cross-tools "
"--enable-cloog-backend=isl \\\n"
"  --with-isl=/cross-tools --disable-isl-version-check --with-system-zlib "
"\\\n"
"  --enable-checking=release --enable-libstdcxx-time "
"--with-abi=64</userinput>"

