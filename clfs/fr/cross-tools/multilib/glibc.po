#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/cross-tools/multilib/glibc.xml:23
msgid "Installation of Glibc"
msgstr "Installation de Glibc"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/cross-tools/multilib/glibc.xml:41
#, no-wrap
msgid ""
"<userinput>BUILD_CC=\"gcc\" CC=\"${CLFS_TARGET}-gcc ${BUILD32}\" \\\n"
"AR=\"${CLFS_TARGET}-ar\" RANLIB=\"${CLFS_TARGET}-ranlib\" \\\n"
"../glibc-&glibc-version;/configure \\\n"
"    --prefix=/tools \\\n"
"    --host=${CLFS_TARGET32} \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --enable-kernel=&glibc-kernel-version; \\\n"
"    --with-binutils=/cross-tools/bin \\\n"
"    --with-headers=/tools/include \\\n"
"    --enable-obsolete-rpc</userinput>"
msgstr ""
"<userinput>BUILD_CC=\"gcc\" CC=\"${CLFS_TARGET}-gcc ${BUILD32}\" \\\n"
"AR=\"${CLFS_TARGET}-ar\" RANLIB=\"${CLFS_TARGET}-ranlib\" \\\n"
"../glibc-&glibc-version;/configure \\\n"
"    --prefix=/tools \\\n"
"    --host=${CLFS_TARGET32} \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --enable-kernel=&glibc-kernel-version; \\\n"
"    --with-binutils=/cross-tools/bin \\\n"
"    --with-headers=/tools/include \\\n"
"    --enable-obsolete-rpc</userinput>"

