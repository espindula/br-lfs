#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/cross-tools/multilib/binutils.xml:23
msgid "Installation of Cross Binutils"
msgstr "Installation de Cross Binutils"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/cross-tools/multilib/binutils.xml:50
#, no-wrap
msgid ""
"<userinput>AR=ar AS=as \\\n"
"../binutils-&binutils-dir;/configure \\\n"
"    --prefix=/cross-tools \\\n"
"    --host=${CLFS_HOST} \\\n"
"    --target=${CLFS_TARGET} \\\n"
"    --with-sysroot=${CLFS} \\\n"
"    --with-lib-path=/tools/lib:/tools/lib64 \\\n"
"    --disable-nls \\\n"
"    --disable-static \\\n"
"    --enable-64-bit-bfd \\\n"
"    --enable-gold=yes \\\n"
"    --enable-plugins \\\n"
"    --enable-threads \\\n"
"    --disable-werror</userinput>"
msgstr ""
"<userinput>AR=ar AS=as \\\n"
"../binutils-&binutils-dir;/configure \\\n"
"    --prefix=/cross-tools \\\n"
"    --host=${CLFS_HOST} \\\n"
"    --target=${CLFS_TARGET} \\\n"
"    --with-sysroot=${CLFS} \\\n"
"    --with-lib-path=/tools/lib:/tools/lib64 \\\n"
"    --disable-nls \\\n"
"    --disable-static \\\n"
"    --enable-64-bit-bfd \\\n"
"    --enable-gold=yes \\\n"
"    --enable-plugins \\\n"
"    --enable-threads \\\n"
"    --disable-werror</userinput>"

