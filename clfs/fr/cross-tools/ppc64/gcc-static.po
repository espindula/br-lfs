#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/cross-tools/ppc64/gcc-static.xml:23
msgid "Installation of Cross GCC Compiler with Static libgcc and no Threads"
msgstr ""
"Installation du compilateur croisé GCC avec la libgcc statique et sans les "
"threads"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/cross-tools/ppc64/gcc-static.xml:76
#, no-wrap
msgid ""
"<userinput>AR=ar \\\n"
"LDFLAGS=\"-Wl,-rpath,/cross-tools/lib\" \\\n"
"../gcc-&gcc-version;/configure \\\n"
"    --prefix=/cross-tools \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --host=${CLFS_HOST} \\\n"
"    --target=${CLFS_TARGET} \\\n"
"    --with-sysroot=${CLFS} \\\n"
"    --with-local-prefix=/tools \\\n"
"    --with-native-system-header-dir=/tools/include \\\n"
"    --disable-shared \\\n"
"    --with-mpfr=/cross-tools \\\n"
"    --with-gmp=/cross-tools \\\n"
"    --with-isl=/cross-tools \\\n"
"    --with-mpc=/cross-tools \\\n"
"    --without-headers \\\n"
"    --with-newlib \\\n"
"    --disable-decimal-float \\\n"
"    --disable-libgomp \\\n"
"    --disable-libssp \\\n"
"    --disable-libatomic \\\n"
"    --disable-libitm \\\n"
"    --disable-libsanitizer \\\n"
"    --disable-libquadmath \\\n"
"    --disable-libvtv \\\n"
"    --disable-libcilkrts \\\n"
"    --disable-libstdc++-v3 \\\n"
"    --disable-threads \\\n"
"    --disable-nls \\\n"
"    --disable-libmpx \\\n"
"    --enable-languages=c \\\n"
"    --with-glibc-version=&glibc-version;</userinput>"
msgstr ""
