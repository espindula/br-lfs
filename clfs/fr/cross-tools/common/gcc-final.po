#. type: Content of: <sect1><sect2><title>
#: clfs-en/BOOK/cross-tools/common/gcc-final.xml:23
msgid "Installation of GCC Cross Compiler"
msgstr "Installation de GCC Cross Compiler"

#. type: Content of: <sect1><sect2><screen>
#: clfs-en/BOOK/cross-tools/common/gcc-final.xml:63
#, no-wrap
msgid ""
"<userinput>AR=ar \\\n"
"LDFLAGS=\"-Wl,-rpath,/cross-tools/lib\" \\\n"
"../gcc-&gcc-version;/configure \\\n"
"    --prefix=/cross-tools \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --target=${CLFS_TARGET} \\\n"
"    --host=${CLFS_HOST} \\\n"
"    --with-sysroot=${CLFS} \\\n"
"    --with-local-prefix=/tools \\\n"
"    --with-native-system-header-dir=/tools/include \\\n"
"    --disable-nls \\\n"
"    --disable-static \\\n"
"    --enable-languages=c,c++ \\\n"
"    --disable-multilib \\\n"
"    --with-mpc=/cross-tools \\\n"
"    --with-mpfr=/cross-tools \\\n"
"    --with-gmp=/cross-tools \\\n"
"    --with-isl=/cross-tools</userinput>"
msgstr ""
"<userinput>AR=ar \\\n"
"LDFLAGS=\"-Wl,-rpath,/cross-tools/lib\" \\\n"
"../gcc-&gcc-version;/configure \\\n"
"    --prefix=/cross-tools \\\n"
"    --build=${CLFS_HOST} \\\n"
"    --target=${CLFS_TARGET} \\\n"
"    --host=${CLFS_HOST} \\\n"
"    --with-sysroot=${CLFS} \\\n"
"    --with-local-prefix=/tools \\\n"
"    --with-native-system-header-dir=/tools/include \\\n"
